import "_racing_fuel.mr"

units units()
constants constants()

label cycle(720.0 * units.deg)

label bore(78.0)
label stroke(156.0)
label compression_ratio(5.6)
label con_rod(261.0)
label compression_height(62.0 * units.mm)
label intake_valve_diameter(40.0)
label exhaust_valve_diameter(40.0)
label intake_valves(2.0)
label exhaust_valves(2.0)
label offset(20.0)

label cyl(4.0)
label row(1.0)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)

label redline(3000.0) // RPM

// Peugeot L3 valve timings
label IVL(9.0)
label EVL(9.0)
label IVO(-3.0 * units.deg) //BTDC
label IVC(40.0 * units.deg) //ABDC
label EVO(56.0 * units.deg) //BBDC
label EVC(12.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(900.0)
label piston_mass(700.0)
label crank_mass(15.0)
label flywheel_mass(3.0) // kg

label exhaust_delay_coeff(1.5)


import "engine_sim.mr"
import "_racing_fuel.mr"

units units()
constants constants()

label cycle(720.0 * units.deg)
label bore(104.775) // 4 1/8
label stroke(84.9312) // 3 11/32
label compression_ratio(12.0)
label con_rod(162.56) // 6.4
label compression_height(30.0 * units.mm) // 52 PH
label intake_valve_diameter(57.34) // 2.2575
label exhaust_valve_diameter(41.91) // 1.65 ?
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(8500.0) // RPM

label cyl(8.0)
label row(2.0)
label vee(90.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// Ford 1998 NASCAR
label IVL(17.27) // .680
label EVL(17.27) // ?
label IVO(87.0 * units.deg) //BTDC
label IVC(87.0 * units.deg) //ABDC
label EVO(84.0 * units.deg) //BBDC
label EVC(84.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(450.0) // ? g
label piston_mass(450.0) // ? g
label crank_mass(15.0) // ? kg
label flywheel_mass(4.0) // kg

label exhaust_delay_coeff(1.5)

public node eng_distributor {
    input wires;
    input timing_curve;
    input rev_limit: 8500 * units.rpm;
    input limiter_duration: 0.0001;
    alias output __out:
        ignition_module(timing_curve: timing_curve, rev_limit: rev_limit, limiter_duration: limiter_duration)
            .connect_wire(wires.wire1, rot * 0)
            .connect_wire(wires.wire3, rot * 1)
            .connect_wire(wires.wire7, rot * 2)
            .connect_wire(wires.wire2, rot * 3)
            .connect_wire(wires.wire6, rot * 4)
            .connect_wire(wires.wire5, rot * 5)
            .connect_wire(wires.wire4, rot * 6)
            .connect_wire(wires.wire8, rot * 7);
}

private node wires {
    output wire1: ignition_wire();
    output wire2: ignition_wire();
    output wire3: ignition_wire();
    output wire4: ignition_wire();
    output wire5: ignition_wire();
    output wire6: ignition_wire();
    output wire7: ignition_wire();
    output wire8: ignition_wire();
}


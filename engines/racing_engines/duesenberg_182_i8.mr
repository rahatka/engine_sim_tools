label cycle(720.0 * units.deg)

label bore(63.5) // 2 1/2
label stroke(117.475) // 4 5/8
label compression_ratio(5.6) // 5.6 ; 5.2 Ludvigsen
label con_rod(222.25) // 8 3/4
label compression_height(60.0 * units.mm) // ?
label intake_valve_diameter(38.1) // 1 1/2 ; 39.0 mm Ludvigsen
label exhaust_valve_diameter(28.575) // 1 1/8 ; 27.0 mm Ludvigsen
label intake_valves(1.0)
label exhaust_valves(2.0)

label cyl(8.0)
label row(1.0)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

label redline(5000.0) // RPM

// guesstimate
label IVL(11.176) // .440 ; 9.5 mm Ludvigsen
label EVL(9.652) // .380 ; 7.9 mm Ludvigsen
label IVO(5.0 * units.deg) //BTDC
label IVC(38.0 * units.deg) //ABDC
label EVO(38.0 * units.deg) //BBDC
label EVC(10.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(750.0) // g
label crank_mass(10.0) // kg
label flywheel_mass(3.0) // kg

label exhaust_delay_coeff(1.5)

public node eng_distributor {
    input wires;
    input timing_curve;
    input rev_limit: redline * 1.1 * units.rpm;
    input limiter_duration: 0.0001;
    alias output __out:
        ignition_module(timing_curve: timing_curve, rev_limit: rev_limit, limiter_duration: limiter_duration)
            .connect_wire(wires.wire1, rot * 0)
            .connect_wire(wires.wire5, rot * 1)
            .connect_wire(wires.wire3, rot * 2)
            .connect_wire(wires.wire7, rot * 3)
            .connect_wire(wires.wire4, rot * 4)
            .connect_wire(wires.wire8, rot * 5)
            .connect_wire(wires.wire2, rot * 6)
            .connect_wire(wires.wire6, rot * 7);
}

private node wires {
    output wire1: ignition_wire();
    output wire2: ignition_wire();
    output wire3: ignition_wire();
    output wire4: ignition_wire();
    output wire5: ignition_wire();
    output wire6: ignition_wire();
    output wire7: ignition_wire();
    output wire8: ignition_wire();
}


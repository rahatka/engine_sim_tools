import "engine_sim.mr"
import "../0s/_fuel_10.mr"

units units()
constants constants()

// Engine Sim V0.1.14A
// Mercedes 18/100 4.5-Litre Four (M93654) 1914 106 @ 3100 1.00 Atm
// 2_mb_18_100_i4
// Created by oror 2023
// 1-3-4-2

label cycle(720.0 * units.deg)

label bore(93.0)
label stroke(165.0)
label compression_ratio(5.4) // 4.89 appendix_5
label con_rod(330.0)
label compression_height(60.0 * units.mm) // ?
label intake_valve_diameter(43.0) // 48 appendix_5
label exhaust_valve_diameter(43.0)
label intake_valves(2.0)
label exhaust_valves(2.0)

label cyl(4.0)
label row(1.0)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)

label redline(3500.0) // RPM

// Mercedes 18/100 valve timings
label IVL(8.0)
label EVL(8.0)
label IVO(0.0 * units.deg) //BTDC
label IVC(35.0 * units.deg) //ABDC
label EVO(50.0 * units.deg) //BBDC
label EVC(9.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(1000.0)
label piston_mass(800.0)
label crank_mass(16.0)
label flywheel_mass(3.0) // kg

label exhaust_delay_coeff(1.5)

public node eng_distributor {
    input wires;
    input timing_curve;
    input rev_limit: 3750 * units.rpm;
    input limiter_duration: 0.0001;
    alias output __out:
        ignition_module(timing_curve: timing_curve, rev_limit: rev_limit, limiter_duration: limiter_duration)
            .connect_wire(wires.wire1, rot * 0)
            .connect_wire(wires.wire3, rot * 1)
            .connect_wire(wires.wire4, rot * 2)
            .connect_wire(wires.wire2, rot * 3);
}

private node wires {
    output wire1: ignition_wire();
    output wire2: ignition_wire();
    output wire3: ignition_wire();
    output wire4: ignition_wire();
}


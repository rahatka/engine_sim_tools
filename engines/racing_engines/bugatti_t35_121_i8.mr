// Engine Sim V0.1.14A
// Bugatti Type 35 2-Litre Eight 1924 100 @ 5000 1.00 Atm
// 4_bugatti_t35
// Created by oror 2023
// 1-5-2-6-3-7-4-8

import "engine_sim.mr"
import "_racing_fuel.mr"

units units()
constants constants()

label bore(60.0)
label stroke(88.0)
label compression_ratio(6.0)
label con_rod(185.0)
label compression_height(37.0 * units.mm) // ~
label intake_valve_diameter(23.5) // 25.5 late
label exhaust_valve_diameter(35.0)
label intake_valves(2.0)
label exhaust_valves(1.0)

label redline(5500.0) // RPM

label cyl(8.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// Bugatti T35 valve timings
label IVL(7.5)
label EVL(9.0)
label IVO(10.0 * units.deg) //BTDC
label IVC(35.0 * units.deg) //ABDC
label EVO(50.0 * units.deg) //BBDC
label EVC(20.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(650.0) // g
label crank_mass(10.0) // kg
label flywheel_mass(3.0) // kg

label exhaust_delay_coeff(1.5)

public node eng_distributor {
    input wires;
    input timing_curve;
    input rev_limit: 6000 * units.rpm;
    input limiter_duration: 0.00001;
    alias output __out:
        ignition_module(timing_curve: timing_curve, rev_limit: rev_limit, limiter_duration: limiter_duration)
            .connect_wire(wires.wire1, rot * 0)
            .connect_wire(wires.wire5, rot * 1)
            .connect_wire(wires.wire2, rot * 2)
            .connect_wire(wires.wire6, rot * 3)
            .connect_wire(wires.wire3, rot * 4)
            .connect_wire(wires.wire7, rot * 5)
            .connect_wire(wires.wire4, rot * 6)
            .connect_wire(wires.wire8, rot * 7);
}

private node wires {
    output wire1: ignition_wire();
    output wire2: ignition_wire();
    output wire3: ignition_wire();
    output wire4: ignition_wire();
    output wire5: ignition_wire();
    output wire6: ignition_wire();
    output wire7: ignition_wire();
    output wire8: ignition_wire();
}


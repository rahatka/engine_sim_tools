public node camshaft_builder {
    output intake_cam_0: _intake_cam_0;
    output exhaust_cam_0: _exhaust_cam_0;

    camshaft _intake_cam_0(base_radius: 13.7 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 16.5 * units.mm, lobe_profile: exhaust_lobe_profile)

    _intake_cam_0
        .add_lobe(ILC + 0 * rot)
        .add_lobe(ILC + 2 * rot)
        .add_lobe(ILC + 4 * rot)
        .add_lobe(ILC + 6 * rot)
        .add_lobe(ILC + 1 * rot)
        .add_lobe(ILC + 3 * rot)
        .add_lobe(ILC + 5 * rot)
        .add_lobe(ILC + 7 * rot)

    _exhaust_cam_0
        .add_lobe(ELC + 0 * rot)
        .add_lobe(ELC + 2 * rot)
        .add_lobe(ELC + 4 * rot)
        .add_lobe(ELC + 6 * rot)
        .add_lobe(ELC + 1 * rot)
        .add_lobe(ELC + 3 * rot)
        .add_lobe(ELC + 5 * rot)
        .add_lobe(ELC + 7 * rot)
}

public node head {
    // port_flow.py v1.5
    // intake port area: 6.9 cm²; saturated lift: 5.09 mm
    // exhaust port area: 7.6 cm²; saturated lift: 7.59 mm
    // cylinder volume: 248.8 cm³ (15.2 CI); engine volume: 1.991 L (121.5 CI)
    // chamber volume: 49.8 cm³ (3.0 CI)
    // 16 harmonic intake runner length: 18.6 cm; diameter: 2.7 cm
    // primary length: 31.6 cm, area: 8.4 cm², diameter: 3.3 cm
    // collector diameter: 5.8 cm, area: 26.8 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 49.763 * units.cc,
        intake_runner_volume: 103.4 * units.cc,
        intake_runner_cross_section_area: 5.6 * units.cm2,
        exhaust_runner_volume: 35.6 * units.cc,
        exhaust_runner_cross_section_area: 8.4 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "Bugatti Type 35",
        starter_torque: 50 * units.Nm,
        starter_speed: 400 * units.rpm,
        redline: redline * units.rpm,
        fuel: r20(),
        throttle_gamma: 1.5,
        jitter: 0.6,
        noise: 0.1,
        hf_gain: 0.006,
        simulation_frequency: 10000
    )

    wires wires()

    crankshaft c0(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: 3.2 * units.kg,
        mass: 8.0 * units.kg,
        friction_torque: 3.0 * units.Nm,
        moment_of_inertia: 0.20,
        tdc: 0.0
    )

    rod_journal rj0(angle: rot * 0 + rot90)
    rod_journal rj1(angle: rot * 2 + rot90)
    rod_journal rj2(angle: rot * 4 + rot90)
    rod_journal rj3(angle: rot * 6 + rot90)
    rod_journal rj4(angle: rot * 1 + rot90)
    rod_journal rj5(angle: rot * 3 + rot90)
    rod_journal rj6(angle: rot * 5 + rot90)
    rod_journal rj7(angle: rot * 7 + rot90)
    
    c0
        .add_rod_journal(rj0)
        .add_rod_journal(rj1)
        .add_rod_journal(rj2)
        .add_rod_journal(rj3)
        .add_rod_journal(rj4)
        .add_rod_journal(rj5)
        .add_rod_journal(rj6)
        .add_rod_journal(rj7)

    piston_parameters piston_params(
        mass: 400 * units.g,
        blowby: k_28inH2O(0.019),
        compression_height: compression_height,
        wrist_pin_position: 0.0,
        displacement: 0.0
    )

    connecting_rod_parameters cr_params(
        mass: con_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.g,
            length: con_rod * units.mm
        ),
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    label collector_cross_section_area(26.8)

    label exhaust_pipe_length_0(200.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 100.0) // Litres
    
    label spacing_factor(1.3)
    label flange_density(1.2 * exhaust_delay_coeff)
    
    intake intake(
        plenum_volume: 1.6 * units.L,
        plenum_cross_section_area: 22.2 * units.cm2,
        runner_length: 18.6 * units.cm,
        intake_flow_rate: k_carb(200.7),
        idle_flow_rate: k_carb(0.02),
        idle_throttle_plate_position: 0.992,
        runner_flow_rate: k_carb(55.2),
        velocity_decay: 0.05
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(351.2),
        collector_cross_section_area: 26.8 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 31.6 * units.cm,
        primary_flow_rate: k_carb(92.2),
        velocity_decay: 1.0,
        volume: exhaust_volume_0 * units.L
    )

    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_51.wav", volume: 0.001))
    
    // racing manifold
    label sh((20.0 / exhaust_delay_coeff) * units.cm)
    label pl0 ((2.5 * bore * spacing_factor / flange_density) * units.mm)
    label pl1 ((1.5 * bore * spacing_factor / flange_density) * units.mm)
    label pl2 ((1.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl3 ((1.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl4 ((1.0 * bore * spacing_factor / flange_density) * units.mm + sh)
    label pl5 ((1.0 * bore * spacing_factor / flange_density) * units.mm + sh)
    label pl6 ((1.5 * bore * spacing_factor / flange_density) * units.mm + sh)
    label pl7 ((2.5 * bore * spacing_factor / flange_density) * units.mm + sh)
    
    // "middle" manifold
    // label pl0 ((3.5 * bore * spacing_factor / flange_density) * units.mm)
    // label pl1 ((3.0 * bore * spacing_factor / flange_density) * units.mm)
    // label pl2 ((1.5 * bore * spacing_factor / flange_density) * units.mm)
    // label pl3 ((1.0 * bore * spacing_factor / flange_density) * units.mm)
    // label pl4 ((1.0 * bore * spacing_factor / flange_density) * units.mm)
    // label pl5 ((1.5 * bore * spacing_factor / flange_density) * units.mm)
    // label pl6 ((3.0 * bore * spacing_factor / flange_density) * units.mm)
    // label pl7 ((3.5 * bore * spacing_factor / flange_density) * units.mm)
 
    cylinder_bank b0(bank_params, angle: 0 * units.deg)
    b0
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj0,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl0,
            ignition_wire: wires.wire1
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj1,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl1,
            ignition_wire: wires.wire2
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj2,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl2,
            ignition_wire: wires.wire3
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj3,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl3,
            ignition_wire: wires.wire4
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj4,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl4,
            ignition_wire: wires.wire5
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj5,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl5,
            ignition_wire: wires.wire6
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj6,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl6,
            ignition_wire: wires.wire7
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj7,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl7,
            ignition_wire: wires.wire8
        )

    engine
        .add_cylinder_bank(b0)

    engine.add_crankshaft(c0)

    camshaft_builder camshaft()

    b0.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_0,
            exhaust_camshaft: camshaft.exhaust_cam_0
        )
    )

    // ignition_timing.py v0.3 from = 5.0, to = 30.0, i = 700, r = 5500
    function timing_curve(550.0 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 0.0 * units.deg)
        .add_sample(550.0 * units.rpm, 2.5 * units.deg)
        .add_sample(1100.0 * units.rpm, 5.0 * units.deg)
        .add_sample(1650.0 * units.rpm, 11.0 * units.deg)
        .add_sample(2200.0 * units.rpm, 15.8 * units.deg)
        .add_sample(2750.0 * units.rpm, 19.7 * units.deg)
        .add_sample(3300.0 * units.rpm, 22.8 * units.deg)
        .add_sample(3850.0 * units.rpm, 25.2 * units.deg)
        .add_sample(4400.0 * units.rpm, 27.2 * units.deg)
        .add_sample(4950.0 * units.rpm, 28.7 * units.deg)
        .add_sample(5500.0 * units.rpm, 30.0 * units.deg)
        .add_sample(6050.0 * units.rpm, -15.0 * units.deg)
        .add_sample(6600.0 * units.rpm, -60.0 * units.deg)

    engine.add_ignition_module(
        eng_distributor(
            wires: wires,
            timing_curve: timing_curve
        )
    )
}

// Bugatti Type 35
public node veh {
    alias output __out: vehicle;
    label car_mass(900.0) // 839.15 loaded
    vehicle vehicle(
        mass: car_mass * units.kg,
        drag_coefficient: 0.5,
        cross_sectional_area: 10.8 * (units.foot * units.foot),
        diff_ratio: 3.6,
        tire_radius: (737.0 / 2) * units.mm, // 29x5.0
        rolling_resistance: 20
        )
}

// Bugatti Type 35
private node trn {
    alias output __out:
        transmission(
            max_clutch_torque: 200 * units.Nm,
            max_clutch_flex: 30 * units.deg,
            limit_clutch_flex: true,
            clutch_stiffness: 10 * units.Nm / units.deg,
            clutch_damping: 5.0,
            simulate_flex: true
        )
        .add_gear(2.42)
        .add_gear(1.85)
        .add_gear(1.31)
        .add_gear(1.0);
}

run(
    engine: eng(),
    transmission: trn(),
    vehicle: veh()
)

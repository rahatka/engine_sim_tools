import "engine_sim.mr"
import "_racing_fuel.mr"

units units()
constants constants()

label cycle(720.0 * units.deg)

label bore(93.5)
label stroke(90.0)
label compression_ratio(12.5)
label con_rod(177.8) // 7
label compression_height(94.0 / 2 * units.mm) // 94 PH
label intake_valve_diameter(54.0)
label exhaust_valve_diameter(54.0) // ?
label intake_valves(1.0)
label exhaust_valves(1.0)

label cyl(4.0)
label row(1.0)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)

label redline(7500.0) // RPM

// IO 309
label IVL(13.5)
label EVL(13.5) // ?
label IVO(49.5 * units.deg) //BTDC
label IVC(79.5 * units.deg) //ABDC
label EVO(75.5 * units.deg) //BBDC
label EVC(53.5 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(500.0)
label piston_mass(350.0)
label crank_mass(8.0)
label flywheel_mass(2.0) // kg

label exhaust_delay_coeff(1.5)


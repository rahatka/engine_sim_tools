import "engine_sim.mr"
import "_racing_fuel.mr"

units units()
constants constants()

label cycle(720.0 * units.deg)

label bore(72.39) // 2.85
label stroke(66.675) // 2 5/8
label compression_ratio(10.5)
label con_rod(120.65) // 4 3/4
label compression_height(bore / 2 * units.mm) // ?
label intake_valve_diameter(34.29) // 1.35
label exhaust_valve_diameter(30.48) // ~ 1.2
label intake_valves(1.0)
label exhaust_valves(1.0)
label offset(0.0)

label cyl(4.0)
label row(1.0)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)

label redline(7500.0) // RPM

// FWA3
label IVL(9.525) // 3/8
label EVL(8.73125) // 11/32 ?
label IVO(30.0 * units.deg) //BTDC
label IVC(60.0 * units.deg) //ABDC
label EVO(60.0 * units.deg) //BBDC
label EVC(30.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(300.0)
label piston_mass(200.0)
label crank_mass(7.0)
label flywheel_mass(1.5) // kg

label exhaust_delay_coeff(1.5)


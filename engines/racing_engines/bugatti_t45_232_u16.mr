import "engine_sim.mr"
import "_racing_fuel.mr"

units units()
constants constants()

// Engine Sim V0.1.14A
// Bugatti T45 3.8L 1924 250 @ 4000
// Created by oror 2023
// 1-16-5-9-2-13-6-10-3-14-7-11-4-15-8-12

label bore(60.0)
label stroke(84.0)
label compression_ratio(6.0) // ?
label con_rod(185.0) // T39 ?
label compression_height(37.0 * units.mm) // ?
label intake_valve_diameter(25.5) // ? Type 43
label exhaust_valve_diameter(35.0) // ? Type 43
label intake_valves(2.0)
label exhaust_valves(1.0)

label redline(5000.0) // RPM

label cyl(16.0)
label row(2.0)
label cycle(720.0 * units.deg)
label separation(75.0 * units.mm)
label rot(cycle / cyl)
label rot45(45.0 * units.deg)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// Bugatti T35-T43 valve timings
label IVL(8.0) // ?
label EVL(9.0) // ?
label IVO(10.0 * units.deg) //BTDC
label IVC(35.0 * units.deg) //ABDC
label EVO(50.0 * units.deg) //BBDC
label EVC(20.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(700.0) // ?
label piston_mass(500.0) // ?
label crank_mass(10.0) // ?
label flywheel_mass(3.0) // kg

label exhaust_delay_coeff(1.5)

public node eng_distributor {
    input wires;
    input timing_curve;
    input rev_limit: 7000 * units.rpm;
    input limiter_duration: 0.00001;
    alias output __out:
        ignition_module(timing_curve: timing_curve, rev_limit: rev_limit, limiter_duration: limiter_duration)
            .connect_wire(wires.wire1, rot * 0)
            .connect_wire(wires.wire16, rot * 1)
            .connect_wire(wires.wire5, rot * 2)
            .connect_wire(wires.wire9, rot * 3)
            .connect_wire(wires.wire2, rot * 4)
            .connect_wire(wires.wire13, rot * 5)
            .connect_wire(wires.wire6, rot * 6)
            .connect_wire(wires.wire10, rot * 7)
            .connect_wire(wires.wire3, rot * 8)
            .connect_wire(wires.wire14, rot * 9)
            .connect_wire(wires.wire7, rot * 10)
            .connect_wire(wires.wire11, rot * 11)
            .connect_wire(wires.wire4, rot * 12)
            .connect_wire(wires.wire15, rot * 13)
            .connect_wire(wires.wire8, rot * 14)
            .connect_wire(wires.wire12, rot * 15);
}

private node wires {
    output wire1: ignition_wire();
    output wire2: ignition_wire();
    output wire3: ignition_wire();
    output wire4: ignition_wire();
    output wire5: ignition_wire();
    output wire6: ignition_wire();
    output wire7: ignition_wire();
    output wire8: ignition_wire();
    output wire9: ignition_wire();
    output wire10: ignition_wire();
    output wire11: ignition_wire();
    output wire12: ignition_wire();
    output wire13: ignition_wire();
    output wire14: ignition_wire();
    output wire15: ignition_wire();
    output wire16: ignition_wire();
}


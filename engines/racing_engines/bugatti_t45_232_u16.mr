public node camshaft_builder {
    output intake_cam_0: _intake_cam_0;
    output exhaust_cam_0: _exhaust_cam_0;

    output intake_cam_1: _intake_cam_1;
    output exhaust_cam_1: _exhaust_cam_1;

    camshaft _intake_cam_0(base_radius: 14.9 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 14.9 * units.mm, lobe_profile: exhaust_lobe_profile)
    
    camshaft _intake_cam_1(base_radius: 14.9 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_1(base_radius: 14.9 * units.mm, lobe_profile: exhaust_lobe_profile)

    _intake_cam_0
        .add_lobe(ILC + 0 * rot)
        .add_lobe(ILC + 4 * rot)
        .add_lobe(ILC + 8 * rot)
        .add_lobe(ILC + 12 * rot)
        .add_lobe(ILC + 2 * rot)
        .add_lobe(ILC + 6 * rot)
        .add_lobe(ILC + 10 * rot)
        .add_lobe(ILC + 14 * rot)

    _exhaust_cam_0
        .add_lobe(ELC + 0 * rot)
        .add_lobe(ELC + 4 * rot)
        .add_lobe(ELC + 8 * rot)
        .add_lobe(ELC + 12 * rot)
        .add_lobe(ELC + 2 * rot)
        .add_lobe(ELC + 6 * rot)
        .add_lobe(ELC + 10 * rot)
        .add_lobe(ELC + 14 * rot)
        
    _intake_cam_1
        .add_lobe(ILC + 3 * rot)
        .add_lobe(ILC + 7 * rot)
        .add_lobe(ILC + 11 * rot)
        .add_lobe(ILC + 15 * rot)
        .add_lobe(ILC + 5 * rot)
        .add_lobe(ILC + 9 * rot)
        .add_lobe(ILC + 13 * rot)
        .add_lobe(ILC + 1 * rot)

    _exhaust_cam_1
        .add_lobe(ELC + 3 * rot)
        .add_lobe(ELC + 7 * rot)
        .add_lobe(ELC + 11 * rot)
        .add_lobe(ELC + 15 * rot)
        .add_lobe(ELC + 5 * rot)
        .add_lobe(ELC + 9 * rot)
        .add_lobe(ELC + 13 * rot)
        .add_lobe(ELC + 1 * rot)
}

public node head {
    // port_flow.py v1.5
    // intake port area: 8.3 cm²; saturated lift: 5.60 mm
    // exhaust port area: 7.8 cm²; saturated lift: 7.69 mm
    // cylinder volume: 237.5 cm³ (14.5 CI); engine volume: 3.800 L (231.9 CI)
    // chamber volume: 47.5 cm³ (2.9 CI)
    // 16 harmonic intake runner length: 23.2 cm; diameter: 2.5 cm
    // primary length: 39.9 cm, area: 7.8 cm², diameter: 3.1 cm
    // collector diameter: 8.0 cm, area: 49.9 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 47.501 * units.cc,
        intake_runner_volume: 110.8 * units.cc,
        intake_runner_cross_section_area: 4.8 * units.cm2,
        exhaust_runner_volume: 36.9 * units.cc,
        exhaust_runner_cross_section_area: 7.8 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "Bugatti Type 45",
        starter_torque: 100 * units.Nm,
        starter_speed: 400 * units.rpm,
        redline: redline * units.rpm,
        fuel: m9b1(),
        throttle_gamma: 1.5,
        jitter: 0.8,
        noise: 0.1,
        hf_gain: 0.008,
        simulation_frequency: 8000,
        fluid_simulation_steps: 6,
        max_sle_solver_steps: 32,
        dyno_min_speed: 500 * units.rpm
    )

    wires wires()

    crankshaft c0(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: 3.2 * units.kg,
        mass: 16.0 * units.kg,
        friction_torque: 5.7 * units.Nm,
        moment_of_inertia: 0.3,
        position_x: -separation,
        tdc: 0.0
    )
    
    crankshaft c1(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: 3.2 * units.kg,
        mass: 16.0 * units.kg,
        friction_torque: 5.7 * units.Nm,
        moment_of_inertia: 0.3,
        position_x: separation,
        tdc: 0.0
    )

    rod_journal rj0_0(angle: rot * 0 + rot90)
    rod_journal rj1_0(angle: rot * 4 + rot90)
    rod_journal rj2_0(angle: rot * 8 + rot90)
    rod_journal rj3_0(angle: rot * 12 + rot90)
    rod_journal rj4_0(angle: rot * 2 + rot90)
    rod_journal rj5_0(angle: rot * 6 + rot90)
    rod_journal rj6_0(angle: rot * 10 + rot90)
    rod_journal rj7_0(angle: rot * 14 + rot90)
    
    rod_journal rj0_1(angle: rot * 3 + rot90)
    rod_journal rj1_1(angle: rot * 7 + rot90)
    rod_journal rj2_1(angle: rot * 11 + rot90)
    rod_journal rj3_1(angle: rot * 15 + rot90)
    rod_journal rj4_1(angle: rot * 5 + rot90)
    rod_journal rj5_1(angle: rot * 9 + rot90)
    rod_journal rj6_1(angle: rot * 13 + rot90)
    rod_journal rj7_1(angle: rot * 1 + rot90)
    
    c0
        .add_rod_journal(rj0_0)
        .add_rod_journal(rj1_0)
        .add_rod_journal(rj2_0)
        .add_rod_journal(rj3_0)
        .add_rod_journal(rj4_0)
        .add_rod_journal(rj5_0)
        .add_rod_journal(rj6_0)
        .add_rod_journal(rj7_0)
        
    c1
        .add_rod_journal(rj0_1)
        .add_rod_journal(rj1_1)
        .add_rod_journal(rj2_1)
        .add_rod_journal(rj3_1)
        .add_rod_journal(rj4_1)
        .add_rod_journal(rj5_1)
        .add_rod_journal(rj6_1)
        .add_rod_journal(rj7_1)

    piston_parameters piston_params(
        mass: 300 * units.g,
        blowby: k_28inH2O(0.019),
        compression_height: compression_height,
        wrist_pin_position: 0.0,
        displacement: 0.0
    )

    connecting_rod_parameters cr_params(
        mass: con_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.g,
            length: con_rod * units.mm
        ),
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    label collector_cross_section_area(24.9)

    label exhaust_pipe_length_0(150.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 100.0) // Litres
    
    label exhaust_pipe_length_1(150.0) // cm
    label exhaust_volume_1(collector_cross_section_area * exhaust_pipe_length_1 / 100.0) // Litres
    
    label spacing_factor(1.2)
    label flange_density(1.0 * exhaust_delay_coeff)

    intake intake(
        plenum_volume: 2.5 * units.L,
        plenum_cross_section_area: 38.1 * units.cm2,
        runner_length: 23.2 * units.cm,
        intake_flow_rate: k_carb(340.4),
        idle_flow_rate: k_carb(0.02),
        idle_throttle_plate_position: 0.994,
        runner_flow_rate: k_carb(45.7),
        velocity_decay: 0.05
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(294.3),
        collector_cross_section_area: 24.9 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 39.9 * units.cm,
        primary_flow_rate: k_carb(76.3),
        velocity_decay: 0.9,
        volume: exhaust_volume_0 * units.L
    )
    
    exhaust_system_parameters es_params1(
        outlet_flow_rate: k_carb(294.3),
        collector_cross_section_area: 24.9 * units.cm2,
        length: exhaust_pipe_length_1 * units.cm,
        primary_tube_length: 39.9 * units.cm,
        primary_flow_rate: k_carb(76.3),
        velocity_decay: 0.9,
        volume: exhaust_volume_1 * units.L
    )

    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_45.wav", volume: 0.001))
    exhaust_system exhaust1(es_params1, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_58.wav", volume: 0.001))
 
    label pl0 ((8.2 * bore * spacing_factor / flange_density) * units.mm)
    label pl1 ((7.2 * bore * spacing_factor / flange_density) * units.mm)
    label pl2 ((6.2 * bore * spacing_factor / flange_density) * units.mm)
    label pl3 ((5.2 * bore * spacing_factor / flange_density) * units.mm)
    label pl4 ((4.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl5 ((3.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl6 ((2.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl7 ((1.0 * bore * spacing_factor / flange_density) * units.mm)
    
    // "middle" manifold
    // label pl0 ((4.0 * bore * spacing_factor / flange_density) * units.mm)
    // label pl1 ((2.8 * bore * spacing_factor / flange_density) * units.mm)
    // label pl2 ((1.9 * bore * spacing_factor / flange_density) * units.mm)
    // label pl3 ((1.0 * bore * spacing_factor / flange_density) * units.mm)
    // label pl4 ((1.0 * bore * spacing_factor / flange_density) * units.mm)
    // label pl5 ((1.9 * bore * spacing_factor / flange_density) * units.mm)
    // label pl6 ((2.8 * bore * spacing_factor / flange_density) * units.mm)
    // label pl7 ((4.0 * bore * spacing_factor / flange_density) * units.mm)
 
    cylinder_bank b0(bank_params, angle: 0 * units.deg, position_x: -separation)
    b0
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj0_0,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl0,
            ignition_wire: wires.wire1
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj1_0,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl1,
            ignition_wire: wires.wire2
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj2_0,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl2,
            ignition_wire: wires.wire3
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj3_0,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl3,
            ignition_wire: wires.wire4
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj4_0,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl4,
            ignition_wire: wires.wire5
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj5_0,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl5,
            ignition_wire: wires.wire6
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj6_0,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl6,
            ignition_wire: wires.wire7
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj7_0,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl7,
            ignition_wire: wires.wire8
        )

    cylinder_bank b1(bank_params, angle: 0 * units.deg, position_x: separation)
    b1
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj0_1,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl0,
            ignition_wire: wires.wire9
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj1_1,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl1,
            ignition_wire: wires.wire10
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj2_1,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl2,
            ignition_wire: wires.wire11
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj3_1,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl3,
            ignition_wire: wires.wire12
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj4_1,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl4,
            ignition_wire: wires.wire13
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj5_1,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl5,
            ignition_wire: wires.wire14
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj6_1,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl6,
            ignition_wire: wires.wire15
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj7_1,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl7,
            ignition_wire: wires.wire16
        )

    engine
        .add_cylinder_bank(b0)
        .add_cylinder_bank(b1)

    engine
        .add_crankshaft(c0)
        .add_crankshaft(c1)

    camshaft_builder camshaft()

    b0.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_0,
            exhaust_camshaft: camshaft.exhaust_cam_0,
            flip_display: true
        )
    )
    
    b1.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_1,
            exhaust_camshaft: camshaft.exhaust_cam_1
        )
    )

    function timing_curve(333.3 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 5.0 * units.deg)
        .add_sample(333.3 * units.rpm, 5.0 * units.deg)
        .add_sample(666.7 * units.rpm, 9.0 * units.deg)
        .add_sample(1000.0 * units.rpm, 15.9 * units.deg)
        .add_sample(1333.3 * units.rpm, 21.4 * units.deg)
        .add_sample(1666.7 * units.rpm, 25.7 * units.deg)
        .add_sample(2000.0 * units.rpm, 29.2 * units.deg)
        .add_sample(2333.3 * units.rpm, 32.0 * units.deg)
        .add_sample(2666.7 * units.rpm, 34.2 * units.deg)
        .add_sample(3000.0 * units.rpm, 36.0 * units.deg)
        .add_sample(3333.3 * units.rpm, 37.4 * units.deg)
        .add_sample(3666.7 * units.rpm, 38.0 * units.deg)
        .add_sample(4000.0 * units.rpm, 38.0 * units.deg)
        .add_sample(4333.3 * units.rpm, 38.0 * units.deg)
        .add_sample(4666.7 * units.rpm, 38.0 * units.deg)
        .add_sample(5000.0 * units.rpm, 38.0 * units.deg)
        .add_sample(5333.3 * units.rpm, -15.0 * units.deg)
        .add_sample(5666.7 * units.rpm, -45.0 * units.deg)

    engine.add_ignition_module(
        eng_distributor(
            wires: wires,
            timing_curve: timing_curve
        )
    )
}

// T45
public node veh {
    alias output __out: vehicle;
    vehicle vehicle(
        mass: 1100  * units.kg,
        drag_coefficient: 0.4,
        cross_sectional_area: (1250 * units.mm) * (1050 * units.mm),
        diff_ratio: 2.8,
        tire_radius: (736.6 / 2) * units.mm,
        rolling_resistance: 20
        )
}

// T46
private node trn {
    alias output __out:
        transmission(
            max_clutch_torque: 500 * units.Nm,
            max_clutch_flex: 15 * units.deg,
            limit_clutch_flex: true,
            clutch_stiffness: 20 * units.Nm / units.deg,
            clutch_damping: 1.0,
            simulate_flex: true
        )
        .add_gear(2.51)
        .add_gear(1.40)
        .add_gear(1.0);
}

run(
    engine: eng(),
    transmission: trn(),
    vehicle: veh()
)
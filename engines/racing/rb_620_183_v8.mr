public node camshaft_builder {
    output intake_cam_0: _intake_cam_0;
    output exhaust_cam_0: _exhaust_cam_0;

    output intake_cam_1: _intake_cam_1;
    output exhaust_cam_1: _exhaust_cam_1;

    camshaft _intake_cam_0(base_radius: 18.6 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 18.6 * units.mm, lobe_profile: exhaust_lobe_profile)
    
    camshaft _intake_cam_1(base_radius: 18.6 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_1(base_radius: 18.6 * units.mm, lobe_profile: exhaust_lobe_profile)

    label rot(cycle / cyl)

    _intake_cam_0
        .add_lobe(ILC + 0 * rot)
        .add_lobe(ILC + 6 * rot)
        .add_lobe(ILC + 2 * rot)
        .add_lobe(ILC + 4 * rot)
    _exhaust_cam_0
        .add_lobe(ELC + 0 * rot)
        .add_lobe(ELC + 6 * rot)
        .add_lobe(ELC + 2 * rot)
        .add_lobe(ELC + 4 * rot)
        
    _intake_cam_1
        .add_lobe(ILC + 1 * rot + rot360)
        .add_lobe(ILC + 7 * rot + rot360)
        .add_lobe(ILC + 3 * rot + rot360)
        .add_lobe(ILC + 5 * rot + rot360)
    _exhaust_cam_1
        .add_lobe(ELC + 1 * rot + rot360)
        .add_lobe(ELC + 7 * rot + rot360)
        .add_lobe(ELC + 3 * rot + rot360)
        .add_lobe(ELC + 5 * rot + rot360)
}

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node head {
    // port_flow.py v1.7
    // intake port area: 10.8 cm²; saturated lift: 9.05 mm
    // exhaust port area: 7.7 cm²; saturated lift: 7.65 mm
    // cylinder volume: 374.4 cm³ (22.85 CI); engine volume: 2.996 L (182.80 CI)
    // chamber volume: 37.4 cm³ (2.29 CI); conrod/stroke 2.654; N.A.C.C. 39.20
    // 8 harmonic intake runner length: 20.7 cm; diameter: 4.0 cm
    // primary length: 15.0 cm, area: 8.5 cm², diameter: 3.3 cm
    // collector diameter: 8.1 cm, area: 51.0 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 37.445 * units.cc,
        intake_runner_volume: 260.1 * units.cc,
        intake_runner_cross_section_area: 12.5 * units.cm2,
        exhaust_runner_volume: 86.7 * units.cc,
        exhaust_runner_cross_section_area: 8.5 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "Repco-Brabham 620",
        starter_torque: 75 * units.Nm,
        starter_speed: 750 * units.rpm,
        redline: redline * units.rpm,
        fuel: r50(),
        throttle_gamma: 1.5,
        jitter: 0.8,
        noise: 0.2,
        hf_gain: 0.008,
        simulation_frequency: 8000
    )

    wires wires()
    
    connecting_rod_parameters cr_params(
        mass: con_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.g,
            length: con_rod * units.mm
        ),
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    crankshaft c0(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: 4 * units.kg,
        mass: 9 * units.kg,
        friction_torque: 4.5 * units.Nm,
        moment_of_inertia: 0.1,
        tdc: 0.0
    )

    rod_journal rj0(angle: rot * 0 + rot90 + vee / 2)
    rod_journal rj1(angle: rot * 6 + rot90 + vee / 2)
    rod_journal rj2(angle: rot * 2 + rot90 + vee / 2)
    rod_journal rj3(angle: rot * 4 + rot90 + vee / 2)
    
    c0
        .add_rod_journal(rj0)
        .add_rod_journal(rj1)
        .add_rod_journal(rj2)
        .add_rod_journal(rj3)

    piston_parameters piston_params(
        mass: piston_mass * units.g,
        blowby: k_28inH2O(0.028),
        compression_height: compression_height
    )

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    label collector_cross_section_area(25.5)

    label exhaust_pipe_length_0(105.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 100.0) // Litres
    
    label exhaust_pipe_length_1(100.0) // cm
    label exhaust_volume_1(collector_cross_section_area * exhaust_pipe_length_1 / 100.0) // Litres
    
    label spacing_factor(1.1)
    label flange_density(1.0 * exhaust_delay_coeff)
    
    intake intake(
        plenum_volume: 4.2 * units.L,
        plenum_cross_section_area: 50.1 * units.cm2,
        intake_flow_rate: k_carb(499.3),
        idle_flow_rate: k_carb(0.11),
        idle_throttle_plate_position: 0.9972,
        runner_flow_rate: k_carb(156.0),
        runner_length: 20.7 * units.cm,
        velocity_decay: 0.05
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(468.1),
        collector_cross_section_area: 25.5 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 15.0 * units.cm,
        primary_flow_rate: k_carb(263.3),
        velocity_decay: 0.7,
        volume: exhaust_volume_0 * units.L
    )
    
    exhaust_system_parameters es_params1(
        outlet_flow_rate: k_carb(468.1),
        collector_cross_section_area: 25.5 * units.cm2,
        length: exhaust_pipe_length_1 * units.cm,
        primary_tube_length: 15.0 * units.cm,
        primary_flow_rate: k_carb(263.3),
        velocity_decay: 0.7,
        volume: exhaust_volume_1 * units.L
    )

    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_45.wav", volume: 0.001))
    exhaust_system exhaust1(es_params1, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_45.wav", volume: 0.001))

    cylinder_bank b0(bank_params, angle: vee / 2)
    cylinder_bank b1(bank_params, angle: -vee / 2)
    
    label pl0 ((1.4 * bore * spacing_factor / flange_density) * units.mm)
    label pl1 ((1.7 * bore * spacing_factor / flange_density) * units.mm)
    label pl2 ((1.2 * bore * spacing_factor / flange_density) * units.mm)
    label pl3 ((1.0 * bore * spacing_factor / flange_density) * units.mm)

    b0
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj0,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl0,
            ignition_wire: wires.wire1
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj1,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl1,
            ignition_wire: wires.wire2
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj2,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl2,
            ignition_wire: wires.wire3
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj3,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl3,
            ignition_wire: wires.wire4
        )
        
    b1
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj0,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl3,
            ignition_wire: wires.wire5
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj1,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl2,
            ignition_wire: wires.wire6
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj2,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl1,
            ignition_wire: wires.wire7
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj3,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl0,
            ignition_wire: wires.wire8
        )

    engine
        .add_cylinder_bank(b0)
        .add_cylinder_bank(b1)

    engine.add_crankshaft(c0)

    camshaft_builder camshaft()

    b0.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_0,
            exhaust_camshaft: camshaft.exhaust_cam_0,
            flip_display: true
        )
    )
    b1.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_1,
            exhaust_camshaft: camshaft.exhaust_cam_1
        )
    )

    function timing_curve(533.3 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 0.0 * units.deg)
        .add_sample(533.3 * units.rpm, 1.5 * units.deg)
        .add_sample(1066.7 * units.rpm, 9.2 * units.deg)
        .add_sample(1600.0 * units.rpm, 15.2 * units.deg)
        .add_sample(2133.3 * units.rpm, 19.9 * units.deg)
        .add_sample(2666.7 * units.rpm, 23.5 * units.deg)
        .add_sample(3200.0 * units.rpm, 26.3 * units.deg)
        .add_sample(3733.3 * units.rpm, 28.4 * units.deg)
        .add_sample(4266.7 * units.rpm, 30.1 * units.deg)
        .add_sample(4800.0 * units.rpm, 31.4 * units.deg)
        .add_sample(5333.3 * units.rpm, 32.5 * units.deg)
        .add_sample(5866.7 * units.rpm, 33.3 * units.deg)
        .add_sample(6400.0 * units.rpm, 33.9 * units.deg)
        .add_sample(6933.3 * units.rpm, 34.3 * units.deg)
        .add_sample(7466.7 * units.rpm, 34.7 * units.deg)
        .add_sample(8000.0 * units.rpm, 35.0 * units.deg)
        .add_sample(8533.3 * units.rpm, -12.5 * units.deg)
        .add_sample(9066.7 * units.rpm, -25.0 * units.deg)

    engine.add_ignition_module(
        eng_distributor(
            wires: wires,
            timing_curve: timing_curve
        ))
}

// Ferrari 500 F2
public node veh {
    alias output __out: vehicle;
    vehicle vehicle(
        mass: 560 * units.kg,
        drag_coefficient: 0.4,
        cross_sectional_area: (1400 * units.mm) * (1050 * units.mm),
        diff_ratio: 3.57, // ?
        tire_radius: (685.8 / 2) * units.mm, // 5.50x16
        rolling_resistance: 20
    )
}

// Ferrari 750
private node trn {
    alias output __out:
        transmission(
            max_clutch_torque: 400 * units.Nm,
            max_clutch_flex: 10 * units.deg,
            limit_clutch_flex: true,
            clutch_stiffness: 20 * units.Nm / units.deg,
            clutch_damping: 2.0,
            simulate_flex: true
        )
        .add_gear(2.653)
        .add_gear(1.596)
        .add_gear(1.208)
        .add_gear(1.0);
}

run(
    engine: eng(),
    transmission: trn(),
    vehicle: veh()
)

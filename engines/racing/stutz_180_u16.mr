import "engine_sim.mr"
import "../../fuel/racing.mr"

units units()
constants constants()

label bore(55.5625) // 2 3/16
label stroke(76.2) // 3
label compression_ratio(9.0)
label con_rod(185.0) // 6
label compression_height(55.0 / 2 * units.mm)
label intake_valve_diameter(31.75) // 1 1/4
label exhaust_valve_diameter(30.1625) // 1 3/16
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(8500.0) // RPM

label cyl(16.0)
label row(2.0)
label cyl_base(cyl / row)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label rot_base(cycle / cyl_base)
label rot45(45.0 * units.deg)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label sb_shift(rot * 10)

// Miller 90
label IVL(8.73125) // 11/32
label EVL(8.73125) // 11/32
label IVO(5.0 * units.deg) //BTDC
label IVC(38.0 * units.deg) //ABDC
label EVO(35.0 * units.deg) //BBDC
label EVC(8.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(300.0) // ?
label piston_mass(150.0) // 4.5 oz
label crank_mass(8.0) // ?
label flywheel_mass(2.0) // kg

label exhaust_delay_coeff(1.5)

label separation(2.5 * units.inch) // ?
label vee(30.0 * units.deg)


private node add_lobes {
    input base;
    input shift: 0.0;
    input var;
    input this;
    alias output __out: this;

    this.add_lobe(base + var.s1 * rot_base + shift)
    this.add_lobe(base + var.s2 * rot_base + shift)
    this.add_lobe(base + var.s3 * rot_base + shift)
    this.add_lobe(base + var.s4 * rot_base + shift)
    this.add_lobe(base + var.s5 * rot_base + shift)
    this.add_lobe(base + var.s6 * rot_base + shift)
    this.add_lobe(base + var.s7 * rot_base + shift)
    this.add_lobe(base + var.s8 * rot_base + shift)
}

public node bank_builder {
    input var;
    input crankshaft;
    input phasing;
    input ignition;
    input intake;
    input exhaust;
    input x_shift;
    input incline;
    input flip: false;

    output bank: _b0;

    ignition_wire _wire1()
    ignition_wire _wire2()
    ignition_wire _wire3()
    ignition_wire _wire4()
    ignition_wire _wire5()
    ignition_wire _wire6()
    ignition_wire _wire7()
    ignition_wire _wire8()

    piston_parameters piston_params(
        mass: piston_mass * units.g,
        blowby: k_28inH2O(0.017),
        compression_height: compression_height,
        wrist_pin_position: 0.0,
        displacement: 0.0
    )

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    connecting_rod_parameters cr_params(
        mass: con_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.g,
            length: con_rod * units.mm
        ),
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    camshaft _intake_cam_0(base_radius: 14.6 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 14.6 * units.mm, lobe_profile: exhaust_lobe_profile)

    _intake_cam_0.add_lobes(ILC, phasing, var)
    _exhaust_cam_0.add_lobes(ELC, phasing, var)

    ignition.connect_wire(_wire1, rot_base * var.s1 + phasing)
    ignition.connect_wire(_wire2, rot_base * var.s2 + phasing)
    ignition.connect_wire(_wire3, rot_base * var.s3 + phasing)
    ignition.connect_wire(_wire4, rot_base * var.s4 + phasing)
    ignition.connect_wire(_wire5, rot_base * var.s5 + phasing)
    ignition.connect_wire(_wire6, rot_base * var.s6 + phasing)
    ignition.connect_wire(_wire7, rot_base * var.s7 + phasing)
    ignition.connect_wire(_wire8, rot_base * var.s8 + phasing)

    label spacing_factor(1.2)
    label flange_density(2.0 * exhaust_delay_coeff)

    label pl0 ((7.7 * bore * spacing_factor / flange_density) * units.mm)
    label pl1 ((7.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl2 ((5.7 * bore * spacing_factor / flange_density) * units.mm)
    label pl3 ((5.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl4 ((3.7 * bore * spacing_factor / flange_density) * units.mm)
    label pl5 ((3.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl6 ((1.7 * bore * spacing_factor / flange_density) * units.mm)
    label pl7 ((1.0 * bore * spacing_factor / flange_density) * units.mm)

    cylinder_bank _b0(bank_params, position_x: x_shift, angle: incline)

    _b0
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj0,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl0,
            ignition_wire: _wire1
        )
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj1,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl1,
            ignition_wire: _wire2
        )
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj2,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl2,
            ignition_wire: _wire3
        )
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj3,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl3,
            ignition_wire: _wire4
        )
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj4,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl4,
            ignition_wire: _wire5
        )
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj5,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl5,
            ignition_wire: _wire6
        )
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj6,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl6,
            ignition_wire: _wire7
        )
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj7,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl7,
            ignition_wire: _wire8
        )

    _b0.set_cylinder_head(
        head(
            intake_camshaft: _intake_cam_0,
            exhaust_camshaft: _exhaust_cam_0,
            flip_display: flip
        )
    )
}

public node head {
    // port_flow.py v1.7
    // intake port area: 6.2 cm²; saturated lift: 6.85 mm
    // exhaust port area: 5.6 cm²; saturated lift: 6.51 mm
    // cylinder volume: 184.8 cm³ (11.27 CI); engine volume: 2.956 L (180.40 CI)
    // chamber volume: 23.1 cm³ (1.41 CI); conrod/stroke 2.428; N.A.C.C. 30.62
    // 8 harmonic intake runner length: 23.1 cm; diameter: 2.9 cm
    // primary length: 15.6 cm, area: 6.7 cm², diameter: 2.9 cm
    // collector diameter: 7.4 cm, area: 43.2 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 23.095 * units.cc,
        intake_runner_volume: 154.4 * units.cc,
        intake_runner_cross_section_area: 6.7 * units.cm2,
        exhaust_runner_volume: 51.5 * units.cc,
        exhaust_runner_cross_section_area: 6.7 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node crank {
    input phasing;
    input var;
    input x_shift;
    input incline;

    output c: _c;
    output rj0: _rj0;
    output rj1: _rj1;
    output rj2: _rj2;
    output rj3: _rj3;
    output rj4: _rj4;
    output rj5: _rj5;
    output rj6: _rj6;
    output rj7: _rj7;

    label crank_moment(disk_moment_of_inertia(mass: crank_mass * units.kg, radius: (stroke / 2.0) * units.mm))
    label flywheel_moment(disk_moment_of_inertia(mass: flywheel_mass * units.kg, radius: (stroke * 3.5) * units.mm))
    label other_moment(disk_moment_of_inertia(mass: 1 * units.kg, radius: 1.0 * units.cm))

    crankshaft _c(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: flywheel_mass * units.kg,
        mass: crank_mass * units.kg,
        friction_torque: 2.2 * units.Nm,
        moment_of_inertia: crank_moment + flywheel_moment + other_moment,
        position_x: x_shift,
        tdc: 0.0
    )

    rod_journal _rj0(angle: rot_base * var.s1 + phasing + incline)
    rod_journal _rj1(angle: rot_base * var.s2 + phasing + incline)
    rod_journal _rj2(angle: rot_base * var.s3 + phasing + incline)
    rod_journal _rj3(angle: rot_base * var.s4 + phasing + incline)
    rod_journal _rj4(angle: rot_base * var.s5 + phasing + incline)
    rod_journal _rj5(angle: rot_base * var.s6 + phasing + incline)
    rod_journal _rj6(angle: rot_base * var.s7 + phasing + incline)
    rod_journal _rj7(angle: rot_base * var.s8 + phasing + incline)

    _c
        .add_rod_journal(_rj0)
        .add_rod_journal(_rj1)
        .add_rod_journal(_rj2)
        .add_rod_journal(_rj3)
        .add_rod_journal(_rj4)
        .add_rod_journal(_rj5)
        .add_rod_journal(_rj6)
        .add_rod_journal(_rj7)
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "Stutz Blackhawk Special",
        starter_torque: 100 * units.Nm,
        starter_speed: 250 * units.rpm,
        redline: redline * units.rpm,
        fuel: r20(),
        throttle_gamma: 1.5,
        jitter: 0.8,
        noise: 0.1,
        hf_gain: 0.007,
        simulation_frequency: 6500,
        dyno_min_speed: 700 * units.rpm
    )

    variant_0 variant()

    crank c0(rot90, variant, -separation, vee / 2)
    crank c1(rot90 + sb_shift, variant, separation, -vee / 2)

    bank_builder bank0(
        var: variant,
        crankshaft: c0,
        phasing: 0,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust0,
        x_shift: -separation,
        incline: vee / 2,
        flip: true
    )

    bank_builder bank1(
        var: variant,
        crankshaft: c1,
        phasing: sb_shift,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust1,
        x_shift: separation,
        incline: -vee / 2
    )

    ignition_module ignition(timing_curve: timing_curve, rev_limit: redline * 1.5 * units.rpm, limiter_duration: 0.03)

    label collector_cross_section_area(21.6)

    label exhaust_pipe_length_0(50.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 100.0) // Litres

    intake intake(
        plenum_volume: 4.0 * units.L,
        plenum_cross_section_area: 53.4 * units.cm2,
        runner_length: 23.1 * units.cm,
        intake_flow_rate: k_carb(471.1),
        idle_flow_rate: k_carb(0.02),
        idle_throttle_plate_position: 0.996,
        runner_flow_rate: k_carb(66.3),
        velocity_decay: 0.05
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(417.2),
        collector_cross_section_area: 21.6 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 15.6 * units.cm,
        primary_flow_rate: k_carb(110.8),
        velocity_decay: 0.8,
        volume: exhaust_volume_0 * units.L
    )

    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_45.wav", volume: 0.0001))
    exhaust_system exhaust1(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_58.wav", volume: 0.0001)) 

    function timing_curve(566.7 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 2.0 * units.deg)
        .add_sample(566.7 * units.rpm, 2.0 * units.deg)
        .add_sample(1133.3 * units.rpm, 9.7 * units.deg)
        .add_sample(1700.0 * units.rpm, 15.8 * units.deg)
        .add_sample(2266.7 * units.rpm, 20.4 * units.deg)
        .add_sample(2833.3 * units.rpm, 23.8 * units.deg)
        .add_sample(3400.0 * units.rpm, 26.4 * units.deg)
        .add_sample(3966.7 * units.rpm, 28.3 * units.deg)
        .add_sample(4533.3 * units.rpm, 29.8 * units.deg)
        .add_sample(5100.0 * units.rpm, 30.8 * units.deg)
        .add_sample(5666.7 * units.rpm, 31.6 * units.deg)
        .add_sample(6233.3 * units.rpm, 32.0 * units.deg)
        .add_sample(6800.0 * units.rpm, 32.0 * units.deg)
        .add_sample(7366.7 * units.rpm, 32.0 * units.deg)
        .add_sample(7933.3 * units.rpm, 32.0 * units.deg)
        .add_sample(8500.0 * units.rpm, 32.0 * units.deg)
        .add_sample(9066.7 * units.rpm, -12.5 * units.deg)
        .add_sample(9633.3 * units.rpm, -25.0 * units.deg)

    engine
        .add_crankshaft(c0.c)
        .add_crankshaft(c1.c)
        .add_cylinder_bank(bank0.bank)
        .add_cylinder_bank(bank1.bank)
        .add_ignition_module(ignition)
}

// Stutz Blackhawk Special https://archive.org/details/sim_motor-age_1928-02-16_53_7/page/30/mode/2up?q=%221-5-3-7-4-8-2-6%22
public node veh {
    alias output __out: vehicle;

    label car_mass(1270) // https://oldmachinepress.com/2013/02/23/lockhart-stutz-black-hawk-lsr-car/

    vehicle vehicle(
        mass: car_mass * units.kg,
        drag_coefficient: 0.29, // ?
        cross_sectional_area: 11.26 * (units.foot * units.foot),
        diff_ratio: 2.83,
        tire_radius: (837.946 / 2) * units.mm, // 30 x 5
        rolling_resistance: 0.06 * car_mass * 9.81
        )
}

// Stutz Blackhawk Special https://archive.org/details/sim_motor-age_1928-02-16_53_7/page/30/mode/2up?q=%221-5-3-7-4-8-2-6%22
private node trn {
    alias output __out:
        transmission(
            max_clutch_torque: 300 * units.Nm,
            max_clutch_flex: 15 * units.deg,
            limit_clutch_flex: true,
            clutch_stiffness: 50 * units.Nm / units.deg,
            clutch_damping: 1.0,
            simulate_flex: true
        )
        .add_gear(3.0) // ?
        .add_gear(1.5) // ?
        .add_gear(1.0);
}

run(
    engine: eng(),
    transmission: trn(),
    vehicle: veh()
)

units units()
constants constants()

label vee(45.0 * units.deg)
label bore(75.0)
label stroke(85.0)
label compression_ratio(9.2)
label con_rod(168.0)
label compression_height(26.7 * units.mm) // 59 PH
label intake_valve_diameter(35.0)
label exhaust_valve_diameter(32.0)
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(4500.0) // RPM

label cyl(16.0)
label row(2.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)

// AU Type C
label IVL(10.0)
label EVL(10.0) // ?
label IVO(20.0 * units.deg) //BTDC
label IVC(60.0 * units.deg) //ABDC
label EVO(60.0 * units.deg) //BBDC
label EVC(20.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label piston_mass(300.0) // g
label con_rod_mass(500.0) // g
label crank_mass(18.0) // kg
label flywheel_mass(5.0) // kg


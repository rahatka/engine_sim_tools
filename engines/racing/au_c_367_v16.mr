    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node head {
    // port_flow.py v1.5
    // intake port area: 7.6 cm²; saturated lift: 7.59 mm
    // exhaust port area: 6.4 cm²; saturated lift: 6.94 mm
    // cylinder volume: 375.5 cm³ (22.9 CI); engine volume: 6.008 L (366.6 CI)
    // chamber volume: 45.8 cm³ (2.8 CI)
    // 16 harmonic intake runner length: 17.3 cm; diameter: 3.3 cm
    // primary length: 30.5 cm, area: 6.7 cm², diameter: 2.9 cm
    // collector diameter: 11.7 cm, area: 106.7 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 45.795 * units.cc,
        intake_runner_volume: 145.1 * units.cc,
        intake_runner_cross_section_area: 8.4 * units.cm2,
        exhaust_runner_volume: 48.4 * units.cc,
        exhaust_runner_cross_section_area: 6.7 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

public node crank {
    input phasing;
    input var;

    output c: _c;
    output rj0: _rj0;
    output rj1: _rj1;
    output rj2: _rj2;
    output rj3: _rj3;
    output rj4: _rj4;
    output rj5: _rj5;
    output rj6: _rj6;
    output rj7: _rj7;

    label crank_moment(disk_moment_of_inertia(mass: crank_mass * units.kg, radius: (stroke / 2.0) * units.mm))
    label flywheel_moment(disk_moment_of_inertia(mass: flywheel_mass * units.kg, radius: (stroke * 3.5) * units.mm))
    label other_moment(disk_moment_of_inertia(mass: 1 * units.kg, radius: 1.0 * units.cm))

    crankshaft _c(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: flywheel_mass * units.kg,
        mass: crank_mass * units.kg,
        friction_torque: 9.0 * units.Nm,
        moment_of_inertia: crank_moment + flywheel_moment + other_moment,
        tdc: 0.0
    )

    rod_journal _rj0(angle: rot_base * var.s1 + rot90 + vee / 2)
    rod_journal _rj1(angle: rot_base * var.s2 + rot90 + vee / 2)
    rod_journal _rj2(angle: rot_base * var.s3 + rot90 + vee / 2)
    rod_journal _rj3(angle: rot_base * var.s4 + rot90 + vee / 2)
    rod_journal _rj4(angle: rot_base * var.s5 + rot90 + vee / 2)
    rod_journal _rj5(angle: rot_base * var.s6 + rot90 + vee / 2)
    rod_journal _rj6(angle: rot_base * var.s7 + rot90 + vee / 2)
    rod_journal _rj7(angle: rot_base * var.s8 + rot90 + vee / 2)

    _c
        .add_rod_journal(_rj0)
        .add_rod_journal(_rj1)
        .add_rod_journal(_rj2)
        .add_rod_journal(_rj3)
        .add_rod_journal(_rj4)
        .add_rod_journal(_rj5)
        .add_rod_journal(_rj6)
        .add_rod_journal(_rj7)
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "Auto Union Type C V16",
        starter_torque: 150 * units.Nm,
        starter_speed: 300 * units.rpm,
        redline: redline * units.rpm,
        fuel: r20(),
        throttle_gamma: 1.5,
        jitter: 0.5,
        noise: 0.1,
        hf_gain: 0.003,
        simulation_frequency: 7000,
        dyno_min_speed: 1000 * units.rpm,
        fluid_simulation_steps: 6,
        max_sle_solver_steps: 32
    )

    ////////////////////////////////////////
    ///////// change variants here /////////
    ////////////////////////////////////////
    variant_0 variant()

    crank c0(vee / 2 + rot90, variant)

    ignition_module ignition(timing_curve: timing_curve, rev_limit: redline * 1.5 * units.rpm, limiter_duration: 0.05)

    bank_builder bank0(
        var: variant,
        crankshaft: c0,
        incline: vee / 2,
        phasing: 0,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust0,
        flip: true
    )

    bank_builder bank1(
        var: variant,
        crankshaft: c0,
        incline: -vee / 2,
        phasing: vee,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust1
    )

    label collector_cross_section_area(53.4)

    label exhaust_pipe_length_0(75.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 100.0) // Litres
    
    label exhaust_pipe_length_1(77.0) // cm
    label exhaust_volume_1(collector_cross_section_area * exhaust_pipe_length_1 / 100.0) // Litres

    intake intake(
        plenum_volume: 5.1 * units.L,
        plenum_cross_section_area: 67.0 * units.cm2,
        runner_length: 17.3 * units.cm,
        intake_flow_rate: k_carb(506.9),
        idle_flow_rate: k_carb(0.02),
        idle_throttle_plate_position: 0.996,
        runner_flow_rate: k_carb(71.3),
        velocity_decay: 0.05
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(448.9),
        collector_cross_section_area: 53.4 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 30.5 * units.cm,
        primary_flow_rate: k_carb(119.2),
        velocity_decay: 0.7,
        volume: exhaust_volume_0 * units.L
    )
    
    exhaust_system_parameters es_params1(
        outlet_flow_rate: k_carb(448.9),
        collector_cross_section_area: 53.4 * units.cm2,
        length: exhaust_pipe_length_1 * units.cm,
        primary_tube_length: 30.5 * units.cm,
        primary_flow_rate: k_carb(119.2),
        velocity_decay: 0.7,
        volume: exhaust_volume_1 * units.L
    )
    
    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_14.wav", volume: 0.001))
    exhaust_system exhaust1(es_params1, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_14.wav", volume: 0.001))
 
    function timing_curve(300.0 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 0.0 * units.deg)
        .add_sample(300.0 * units.rpm, 0.0 * units.deg)
        .add_sample(600.0 * units.rpm, 3.8 * units.deg)
        .add_sample(900.0 * units.rpm, 10.7 * units.deg)
        .add_sample(1200.0 * units.rpm, 16.1 * units.deg)
        .add_sample(1500.0 * units.rpm, 20.2 * units.deg)
        .add_sample(1800.0 * units.rpm, 23.4 * units.deg)
        .add_sample(2100.0 * units.rpm, 25.8 * units.deg)
        .add_sample(2400.0 * units.rpm, 27.7 * units.deg)
        .add_sample(2700.0 * units.rpm, 29.2 * units.deg)
        .add_sample(3000.0 * units.rpm, 30.3 * units.deg)
        .add_sample(3300.0 * units.rpm, 31.1 * units.deg)
        .add_sample(3600.0 * units.rpm, 31.8 * units.deg)
        .add_sample(3900.0 * units.rpm, 32.3 * units.deg)
        .add_sample(4200.0 * units.rpm, 32.7 * units.deg)
        .add_sample(4500.0 * units.rpm, 33.0 * units.deg)
        .add_sample(4800.0 * units.rpm, -15.0 * units.deg)
        .add_sample(5100.0 * units.rpm, -45.0 * units.deg)

    engine
        .add_crankshaft(c0.c)
        .add_cylinder_bank(bank0.bank)
        .add_cylinder_bank(bank1.bank)
        .add_ignition_module(ignition)
}

// AU Type C
public node veh {
    alias output __out: vehicle;
    label car_mass(1100.0) // 1016.047 loaded
    vehicle vehicle(
        mass: car_mass * units.kg,
        drag_coefficient: 0.4,
        cross_sectional_area: 10.8 * (units.foot * units.foot),
        diff_ratio: 3.045, // 3.045
        tire_radius: (837.0 / 2) * units.mm, // 7.00x19
        rolling_resistance: 0.012 * car_mass * 9.81
        )
}

// AU Type C
private node trn {
    alias output __out:
        transmission(
            max_clutch_torque: 700 * units.Nm,
            max_clutch_flex: 30 * units.deg,
            limit_clutch_flex: true,
            clutch_stiffness: 20 * units.Nm / units.deg,
            clutch_damping: 1.0,
            simulate_flex: true
        )
        .add_gear(2.000) // 2.000
        .add_gear(1.292) // 1.288
        .add_gear(1.078) // 0.976
        .add_gear(0.965) // 0.967
        .add_gear(0.863); // 0.861
}

run(
    engine: eng(),
    transmission: trn(),
    vehicle: veh()
)

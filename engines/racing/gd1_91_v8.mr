label cycle(720.0 * units.deg)
label bore(62.0)
label stroke(62.0)
label compression_ratio(10.7)
label con_rod(120.0) // ?
label IVL(8.0) // ?
label EVL(8.0) // ?
label compression_height(25.0 * units.mm) // ?
label intake_valve_diameter(34.0) // ~
label exhaust_valve_diameter(30.0) // ~
label intake_valves(1.0)
label exhaust_valves(1.0)

label cyl(8.0)
label row(2.0)
label vee(110.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label rot(cycle / cyl)

label redline(11500.0) // RPM

// Vanwall
label IVO(70.0 * units.deg) //BTDC
label IVC(90.0 * units.deg) //ABDC
label EVO(75.0 * units.deg) //BBDC
label EVC(49.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(250.0) // g
label crank_mass(5.0) // kg
label flywheel_mass(3.0) // kg

public node eng_distributor {
    input wires;
    input timing_curve;
    input rev_limit: 13000.0 * units.rpm;
    input limiter_duration: 0.0001;
    alias output __out:
        ignition_module(timing_curve: timing_curve, rev_limit: rev_limit, limiter_duration: limiter_duration)
            .connect_wire(wires.wire1, rot * 0)
            .connect_wire(wires.wire3, rot * 2)
            .connect_wire(wires.wire4, rot * 4)
            .connect_wire(wires.wire2, rot * 6)

            .connect_wire(wires.wire5, rot * 0 + vee + rot360)
            .connect_wire(wires.wire7, rot * 2 + vee + rot360)
            .connect_wire(wires.wire8, rot * 4 + vee + rot360)
            .connect_wire(wires.wire6, rot * 6 + vee + rot360);
}

private node wires {
    output wire1: ignition_wire();
    output wire2: ignition_wire();
    output wire3: ignition_wire();
    output wire4: ignition_wire();
    output wire5: ignition_wire();
    output wire6: ignition_wire();
    output wire7: ignition_wire();
    output wire8: ignition_wire();
}


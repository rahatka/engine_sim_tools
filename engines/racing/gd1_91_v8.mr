public node camshaft_builder {
    output intake_cam_0: _intake_cam_0;
    output exhaust_cam_0: _exhaust_cam_0;

    output intake_cam_1: _intake_cam_1;
    output exhaust_cam_1: _exhaust_cam_1;

    camshaft _intake_cam_0(base_radius: 14.0 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 14.0 * units.mm, lobe_profile: exhaust_lobe_profile)
    
    camshaft _intake_cam_1(base_radius: 14.0 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_1(base_radius: 14.0 * units.mm, lobe_profile: exhaust_lobe_profile)

    _intake_cam_0
        .add_lobe(ILC + 0 * rot)
        .add_lobe(ILC + 6 * rot)
        .add_lobe(ILC + 2 * rot)
        .add_lobe(ILC + 4 * rot)
    _exhaust_cam_0
        .add_lobe(ELC + 0 * rot)
        .add_lobe(ELC + 6 * rot)
        .add_lobe(ELC + 2 * rot)
        .add_lobe(ELC + 4 * rot)
        
    _intake_cam_1
        .add_lobe(ILC + rot360 + 0 * rot + vee)
        .add_lobe(ILC + rot360 + 6 * rot + vee)
        .add_lobe(ILC + rot360 + 2 * rot + vee)
        .add_lobe(ILC + rot360 + 4 * rot + vee)
    _exhaust_cam_1
        .add_lobe(ELC + rot360 + 0 * rot + vee)
        .add_lobe(ELC + rot360 + 6 * rot + vee)
        .add_lobe(ELC + rot360 + 2 * rot + vee)
        .add_lobe(ELC + rot360 + 4 * rot + vee)
}

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node head {
    // port_flow.py v1.7
    // intake port area: 7.3 cm²; saturated lift: 7.45 mm
    // exhaust port area: 5.7 cm²; saturated lift: 6.58 mm
    // cylinder volume: 187.2 cm³ (11.42 CI); engine volume: 1.497 L (91.38 CI)
    // chamber volume: 19.3 cm³ (1.18 CI); conrod/stroke 1.935; N.A.C.C. 19.07
    // 8 harmonic intake runner length: 14.4 cm; diameter: 3.3 cm
    // primary length: 10.6 cm, area: 6.3 cm², diameter: 2.8 cm
    // collector diameter: 6.7 cm, area: 35.2 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 19.297 * units.cc,
        intake_runner_volume: 120.1 * units.cc,
        intake_runner_cross_section_area: 8.4 * units.cm2,
        exhaust_runner_volume: 40.0 * units.cc,
        exhaust_runner_cross_section_area: 6.3 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "GD-1",
        starter_torque: 30 * units.Nm,
        starter_speed: 700 * units.rpm,
        redline: redline * units.rpm,
        fuel: r50(),
        throttle_gamma: 1.2,
        jitter: 0.8,
        noise: 0.1,
        hf_gain: 0.008,
        simulation_frequency: 12000
    )

    wires wires()

    connecting_rod_parameters cr_params(
        mass: con_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.g,
            length: con_rod * units.mm
        ),
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    label crank_moment(disk_moment_of_inertia(mass: crank_mass * units.kg, radius: (stroke / 2.0) * units.mm))
    label flywheel_moment(disk_moment_of_inertia(mass: flywheel_mass * units.kg, radius: (stroke * 3.5) * units.mm))
    label other_moment(disk_moment_of_inertia(mass: 1 * units.kg, radius: 1.0 * units.cm))

    crankshaft c0(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: flywheel_mass * units.kg,
        mass: crank_mass * units.kg,
        friction_torque: 2.2 * units.Nm,
        moment_of_inertia: crank_moment + flywheel_moment + other_moment,
        position_x: 0.0,
        position_y: 0.0,
        tdc: 0.0
    )

    rod_journal rj0(angle: (0.0 + 90.0) * units.deg + vee / 2)
    rod_journal rj1(angle: (180.0 + 90.0) * units.deg + vee / 2)
    rod_journal rj2(angle: (180.0 + 90.0) * units.deg + vee / 2)
    rod_journal rj3(angle: (0.0 + 90.0) * units.deg + vee / 2)
    
    c0
        .add_rod_journal(rj0)
        .add_rod_journal(rj1)
        .add_rod_journal(rj2)
        .add_rod_journal(rj3)

    piston_parameters piston_params(
        mass: 250.0 * units.g,
        blowby: k_28inH2O(0.019),
        compression_height: compression_height,
        wrist_pin_position: 0.0,
        displacement: 0.0
    )

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    label collector_cross_section_area(17.6)

    label exhaust_pipe_length_0(100.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 100.0) // Litres
    
    label exhaust_pipe_length_1(100.0) // cm
    label exhaust_volume_1(collector_cross_section_area * exhaust_pipe_length_1 / 100.0) // Litres
    
    label spacing_factor(1.5)
    label flange_density(3.0)

    intake intake0(
        plenum_volume: 2.4 * units.L,
        plenum_cross_section_area: 33.4 * units.cm2,
        runner_length: 14.4 * units.cm,
        intake_flow_rate: k_carb(308.5),
        idle_flow_rate: k_carb(0.06),
        idle_throttle_plate_position: 0.9965,
        runner_flow_rate: k_carb(82.9),
        velocity_decay: 0.05
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(266.8),
        collector_cross_section_area: 17.6 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 10.6 * units.cm,
        primary_flow_rate: k_carb(138.4),
        velocity_decay: 0.5,
        volume: exhaust_volume_0 * units.L
    )
    
    exhaust_system_parameters es_params1(
        outlet_flow_rate: k_carb(266.8),
        collector_cross_section_area: 17.6 * units.cm2,
        length: exhaust_pipe_length_1 * units.cm,
        primary_tube_length: 10.6 * units.cm,
        primary_flow_rate: k_carb(138.4),
        velocity_decay: 0.5,
        volume: exhaust_volume_1 * units.L
    )

    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_45.wav", volume: 0.001))
    exhaust_system exhaust1(es_params1, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_58.wav", volume: 0.001))
 
    cylinder_bank b0(bank_params, angle: vee / 2)
    cylinder_bank b1(bank_params, angle: -vee / 2)
    
    label pl0 ((4.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl1 ((3.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl2 ((2.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl3 ((1.0 * bore * spacing_factor / flange_density) * units.mm)

    b0
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj0,
            intake: intake0,
            exhaust_system: exhaust0,
            primary_length: pl0,
            ignition_wire: wires.wire1
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj1,
            intake: intake0,
            exhaust_system: exhaust0,
            primary_length: pl1,
            ignition_wire: wires.wire2
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj2,
            intake: intake0,
            exhaust_system: exhaust0,
            primary_length: pl2,
            ignition_wire: wires.wire3
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj3,
            intake: intake0,
            exhaust_system: exhaust0,
            primary_length: pl3,
            ignition_wire: wires.wire4
        )
        
    b1
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj0,
            intake: intake0,
            exhaust_system: exhaust1,
            primary_length: pl0,
            ignition_wire: wires.wire5
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj1,
            intake: intake0,
            exhaust_system: exhaust1,
            primary_length: pl1,
            ignition_wire: wires.wire6
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj2,
            intake: intake0,
            exhaust_system: exhaust1,
            primary_length: pl2,
            ignition_wire: wires.wire7
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj3,
            intake: intake0,
            exhaust_system: exhaust1,
            primary_length: pl3,
            ignition_wire: wires.wire8
        )

    engine
        .add_cylinder_bank(b0)
        .add_cylinder_bank(b1)

    engine.add_crankshaft(c0)

    camshaft_builder camshaft()

    b0.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_0,
            exhaust_camshaft: camshaft.exhaust_cam_0,
            flip_display: true
        )
    )
    b1.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_1,
            exhaust_camshaft: camshaft.exhaust_cam_1
        )
    )

    function timing_curve(766.7 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 5.0 * units.deg)
        .add_sample(766.7 * units.rpm, 5.0 * units.deg)
        .add_sample(1533.3 * units.rpm, 7.0 * units.deg)
        .add_sample(2300.0 * units.rpm, 10.0 * units.deg)
        .add_sample(3066.7 * units.rpm, 12.9 * units.deg)
        .add_sample(3833.3 * units.rpm, 15.8 * units.deg)
        .add_sample(4600.0 * units.rpm, 18.7 * units.deg)
        .add_sample(5366.7 * units.rpm, 21.6 * units.deg)
        .add_sample(6133.3 * units.rpm, 24.6 * units.deg)
        .add_sample(6900.0 * units.rpm, 27.5 * units.deg)
        .add_sample(7666.7 * units.rpm, 30.4 * units.deg)
        .add_sample(8433.3 * units.rpm, 33.3 * units.deg)
        .add_sample(9200.0 * units.rpm, 36.2 * units.deg)
        .add_sample(9966.7 * units.rpm, 39.2 * units.deg)
        .add_sample(10733.3 * units.rpm, 42.1 * units.deg)
        .add_sample(11500.0 * units.rpm, 45.0 * units.deg)
        .add_sample(12266.7 * units.rpm, -15.0 * units.deg)
        .add_sample(13033.3 * units.rpm, -45.0 * units.deg)

    engine.add_ignition_module(
        eng_distributor(
            wires: wires,
            timing_curve: timing_curve
        ))
}


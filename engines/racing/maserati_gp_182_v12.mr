import "../../fuel/racing.mr"

units units()
constants constants()

label bore(70.4)
label stroke(64.0)
label compression_ratio(11.0)
label con_rod(118.0)
label compression_height(65.0 / 2 * units.mm) // 65 PH
label intake_valve_diameter(39.0)
label exhaust_valve_diameter(35.0) // ?
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(9750.0) // RPM

label cyl(12.0)
label cyl_base(cyl / row)
label row(2.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label rot_base(cycle / cyl_base)
label vee(60.0 * units.deg)

// IOD 320 ; LSA 105 ?
label IVL(8.0)
label EVL(8.0) // ?
label IVO(55.0 * units.deg) //BTDC
label IVC(85.0 * units.deg) //ABDC
label EVO(85.0 * units.deg) //BBDC
label EVC(55.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(400.0) // ? g
label piston_mass(400.0) // ?
label crank_mass(18.0) // ?
label flywheel_mass(5.0) // kg

label exhaust_delay_coeff(1.5)


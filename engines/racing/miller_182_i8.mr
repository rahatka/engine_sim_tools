import "engine_sim.mr"
import "../../fuel/racing.mr"

units units()
constants constants()

label bore(68.2625) // 2 11/16
label stroke(101.6) // 4
label compression_ratio(8.0)
label con_rod(196.9) // 7 3/4
label compression_height(68.0 / 2 * units.mm) // 68 PH
label intake_valve_diameter(30.1625) // 1 3/16 ; 1 5/16 appendix
label exhaust_valve_diameter(30.1625) // 1 3/16
label intake_valves(2.0)
label exhaust_valves(2.0)

label redline(5000.0) // RPM

label cyl(8.0)
label row(1.0)
label cycle(720.0 * units.deg)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label rot(cycle / cyl)

// Miller 183
label IVL(11.9) // 15/32
label EVL(11.9) // ?
label IVO(5.0 * units.deg) //BTDC
label IVC(38.0 * units.deg) //ABDC
label EVO(35.0 * units.deg) //BBDC
label EVC(8.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(400.0) // ?
label piston_mass(500.0) // ?
label crank_mass(15.0) // ?
label flywheel_mass(4.0) // kg

label exhaust_delay_coeff(1.5)


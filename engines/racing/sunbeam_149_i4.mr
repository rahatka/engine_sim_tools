import "engine_sim.mr"
import "../../fuel/racing.mr"

units units()
constants constants()

label cycle(720.0 * units.deg)

label bore(81.5)
label stroke(117.0)
label compression_ratio(5.06)
label con_rod(280.0)
label compression_height(bore / 2 * units.mm) // ?
label intake_valve_diameter(40.0)
label exhaust_valve_diameter(40.0) // ?
label intake_valves(2.0)
label exhaust_valves(2.0)
label offset(0.0)

label cyl(4.0)
label row(1.0)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)

label redline(4500.0) // RPM

// Sunbeam IVC 33 ID 216
label IVL(7.5)
label EVL(7.5) // ?
label IVO(3.0 * units.deg) //BTDC
label IVC(33.0 * units.deg) //ABDC
label EVO(52.0 * units.deg) //BBDC
label EVC(14.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(800.0)
label piston_mass(300.0)
label crank_mass(8.0)
label flywheel_mass(2.0) // kg

label exhaust_delay_coeff(1.5)


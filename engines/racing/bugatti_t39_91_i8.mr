import "../racing_engines/_racing_fuel.mr"

units units()
constants constants()

label bore(52.0)
label stroke(88.0)
label compression_ratio(6.0)
label con_rod(185.0)
label compression_height(37.0 * units.mm) // ~
label intake_valve_diameter(23.5)
label exhaust_valve_diameter(38.0)
label intake_valves(2.0)
label exhaust_valves(1.0)

label redline(5500.0) // RPM

label cyl(8.0)
label row(1.0)
label cycle(720.0 * units.deg)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label rot(cycle / cyl)

// Bugatti T37 late valve timings
label IVL(7.5)
label EVL(9.0)
label IVO(3.0 * units.deg) //BTDC
label IVC(40.0 * units.deg) //ABDC
label EVO(50.0 * units.deg) //BBDC
label EVC(20.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(700.0) // ?
label piston_mass(500.0) // ?
label crank_mass(12.0) // ?
label flywheel_mass(4.0) // kg

label exhaust_delay_coeff(1.5)


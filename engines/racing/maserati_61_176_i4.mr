public node camshaft_builder {
    output intake_cam_0: _intake_cam_0;
    output exhaust_cam_0: _exhaust_cam_0;

    camshaft _intake_cam_0(base_radius: 18.4 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 18.4 * units.mm, lobe_profile: exhaust_lobe_profile)

    _intake_cam_0
        .add_lobe(ILC + 0 * rot)
        .add_lobe(ILC + 3 * rot)
        .add_lobe(ILC + 1 * rot)
        .add_lobe(ILC + 2 * rot)
    _exhaust_cam_0
        .add_lobe(ELC + 0 * rot)
        .add_lobe(ELC + 3 * rot)
        .add_lobe(ELC + 1 * rot)
        .add_lobe(ELC + 2 * rot)
}

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node head {
    // port_flow.py v1.7
    // intake port area: 18.2 cm²; saturated lift: 11.73 mm
    // exhaust port area: 11.3 cm²; saturated lift: 9.23 mm
    // cylinder volume: 722.6 cm³ (44.09 CI); engine volume: 2.890 L (176.37 CI)
    // chamber volume: 82.1 cm³ (5.01 CI); conrod/stroke 1.554; N.A.C.C. 24.80
    // 8 harmonic intake runner length: 24.9 cm; diameter: 5.2 cm
    // primary length: 17.5 cm, area: 11.9 cm², diameter: 3.9 cm
    // collector diameter: 5.5 cm, area: 23.7 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 82.110 * units.cc,
        intake_runner_volume: 521.7 * units.cc,
        intake_runner_cross_section_area: 21.0 * units.cm2,
        exhaust_runner_volume: 173.9 * units.cc,
        exhaust_runner_cross_section_area: 11.9 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "Maserati 61",
        starter_torque: 100 * units.Nm,
        starter_speed: 500 * units.rpm,
        redline: redline * units.rpm,
        fuel: r50(),
        throttle_gamma: 1.8,
        jitter: 0.8,
        noise: 0.2,
        hf_gain: 0.008,
        simulation_frequency: 12000
    )

    wires wires()

    connecting_rod_parameters cr_params(
        mass: con_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.g,
            length: con_rod * units.mm
        ),
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    label crank_moment(disk_moment_of_inertia(mass: crank_mass * units.kg, radius: (stroke / 2.0) * units.mm))
    label flywheel_moment(disk_moment_of_inertia(mass: flywheel_mass * units.kg, radius: (stroke * 3.5) * units.mm))
    label other_moment(disk_moment_of_inertia(mass: 1 * units.kg, radius: 1.0 * units.cm))

    crankshaft c0(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: flywheel_mass * units.kg,
        mass: crank_mass * units.kg,
        friction_torque: 4.3 * units.Nm,
        moment_of_inertia: crank_moment + flywheel_moment + other_moment,
        tdc: 0.0
    )

    piston_parameters piston_params(
        mass: piston_mass * units.g,
        blowby: k_28inH2O(0.031),
        compression_height: compression_height,
        wrist_pin_position: 0.0,
        displacement: 0.0
    )

    rod_journal rj0(angle: rot * 0 + rot90 + tilt)
    rod_journal rj1(angle: rot * 3 + rot90 + tilt)
    rod_journal rj2(angle: rot * 1 + rot90 + tilt)
    rod_journal rj3(angle: rot * 2 + rot90 + tilt)
    
    c0
        .add_rod_journal(rj0)
        .add_rod_journal(rj1)
        .add_rod_journal(rj2)
        .add_rod_journal(rj3)

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    label intake_valve_circ(intake_valve_diameter * constants.pi)
    label intake_valve_area(intake_valve_circ * IVL / 100.0) // cm2

    label intake_plenum_area(intake_valve_area * cyl * intake_valves) // cm2
    label collector_cross_section_area(23.7)

    label exhaust_pipe_length_0(120.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 100.0) // Litres
    
    label spacing_factor(1.1)
    label flange_density(1.0 * exhaust_delay_coeff)

    intake intake(
        plenum_volume: 3.2 * units.L,
        plenum_cross_section_area: 41.9 * units.cm2,
        intake_flow_rate: k_carb(379.3),
        idle_flow_rate: k_carb(0.03),
        idle_throttle_plate_position: 0.9977,
        runner_flow_rate: k_carb(213.4),
        runner_length: 24.9 * units.cm,
        velocity_decay: 0.05
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(671.8),
        collector_cross_section_area: 23.7 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 17.5 * units.cm,
        primary_flow_rate: k_carb(356.9),
        velocity_decay: 1.0,
        volume: exhaust_volume_0 * units.L
    )

    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_56.wav", volume: 0.001))
 
    cylinder_bank b0(bank_params, angle: tilt)

    label pl0((3.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl1((2.5 * bore * spacing_factor / flange_density) * units.mm)
    label pl2((2.5 * bore * spacing_factor / flange_density) * units.mm)
    label pl3((1.0 * bore * spacing_factor / flange_density) * units.mm)

    b0
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj0,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl0,
            ignition_wire: wires.wire1
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj1,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl1,
            ignition_wire: wires.wire2
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj2,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl2,
            ignition_wire: wires.wire3
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj3,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl3,
            ignition_wire: wires.wire4
        )

    engine
        .add_cylinder_bank(b0)

    engine.add_crankshaft(c0)

    camshaft_builder camshaft()

    b0.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_0,
            exhaust_camshaft: camshaft.exhaust_cam_0,
            flip_display: true
        )
    )

    // ignition_timing.py v0.3 from = 8.0, to = 35.0, i = 500, r = 7000
    function timing_curve(700.0 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 0.0 * units.deg)
        .add_sample(700.0 * units.rpm, 8.0 * units.deg)
        .add_sample(1400.0 * units.rpm, 9.4 * units.deg)
        .add_sample(2100.0 * units.rpm, 11.1 * units.deg)
        .add_sample(2800.0 * units.rpm, 13.1 * units.deg)
        .add_sample(3500.0 * units.rpm, 15.4 * units.deg)
        .add_sample(4200.0 * units.rpm, 18.2 * units.deg)
        .add_sample(4900.0 * units.rpm, 21.4 * units.deg)
        .add_sample(5600.0 * units.rpm, 25.2 * units.deg)
        .add_sample(6300.0 * units.rpm, 29.7 * units.deg)
        .add_sample(7000.0 * units.rpm, 35.0 * units.deg)
        .add_sample(7700.0 * units.rpm, -15.0 * units.deg)
        .add_sample(8400.0 * units.rpm, -60.0 * units.deg)

    engine.add_ignition_module(
        eng_distributor(
            wires: wires,
            timing_curve: timing_curve
        )
    )
}

// Ferrari 500 F2
public node veh {
    alias output __out: vehicle;
    vehicle vehicle(
        mass: 560 * units.kg,
        drag_coefficient: 0.4,
        cross_sectional_area: (1400 * units.mm) * (1050 * units.mm),
        diff_ratio: 3.57, // ?
        tire_radius: (685.8 / 2) * units.mm, // 5.50x16
        rolling_resistance: 20
    )
}

// Peugeot L3 (?)
private node trn {
    alias output __out:
        transmission(
            max_clutch_torque: 400 * units.Nm,
            max_clutch_flex: 10 * units.deg,
            limit_clutch_flex: true,
            clutch_stiffness: 20 * units.Nm / units.deg,
            clutch_damping: 2.0,
            simulate_flex: true
        )
        .add_gear(3.5)
        .add_gear(2.0)
        .add_gear(1.5)
        .add_gear(1.0);
}

run(
    engine: eng(),
    transmission: trn(),
    vehicle: veh()
)


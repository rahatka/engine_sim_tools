units units()
constants constants()

label bore(57.404) // 2.26
label stroke(76.2) // 3
label compression_ratio(8.0)
label con_rod(152.4) // 6
label compression_height(bore / 2 * units.mm) // ?
label intake_valve_diameter(27.9) // ~ 1.1
label exhaust_valve_diameter(27.9) // ?
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(6500.0) // RPM

label cyl(4.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// 750 Spec
label IVL(7.1)
label EVL(7.1) // ?

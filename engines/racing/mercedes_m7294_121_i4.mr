units units()
constants constants()

label cycle(720.0 * units.deg)

label bore(70.0)
label stroke(129.0)
label compression_ratio(4.5)
label con_rod(250.0)
label compression_height(62.0 * units.mm) // 106 PH
label intake_valve_diameter(34.0) // Ludvigsen
label exhaust_valve_diameter(34.0) // Ludvigsen
label intake_valves(2.0)
label exhaust_valves(2.0)
label offset(0.0)

label cyl(4.0)
label row(1.0)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)

label redline(5000.0) // RPM

// Mercedes M 7294
label IVL(8.0)
label EVL(8.0)
label IVO(20.0 * units.deg) //BTDC
label IVC(50.0 * units.deg) //ABDC
label EVO(45.0 * units.deg) //BBDC
label EVC(20.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(800.0)
label piston_mass(400.0)
label crank_mass(8.0)
label flywheel_mass(2.0) // kg

label exhaust_delay_coeff(1.5)


import "engine_sim.mr"
import "../../fuel/50.mr"

units units()
constants constants()

label bore(78.0)
label stroke(85.5)
label compression_ratio(8.4)
label con_rod(160.0)
label compression_height(85.0 / 2 * units.mm) // 85 PH
label intake_valve_diameter(40.0)
label exhaust_valve_diameter(35.0)
label intake_valves(1.0)
label exhaust_valves(1.0)

label cyl(6.0)
label row(2.0)
label cycle(720.0 * units.deg)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label vee(60.0 * units.deg)

label comp(vee - rot)

label redline(5500.0) // RPM

// B20GT
label IVL(7.5)
label EVL(7.5)
label IVO(31.0 * units.deg) //BTDC
label IVC(81.0 * units.deg) //ABDC
label EVO(67.0 * units.deg) //BBDC
label EVC(25.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(400.0) // g
label crank_mass(9.0) // kg
label flywheel_mass(3.0) // kg

label exhaust_delay_coeff(1.5)


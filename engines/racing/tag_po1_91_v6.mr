label bore(82.0)
label stroke(47.3)
label compression_ratio(8.0)
label con_rod(115.0)
label compression_height(30.0 * units.mm) // ?
label intake_valve_diameter(30.5)
label exhaust_valve_diameter(27.5)
label intake_valves(2.0)
label exhaust_valves(2.0)

label redline(12500.0)

label cyl(6.0)
label row(2.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label vee(80.0 * units.deg)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// Ford DFV
label IVL(10.0) // ?
label EVL(10.0) // ?
label IVO(58.0 * units.deg) //BTDC
label IVC(82.0 * units.deg) //ABDC
label EVO(98.0 * units.deg) //BBDC
label EVC(58.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(280.0) // ? g
label crank_mass(7.0) // ? kg
label flywheel_mass(3.0) // kg

public node eng_distributor {
    input wires;
    input timing_curve;
    input rev_limit: 13000 * units.rpm;
    input limiter_duration: 0.001;
    alias output __out:
        ignition_module(timing_curve: timing_curve, rev_limit: rev_limit, limiter_duration: limiter_duration)
            .connect_wire(wires.wire1, rot * 0)
            .connect_wire(wires.wire2, rot * 2)
            .connect_wire(wires.wire3, rot * 4)
            .connect_wire(wires.wire4, rot * 0 + vee)
            .connect_wire(wires.wire5, rot * 2 + vee)
            .connect_wire(wires.wire6, rot * 4 + vee);
}

private node wires {
    output wire1: ignition_wire();
    output wire2: ignition_wire();
    output wire3: ignition_wire();
    output wire4: ignition_wire();
    output wire5: ignition_wire();
    output wire6: ignition_wire();
}


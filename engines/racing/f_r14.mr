import "engine_sim.mr"
import "../../fuel/racing.mr"

units units()
constants constants()

label bore(80.0)
label stroke(70.0)
label compression_ratio(9.0)
label con_rod(135.0)
label L1(105.0)
label R1(30.0)
label compression_height(40.0 * units.mm)
label intake_valve_diameter(40.0)
label exhaust_valve_diameter(35.0)
label intake_valves(1.0)
label exhaust_valves(1.0)

label cyl(14.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

label redline(7000.0) // RPM

label IVL(8.0)
label EVL(7.5)
label IVO(24.0 * units.deg) //BTDC
label IVC(68.0 * units.deg) //ABDC
label EVO(70.0 * units.deg) //BBDC
label EVC(20.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(700.0) // ? g
label slave_rod_mass(200.0)
label crank_mass(8.0) // ? kg
label flywheel_mass(7.0) // kg

label exhaust_delay_coeff(1.5)

public node eng_distributor {
    input wires;
    input timing_curve;
    input rev_limit: redline * 1.1 * units.rpm;
    input limiter_duration: 0.02;
    alias output __out:
        ignition_module(timing_curve: timing_curve, rev_limit: rev_limit, limiter_duration: limiter_duration)
            .connect_wire(wires.wire1, rot * 0)
            .connect_wire(wires.wire3, rot * 2 + 3.443 * units.deg)
            .connect_wire(wires.wire5, rot * 4 - 2.114 * units.deg)
            .connect_wire(wires.wire7, rot * 6 - 0.871 * units.deg)
            .connect_wire(wires.wire2, rot * 8 + 0.871 * units.deg)
            .connect_wire(wires.wire4, rot * 10 + 2.114 * units.deg)
            .connect_wire(wires.wire6, rot * 12 - 3.443 * units.deg)

            .connect_wire(wires.wire8, rot * 0 + rot360)
            .connect_wire(wires.wire10, rot * 2 + rot360 + 3.443 * units.deg)
            .connect_wire(wires.wire12, rot * 4 + rot360 - 2.114 * units.deg)
            .connect_wire(wires.wire14, rot * 6 + rot360 - 0.871 * units.deg)
            .connect_wire(wires.wire9, rot * 8 + rot360 + 0.871 * units.deg)
            .connect_wire(wires.wire11, rot * 10 + rot360 + 2.114 * units.deg)
            .connect_wire(wires.wire13, rot * 12 + rot360 - 3.443 * units.deg);
}


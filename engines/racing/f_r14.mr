// D25
public node veh {
    alias output __out: vehicle;

    label car_mass(850) // 750 empty

    vehicle vehicle(
        mass: car_mass * units.kg,
        drag_coefficient: 0.5,
        cross_sectional_area: (1440  * units.mm) * (960 * units.mm),
        diff_ratio: 4.1, // 4.444 4.667 4.1 3.7 3.9 4.0 5.0
        tire_radius: (786.0 / 2) * units.mm, // 7.00x16
        rolling_resistance: 0.012 * car_mass * 9.81
        )
}

// D25
private node trn {
    alias output __out:
        transmission(
            max_clutch_torque: 600 * units.Nm,
            max_clutch_flex: 30 * units.deg,
            limit_clutch_flex: true,
            clutch_stiffness: 20 * units.Nm / units.deg,
            clutch_damping: 1.0,
            simulate_flex: true
        )
        .add_gear(2.588)
        .add_gear(1.346)
        .add_gear(1.033)
        .add_gear(0.848);
}

run(
    engine: eng(),
    transmission: trn(),
    vehicle: veh()
)

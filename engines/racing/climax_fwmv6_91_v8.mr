label bore(72.39) // 2.85
label stroke(45.466) // 1.79
label compression_ratio(12.0)
label con_rod(106.7) // 4 1/5
label compression_height(57.0 / 2 * units.mm) // 57 PH

label cycle(720.0 * units.deg)
label cyl(8.0)
label row(2.0)
label vee(90.0 * units.deg)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label rot(cycle / cyl)

label redline(10500.0) // RPM

label intake_valve_diameter(26.4)
label exhaust_valve_diameter(23.7)
label intake_valves(2.0)
label exhaust_valves(2.0)


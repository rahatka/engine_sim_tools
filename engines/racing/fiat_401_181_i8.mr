import "../../fuel/racing.mr"

units units()
constants constants()

label bore(65.0)
label stroke(112.0)
label compression_ratio(6.0)
label con_rod(250.0)
label compression_height(90.0 / 2 * units.mm) // ?
label intake_valve_diameter(30.0) // ~
label exhaust_valve_diameter(30.0) // ~
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(4750.0) // RPM

label cyl(8.0)
label row(1.0)
label cycle(720.0 * units.deg)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label rot(cycle / cyl)

// Fiat 401 only IVO and EVC are known
label IVL(7.0) // ?
label EVL(7.0) // ?
label IVO(0.0 * units.deg) //BTDC =
label IVC(50.0 * units.deg) //ABDC // Six Cyl Pomeroy
label EVO(50.0 * units.deg) //BBDC // Six Cyl Pomeroy
label EVC(10.0 * units.deg) //ATDC =

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(600.0) // ?
label piston_mass(300.0) // ?
label crank_mass(15.0) // ?
label flywheel_mass(4.0) // kg

label exhaust_delay_coeff(1.5)


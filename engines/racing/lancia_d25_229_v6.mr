import "../../fuel/racing.mr"

units units()
constants constants()

label bore(93.0)
label stroke(92.0)
label compression_ratio(9.0)
label con_rod(164.0)
label compression_height(75.0 / 2 * units.mm) // 75 PH
label intake_valve_diameter(52.0)
label exhaust_valve_diameter(45.0) // ?
label intake_valves(1.0)
label exhaust_valves(1.0)

label cyl(6.0)
label row(2.0)
label cycle(720.0 * units.deg)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label vee(60.0 * units.deg)

label comp(vee - rot)

label redline(6500.0) // RPM

// D25 ID 300
label IVL(12.0)
label EVL(12.0) // ?
label IVO(35.0 * units.deg) //BTDC
label IVC(85.0 * units.deg) //ABDC
label EVO(71.0 * units.deg) //BBDC
label EVC(29.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(400.0) // g
label crank_mass(9.0) // kg
label flywheel_mass(3.0) // kg

label exhaust_delay_coeff(1.5)


private node add_lobes {
    input base;
    input shift: 0.0;
    input var;
    input this;
    alias output __out: this;

    this.add_lobe(base + var.s1 * rot_base + shift)
    this.add_lobe(base + var.s2 * rot_base + shift)
    this.add_lobe(base + var.s3 * rot_base + shift)
}

public node bank_builder {
    input var;
    input crankshaft;
    input incline;
    input phasing;
    input ignition;
    input intake;
    input exhaust;
    input primaries;
    input flip: false;

    output bank: _b0;

    ignition_wire _wire1()
    ignition_wire _wire2()
    ignition_wire _wire3()

    piston_parameters piston_params(
        mass: 400 * units.g,
        blowby: k_28inH2O(0.029),
        compression_height: compression_height,
        wrist_pin_position: 0.0,
        displacement: 0.0
    )

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    connecting_rod_parameters cr_params(
        mass: con_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.g,
            length: con_rod * units.mm
        ),
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    camshaft _intake_cam_0(base_radius: 22.0 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 22.0 * units.mm, lobe_profile: exhaust_lobe_profile)

    _intake_cam_0.add_lobes(ILC, phasing, var)
    _exhaust_cam_0.add_lobes(ELC, phasing, var)

    ignition.connect_wire(_wire1, rot_base * var.s1 + phasing)
    ignition.connect_wire(_wire2, rot_base * var.s2 + phasing)
    ignition.connect_wire(_wire3, rot_base * var.s3 + phasing)

    cylinder_bank _b0(bank_params, angle: incline)

    _b0
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj0,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: primaries.pl0,
            ignition_wire: _wire1
        )
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj1,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: primaries.pl1,
            ignition_wire: _wire2
        )
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj2,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: primaries.pl2,
            ignition_wire: _wire3
        )

    _b0.set_cylinder_head(
        head(
            intake_camshaft: _intake_cam_0,
            exhaust_camshaft: _exhaust_cam_0,
            flip_display: flip
        )
    )
}

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node head {
    // port_flow.py v1.7
    // intake port area: 17.0 cm²; saturated lift: 11.37 mm
    // exhaust port area: 12.7 cm²; saturated lift: 9.84 mm
    // cylinder volume: 624.9 cm³ (38.14 CI); engine volume: 3.750 L (228.82 CI)
    // chamber volume: 78.1 cm³ (4.77 CI); conrod/stroke 1.783; N.A.C.C. 32.17
    // 8 harmonic intake runner length: 25.6 cm; diameter: 4.7 cm
    // primary length: 18.3 cm, area: 13.4 cm², diameter: 4.1 cm
    // collector diameter: 6.4 cm, area: 32.1 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 78.118 * units.cc,
        intake_runner_volume: 442.4 * units.cc,
        intake_runner_cross_section_area: 17.3 * units.cm2,
        exhaust_runner_volume: 147.5 * units.cc,
        exhaust_runner_cross_section_area: 13.4 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

public node crank {
    input phasing;
    input var;

    output c: _c;
    output rj0: _rj0;
    output rj1: _rj1;
    output rj2: _rj2;
    output rj0_1: _rj0_1;
    output rj1_1: _rj1_1;
    output rj2_1: _rj2_1;

    label crank_moment(disk_moment_of_inertia(mass: crank_mass * units.kg, radius: (stroke / 2.0) * units.mm))
    label flywheel_moment(disk_moment_of_inertia(mass: flywheel_mass * units.kg, radius: (stroke * 3.5) * units.mm))
    label other_moment(disk_moment_of_inertia(mass: 1 * units.kg, radius: 1.0 * units.cm))

    crankshaft _c(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: flywheel_mass * units.kg,
        mass: crank_mass * units.kg,
        friction_torque: 5.6 * units.Nm,
        moment_of_inertia: crank_moment + flywheel_moment + other_moment,
        tdc: 0.0
    )

    rod_journal _rj0(angle: rot_base * var.s1 + rot90 + vee / 2)
    rod_journal _rj1(angle: rot_base * var.s2 + rot90 + vee / 2)
    rod_journal _rj2(angle: rot_base * var.s3 + rot90 + vee / 2)

    rod_journal _rj0_1(angle: rot_base * var.s1 + rot90 + vee / 2 - comp)
    rod_journal _rj1_1(angle: rot_base * var.s2 + rot90 + vee / 2 - comp)
    rod_journal _rj2_1(angle: rot_base * var.s3 + rot90 + vee / 2 - comp)

    _c
        .add_rod_journal(_rj0)
        .add_rod_journal(_rj0_1)
        .add_rod_journal(_rj1)
        .add_rod_journal(_rj1_1)
        .add_rod_journal(_rj2)
        .add_rod_journal(_rj2_1)
}

public node c0_1 {
    input crank;
    output rj0: crank.rj0;
    output rj1: crank.rj1;
    output rj2: crank.rj2;
}

public node c1_1 {
    input crank;
    output rj0: crank.rj0_1;
    output rj1: crank.rj1_1;
    output rj2: crank.rj2_1;
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "Lancia D25",
        starter_torque: 50 * units.Nm,
        starter_speed: 300 * units.rpm,
        redline: redline * units.rpm,
        fuel: m9b1(),
        throttle_gamma: 1.5,
        jitter: 1.0,
        noise: 0.1,
        hf_gain: 0.01,
        simulation_frequency: 8500,
        dyno_min_speed: 750 * units.rpm
    )

    variant_0 variant()

    crank c0(vee / 2 + rot90, variant)

    bank_builder bank0(
        var: variant,
        crankshaft: c0_1(c0),
        incline: vee / 2,
        phasing: 0,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust0,
        primaries: em_1(),
        flip: true
    )

    bank_builder bank1(
        var: variant,
        crankshaft: c1_1(c0),
        incline: -vee / 2,
        phasing: vee - comp,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust1,
        primaries: em_1()
    )

    ignition_module ignition(timing_curve: timing_curve, rev_limit: redline * 1.25 * units.rpm, limiter_duration: 0.05)

    label collector_cross_section_area(16.1)

    label exhaust_pipe_length_0(150.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 1000.0) // Litres
    
    label exhaust_pipe_length_1(150.0) // cm
    label exhaust_volume_1(collector_cross_section_area * exhaust_pipe_length_1 / 1000.0) // Litres

    intake intake(
        plenum_volume: 3.7 * units.L,
        plenum_cross_section_area: 51.9 * units.cm2,
        runner_length: 25.6 * units.cm,
        intake_flow_rate: k_carb(436.7),
        idle_flow_rate: k_carb(0.08),
        idle_throttle_plate_position: 0.9965,
        runner_flow_rate: k_carb(156.5),
        velocity_decay: 0.03
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(377.5),
        collector_cross_section_area: 16.1 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 18.3 * units.cm,
        primary_flow_rate: k_carb(261.1),
        velocity_decay: 0.8,
        volume: (exhaust_volume_0 + 3.0) * units.L
    )
    
    exhaust_system_parameters es_params1(
        outlet_flow_rate: k_carb(377.5),
        collector_cross_section_area: 16.1 * units.cm2,
        length: exhaust_pipe_length_1 * units.cm,
        primary_tube_length: 18.3 * units.cm,
        primary_flow_rate: k_carb(261.1),
        velocity_decay: 0.8,
        volume: (exhaust_volume_1 + 3.0) * units.L
    )
    
    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_45.wav", volume: 0.0001))
    exhaust_system exhaust1(es_params1, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_58.wav", volume: 0.0001))

    engine
        .add_crankshaft(c0.c)
        .add_cylinder_bank(bank0.bank)
        .add_cylinder_bank(bank1.bank)
        .add_ignition_module(ignition)

    function timing_curve(433.3 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 2.0 * units.deg)
        .add_sample(433.3 * units.rpm, 2.0 * units.deg)
        .add_sample(866.7 * units.rpm, 4.0 * units.deg)
        .add_sample(1300.0 * units.rpm, 6.4 * units.deg)
        .add_sample(1733.3 * units.rpm, 8.8 * units.deg)
        .add_sample(2166.7 * units.rpm, 11.2 * units.deg)
        .add_sample(2600.0 * units.rpm, 13.5 * units.deg)
        .add_sample(3033.3 * units.rpm, 15.9 * units.deg)
        .add_sample(3466.7 * units.rpm, 18.3 * units.deg)
        .add_sample(3900.0 * units.rpm, 20.7 * units.deg)
        .add_sample(4333.3 * units.rpm, 23.1 * units.deg)
        .add_sample(4766.7 * units.rpm, 25.5 * units.deg)
        .add_sample(5200.0 * units.rpm, 27.8 * units.deg)
        .add_sample(5633.3 * units.rpm, 30.2 * units.deg)
        .add_sample(6066.7 * units.rpm, 32.6 * units.deg)
        .add_sample(6500.0 * units.rpm, 35.0 * units.deg)
        .add_sample(6933.3 * units.rpm, -12.5 * units.deg)
        .add_sample(7366.7 * units.rpm, -25.0 * units.deg)
}

label spacing_factor(1.15)
label flange_density(1.0 * exhaust_delay_coeff)
label em_unit(bore * spacing_factor / flange_density)
label sh(30.0 * units.cm)

private node em_1{
    output pl0: (2.4 * em_unit) * units.mm + sh;
    output pl1: (1.5 * em_unit) * units.mm + sh;
    output pl2: (1.2 * em_unit) * units.mm + sh;
}

// D25
public node veh {
    alias output __out: vehicle;

    label car_mass(850) // 750 empty

    vehicle vehicle(
        mass: car_mass * units.kg,
        drag_coefficient: 0.5,
        cross_sectional_area: (1440  * units.mm) * (960 * units.mm),
        diff_ratio: 4.2, // 4.444 4.667 4.1 3.7 3.9 4.0 5.0
        tire_radius: (786.0 / 2) * units.mm, // 7.00x16
        rolling_resistance: 0.012 * car_mass * 9.81
        )
}

// D25
private node trn {
    alias output __out:
        transmission(
            max_clutch_torque: 400 * units.Nm,
            max_clutch_flex: 30 * units.deg,
            limit_clutch_flex: true,
            clutch_stiffness: 20 * units.Nm / units.deg,
            clutch_damping: 1.0,
            simulate_flex: true
        )
        .add_gear(2.588)
        .add_gear(1.346)
        .add_gear(1.033)
        .add_gear(0.848);
}

run(
    engine: eng(),
    transmission: trn(),
    vehicle: veh()
)

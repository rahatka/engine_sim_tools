import "engine_sim.mr"
import "../../fuel/racing.mr"

units units()
constants constants()

label bore(65.0)
label stroke(112.0)
label compression_ratio(6.6)
label con_rod(190.0) // 192 appendix
label compression_height(73.0 / 2 * units.mm) // 73 PH
label intake_valve_diameter(34.0)
label exhaust_valve_diameter(30.9)
label intake_valves(2.0)
label exhaust_valves(2.0)

label redline(4200.0) // RPM

label cyl(8.0)
label row(1.0)
label cycle(720.0 * units.deg)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label rot(cycle / cyl)

// Ballot
label IVL(7.0)
label EVL(7.0) // ?
label IVO(03.0 * units.deg) //BTDC
label IVC(41.0 * units.deg) //ABDC
label EVO(68.0 * units.deg) //BBDC
label EVC(13.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(400.0) // ?
label piston_mass(500.0) // ?
label crank_mass(15.0) // ?
label flywheel_mass(4.0) // kg

label exhaust_delay_coeff(1.5)


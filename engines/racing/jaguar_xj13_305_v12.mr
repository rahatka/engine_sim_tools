units units()
constants constants()

label bore(87.0)
label stroke(70.0)
label compression_ratio(10.4)
label con_rod(152.4) // 6
label compression_height(bore / 2 * units.mm) // ?
label intake_valve_diameter(47.6)
label exhaust_valve_diameter(41.3) // XK210
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(7800.0) // RPM

label cyl(12.0)
label cyl_base(cyl / row)
label row(2.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label rot_base(cycle / cyl_base)
label vee(60.0 * units.deg)

// IOD 290
label IVL(11.2)
label EVL(11.2) // ?
label IVO(35.0 * units.deg) //BTDC
label IVC(75.0 * units.deg) //ABDC
label EVO(75.0 * units.deg) //BBDC
label EVC(35.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(400.0) // ? g
label piston_mass(400.0) // ?
label crank_mass(25.0) // ?
label flywheel_mass(5.0) // kg

label exhaust_delay_coeff(1.5)


import "engine_sim.mr"
import "../../fuel/10.mr"

units units()
constants constants()

label bore(154.7)
label stroke(180.0)
label compression_ratio(4.5)
label con_rod(360.0)
label compression_height(169.0 / 2 * units.mm) // 169 PH
label intake_valve_diameter(63.4)
label exhaust_valve_diameter(63.4) // ?
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(1800.0) // RPM

label cyl(4.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// 1908 Benz 200 ID
label IVL(13.0)
label EVL(13.0) // ?
label IVO(-3.0 * units.deg) //BTDC
label IVC(23.0 * units.deg) //ABDC
label EVO(58.0 * units.deg) //BBDC
label EVC(-8.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(1500.0) // ?
label piston_mass(850.0) // ?
label crank_mass(20.0) // ?
label flywheel_mass(6.0) // kg

label exhaust_delay_coeff(1.5)


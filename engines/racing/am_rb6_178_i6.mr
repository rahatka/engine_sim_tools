public node camshaft_builder {
    output intake_cam_0: _intake_cam_0;
    output exhaust_cam_0: _exhaust_cam_0;

    camshaft _intake_cam_0(base_radius: 19.1 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 19.1 * units.mm, lobe_profile: exhaust_lobe_profile)

    _intake_cam_0
        .add_lobe(ILC + 0 * rot)
        .add_lobe(ILC + 4 * rot)
        .add_lobe(ILC + 2 * rot)
        .add_lobe(ILC + 5 * rot)
        .add_lobe(ILC + 1 * rot)
        .add_lobe(ILC + 3 * rot)
    _exhaust_cam_0
        .add_lobe(ELC + 0 * rot)
        .add_lobe(ELC + 4 * rot)
        .add_lobe(ELC + 2 * rot)
        .add_lobe(ELC + 5 * rot)
        .add_lobe(ELC + 1 * rot)
        .add_lobe(ELC + 3 * rot)
}

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node head {
    // port_flow.py v1.7
    // intake port area: 15.6 cm²; saturated lift: 10.86 mm
    // exhaust port area: 10.0 cm²; saturated lift: 8.69 mm
    // cylinder volume: 487.0 cm³ (29.72 CI); engine volume: 2.922 L (178.29 CI)
    // chamber volume: 55.3 cm³ (3.38 CI); conrod/stroke 1.844; N.A.C.C. 25.63
    // 8 harmonic intake runner length: 28.3 cm; diameter: 4.1 cm
    // primary length: 18.8 cm, area: 10.5 cm², diameter: 3.7 cm
    // collector diameter: 6.3 cm, area: 31.5 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 55.336 * units.cc,
        intake_runner_volume: 369.6 * units.cc,
        intake_runner_cross_section_area: 13.0 * units.cm2,
        exhaust_runner_volume: 123.2 * units.cc,
        exhaust_runner_cross_section_area: 10.5 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "Aston Martin RB6",
        starter_torque: 150 * units.Nm,
        starter_speed: 300 * units.rpm,
        redline: redline * units.rpm,
        fuel: m9b1(),
        throttle_gamma: 1.5,
        jitter: 0.8,
        noise: 0.01,
        hf_gain: 0.008,
        simulation_frequency: 12000 
    )

    wires wires()

    crankshaft c0(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: 25.0 * units.kg,
        mass: 22.0 * units.kg,
        friction_torque: 4.4 * units.Nm,
        moment_of_inertia: 0.2,
        tdc: 0.0
    )

    rod_journal rj0(angle: rot * 0 + rot90)
    rod_journal rj1(angle: rot * 4 + rot90)
    rod_journal rj2(angle: rot * 2 + rot90)
    rod_journal rj3(angle: rot * 5 + rot90)
    rod_journal rj4(angle: rot * 1 + rot90)
    rod_journal rj5(angle: rot * 3 + rot90)
    
    c0
        .add_rod_journal(rj0)
        .add_rod_journal(rj1)
        .add_rod_journal(rj2)
        .add_rod_journal(rj3)
        .add_rod_journal(rj4)
        .add_rod_journal(rj5)

    piston_parameters piston_params(
        mass: 450.0 * units.g,
        blowby: k_28inH2O(0.026),
        compression_height: compression_height,
        wrist_pin_position: 0.0,
        displacement: 0.0
    )

    connecting_rod_parameters cr_params(
        mass: 400.0 * units.g,
        moment_of_inertia: 0.0015,
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    label collector_cross_section_area(15.8)

    label exhaust_pipe_length_0(138.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 1000.0) // Litres
    
    label exhaust_pipe_length_1(120.0) // cm
    label exhaust_volume_1(collector_cross_section_area * exhaust_pipe_length_1 / 1000.0) // Litres
    
    label spacing_factor(1.1)
    label flange_density(1.0)

    intake intake(
        plenum_volume: 3.0 * units.L,
        plenum_cross_section_area: 39.1 * units.cm2,
        runner_length: 28.3 * units.cm,
        intake_flow_rate: k_carb(356.1),
        idle_flow_rate: k_carb(0.05),
        idle_throttle_plate_position: 0.9965,
        runner_flow_rate: k_carb(133.5),
        velocity_decay: 0.05
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(315.3),
        collector_cross_section_area: 15.8 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 18.8 * units.cm,
        primary_flow_rate: k_carb(223.3),
        velocity_decay: 1.0,
        volume: exhaust_volume_0 * units.L
    )

    exhaust_system_parameters es_params1(
        outlet_flow_rate: k_carb(315.3),
        collector_cross_section_area: 15.8 * units.cm2,
        length: exhaust_pipe_length_1 * units.cm,
        primary_tube_length: 18.8 * units.cm,
        primary_flow_rate: k_carb(223.3),
        velocity_decay: 1.0,
        volume: exhaust_volume_1 * units.L
    )

    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_56.wav", volume: 0.001))
    exhaust_system exhaust1(es_params1, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_56.wav", volume: 0.001))
 
    cylinder_bank b0(bank_params, angle: 0)

    label pl0((3.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl1((1.8 * bore * spacing_factor / flange_density) * units.mm)
    label pl2((1.0 * bore * spacing_factor / flange_density) * units.mm)

    b0
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj0,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl0,
            ignition_wire: wires.wire1
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj1,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl1,
            ignition_wire: wires.wire2
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj2,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl2,
            ignition_wire: wires.wire3
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj3,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl0,
            ignition_wire: wires.wire4
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj4,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl1,
            ignition_wire: wires.wire5
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj5,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl2,
            ignition_wire: wires.wire6
        )

    engine
        .add_cylinder_bank(b0)

    engine.add_crankshaft(c0)

    camshaft_builder camshaft()

    b0.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_0,
            exhaust_camshaft: camshaft.exhaust_cam_0
        )
    )

    function timing_curve(433.3 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 2.0 * units.deg)
        .add_sample(433.3 * units.rpm, 2.0 * units.deg)
        .add_sample(866.7 * units.rpm, 4.0 * units.deg)
        .add_sample(1300.0 * units.rpm, 6.4 * units.deg)
        .add_sample(1733.3 * units.rpm, 8.8 * units.deg)
        .add_sample(2166.7 * units.rpm, 11.2 * units.deg)
        .add_sample(2600.0 * units.rpm, 13.5 * units.deg)
        .add_sample(3033.3 * units.rpm, 15.9 * units.deg)
        .add_sample(3466.7 * units.rpm, 18.3 * units.deg)
        .add_sample(3900.0 * units.rpm, 20.7 * units.deg)
        .add_sample(4333.3 * units.rpm, 23.1 * units.deg)
        .add_sample(4766.7 * units.rpm, 25.5 * units.deg)
        .add_sample(5200.0 * units.rpm, 27.8 * units.deg)
        .add_sample(5633.3 * units.rpm, 30.2 * units.deg)
        .add_sample(6066.7 * units.rpm, 32.6 * units.deg)
        .add_sample(6500.0 * units.rpm, 35.0 * units.deg)
        .add_sample(6933.3 * units.rpm, -12.5 * units.deg)
        .add_sample(7366.7 * units.rpm, -25.0 * units.deg)

    engine.add_ignition_module(
        eng_distributor(
            wires: wires,
            timing_curve: timing_curve
        )
    )
}

// Jaguar D-Type
public node veh {
    alias output __out: vehicle;
    vehicle vehicle(
        mass: 950  * units.kg,
        drag_coefficient: 0.45,
        cross_sectional_area: (1660 * units.mm) * (813 * units.mm),
        diff_ratio: 3.54, // or 2.786
        tire_radius: (736.6 / 2) * units.mm, // 6.50 - 16"
        rolling_resistance: 20
        )
}

// Jaguar D-Type
private node trn {
    alias output __out:
        transmission(
            max_clutch_torque: 400 * units.Nm,
            max_clutch_flex: 10 * units.deg,
            limit_clutch_flex: true,
            clutch_stiffness: 20 * units.Nm / units.deg,
            clutch_damping: 2.0,
            simulate_flex: true
        )
        .add_gear(2.14)
        .add_gear(1.64)
        .add_gear(1.27)
        .add_gear(1.00);
}

run(
    engine: eng(),
    transmission: trn(),
    vehicle: veh()
)

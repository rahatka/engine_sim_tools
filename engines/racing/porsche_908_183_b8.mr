    output intake_cam_1: _intake_cam_1;
    output exhaust_cam_1: _exhaust_cam_1;

    output rj0: _rj0;
    output rj1: _rj1;
    output rj2: _rj2;
    output rj3: _rj3;
    output rj4: _rj4;
    output rj5: _rj5;
    output rj6: _rj6;
    output rj7: _rj7;

    output ignition: _ign;
    output name: "Porsche 908 v2";

    ignition_module _ign(timing_curve: timing_curve, rev_limit: rev_limit, limiter_duration: limiter_duration)
    _ign
        .connect_wire(wires.wire1, rot * 0)
        .connect_wire(wires.wire5, rot * 1)
        .connect_wire(wires.wire2, rot * 2)
        .connect_wire(wires.wire7, rot * 3)

import "engine_sim.mr"
import "../../fuel/racing.mr"

units units()
constants constants()

label bore(55.9)
label stroke(76.0)
label compression_ratio(6.5)
label con_rod(152.0)
label IVL(7.0)
label EVL(7.0)
label compression_height(40.0 * units.mm) // ?
label intake_valve_diameter(31.0)
label exhaust_valve_diameter(29.0)
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(8000.0) // RPM

label cyl(8.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)

// Delage 15-S-8 valve timings
label IVO(18.0 * units.deg) //BTDC
label IVC(50.0 * units.deg) //ABDC
label EVO(58.0 * units.deg) //BBDC
label EVC(25.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(411.068) // 14 1/2 oz
label piston_mass(219.709) // 7 3/4 oz
label crank_mass(10.0) // ? kg
label flywheel_mass(3.0) // kg

label exhaust_delay_coeff(1.5)


public node camshaft_builder {
    output intake_cam_0: _intake_cam_0;
    output exhaust_cam_0: _exhaust_cam_0;

    output intake_cam_1: _intake_cam_1;
    output exhaust_cam_1: _exhaust_cam_1;

    camshaft _intake_cam_0(base_radius: 21.7 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 21.7 * units.mm, lobe_profile: exhaust_lobe_profile)
    
    camshaft _intake_cam_1(base_radius: 21.7 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_1(base_radius: 21.7 * units.mm, lobe_profile: exhaust_lobe_profile)

    _intake_cam_0
        .add_lobe(ILC + 0 * rot)
        .add_lobe(ILC + 8 * rot)
        .add_lobe(ILC + 4 * rot)
        .add_lobe(ILC + 10 * rot)
        .add_lobe(ILC + 2 * rot)
        .add_lobe(ILC + 6 * rot)
    _exhaust_cam_0
        .add_lobe(ELC + 0 * rot)
        .add_lobe(ELC + 8 * rot)
        .add_lobe(ELC + 4 * rot)
        .add_lobe(ELC + 10 * rot)
        .add_lobe(ELC + 2 * rot)
        .add_lobe(ELC + 6 * rot)
        
    _intake_cam_1
        .add_lobe(ILC + 0 * rot + vee + rot360)
        .add_lobe(ILC + 8 * rot + vee + rot360)
        .add_lobe(ILC + 4 * rot + vee + rot360)
        .add_lobe(ILC + 10 * rot + vee + rot360)
        .add_lobe(ILC + 2 * rot + vee + rot360)
        .add_lobe(ILC + 6 * rot + vee + rot360)
    _exhaust_cam_1
        .add_lobe(ELC + 0 * rot + vee + rot360)
        .add_lobe(ELC + 8 * rot + vee + rot360)
        .add_lobe(ELC + 4 * rot + vee + rot360)
        .add_lobe(ELC + 10 * rot + vee + rot360)
        .add_lobe(ELC + 2 * rot + vee + rot360)
        .add_lobe(ELC + 6 * rot + vee + rot360)
}

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node head {
    // port_flow.py v1.7
    // intake port area: 14.3 cm²; saturated lift: 10.41 mm
    // exhaust port area: 10.4 cm²; saturated lift: 8.88 mm
    // cylinder volume: 408.9 cm³ (24.96 CI); engine volume: 4.907 L (299.46 CI)
    // chamber volume: 43.0 cm³ (2.63 CI); conrod/stroke 1.815; N.A.C.C. 55.03
    // 8 harmonic intake runner length: 15.0 cm; diameter: 4.4 cm
    // primary length: 11.4 cm, area: 12.5 cm², diameter: 4.0 cm
    // collector diameter: 12.0 cm, area: 112.5 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 43.046 * units.cc,
        intake_runner_volume: 229.8 * units.cc,
        intake_runner_cross_section_area: 15.3 * units.cm2,
        exhaust_runner_volume: 76.6 * units.cc,
        exhaust_runner_cross_section_area: 12.5 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "Porsche Type 912",
        starter_torque: 120 * units.Nm,
        starter_speed: 900 * units.rpm,
        redline: redline * units.rpm,
        fuel: r50(),
        throttle_gamma: 1.5,
        jitter: 0.4,
        noise: 0.2,
        hf_gain: 0.008,
        simulation_frequency: 8000
    )

    wires wires()
    
    connecting_rod_parameters cr_params(
        mass: con_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.g,
            length: con_rod * units.mm
        ),
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    label crank_moment(disk_moment_of_inertia(mass: crank_mass * units.kg, radius: (stroke / 2.0) * units.mm))
    label flywheel_moment(disk_moment_of_inertia(mass: flywheel_mass * units.kg, radius: (stroke * 3.5) * units.mm))
    label other_moment(disk_moment_of_inertia(mass: 1 * units.kg, radius: 1.0 * units.cm))

    crankshaft c0(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: flywheel_mass * units.kg,
        mass: crank_mass * units.kg,
        friction_torque: 7.4 * units.Nm,
        moment_of_inertia: crank_moment + flywheel_moment + other_moment,
        position_x: 0.0,
        position_y: 0.0,
        tdc: 0.0
    )

    piston_parameters piston_params(
        mass: piston_mass * units.g,
        blowby: k_28inH2O(0.027),
        compression_height: compression_height,
        wrist_pin_position: 0.0,
        displacement: 0.0
    )

    rod_journal rj0(angle: rot * 0 + rot90 + vee / 2)
    rod_journal rj1(angle: rot * 8 + rot90 + vee / 2)
    rod_journal rj2(angle: rot * 4 + rot90 + vee / 2)
    rod_journal rj3(angle: rot * 10 + rot90 + vee / 2)
    rod_journal rj4(angle: rot * 2 + rot90 + vee / 2)
    rod_journal rj5(angle: rot * 6 + rot90 + vee / 2)
    
    c0
        .add_rod_journal(rj0)
        .add_rod_journal(rj1)
        .add_rod_journal(rj2)
        .add_rod_journal(rj3)
        .add_rod_journal(rj4)
        .add_rod_journal(rj5)

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    label collector_cross_section_area(56.2)

    label exhaust_pipe_length_0(105.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 1000.0) // Litres
    
    label exhaust_pipe_length_1(100.0) // cm
    label exhaust_volume_1(collector_cross_section_area * exhaust_pipe_length_1 / 1000.0) // Litres

    intake intake(
        plenum_volume: 7.4 * units.L,
        plenum_cross_section_area: 92.0 * units.cm2,
        runner_length: 15.0 * units.cm,
        intake_flow_rate: k_carb(858.8),
        idle_flow_rate: k_carb(0.02),
        idle_throttle_plate_position: 0.9955,
        runner_flow_rate: k_carb(171.8),
        velocity_decay: 0.05
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(787.2),
        collector_cross_section_area: 56.2 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 11.4 * units.cm,
        primary_flow_rate: k_carb(288.6),
        velocity_decay: 0.75,
        volume: exhaust_volume_0 * units.L
    )
    
    exhaust_system_parameters es_params1(
        outlet_flow_rate: k_carb(787.2),
        collector_cross_section_area: 56.2 * units.cm2,
        length: exhaust_pipe_length_1 * units.cm,
        primary_tube_length: 11.4 * units.cm,
        primary_flow_rate: k_carb(288.6),
        velocity_decay: 0.75,
        volume: exhaust_volume_1 * units.L
    )

    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_20.wav", volume: 0.001))
    exhaust_system exhaust1(es_params1, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_20.wav", volume: 0.001))

    cylinder_bank b0(bank_params, angle: vee / 2)
    cylinder_bank b1(bank_params, angle: -vee / 2)
    
    label spacing_factor(1.3)
    label flange_density(1.0 * exhaust_delay_coeff)
    label x(0.1)
    label pl0 (((1.3 + x) * bore * spacing_factor / flange_density) * units.mm)
    label pl1 (((1.2 + x) * bore * spacing_factor / flange_density) * units.mm)
    label pl2 (((1.0 + x) * bore * spacing_factor / flange_density) * units.mm)
    label pl3 ((1.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl4 ((1.2 * bore * spacing_factor / flange_density) * units.mm)
    label pl5 ((1.3 * bore * spacing_factor / flange_density) * units.mm)

    b0
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj0,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl0,
            ignition_wire: wires.wire1
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj1,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl1,
            ignition_wire: wires.wire2
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj2,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl2,
            ignition_wire: wires.wire3
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj3,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl3,
            ignition_wire: wires.wire4
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj4,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl4,
            ignition_wire: wires.wire5
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj5,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl5,
            ignition_wire: wires.wire6
        )
        
    b1
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj0,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl0,
            ignition_wire: wires.wire7
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj1,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl1,
            ignition_wire: wires.wire8
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj2,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl2,
            ignition_wire: wires.wire9
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj3,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl3,
            ignition_wire: wires.wire10
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj4,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl4,
            ignition_wire: wires.wire11
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj5,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl5,
            ignition_wire: wires.wire12
        )

    engine
        .add_cylinder_bank(b0)
        .add_cylinder_bank(b1)

    engine.add_crankshaft(c0)

    camshaft_builder camshaft()

    b0.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_0,
            exhaust_camshaft: camshaft.exhaust_cam_0,
            flip_display: true
        )
    )
    b1.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_1,
            exhaust_camshaft: camshaft.exhaust_cam_1
        )
    )

    function timing_curve(583.3 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 2.0 * units.deg)
        .add_sample(583.3 * units.rpm, 2.0 * units.deg)
        .add_sample(1166.7 * units.rpm, 2.9 * units.deg)
        .add_sample(1750.0 * units.rpm, 5.9 * units.deg)
        .add_sample(2333.3 * units.rpm, 8.9 * units.deg)
        .add_sample(2916.7 * units.rpm, 11.9 * units.deg)
        .add_sample(3500.0 * units.rpm, 14.9 * units.deg)
        .add_sample(4083.3 * units.rpm, 17.9 * units.deg)
        .add_sample(4666.7 * units.rpm, 20.9 * units.deg)
        .add_sample(5250.0 * units.rpm, 23.9 * units.deg)
        .add_sample(5833.3 * units.rpm, 26.9 * units.deg)
        .add_sample(6416.7 * units.rpm, 30.0 * units.deg)
        .add_sample(7000.0 * units.rpm, 33.0 * units.deg)
        .add_sample(7583.3 * units.rpm, 36.0 * units.deg)
        .add_sample(8166.7 * units.rpm, 39.0 * units.deg)
        .add_sample(8750.0 * units.rpm, 42.0 * units.deg)
        .add_sample(9333.3 * units.rpm, -12.5 * units.deg)
        .add_sample(9916.7 * units.rpm, -25.0 * units.deg)

    engine.add_ignition_module(
        eng_distributor(
            wires: wires,
            timing_curve: timing_curve
        )
    )
}

// 917 longtail
public node veh {
    alias output __out: vehicle;
    vehicle vehicle(
        mass: 900.0  * units.kg, // 800 empty
        drag_coefficient: 0.31,
        cross_sectional_area: (1980 * units.mm) * (940 * units.mm),
        diff_ratio: 4.625, // 37/8
        tire_radius: (883.0 / 2) * units.mm, // 12.5/26.0 - 15 (R)
        rolling_resistance: 20
        )
}

// 917 long tail
private node trn {
    alias output __out:
        transmission(
            max_clutch_torque: 1000 * units.Nm,
            max_clutch_flex: 10 * units.deg,
            limit_clutch_flex: true,
            clutch_stiffness: 20 * units.Nm / units.deg,
            clutch_damping: 2.0,
            simulate_flex: true
        )
        .add_gear(1.933) // 29/15
        .add_gear(1.381) // 29/21
        .add_gear(1.043) // 24/23
        .add_gear(0.808) // 21/26
        .add_gear(0.7);  // 21/30
}

run(
    engine: eng(),
    transmission: trn(),
    vehicle: veh()
)

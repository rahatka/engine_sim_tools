private node add_lobes {
    input base;
    input shift: 0.0;
    input var;
    input this;
    alias output __out: this;

    this.add_lobe(base + var.s1 * rot_base + shift)
    this.add_lobe(base + var.s2 * rot_base + shift)
    this.add_lobe(base + var.s2 * rot_base + second_phase + shift)
    this.add_lobe(base + var.s1 * rot_base + second_phase + shift)
}

public node bank_builder {
    input var;
    input incline;
    input phasing;
    input ignition;
    input intake;
    input exhaust;
    input rj0;
    input rj1;
    input rj2;
    input rj3;
    input sh;
    input flip_display: false;

    output b0: _b0;

    ignition_wire _wire1()
    ignition_wire _wire2()
    ignition_wire _wire3()
    ignition_wire _wire4()

    piston_parameters piston_params(
        mass: piston_mass * units.g,
        blowby: k_28inH2O(0.027),
        compression_height: compression_height,
        wrist_pin_position: 0.0,
        displacement: 0.0
    )

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    connecting_rod_parameters cr_params(
        mass: con_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.g,
            length: con_rod * units.mm
        ),
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    camshaft _intake_cam_0(base_radius: 20.0 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 20.0 * units.mm, lobe_profile: exhaust_lobe_profile)

    _intake_cam_0.add_lobes(ILC, phasing, var)
    _exhaust_cam_0.add_lobes(ELC, phasing, var)

    ignition.connect_wire(_wire1, rot_base * var.s1 + phasing)
    ignition.connect_wire(_wire2, rot_base * var.s2 + phasing)
    ignition.connect_wire(_wire3, rot_base * var.s2 + second_phase + phasing)
    ignition.connect_wire(_wire4, rot_base * var.s1 + second_phase + phasing)

    cylinder_bank _b0(bank_params, angle: incline)

    label spacing_factor(1.1)
    label flange_density(5.0 * exhaust_delay_coeff)

    label pl0 ((4.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl1 ((3.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl2 ((2.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl3 ((1.0 * bore * spacing_factor / flange_density) * units.mm)

    _b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj0,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl0 + sh,
            ignition_wire: _wire1
        )
    _b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj1,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl1 + sh,
            ignition_wire: _wire2
        )
    _b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj2,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl2 + sh,
            ignition_wire: _wire3
        )
    _b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj3,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl3 + sh,
            ignition_wire: _wire4
        )
    _b0.set_cylinder_head(
        head(
            intake_camshaft: _intake_cam_0,
            exhaust_camshaft: _exhaust_cam_0,
            flip_display: flip_display
        )
    )
}

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node head {
    // port_flow.py v1.7
    // intake port area: 17.6 cm²; saturated lift: 11.55 mm
    // exhaust port area: 12.6 cm²; saturated lift: 9.78 mm
    // cylinder volume: 374.5 cm³ (22.85 CI); engine volume: 5.992 L (365.67 CI)
    // chamber volume: 40.3 cm³ (2.46 CI); conrod/stroke 2.500; N.A.C.C. 71.67
    // 8 harmonic intake runner length: 17.3 cm; diameter: 4.3 cm
    // primary length: 12.4 cm, area: 13.9 cm², diameter: 4.2 cm
    // collector diameter: 11.9 cm, area: 111.1 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 40.271 * units.cc,
        intake_runner_volume: 245.5 * units.cc,
        intake_runner_cross_section_area: 14.2 * units.cm2,
        exhaust_runner_volume: 81.8 * units.cc,
        exhaust_runner_cross_section_area: 13.9 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

public node crank {
    input phase;
    input var;

    output c: _c;
    output rj0: _rj0;
    output rj2: _rj2;
    output rj4: _rj4;
    output rj6: _rj6;

    output rj1: _rj1;
    output rj3: _rj3;
    output rj5: _rj5;
    output rj7: _rj7;

    label crank_moment(disk_moment_of_inertia(mass: crank_mass * units.kg, radius: (stroke / 2.0) * units.mm))
    label flywheel_moment(disk_moment_of_inertia(mass: flywheel_mass * units.kg, radius: (stroke * 3.5) * units.mm))
    label other_moment(disk_moment_of_inertia(mass: 1 * units.kg, radius: 1.0 * units.cm))

    crankshaft _c(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: flywheel_mass * units.kg,
        mass: crank_mass * units.kg,
        friction_torque: 9.0 * units.Nm,
        moment_of_inertia: crank_moment + flywheel_moment + other_moment,
        tdc: 0
    )

    rod_journal _rj0(angle: rot_base * var.s1 + phase)
    rod_journal _rj2(angle: rot_base * var.s2 + phase)
    rod_journal _rj4(angle: rot_base * var.s2 + phase + second_phase)
    rod_journal _rj6(angle: rot_base * var.s1 + phase + second_phase)

    rod_journal _rj1(angle: rot_base * var.s1 + phase + rot180)
    rod_journal _rj3(angle: rot_base * var.s2 + phase + rot180)
    rod_journal _rj5(angle: rot_base * var.s2 + phase + second_phase + rot180)
    rod_journal _rj7(angle: rot_base * var.s1 + phase + second_phase + rot180)

    _c
        .add_rod_journal(_rj0)
        .add_rod_journal(_rj1)
        .add_rod_journal(_rj2)
        .add_rod_journal(_rj3)
        .add_rod_journal(_rj4)
        .add_rod_journal(_rj5)
        .add_rod_journal(_rj6)
        .add_rod_journal(_rj7)
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "908 X16",
        starter_torque: 100 * units.Nm,
        starter_speed: 500 * units.rpm,
        redline: redline * units.rpm,
        fuel: r50(),
        throttle_gamma: 1.5,
        jitter: 0.8,
        noise: 0.1,
        hf_gain: 0.008,
        simulation_frequency: 7000,
        dyno_min_speed: 1000 * units.rpm,
        fluid_simulation_steps: 6,
        max_sle_solver_steps: 32
    )

    //////////////////////////////
    //// change variants here ////
    //////////////////////////////
    variant_0 variant()

    crank c0(vee - phase_diff / 2, variant)

    ignition_module ignition(timing_curve: timing_curve, rev_limit: redline * 1.05 * units.rpm, limiter_duration: 0.05)

    bank_builder bank0(
        var: variant,
        incline: vee / 2 - phase_diff / 2,
        phasing: 0,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust0,
        rj0: c0.rj0, rj1: c0.rj2, rj2: c0.rj4, rj3: c0.rj6,
        sh: 0,
        flip_display: true
    )

    bank_builder bank1(
        var: variant,
        incline: vee / 2 - phase_diff / 2 + rot180,
        phasing: vee + rot180,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust1,
        rj0: c0.rj1, rj1: c0.rj3, rj2: c0.rj5, rj3: c0.rj7,
        sh: sh,
        flip_display: true
    )

    bank_builder bank2(
        var: variant,
        incline: vee / 2 + phase_diff / 2,
        phasing: -phase_diff + rot360,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust0,
        rj0: c0.rj0, rj1: c0.rj2, rj2: c0.rj4, rj3: c0.rj6,
        sh: sh
    )

    bank_builder bank3(
        var: variant,
        incline: vee / 2 + phase_diff / 2 + rot180,
        phasing: -phase_diff + vee + rot180 + rot360,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust1,
        rj0: c0.rj1, rj1: c0.rj3, rj2: c0.rj5, rj3: c0.rj7,
        sh: 0
    )

    label collector_cross_section_area(55.6)

    label sh(3.3 * units.cm)
    label exhaust_pipe_length_0(103.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 100.0) // Litres
    
    label exhaust_pipe_length_1(100.0) // cm
    label exhaust_volume_1(collector_cross_section_area * exhaust_pipe_length_1 / 100.0) // Litres

    intake intake(
        plenum_volume: 9.6 * units.L,
        plenum_cross_section_area: 113.7 * units.cm2,
        intake_flow_rate: k_carb(1123.6),
        idle_flow_rate: k_carb(0.12),
        idle_throttle_plate_position: 0.9975,
        runner_flow_rate: k_carb(175.6),
        runner_length: 17.3 * units.cm,
        velocity_decay: 0.05
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(1053.3),
        collector_cross_section_area: 55.6 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 12.4 * units.cm,
        primary_flow_rate: k_carb(296.2),
        velocity_decay: 0.7,
        volume: exhaust_volume_0 * units.L
    )
    
    exhaust_system_parameters es_params1(
        outlet_flow_rate: k_carb(1053.3),
        collector_cross_section_area: 55.6 * units.cm2,
        length: exhaust_pipe_length_1 * units.cm,
        primary_tube_length: 12.4 * units.cm,
        primary_flow_rate: k_carb(296.2),
        velocity_decay: 0.7,
        volume: exhaust_volume_1 * units.L
    )
    
    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_45.wav", volume: 0.001))
    exhaust_system exhaust1(es_params1, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_58.wav", volume: 0.001))
 
    // ignition_timing.py v0.3 from = 8.0, to = 42.0, i = 900, r = 9000
    function timing_curve(900.0 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 0.0 * units.deg)
        .add_sample(900.0 * units.rpm, 4.0 * units.deg)
        .add_sample(1800.0 * units.rpm, 8.0 * units.deg)
        .add_sample(2700.0 * units.rpm, 15.9 * units.deg)
        .add_sample(3600.0 * units.rpm, 22.3 * units.deg)
        .add_sample(4500.0 * units.rpm, 27.4 * units.deg)
        .add_sample(5400.0 * units.rpm, 31.7 * units.deg)
        .add_sample(6300.0 * units.rpm, 35.1 * units.deg)
        .add_sample(7200.0 * units.rpm, 37.9 * units.deg)
        .add_sample(8100.0 * units.rpm, 40.2 * units.deg)
        .add_sample(9000.0 * units.rpm, 42.0 * units.deg)
        .add_sample(9900.0 * units.rpm, -15.0 * units.deg)
        .add_sample(10800.0 * units.rpm, -60.0 * units.deg)

    engine
        .add_crankshaft(c0.c)
        .add_cylinder_bank(bank0.b0)
        .add_cylinder_bank(bank1.b0)
        .add_cylinder_bank(bank2.b0)
        .add_cylinder_bank(bank3.b0)
        .add_ignition_module(ignition)
}

public node veh {
    alias output __out: vehicle;
    vehicle vehicle(
        mass: 650 * units.kg,
        drag_coefficient: 0.35,
        cross_sectional_area: (1720 * units.mm) * (930 * units.mm),
        diff_ratio: 3.3,
        tire_radius: (621.0 / 2) * units.mm,
        rolling_resistance: 20
        )
}

private node trn {
    alias output __out:
        transmission(
            max_clutch_torque: 1000 * units.Nm,
            max_clutch_flex: 30 * units.deg,
            limit_clutch_flex: true,
            clutch_stiffness: 20 * units.Nm / units.deg,
            clutch_damping: 1.0,
            simulate_flex: true
        )
        .add_gear(2.66)
        .add_gear(1.78)
        .add_gear(1.30)
        .add_gear(1.00)
        .add_gear(0.74);
}

run(
    engine: eng(),
    transmission: trn(),
    vehicle: veh()
)

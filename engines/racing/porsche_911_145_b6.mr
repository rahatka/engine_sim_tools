private node add_lobes_0 {
    input base;
    input shift: 0.0;
    input var;
    input this;
    alias output __out: this;

    this.add_lobe(base + var.s1 * rot_base + shift)
    this.add_lobe(base + var.s2 * rot_base + shift)
    this.add_lobe(base + var.s3 * rot_base + shift)
}

public node bank_builder {
    input var;
    input incline;
    input phasing;
    input ignition;
    input intake;
    input exhaust;
    input sh;
    input rj0;
    input rj1;
    input rj2;
    input flip_display: false;

    output b0: _b0;

    ignition_wire _wire1()
    ignition_wire _wire2()
    ignition_wire _wire3()

    piston_parameters piston_params(
        mass: piston_mass * units.g,
        blowby: k_28inH2O(0.027),
        compression_height: compression_height,
        wrist_pin_position: 0.0,
        displacement: 0.0
    )

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    connecting_rod_parameters cr_params(
        mass: con_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.g,
            length: con_rod * units.mm
        ),
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    camshaft _intake_cam_0(base_radius: 19.4 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 19.4 * units.mm, lobe_profile: exhaust_lobe_profile)

    _intake_cam_0.add_lobes_0(ILC, phasing, var)
    _exhaust_cam_0.add_lobes_0(ELC, phasing, var)

    ignition.connect_wire(_wire1, rot_base * var.s1 + phasing)
    ignition.connect_wire(_wire2, rot_base * var.s2 + phasing)
    ignition.connect_wire(_wire3, rot_base * var.s3 + phasing)

    cylinder_bank _b0(bank_params, angle: incline)

    label spacing_factor(1.3)
    label flange_density(2.5)

    label pl0 ((6.0 * bore * spacing_factor / flange_density) * units.mm + sh)
    label pl1 ((5.5 * bore * spacing_factor / flange_density) * units.mm + sh)
    label pl2 ((4.0 * bore * spacing_factor / flange_density) * units.mm + sh)

    _b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj0,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl0,
            ignition_wire: _wire1
        )
    _b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj1,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl1,
            ignition_wire: _wire2
        )
    _b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj2,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl2,
            ignition_wire: _wire3
        )

    _b0.set_cylinder_head(
        head(
            intake_camshaft: _intake_cam_0,
            exhaust_camshaft: _exhaust_cam_0,
            flip_display: flip_display
        )
    )
}

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node head {
    // port_flow.py v1.7
    // intake port area: 10.7 cm²; saturated lift: 8.97 mm
    // exhaust port area: 10.7 cm²; saturated lift: 8.97 mm
    // cylinder volume: 396.9 cm³ (24.22 CI); engine volume: 2.381 L (145.31 CI)
    // chamber volume: 42.7 cm³ (2.60 CI); conrod/stroke 1.970; N.A.C.C. 28.48
    // 8 harmonic intake runner length: 16.1 cm; diameter: 4.2 cm
    // primary length: 12.7 cm, area: 11.2 cm², diameter: 3.8 cm
    // collector diameter: 5.5 cm, area: 23.6 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 42.674 * units.cc,
        intake_runner_volume: 222.7 * units.cc,
        intake_runner_cross_section_area: 13.8 * units.cm2,
        exhaust_runner_volume: 74.2 * units.cc,
        exhaust_runner_cross_section_area: 11.2 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

public node crank {
    input phasing;
    input var;

    output c: _c;
    output rj0: _rj0;
    output rj2: _rj2;
    output rj4: _rj4;

    output rj1: _rj1;
    output rj3: _rj3;
    output rj5: _rj5;

    label crank_moment(disk_moment_of_inertia(mass: crank_mass * units.kg, radius: (stroke / 2.0) * units.mm))
    label flywheel_moment(disk_moment_of_inertia(mass: flywheel_mass * units.kg, radius: (stroke * 3.5) * units.mm))
    label other_moment(disk_moment_of_inertia(mass: 1 * units.kg, radius: 1.0 * units.cm))

    crankshaft _c(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: flywheel_mass * units.kg,
        mass: crank_mass * units.kg,
        friction_torque: 3.6 * units.Nm,
        moment_of_inertia: crank_moment + flywheel_moment + other_moment,
        tdc: 0.0
    )

    rod_journal _rj0(angle: rot_base * var.s1 + rot90 + vee / 2)
    rod_journal _rj2(angle: rot_base * var.s2 + rot90 + vee / 2)
    rod_journal _rj4(angle: rot_base * var.s3 + rot90 + vee / 2)

    rod_journal _rj1(angle: rot_base * var.s1 + rot90 + vee / 2 + rot180)
    rod_journal _rj3(angle: rot_base * var.s2 + rot90 + vee / 2 + rot180)
    rod_journal _rj5(angle: rot_base * var.s3 + rot90 + vee / 2 + rot180)

    _c
        .add_rod_journal(_rj0)
        .add_rod_journal(_rj1)
        .add_rod_journal(_rj2)
        .add_rod_journal(_rj3)
        .add_rod_journal(_rj4)
        .add_rod_journal(_rj5)
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "Porsche 911 2.4 Renn",
        starter_torque: 70 * units.Nm,
        starter_speed: 300 * units.rpm,
        redline: redline * units.rpm,
        fuel: r50(),
        throttle_gamma: 1.75,
        jitter: 0.8,
        noise: 0.1,
        hf_gain: 0.007,
        simulation_frequency: 10000,
        fluid_simulation_steps: 8,
        max_sle_solver_steps: 64,
        dyno_min_speed: 1000 * units.rpm
    )

    //////////////////////////////
    //// change variants here ////
    //////////////////////////////
    variant_1 variant()

    crank c0(vee + rot90, variant)

    ignition_module ignition(timing_curve: timing_curve, rev_limit: redline * 1.2 * units.rpm, limiter_duration: 0.05)

    bank_builder bank0(
        var: variant,
        incline: vee / 2,
        phasing: 0,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust0,
        sh: 0 * units.cm,
        rj0: c0.rj0, rj1: c0.rj2, rj2: c0.rj4,
        flip_display: true
    )

    bank_builder bank1(
        var: variant,
        incline: -vee / 2,
        phasing: vee + rot180,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust1,
        sh: 0 * units.cm,
        rj0: c0.rj1, rj1: c0.rj3, rj2: c0.rj5
    )

    label collector_cross_section_area(11.8)

    label exhaust_pipe_length_0(100.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 100.0) // Litres
    
    label exhaust_pipe_length_1(133.0) // cm
    label exhaust_volume_1(collector_cross_section_area * exhaust_pipe_length_1 / 100.0) // Litres

    intake intake(
        plenum_volume: 3.1 * units.L,
        plenum_cross_section_area: 41.5 * units.cm2,
        runner_length: 16.1 * units.cm,
        intake_flow_rate: k_carb(379.5),
        idle_flow_rate: k_carb(0.05),
        idle_throttle_plate_position: 0.996,
        runner_flow_rate: k_carb(142.3),
        velocity_decay: 0.03
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(336.0),
        collector_cross_section_area: 11.8 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 12.7 * units.cm,
        primary_flow_rate: k_carb(238.0),
        velocity_decay: 0.75,
        volume: exhaust_volume_0 * units.L
    )
    
    exhaust_system_parameters es_params1(
        outlet_flow_rate: k_carb(336.0),
        collector_cross_section_area: 11.8 * units.cm2,
        length: exhaust_pipe_length_1 * units.cm,
        primary_tube_length: 12.7 * units.cm,
        primary_flow_rate: k_carb(238.0),
        velocity_decay: 0.75,
        volume: exhaust_volume_1 * units.L
    )

    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_19.wav", volume: 0.00025))
    exhaust_system exhaust1(es_params1, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_19.wav", volume: 0.00024))

    engine
        .add_crankshaft(c0.c)
        .add_cylinder_bank(bank0.b0)
        .add_cylinder_bank(bank1.b0)
        .add_ignition_module(ignition)

    function timing_curve(566.7 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 5.0 * units.deg)
        .add_sample(566.7 * units.rpm, 5.0 * units.deg)
        .add_sample(1133.3 * units.rpm, 6.5 * units.deg)
        .add_sample(1700.0 * units.rpm, 8.7 * units.deg)
        .add_sample(2266.7 * units.rpm, 10.9 * units.deg)
        .add_sample(2833.3 * units.rpm, 13.1 * units.deg)
        .add_sample(3400.0 * units.rpm, 15.3 * units.deg)
        .add_sample(3966.7 * units.rpm, 17.5 * units.deg)
        .add_sample(4533.3 * units.rpm, 19.6 * units.deg)
        .add_sample(5100.0 * units.rpm, 21.8 * units.deg)
        .add_sample(5666.7 * units.rpm, 24.0 * units.deg)
        .add_sample(6233.3 * units.rpm, 26.2 * units.deg)
        .add_sample(6800.0 * units.rpm, 28.4 * units.deg)
        .add_sample(7366.7 * units.rpm, 30.6 * units.deg)
        .add_sample(7933.3 * units.rpm, 32.8 * units.deg)
        .add_sample(8500.0 * units.rpm, 35.0 * units.deg)
        .add_sample(9066.7 * units.rpm, -12.5 * units.deg)
        .add_sample(9633.3 * units.rpm, -25.0 * units.deg)
}


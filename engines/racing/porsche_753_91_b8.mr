units units()
constants constants()

label vee(180.0 * units.deg)
label bore(66.0)
label stroke(54.6)
label compression_ratio(10.0)
label con_rod(126.0)
label compression_height(27.0 * units.mm) // ?
label intake_valve_diameter(37.0) // ~
label exhaust_valve_diameter(34.0) // ~
label intake_valves(1.0)
label exhaust_valves(1.0)

label cyl(8.0)
label row(2.0)
label cyl_base(cyl / row)
label cycle(720.0 * units.deg)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label rot(cycle / cyl)
label rot_base(rot180)

label v(1)
label second_phase(rot90 + rot180 * v)

label redline(9750.0) // RPM

// Porsche 753 valve timings
label IVL(10.55)
label EVL(9.3)
label IVO(81.0 * units.deg) //BTDC
label IVC(71.0 * units.deg) //ABDC
label EVO(81.0 * units.deg) //BBDC
label EVC(51.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(350.0) // ? g
label piston_mass(250.0) // ? g
label crank_mass(10.0) // ? kg
label flywheel_mass(4.0) // kg

label exhaust_delay_coeff(1.5)


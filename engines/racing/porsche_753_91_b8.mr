private node add_lobes {
    input base;
    input shift: 0.0;
    input var;
    input this;
    alias output __out: this;

    this.add_lobe(base + var.s1 * rot_base + shift)
    this.add_lobe(base + var.s2 * rot_base + shift)
    this.add_lobe(base + var.s2 * rot_base + second_phase + shift)
    this.add_lobe(base + var.s1 * rot_base + second_phase + shift)
}

public node bank_builder {
    input var;
    input incline;
    input phasing;
    input ignition;
    input intake;
    input exhaust;
    input rj0;
    input rj1;
    input rj2;
    input rj3;
    input flip_display: false;

    output b0: _b0;

    ignition_wire _wire1()
    ignition_wire _wire2()
    ignition_wire _wire3()
    ignition_wire _wire4()

    piston_parameters piston_params(
        mass: piston_mass * units.g,
        blowby: k_28inH2O(0.021),
        compression_height: compression_height,
        wrist_pin_position: 0.0,
        displacement: 0.0
    )

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    connecting_rod_parameters cr_params(
        mass: con_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.g,
            length: con_rod * units.mm
        ),
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    camshaft _intake_cam_0(base_radius: 19.9 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 19.9 * units.mm, lobe_profile: exhaust_lobe_profile)

    _intake_cam_0.add_lobes(ILC, phasing, var)
    _exhaust_cam_0.add_lobes(ELC, phasing, var)

    ignition.connect_wire(_wire1, rot_base * var.s1 + phasing)
    ignition.connect_wire(_wire2, rot_base * var.s2 + phasing)
    ignition.connect_wire(_wire3, rot_base * var.s2 + second_phase + phasing)
    ignition.connect_wire(_wire4, rot_base * var.s1 + second_phase + phasing)

    cylinder_bank _b0(bank_params, angle: incline)

    label spacing_factor(1.1)
    label flange_density(1.0 * exhaust_delay_coeff)

    label pl0 ((4.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl1 ((3.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl2 ((2.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl3 ((1.0 * bore * spacing_factor / flange_density) * units.mm)

    _b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj0,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl0,
            ignition_wire: _wire1
        )
    _b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj1,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl1,
            ignition_wire: _wire2
        )
    _b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj2,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl2,
            ignition_wire: _wire3
        )
    _b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj3,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl3,
            ignition_wire: _wire4
        )
    _b0.set_cylinder_head(
        head(
            intake_camshaft: _intake_cam_0,
            exhaust_camshaft: _exhaust_cam_0,
            flip_display: flip_display
        )
    )
}

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node head {
    // port_flow.py v1.5
    // intake port area: 8.7 cm²; saturated lift: 8.11 mm
    // exhaust port area: 7.3 cm²; saturated lift: 7.45 mm
    // cylinder volume: 186.8 cm³ (11.40 CI); engine volume: 1.494 L (91.19 CI)
    // chamber volume: 20.8 cm³ (1.27 CI); conrod/stroke 2.308
    // 8 harmonic intake runner length: 15.8 cm; diameter: 3.1 cm
    // primary length: 11.2 cm, area: 8.1 cm², diameter: 3.2 cm
    // collector diameter: 7.0 cm, area: 38.8 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 20.755 * units.cc,
        intake_runner_volume: 122.2 * units.cc,
        intake_runner_cross_section_area: 7.8 * units.cm2,
        exhaust_runner_volume: 40.7 * units.cc,
        exhaust_runner_cross_section_area: 8.1 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

public node crank {
    input phasing;
    input var;

    output c: _c;
    output rj0: _rj0;
    output rj2: _rj2;
    output rj4: _rj4;
    output rj6: _rj6;

    output rj1: _rj1;
    output rj3: _rj3;
    output rj5: _rj5;
    output rj7: _rj7;

    label crank_moment(disk_moment_of_inertia(mass: crank_mass * units.kg, radius: (stroke / 2.0) * units.mm))
    label flywheel_moment(disk_moment_of_inertia(mass: flywheel_mass * units.kg, radius: (stroke * 3.5) * units.mm))
    label other_moment(disk_moment_of_inertia(mass: 1 * units.kg, radius: 1.0 * units.cm))

    crankshaft _c(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: flywheel_mass * units.kg,
        mass: crank_mass * units.kg,
        friction_torque: 2.2 * units.Nm,
        moment_of_inertia: crank_moment + flywheel_moment + other_moment,
        tdc: 0
    )

    rod_journal _rj0(angle: rot_base * var.s1 + vee)
    rod_journal _rj2(angle: rot_base * var.s2 + vee)
    rod_journal _rj4(angle: rot_base * var.s2 + vee + second_phase)
    rod_journal _rj6(angle: rot_base * var.s1 + vee + second_phase)

    rod_journal _rj1(angle: rot_base * var.s1 + vee + rot180)
    rod_journal _rj3(angle: rot_base * var.s2 + vee + rot180)
    rod_journal _rj5(angle: rot_base * var.s2 + vee + second_phase + rot180)
    rod_journal _rj7(angle: rot_base * var.s1 + vee + second_phase + rot180)

    _c
        .add_rod_journal(_rj0)
        .add_rod_journal(_rj1)
        .add_rod_journal(_rj2)
        .add_rod_journal(_rj3)
        .add_rod_journal(_rj4)
        .add_rod_journal(_rj5)
        .add_rod_journal(_rj6)
        .add_rod_journal(_rj7)
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "Porsche 753",
        starter_torque: 50 * units.Nm,
        starter_speed: 500 * units.rpm,
        redline: redline * units.rpm,
        fuel: r50(),
        throttle_gamma: 1.5,
        jitter: 0.8,
        noise: 0.1,
        hf_gain: 0.008,
        simulation_frequency: 12000
    )

    //////////////////////////////
    //// change variants here ////
    //////////////////////////////
    variant_0 variant()

    crank c0(vee + rot90, variant)

    ignition_module ignition(timing_curve: timing_curve, rev_limit: redline * 1.05 * units.rpm, limiter_duration: 0.05)

    bank_builder bank0(
        var: variant,
        incline: vee / 2,
        phasing: 0,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust0,
        rj0: c0.rj0, rj1: c0.rj2, rj2: c0.rj4, rj3: c0.rj6,
        flip_display: true
    )

    bank_builder bank1(
        var: variant,
        incline: -vee / 2,
        phasing: vee + rot180,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust1,
        rj0: c0.rj1, rj1: c0.rj3, rj2: c0.rj5, rj3: c0.rj7
    )

    label collector_cross_section_area(19.4)

    label exhaust_pipe_length_0(107.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 100.0) // Litres
    
    label exhaust_pipe_length_1(100.0) // cm
    label exhaust_volume_1(collector_cross_section_area * exhaust_pipe_length_1 / 100.0) // Litres

    intake intake(
        plenum_volume: 2.6 * units.L,
        plenum_cross_section_area: 31.0 * units.cm2,
        runner_length: 15.8 * units.cm,
        intake_flow_rate: k_carb(303.5),
        idle_flow_rate: k_carb(0.08),
        idle_throttle_plate_position: 0.996,
        runner_flow_rate: k_carb(94.9),
        velocity_decay: 0.05
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(284.6),
        collector_cross_section_area: 19.4 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 11.2 * units.cm,
        primary_flow_rate: k_carb(160.1),
        velocity_decay: 0.6,
        volume: exhaust_volume_0 * units.L
    )
    
    exhaust_system_parameters es_params1(
        outlet_flow_rate: k_carb(284.6),
        collector_cross_section_area: 19.4 * units.cm2,
        length: exhaust_pipe_length_1 * units.cm,
        primary_tube_length: 11.2 * units.cm,
        primary_flow_rate: k_carb(160.1),
        velocity_decay: 0.6,
        volume: exhaust_volume_1 * units.L
    )

    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_45.wav", volume: 0.001))
    exhaust_system exhaust1(es_params1, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_45.wav", volume: 0.001))

    engine
        .add_crankshaft(c0.c)
        .add_cylinder_bank(bank0.b0)
        .add_cylinder_bank(bank1.b0)
        .add_ignition_module(ignition)

    // ignition_timing.py v0.3 from = 8.0, to = 33.0, i = 500, r = 9750
    function timing_curve(975.0 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 0.0 * units.deg)
        .add_sample(975.0 * units.rpm, 8.0 * units.deg)
        .add_sample(1950.0 * units.rpm, 12.8 * units.deg)
        .add_sample(2925.0 * units.rpm, 16.9 * units.deg)
        .add_sample(3900.0 * units.rpm, 20.4 * units.deg)
        .add_sample(4875.0 * units.rpm, 23.4 * units.deg)
        .add_sample(5850.0 * units.rpm, 26.0 * units.deg)
        .add_sample(6825.0 * units.rpm, 28.2 * units.deg)
        .add_sample(7800.0 * units.rpm, 30.0 * units.deg)
        .add_sample(8775.0 * units.rpm, 31.6 * units.deg)
        .add_sample(9750.0 * units.rpm, 33.0 * units.deg)
        .add_sample(10725.0 * units.rpm, -15.0 * units.deg)
        .add_sample(11700.0 * units.rpm, -60.0 * units.deg)
}

// Porsche 804
public node veh {
    alias output __out: vehicle;
    vehicle vehicle(
        mass: 520 * units.kg, // 452 dry
        drag_coefficient: 0.35,
        cross_sectional_area: (1615 * units.mm) * (820 * units.mm),
        diff_ratio: 5.5,
        tire_radius: (711.0 / 2) * units.mm, // 5.00 x 15 R front tyres, 6.50 x 15 R rear tyres
        rolling_resistance: 20
        )
}


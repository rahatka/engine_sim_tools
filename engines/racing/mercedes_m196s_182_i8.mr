public node camshaft_builder {
    output intake_cam_0: _intake_cam_0;
    output exhaust_cam_0: _exhaust_cam_0;

    camshaft _intake_cam_0(base_radius: 22.9 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 22.9 * units.mm, lobe_profile: exhaust_lobe_profile)

    _intake_cam_0
        .add_lobe(ILC + 0 * rot)
        .add_lobe(ILC + 1 * rot)
        .add_lobe(ILC + 7 * rot)
        .add_lobe(ILC + 2 * rot)
        .add_lobe(ILC + 6 * rot)
        .add_lobe(ILC + 3 * rot)
        .add_lobe(ILC + 5 * rot)
        .add_lobe(ILC + 4 * rot)

    _exhaust_cam_0
        .add_lobe(ELC + 0 * rot)
        .add_lobe(ELC + 1 * rot)
        .add_lobe(ELC + 7 * rot)
        .add_lobe(ELC + 2 * rot)
        .add_lobe(ELC + 6 * rot)
        .add_lobe(ELC + 3 * rot)
        .add_lobe(ELC + 5 * rot)
        .add_lobe(ELC + 4 * rot)
}

public node head {
    // port_flow.py v1.5
    // intake port area: 15.9 cm²; saturated lift: 10.96 mm
    // exhaust port area: 11.7 cm²; saturated lift: 9.43 mm
    // cylinder volume: 372.7 cm³ (22.74 CI); engine volume: 2.982 L (181.95 CI)
    // chamber volume: 46.6 cm³ (2.84 CI); conrod/stroke 1.859
    // 8 harmonic intake runner length: 24.6 cm; diameter: 3.9 cm
    // primary length: 17.3 cm, area: 17.6 cm², diameter: 4.7 cm
    // collector diameter: 7.3 cm, area: 42.3 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 46.589 * units.cc,
        intake_runner_volume: 286.2 * units.cc,
        intake_runner_cross_section_area: 11.6 * units.cm2,
        exhaust_runner_volume: 95.4 * units.cc,
        exhaust_runner_cross_section_area: 17.6 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "M 196 S",
        starter_torque: 150 *units.Nm,
        starter_speed: 500 * units.rpm,
        redline: 7500 * units.rpm,
        fuel: m9b1(),
        throttle_gamma: 1.5,
        jitter: 1.0,
        noise: 0.2,
        hf_gain: 0.008,
        simulation_frequency: 8000
    )

    wires wires()

    crankshaft c0(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: 3.2 * units.kg,
        mass: 20 * units.kg,
        friction_torque: 4.5 * units.Nm,
        moment_of_inertia: 0.15,
        tdc: 0.0
    )

    rod_journal rj0(angle: rot * 0 + rot90 + incline)
    rod_journal rj1(angle: rot * 1 + rot90 + incline)
    rod_journal rj2(angle: rot * 7 + rot90 + incline)
    rod_journal rj3(angle: rot * 2 + rot90 + incline)
    rod_journal rj4(angle: rot * 6 + rot90 + incline)
    rod_journal rj5(angle: rot * 3 + rot90 + incline)
    rod_journal rj6(angle: rot * 5 + rot90 + incline)
    rod_journal rj7(angle: rot * 4 + rot90 + incline)
    
    c0
        .add_rod_journal(rj0)
        .add_rod_journal(rj1)
        .add_rod_journal(rj2)
        .add_rod_journal(rj3)
        .add_rod_journal(rj4)
        .add_rod_journal(rj5)
        .add_rod_journal(rj6)
        .add_rod_journal(rj7)

    piston_parameters piston_params(
        mass: piston_mass * units.g,
        blowby: k_28inH2O(0.025),
        compression_height: compression_height
    )

    connecting_rod_parameters cr_params(
        mass: con_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.g,
            length: con_rod * units.mm
        ),
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    label collector_cross_section_area(21.1)

    label exhaust_pipe_length_0(120.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 100.0) // Litres
    
    label exhaust_pipe_length_1(125.0) // cm
    label exhaust_volume_1(collector_cross_section_area * exhaust_pipe_length_1 / 100.0) // Litres

    intake intake(
        plenum_volume: 3.6 * units.L,
        plenum_cross_section_area: 46.6 * units.cm2,
        runner_length: 24.6 * units.cm,
        intake_flow_rate: k_carb(445.8),
        idle_flow_rate: k_carb(0.08),
        idle_throttle_plate_position: 0.9967,
        runner_flow_rate: k_carb(128.2),
        velocity_decay: 0.05
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(399.3),
        collector_cross_section_area: 21.1 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 17.3 * units.cm,
        primary_flow_rate: k_carb(214.6),
        velocity_decay: 0.7,
        volume: (exhaust_volume_0 + 3.0) * units.L
    )
    
    exhaust_system_parameters es_params1(
        outlet_flow_rate: k_carb(399.3),
        collector_cross_section_area: 21.1 * units.cm2,
        length: exhaust_pipe_length_1 * units.cm,
        primary_tube_length: 17.3 * units.cm,
        primary_flow_rate: k_carb(214.6),
        velocity_decay: 0.7,
        volume: (exhaust_volume_1 + 3.0) * units.L
    )
    
    // default_0 real_engine_0 real_engine_1 real_engine_2 mild_exhaust_0 mild_exhaust_0_reverb minimal_muffling_01 minimal_muffling_02 minimal_muffling_03
    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_55.wav", volume: 0.001))
    exhaust_system exhaust1(es_params1, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_55.wav", volume: 0.001))
    
    label spacing_factor(1.1)
    label flange_density(1.5 * exhaust_delay_coeff)

    label pl0_0 ((7.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl1_0 ((4.5 * bore * spacing_factor / flange_density) * units.mm)
    label pl2_0 ((3.5 * bore * spacing_factor / flange_density) * units.mm)
    label pl3_0 ((1.0 * bore * spacing_factor / flange_density) * units.mm)

    label pl0_1 ((5.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl1_1 ((4.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl2_1 ((2.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl3_1 ((1.0 * bore * spacing_factor / flange_density) * units.mm)
 
    cylinder_bank b0(bank_params, angle: incline)
    b0
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj0,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl0_0,
            ignition_wire: wires.wire1
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj1,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl0_1,
            ignition_wire: wires.wire2
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj2,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl1_1,
            ignition_wire: wires.wire3
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj3,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl1_0,
            ignition_wire: wires.wire4
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj4,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl2_0,
            ignition_wire: wires.wire5
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj5,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl2_1,
            ignition_wire: wires.wire6
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj6,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl3_1,
            ignition_wire: wires.wire7
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj7,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl3_0,
            ignition_wire: wires.wire8
        )

    engine
        .add_cylinder_bank(b0)

    engine.add_crankshaft(c0)

    camshaft_builder camshaft()

    b0.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_0,
            exhaust_camshaft: camshaft.exhaust_cam_0,
            flip_display: true
        )
    )

    function timing_curve(520.0 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 0.0 * units.deg)
        .add_sample(520.0 * units.rpm, 1.3 * units.deg)
        .add_sample(1040.0 * units.rpm, 8.2 * units.deg)
        .add_sample(1560.0 * units.rpm, 13.7 * units.deg)
        .add_sample(2080.0 * units.rpm, 17.9 * units.deg)
        .add_sample(2600.0 * units.rpm, 21.2 * units.deg)
        .add_sample(3120.0 * units.rpm, 23.8 * units.deg)
        .add_sample(3640.0 * units.rpm, 25.8 * units.deg)
        .add_sample(4160.0 * units.rpm, 27.4 * units.deg)
        .add_sample(4680.0 * units.rpm, 28.6 * units.deg)
        .add_sample(5200.0 * units.rpm, 29.6 * units.deg)
        .add_sample(5720.0 * units.rpm, 30.3 * units.deg)
        .add_sample(6240.0 * units.rpm, 30.9 * units.deg)
        .add_sample(6760.0 * units.rpm, 31.4 * units.deg)
        .add_sample(7280.0 * units.rpm, 31.7 * units.deg)
        .add_sample(7800.0 * units.rpm, 32.0 * units.deg)
        .add_sample(8320.0 * units.rpm, -15.0 * units.deg)
        .add_sample(8840.0 * units.rpm, -45.0 * units.deg)

    engine.add_ignition_module(
        eng_distributor(
            wires: wires,
            timing_curve: timing_curve,
            rev_limit: 8000 * units.rpm
        ))
}

public node veh {
    alias output __out: vehicle;
    vehicle vehicle(
        mass: 1560  * units.kg,
        drag_coefficient: 0.35,
        cross_sectional_area: (1920 * units.mm) * (1200 * units.mm),
        diff_ratio: 5.07,
        tire_radius: (805.0 / 2) * units.mm,
        rolling_resistance: 20
        )
}

// 993
private node trn {
    alias output __out:
        transmission(
            max_clutch_torque: 400 * units.Nm,
            max_clutch_flex: 10 * units.deg,
            limit_clutch_flex: true,
            clutch_stiffness: 20 * units.Nm / units.deg,
            clutch_damping: 2.0,
            simulate_flex: true
        )
        .add_gear(3.82)
        .add_gear(2.05)
        .add_gear(1.41)
        .add_gear(1.12)
        .add_gear(0.93)
        .add_gear(0.78);
}

run(
    engine: eng(),
    transmission: trn(),
    vehicle: veh()
)

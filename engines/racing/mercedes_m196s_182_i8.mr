import "engine_sim.mr"
import "../../fuel/racing.mr"

units units()
constants constants()

label bore(78.0)
label stroke(78.0)
label compression_ratio(9.0) // 9.2 appendix_6
label con_rod(145.0) // appendix_6
label intake_valve_diameter(50.0)
label exhaust_valve_diameter(43.0)
label intake_valves(1.0)
label exhaust_valves(1.0)
label compression_height(93.0 / 2 * units.mm) // PH 93

label cyl(8.0)
label row(1.0)
label cycle(720.0 * units.deg)
label incline(53.0 * units.deg)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label rot(2 * (360 / 8.0) * units.deg)

label redline(7800.0) // RPM

// Mercedes-Benz M 196 S valve timings
label IVL(13.0)
label EVL(12.0)
label IVO(22.0 * units.deg) //BTDC
label IVC(62.0 * units.deg) //ABDC
label EVO(51.0 * units.deg) //BBDC
label EVC(13.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(350.0) // ? g
label piston_mass(350.0) // ? g
label crank_mass(13.0) // ? kg
label flywheel_mass(4.0) // kg

label exhaust_delay_coeff(1.5)

public node eng_distributor {
    input wires;
    input timing_curve;
    input rev_limit: 8000 * units.rpm;
    input limiter_duration: 0.03;
    alias output __out:
        ignition_module(timing_curve: timing_curve, rev_limit: rev_limit, limiter_duration: limiter_duration)
            .connect_wire(wires.wire1, rot * 0)
            .connect_wire(wires.wire2, rot * 1)
            .connect_wire(wires.wire4, rot * 2)
            .connect_wire(wires.wire6, rot * 3)
            .connect_wire(wires.wire8, rot * 4)
            .connect_wire(wires.wire7, rot * 5)
            .connect_wire(wires.wire5, rot * 6)
            .connect_wire(wires.wire3, rot * 7);
}

private node wires {
    output wire1: ignition_wire();
    output wire2: ignition_wire();
    output wire3: ignition_wire();
    output wire4: ignition_wire();
    output wire5: ignition_wire();
    output wire6: ignition_wire();
    output wire7: ignition_wire();
    output wire8: ignition_wire();
}


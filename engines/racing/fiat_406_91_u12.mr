import "engine_sim.mr"
import "../../fuel/racing.mr"

units units()
constants constants()

label bore(50.0)
label stroke(63.0)
label compression_ratio(7.0)
label con_rod(130.0)
label compression_height(60.0 / 2 * units.mm) // 60 PH
label intake_valve_diameter(30.0)
label exhaust_valve_diameter(30.0)
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(8500.0) // RPM

label cyl(12.0)
label row(2.0)
label cyl_base(cyl / row)
label cycle(720.0 * units.deg)
label separation(100.0 * units.mm)
label rot(cycle / cyl)
label rot_base(cycle / cyl_base)
label rot45(45.0 * units.deg)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label sb_shift(rot * 0)

// T404
label IVL(7.0) // 406
label EVL(7.0) // 406
label IVO(10.0 * units.deg) //BTDC
label IVC(62.0 * units.deg) //ABDC
label EVO(50.0 * units.deg) //BBDC
label EVC(15.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(350.0) // ?
label piston_mass(250.0) // ?
label crank_mass(8.0) // ?
label flywheel_mass(3.0) // kg

label exhaust_delay_coeff(1.5)


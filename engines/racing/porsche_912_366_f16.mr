private node add_lobes {
    input base;
    input shift: 0.0;
    input var;
    input this;
    alias output __out: this;

    this.add_lobe(base + var.s1 * rot_base + shift)
    this.add_lobe(base + var.s2 * rot_base + shift)
    this.add_lobe(base + var.s3 * rot_base + shift)
    this.add_lobe(base + var.s4 * rot_base + shift)
    this.add_lobe(base + var.s5 * rot_base + shift)
    this.add_lobe(base + var.s6 * rot_base + shift)
    this.add_lobe(base + var.s7 * rot_base + shift)
    this.add_lobe(base + var.s8 * rot_base + shift)
}

public node bank_builder {
    input var;
    input crankshaft;
    input incline;
    input phasing;
    input ignition;
    input intake;
    input exhaust;
    input flip: false;

    output bank: _b0;

    ignition_wire _wire1()
    ignition_wire _wire2()
    ignition_wire _wire3()
    ignition_wire _wire4()
    ignition_wire _wire5()
    ignition_wire _wire6()
    ignition_wire _wire7()
    ignition_wire _wire8()

    piston_parameters piston_params(
        mass: piston_mass * units.g,
        blowby: k_28inH2O(0.027),
        compression_height: compression_height,
        wrist_pin_position: 0.0,
        displacement: 0.0
    )

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    connecting_rod_parameters cr_params(
        mass: con_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.g,
            length: con_rod * units.mm
        ),
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    camshaft _intake_cam_0(base_radius: 21.7 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 21.7 * units.mm, lobe_profile: exhaust_lobe_profile)

    _intake_cam_0.add_lobes(ILC, phasing, var)
    _exhaust_cam_0.add_lobes(ELC, phasing, var)

    ignition.connect_wire(_wire1, rot_base * var.s1 + phasing)
    ignition.connect_wire(_wire2, rot_base * var.s2 + phasing)
    ignition.connect_wire(_wire3, rot_base * var.s3 + phasing)
    ignition.connect_wire(_wire4, rot_base * var.s4 + phasing)
    ignition.connect_wire(_wire5, rot_base * var.s5 + phasing)
    ignition.connect_wire(_wire6, rot_base * var.s6 + phasing)
    ignition.connect_wire(_wire7, rot_base * var.s7 + phasing)
    ignition.connect_wire(_wire8, rot_base * var.s8 + phasing)

    cylinder_bank _b0(bank_params, angle: incline)

    label spacing_factor(1.3)
    label flange_density(1.0 * exhaust_delay_coeff)
    label x(0.1)
    label pl0 (((1.3 + x) * bore * spacing_factor / flange_density) * units.mm)
    label pl1 (((1.2 + x) * bore * spacing_factor / flange_density) * units.mm)
    label pl2 (((1.0 + x) * bore * spacing_factor / flange_density) * units.mm)
    label pl3 (((0.9 + x) * bore * spacing_factor / flange_density) * units.mm)
    label pl4 ((0.9 * bore * spacing_factor / flange_density) * units.mm)
    label pl5 ((1.2 * bore * spacing_factor / flange_density) * units.mm)
    label pl6 ((1.3 * bore * spacing_factor / flange_density) * units.mm)
    label pl7 ((1.3 * bore * spacing_factor / flange_density) * units.mm)

    _b0
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj0,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl0,
            ignition_wire: _wire1
        )
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj1,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl1,
            ignition_wire: _wire2
        )
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj2,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl2,
            ignition_wire: _wire3
        )
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj3,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl3,
            ignition_wire: _wire4
        )
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj4,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl4,
            ignition_wire: _wire5
        )
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj5,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl5,
            ignition_wire: _wire6
        )
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj6,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl6,
            ignition_wire: _wire7
        )
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj7,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl7,
            ignition_wire: _wire8
        )

    _b0.set_cylinder_head(
        head(
            intake_camshaft: _intake_cam_0,
            exhaust_camshaft: _exhaust_cam_0,
            flip_display: flip
        )
    )
}

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node head {
    // port_flow.py v1.7
    // intake port area: 14.3 cm²; saturated lift: 10.41 mm
    // exhaust port area: 10.4 cm²; saturated lift: 8.88 mm
    // cylinder volume: 374.5 cm³ (22.85 CI); engine volume: 5.992 L (365.67 CI)
    // chamber volume: 39.4 cm³ (2.41 CI); conrod/stroke 1.970; N.A.C.C. 71.67
    // 8 harmonic intake runner length: 15.0 cm; diameter: 4.2 cm
    // primary length: 11.4 cm, area: 12.5 cm², diameter: 4.0 cm
    // collector diameter: 13.8 cm, area: 150.0 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 39.423 * units.cc,
        intake_runner_volume: 210.4 * units.cc,
        intake_runner_cross_section_area: 14.0 * units.cm2,
        exhaust_runner_volume: 70.1 * units.cc,
        exhaust_runner_cross_section_area: 12.5 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

public node crank {
    input phasing;
    input var;

    output c: _c;
    output rj0: _rj0;
    output rj1: _rj1;
    output rj2: _rj2;
    output rj3: _rj3;
    output rj4: _rj4;
    output rj5: _rj5;
    output rj6: _rj6;
    output rj7: _rj7;

    label crank_moment(disk_moment_of_inertia(mass: crank_mass * units.kg, radius: (stroke / 2.0) * units.mm))
    label flywheel_moment(disk_moment_of_inertia(mass: flywheel_mass * units.kg, radius: (stroke * 3.5) * units.mm))
    label other_moment(disk_moment_of_inertia(mass: 1 * units.kg, radius: 1.0 * units.cm))

    crankshaft _c(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: flywheel_mass * units.kg,
        mass: crank_mass * units.kg,
        friction_torque: 9.0 * units.Nm,
        moment_of_inertia: crank_moment + flywheel_moment + other_moment,
        tdc: 0.0
    )

    rod_journal _rj0(angle: rot_base * var.s1 + rot90 + vee / 2)
    rod_journal _rj1(angle: rot_base * var.s2 + rot90 + vee / 2)
    rod_journal _rj2(angle: rot_base * var.s3 + rot90 + vee / 2)
    rod_journal _rj3(angle: rot_base * var.s4 + rot90 + vee / 2)
    rod_journal _rj4(angle: rot_base * var.s5 + rot90 + vee / 2)
    rod_journal _rj5(angle: rot_base * var.s6 + rot90 + vee / 2)
    rod_journal _rj6(angle: rot_base * var.s7 + rot90 + vee / 2)
    rod_journal _rj7(angle: rot_base * var.s8 + rot90 + vee / 2)

    _c
        .add_rod_journal(_rj0)
        .add_rod_journal(_rj1)
        .add_rod_journal(_rj2)
        .add_rod_journal(_rj3)
        .add_rod_journal(_rj4)
        .add_rod_journal(_rj5)
        .add_rod_journal(_rj6)
        .add_rod_journal(_rj7)
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "Porsche 917 6.0",
        starter_torque: 150 * units.Nm,
        starter_speed: 300 * units.rpm,
        redline: redline * units.rpm,
        fuel: r50(),
        throttle_gamma: 1.5,
        jitter: 0.8,
        noise: 0.1,
        hf_gain: 0.008,
        simulation_frequency: 7000,
        dyno_min_speed: 1000 * units.rpm,
        fluid_simulation_steps: 6,
        max_sle_solver_steps: 32
    )

    variant_0 v0()
    variant_1 v1()

    crank c0(vee / 2 + rot90, v0)

    ignition_module ignition(timing_curve: timing_curve, rev_limit: redline * 1.5 * units.rpm, limiter_duration: 0.05)

    bank_builder bank0(
        var: v0,
        crankshaft: c0,
        incline: vee / 2,
        phasing: 0,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust0,
        flip: true
    )

    bank_builder bank1(
        var: v1,
        crankshaft: c0,
        incline: -vee / 2,
        phasing: 0,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust1
    )

    label collector_cross_section_area(75.0)

    label exhaust_pipe_length_0(100.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 100.0) // Litres
    
    label exhaust_pipe_length_1(103.0) // cm
    label exhaust_volume_1(collector_cross_section_area * exhaust_pipe_length_1 / 100.0) // Litres

    intake intake(
        plenum_volume: 9.4 * units.L,
        plenum_cross_section_area: 112.3 * units.cm2,
        runner_length: 15.0 * units.cm,
        intake_flow_rate: k_carb(1092.3),
        idle_flow_rate: k_carb(0.02),
        idle_throttle_plate_position: 0.9955,
        runner_flow_rate: k_carb(170.7),
        velocity_decay: 0.05
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(1024.1),
        collector_cross_section_area: 75.0 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 11.4 * units.cm,
        primary_flow_rate: k_carb(288.0),
        velocity_decay: 0.7,
        volume: exhaust_volume_0 * units.L
    )
    
    exhaust_system_parameters es_params1(
        outlet_flow_rate: k_carb(1024.1),
        collector_cross_section_area: 75.0 * units.cm2,
        length: exhaust_pipe_length_1 * units.cm,
        primary_tube_length: 11.4 * units.cm,
        primary_flow_rate: k_carb(288.0),
        velocity_decay: 0.7,
        volume: exhaust_volume_1 * units.L
    )
    
    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_22.wav", volume: 0.00025))
    exhaust_system exhaust1(es_params1, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_22.wav", volume: 0.00020))
 
    function timing_curve(583.3 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 2.0 * units.deg)
        .add_sample(583.3 * units.rpm, 2.0 * units.deg)
        .add_sample(1166.7 * units.rpm, 2.9 * units.deg)
        .add_sample(1750.0 * units.rpm, 5.9 * units.deg)
        .add_sample(2333.3 * units.rpm, 8.9 * units.deg)
        .add_sample(2916.7 * units.rpm, 11.9 * units.deg)
        .add_sample(3500.0 * units.rpm, 14.9 * units.deg)
        .add_sample(4083.3 * units.rpm, 17.9 * units.deg)
        .add_sample(4666.7 * units.rpm, 20.9 * units.deg)
        .add_sample(5250.0 * units.rpm, 23.9 * units.deg)
        .add_sample(5833.3 * units.rpm, 26.9 * units.deg)
        .add_sample(6416.7 * units.rpm, 30.0 * units.deg)
        .add_sample(7000.0 * units.rpm, 33.0 * units.deg)
        .add_sample(7583.3 * units.rpm, 36.0 * units.deg)
        .add_sample(8166.7 * units.rpm, 39.0 * units.deg)
        .add_sample(8750.0 * units.rpm, 42.0 * units.deg)
        .add_sample(9333.3 * units.rpm, -12.5 * units.deg)
        .add_sample(9916.7 * units.rpm, -25.0 * units.deg)

    engine
        .add_crankshaft(c0.c)
        .add_cylinder_bank(bank0.bank)
        .add_cylinder_bank(bank1.bank)
        .add_ignition_module(ignition)
}

// 917 longtail
public node veh {
    alias output __out: vehicle;
    vehicle vehicle(
        mass: 900.0  * units.kg, // 800 empty
        drag_coefficient: 0.31,
        cross_sectional_area: (1980 * units.mm) * (940 * units.mm),
        diff_ratio: 4.625, // 37/8
        tire_radius: (883.0 / 2) * units.mm, // 12.5/26.0 - 15 (R)
        rolling_resistance: 20
        )
}

// 917 long tail
private node trn {
    alias output __out:
        transmission(
            max_clutch_torque: 850 * units.Nm,
            max_clutch_flex: 10 * units.deg,
            limit_clutch_flex: true,
            clutch_stiffness: 20 * units.Nm / units.deg,
            clutch_damping: 2.0,
            simulate_flex: true
        )
        .add_gear(1.933) // 29/15
        .add_gear(1.381) // 29/21
        .add_gear(1.043) // 24/23
        .add_gear(0.808) // 21/26
        .add_gear(0.7);  // 21/30
}

run(
    engine: eng(),
    transmission: trn(),
    vehicle: veh()
)

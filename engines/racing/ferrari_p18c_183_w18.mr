import "engine_sim.mr"
import "../../fuel/racing.mr"

units units()
constants constants()

label bore(65.0)
label stroke(50.0)
label compression_ratio(12.0)
label con_rod(120.0) // ~
label L1(87.5) // ~
label R1(33.5) // ~
label psi(80.0 * units.deg)
label compression_height(27.0 * units.mm) // ~
label intake_valve_diameter(34.5) // ~
label exhaust_valve_diameter(33.0) // ~
label intake_valves(1.0)
label exhaust_valves(1.0)

label cyl(18.0)
label row(3.0)
label cycle(720.0 * units.deg)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label vee(80.0 * units.deg)

label redline(11500.0) // RPM

// ?
label IVL(7.5)
label EVL(6.9)
label IVO(81.0 * units.deg) //BTDC
label IVC(71.0 * units.deg) //ABDC
label EVO(81.0 * units.deg) //BBDC
label EVC(51.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(300.0) // g
label slave_rod_mass(150.0)
label crank_mass(20.0) // kg
label flywheel_mass(3.0) // kg

label exhaust_delay_coeff(1.5)


public node rods {
    input crank;
    output cr0_0: sr0_0; output cr1_0: sr1_0; output cr2_0: sr2_0; output cr3_0: sr3_0; output cr4_0: sr4_0; output cr5_0: sr5_0;
    output rj0_0: sj0_0; output rj1_0: sj1_0; output rj2_0: sj2_0; output rj3_0: sj3_0; output rj4_0: sj4_0; output rj5_0: sj5_0;
    output cr0_1: mcr0; output cr1_1: mcr1; output cr2_1: mcr2; output cr3_1: mcr3; output cr4_1: mcr4; output cr5_1: mcr5;
    output rj0_1: crank.rj0; output rj1_1: crank.rj1; output rj2_1: crank.rj2; output rj3_1: crank.rj3; output rj4_1: crank.rj4; output rj5_1: crank.rj5;
    output cr0_2: sr0_1; output cr1_2: sr1_1; output cr2_2: sr2_1; output cr3_2: sr3_1; output cr4_2: sr4_1; output cr5_2: sr5_1;
    output rj0_2: sj0_1; output rj1_2: sj1_1; output rj2_2: sj2_1; output rj3_2: sj3_1; output rj4_2: sj4_1; output rj5_2: sj5_1;

    connecting_rod_parameters master_rod_params(
        mass: con_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.g,
            length: con_rod * units.mm
        ),
        slave_throw: R1 * units.mm,
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    connecting_rod_parameters slave_rod_params(
        mass: slave_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: slave_rod_mass * units.g,
            length: L1 * units.mm
        ),
        center_of_mass: 0.0,
        length: L1 * units.mm
    )

    connecting_rod mcr0(master_rod_params)
    connecting_rod mcr1(master_rod_params)
    connecting_rod mcr2(master_rod_params)
    connecting_rod mcr3(master_rod_params)
    connecting_rod mcr4(master_rod_params)
    connecting_rod mcr5(master_rod_params)

    connecting_rod sr0_0(slave_rod_params)
    connecting_rod sr1_0(slave_rod_params)
    connecting_rod sr2_0(slave_rod_params)
    connecting_rod sr3_0(slave_rod_params)
    connecting_rod sr4_0(slave_rod_params)
    connecting_rod sr5_0(slave_rod_params)

    connecting_rod sr0_1(slave_rod_params)
    connecting_rod sr1_1(slave_rod_params)
    connecting_rod sr2_1(slave_rod_params)
    connecting_rod sr3_1(slave_rod_params)
    connecting_rod sr4_1(slave_rod_params)
    connecting_rod sr5_1(slave_rod_params)

    rod_journal sj0_0(angle: psi)
    rod_journal sj1_0(angle: psi)
    rod_journal sj2_0(angle: psi)
    rod_journal sj3_0(angle: psi)
    rod_journal sj4_0(angle: psi)
    rod_journal sj5_0(angle: psi)

    rod_journal sj0_1(angle: -psi)
    rod_journal sj1_1(angle: -psi)
    rod_journal sj2_1(angle: -psi)
    rod_journal sj3_1(angle: -psi)
    rod_journal sj4_1(angle: -psi)
    rod_journal sj5_1(angle: -psi)

    mcr0.add_slave_journal(sj0_0)
    mcr1.add_slave_journal(sj1_0)
    mcr2.add_slave_journal(sj2_0)
    mcr3.add_slave_journal(sj3_0)
    mcr4.add_slave_journal(sj4_0)
    mcr5.add_slave_journal(sj5_0)
    mcr0.add_slave_journal(sj0_1)
    mcr1.add_slave_journal(sj1_1)
    mcr2.add_slave_journal(sj2_1)
    mcr3.add_slave_journal(sj3_1)
    mcr4.add_slave_journal(sj4_1)
    mcr5.add_slave_journal(sj5_1)
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "Ferrari W18",
        starter_torque: 50 * units.Nm,
        starter_speed: 700 * units.rpm,
        redline: redline * units.rpm,
        fuel: r80(),
        throttle_gamma: 1.5,
        jitter: 1.0,
        noise: 0.1,
        hf_gain: 0.01,
        simulation_frequency: 5500,
        dyno_min_speed: 750 * units.rpm
    )

    variant_0 variant()
    em_0 em()
    crank c0(vee / 2 + rot90, variant)
    rods r(c0)

    bank_builder bank0(
        var: variant,
        rods: r0(r),
        incline: vee,
        phasing: 0,
        ignition: ignition,
        ign_comp: -3.8 * units.deg,
        intake: intake,
        exhaust: exhaust0,
        primaries: em,
        sh: 0.0 * units.cm,
        flip: true
    )

    bank_builder bank1(
        var: variant,
        rods: r1(r),
        incline: 0,
        phasing: vee + rot360,
        ignition: ignition,
        ign_comp: 0,
        intake: intake,
        exhaust: exhaust1,
        sh: 0.0 * units.cm,
        primaries: em
    )

    bank_builder bank2(
        var: variant,
        rods: r2(r),
        incline: -vee,
        phasing: vee * 2,
        ignition: ignition,
        ign_comp: 3.8 * units.deg,
        intake: intake,
        exhaust: exhaust2,
        sh: 0.0 * units.cm,
        primaries: em
    )

    ignition_module ignition(timing_curve: timing_curve, rev_limit: redline * 1.05 * units.rpm, limiter_duration: 0.06)

    label collector_cross_section_area(19.2)

    label exhaust_pipe_length_0(250.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 100.0) // Litres

    intake intake(
        plenum_volume: 6.2 * units.L,
        plenum_cross_section_area: 76.3 * units.cm2,
        runner_length: 13.4 * units.cm,
        intake_flow_rate: k_carb(715.5),
        idle_flow_rate: k_carb(0.15),
        idle_throttle_plate_position: 0.9975,
        runner_flow_rate: k_carb(99.4),
        velocity_decay: 0.03
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(447.2),
        collector_cross_section_area: 19.2 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 9.3 * units.cm,
        primary_flow_rate: k_carb(167.7),
        velocity_decay: 0.8,
        volume: (exhaust_volume_0 + 2.0) * units.L
    )
    
    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_56.wav", volume: 0.00025))
    exhaust_system exhaust1(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_57.wav", volume: 0.00025))
    exhaust_system exhaust2(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_58.wav", volume: 0.00025))

    engine
        .add_crankshaft(c0.c)
        .add_cylinder_bank(bank0.bank)
        .add_cylinder_bank(bank1.bank)
        .add_cylinder_bank(bank2.bank)
        .add_ignition_module(ignition)

    function timing_curve(766.7 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 2.0 * units.deg)
        .add_sample(766.7 * units.rpm, 3.0 * units.deg)
        .add_sample(1533.3 * units.rpm, 5.8 * units.deg)
        .add_sample(2300.0 * units.rpm, 8.5 * units.deg)
        .add_sample(3066.7 * units.rpm, 11.3 * units.deg)
        .add_sample(3833.3 * units.rpm, 14.1 * units.deg)
        .add_sample(4600.0 * units.rpm, 16.9 * units.deg)
        .add_sample(5366.7 * units.rpm, 19.7 * units.deg)
        .add_sample(6133.3 * units.rpm, 22.5 * units.deg)
        .add_sample(6900.0 * units.rpm, 25.3 * units.deg)
        .add_sample(7666.7 * units.rpm, 28.1 * units.deg)
        .add_sample(8433.3 * units.rpm, 30.8 * units.deg)
        .add_sample(9200.0 * units.rpm, 33.6 * units.deg)
        .add_sample(9966.7 * units.rpm, 36.4 * units.deg)
        .add_sample(10733.3 * units.rpm, 39.2 * units.deg)
        .add_sample(11500.0 * units.rpm, 42.0 * units.deg)
        .add_sample(12266.7 * units.rpm, -15.0 * units.deg)
        .add_sample(13033.3 * units.rpm, -45.0 * units.deg)
}

label spacing_factor(1.13)
label flange_density(2.0 * exhaust_delay_coeff)
label em_unit(bore * spacing_factor / flange_density)

private node em_0{
    output pl0: (3.8 * em_unit) * units.mm;
    output pl1: (3.3 * em_unit) * units.mm;
    output pl2: (3.1 * em_unit) * units.mm;
    output pl3: (2.8 * em_unit) * units.mm;
    output pl4: (2.0 * em_unit) * units.mm;
    output pl5: (1.5 * em_unit) * units.mm;
}


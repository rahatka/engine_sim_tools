label bore(67.0)
label stroke(70.0)
label compression_ratio(6.6) // 6.6 appendix ; 5.9 - 6.2 Ludvigsen V12
label con_rod(155.0) // 155 M 154 Ludvigsen V12 ; 158 M 163 ; 151 Pomeroy
label compression_height(87.0 / 2 * units.mm) // 87 PH
label intake_valve_diameter(30.0)
label exhaust_valve_diameter(30.0)
label intake_valves(2.0)
label exhaust_valves(2.0)

label redline(8000.0) // RPM

label cyl(12.0)
label row(2.0)
label cycle(720.0 * units.deg)
label vee(60.0 * units.deg)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label rot(cycle / cyl)

label IVL(8.0)
label EVL(8.0)

// M 154 dA-1 ; 266.0 266.0 LSA 102.50 ADV -2.50 OVL 61.0

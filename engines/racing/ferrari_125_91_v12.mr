import "../../fuel/racing.mr"

units units()
constants constants()

label bore(55.0)
label stroke(52.5)
label compression_ratio(7.0)
label con_rod(110.0)
label compression_height(67.0 / 2 * units.mm) // PH 67
label intake_valve_diameter(32.1) // 1.26
label exhaust_valve_diameter(30.0) // 1.18
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(8000.0) // RPM

label cyl(12.0)
label cyl_base(cyl / row)
label row(2.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label rot_base(cycle / cyl_base)
label vee(60.0 * units.deg)

// 125GPC/49
label IVL(6.5)
label EVL(6.5) // ?
label IVO(20.0 * units.deg) //BTDC
label IVC(55.0 * units.deg) //ABDC
label EVO(60.0 * units.deg) //BBDC
label EVC(15.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(300.0) // ? g
label piston_mass(200.0) // ?
label crank_mass(15.0) // ?
label flywheel_mass(5.0) // kg

label exhaust_delay_coeff(1.5)


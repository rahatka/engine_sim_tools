public node camshaft_builder {
    output intake_cam_0: _intake_cam_0;
    output exhaust_cam_0: _exhaust_cam_0;

    camshaft _intake_cam_0(base_radius: 14.7 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 14.7 * units.mm, lobe_profile: exhaust_lobe_profile)

    _intake_cam_0
        .add_lobe(ILC + 0 * rot)
        .add_lobe(ILC + 2 * rot)
        .add_lobe(ILC + 5 * rot)
        .add_lobe(ILC + 7 * rot)
        .add_lobe(ILC + 3 * rot)
        .add_lobe(ILC + 1 * rot)
        .add_lobe(ILC + 6 * rot)
        .add_lobe(ILC + 4 * rot)
    _exhaust_cam_0
        .add_lobe(ELC + 0 * rot)
        .add_lobe(ELC + 2 * rot)
        .add_lobe(ELC + 5 * rot)
        .add_lobe(ELC + 7 * rot)
        .add_lobe(ELC + 3 * rot)
        .add_lobe(ELC + 1 * rot)
        .add_lobe(ELC + 6 * rot)
        .add_lobe(ELC + 4 * rot)
}

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node head {
    // port_flow.py v1.7
    // intake port area: 8.1 cm²; saturated lift: 7.86 mm
    // exhaust port area: 8.1 cm²; saturated lift: 7.86 mm
    // cylinder volume: 184.9 cm³ (11.29 CI); engine volume: 1.480 L (90.29 CI)
    // chamber volume: 28.5 cm³ (1.74 CI); conrod/stroke 2.100; N.A.C.C. 16.69
    // 8 harmonic intake runner length: 19.1 cm; diameter: 3.1 cm
    // primary length: 12.7 cm, area: 8.1 cm², diameter: 3.2 cm
    // collector diameter: 6.4 cm, area: 32.6 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 28.453 * units.cc,
        intake_runner_volume: 146.4 * units.cc,
        intake_runner_cross_section_area: 7.7 * units.cm2,
        exhaust_runner_volume: 48.8 * units.cc,
        exhaust_runner_cross_section_area: 8.1 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "Alfa Romeo Type 159",
        starter_torque: 100 * units.Nm,
        starter_speed: 450 * units.rpm,
        redline: redline * units.rpm,
        fuel: r50(),
        throttle_gamma: 1.2,
        jitter: 0.8,
        noise: 0.3,
        hf_gain: 0.008,
        simulation_frequency: 8000
    )

    wires wires()

    crankshaft c0(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: 5.0 * units.kg,
        mass: 12.0 * units.kg,
        friction_torque: 2.2 * units.Nm,
        moment_of_inertia: 0.10,
        tdc: 0.0
    )

    rod_journal rj0(angle: rot * 0 + rot90)
    rod_journal rj1(angle: rot * 2 + rot90)
    rod_journal rj2(angle: rot * 5 + rot90)
    rod_journal rj3(angle: rot * 7 + rot90)
    rod_journal rj4(angle: rot * 3 + rot90)
    rod_journal rj5(angle: rot * 1 + rot90)
    rod_journal rj6(angle: rot * 6 + rot90)
    rod_journal rj7(angle: rot * 4 + rot90)
    
    c0
        .add_rod_journal(rj0)
        .add_rod_journal(rj1)
        .add_rod_journal(rj2)
        .add_rod_journal(rj3)
        .add_rod_journal(rj4)
        .add_rod_journal(rj5)
        .add_rod_journal(rj6)
        .add_rod_journal(rj7)

    piston_parameters piston_params(
        mass: 250.0 * units.g,
        blowby: k_28inH2O(0.018),
        compression_height: compression_height,
        wrist_pin_position: 0.0,
        displacement: 0.0
    )

    connecting_rod_parameters cr_params(
        mass: 250.0 * units.g,
        moment_of_inertia: 0.002,
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    label collector_cross_section_area(32.6)

    label exhaust_pipe_length_0(120.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 100.0) // Litres

    intake intake(
        plenum_volume: 2.6 * units.L,
        plenum_cross_section_area: 30.7 * units.cm2,
        runner_length: 19.1 * units.cm,
        intake_flow_rate: k_carb(292.8),
        idle_flow_rate: k_carb(0.08),
        idle_throttle_plate_position: 0.9965,
        runner_flow_rate: k_carb(91.5),
        velocity_decay: 0.05
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(549.1),
        collector_cross_section_area: 32.6 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 12.7 * units.cm,
        primary_flow_rate: k_carb(154.4),
        velocity_decay: 0.7,
        volume: exhaust_volume_0 * units.L
    )

    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_45.wav", volume: 0.001))
 
    cylinder_bank b0(bank_params, angle: 0)

    label spacing_factor(1.2)
    label flange_density(1.0 * exhaust_delay_coeff)
    label pl0((6.5 * bore * spacing_factor / flange_density) * units.mm)
    label pl1((6.2 * bore * spacing_factor / flange_density) * units.mm)
    label pl2((5.8 * bore * spacing_factor / flange_density) * units.mm)
    label pl3((5.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl4((4.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl5((3.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl6((2.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl7((1.0 * bore * spacing_factor / flange_density) * units.mm)

    b0
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj0,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl0,
            ignition_wire: wires.wire1
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj1,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl1,
            ignition_wire: wires.wire2
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj2,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl2,
            ignition_wire: wires.wire3
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj3,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl3,
            ignition_wire: wires.wire4
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj4,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl4,
            ignition_wire: wires.wire5
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj5,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl5,
            ignition_wire: wires.wire6
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj6,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl6,
            ignition_wire: wires.wire7
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj7,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl7,
            ignition_wire: wires.wire8
        )

    engine
        .add_cylinder_bank(b0)

    engine.add_crankshaft(c0)

    camshaft_builder camshaft()

    b0.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_0,
            exhaust_camshaft: camshaft.exhaust_cam_0
        )
    )

    function timing_curve(633.3 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 0.0 * units.deg)
        .add_sample(633.3 * units.rpm, 0.0 * units.deg)
        .add_sample(1266.7 * units.rpm, 7.1 * units.deg)
        .add_sample(1900.0 * units.rpm, 13.7 * units.deg)
        .add_sample(2533.3 * units.rpm, 18.8 * units.deg)
        .add_sample(3166.7 * units.rpm, 22.7 * units.deg)
        .add_sample(3800.0 * units.rpm, 25.7 * units.deg)
        .add_sample(4433.3 * units.rpm, 28.1 * units.deg)
        .add_sample(5066.7 * units.rpm, 29.9 * units.deg)
        .add_sample(5700.0 * units.rpm, 31.3 * units.deg)
        .add_sample(6333.3 * units.rpm, 32.4 * units.deg)
        .add_sample(6966.7 * units.rpm, 33.2 * units.deg)
        .add_sample(7600.0 * units.rpm, 33.8 * units.deg)
        .add_sample(8233.3 * units.rpm, 34.3 * units.deg)
        .add_sample(8866.7 * units.rpm, 34.7 * units.deg)
        .add_sample(9500.0 * units.rpm, 35.0 * units.deg)
        .add_sample(10133.3 * units.rpm, -12.5 * units.deg)
        .add_sample(10766.7 * units.rpm, -25.0 * units.deg)

    engine.add_ignition_module(
        eng_distributor(
            wires: wires,
            timing_curve: timing_curve
        )
    )
}

// Alfa Romeo Type 159
public node veh {
    alias output __out: vehicle;
    vehicle vehicle(
        mass: 710 * units.kg,
        drag_coefficient: 0.32,
        cross_sectional_area: (1550 * units.mm) * (1120 * units.mm),
        diff_ratio: 6.0, // or 4.0
        tire_radius: (812.8 / 2) * units.mm, // 7.00 x 18 or 7.00 x 19
        rolling_resistance: 20
    )
}

// Alfa Romeo Type 159
private node trn {
    alias output __out:
        transmission(
            max_clutch_torque: 300 * units.Nm,
            max_clutch_flex: 30 * units.deg,
            limit_clutch_flex: true,
            clutch_stiffness: 20 * units.Nm / units.deg,
            clutch_damping: 1.0,
            simulate_flex: true
        )
        .add_gear(2.39)
        .add_gear(1.52)
        .add_gear(1.18)
        .add_gear(1.0);
}

run(
    engine: eng(),
    transmission: trn(),
    vehicle: veh()
)


private node add_lobes {
    input base;
    input shift: 0.0;
    input var;
    input this;
    alias output __out: this;

    this.add_lobe(base + var.s1 * rot_base + shift)
    this.add_lobe(base + var.s2 * rot_base + shift)
    this.add_lobe(base + var.s3 * rot_base + shift)
    this.add_lobe(base + var.s4 * rot_base + shift)
    this.add_lobe(base + var.s5 * rot_base + shift)
}

public node bank_builder {
    input var;
    input crankshaft;
    input incline;
    input phasing;
    input ignition;
    input intake;
    input exhaust;
    input primaries;
    input flip: false;

    output bank: _b0;

    ignition_wire _wire1()
    ignition_wire _wire2()
    ignition_wire _wire3()
    ignition_wire _wire4()
    ignition_wire _wire5()

    piston_parameters piston_params(
        mass: piston_mass * units.g,
        blowby: k_28inH2O(0.028),
        compression_height: compression_height
    )

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    connecting_rod_parameters cr_params(
        mass: con_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.g,
            length: con_rod * units.mm
        ),
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    camshaft _intake_cam_0(base_radius: 13.3 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 13.3 * units.mm, lobe_profile: exhaust_lobe_profile)

    _intake_cam_0.add_lobes(ILC, phasing, var)
    _exhaust_cam_0.add_lobes(ELC, phasing, var)

    ignition.connect_wire(_wire1, rot_base * var.s1 + phasing)
    ignition.connect_wire(_wire2, rot_base * var.s2 + phasing)
    ignition.connect_wire(_wire3, rot_base * var.s3 + phasing)
    ignition.connect_wire(_wire4, rot_base * var.s4 + phasing)
    ignition.connect_wire(_wire5, rot_base * var.s5 + phasing)

    cylinder_bank _b0(bank_params, angle: incline)

    _b0
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj0,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: primaries.pl0,
            ignition_wire: _wire1
        )
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj1,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: primaries.pl1,
            ignition_wire: _wire2
        )
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj2,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: primaries.pl2,
            ignition_wire: _wire3
        )
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj3,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: primaries.pl3,
            ignition_wire: _wire4
        )
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj4,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: primaries.pl4,
            ignition_wire: _wire5
        )

    _b0.set_cylinder_head(
        head(
            intake_camshaft: _intake_cam_0,
            exhaust_camshaft: _exhaust_cam_0,
            flip_display: flip
        )
    )
}

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node head {
    // port_flow.py v1.7
    // intake port area: 17.9 cm²; saturated lift: 8.24 mm
    // exhaust port area: 11.8 cm²; saturated lift: 6.70 mm
    // cylinder volume: 299.6 cm³ (18.28 CI); engine volume: 2.996 L (182.85 CI)
    // chamber volume: 27.2 cm³ (1.66 CI); conrod/stroke 2.123; N.A.C.C. 50.22
    // 8 harmonic intake runner length: 22.0 cm; diameter: 3.8 cm
    // primary length: 14.7 cm, area: 12.4 cm², diameter: 4.0 cm
    // collector diameter: 8.9 cm, area: 62.1 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 27.240 * units.cc,
        intake_runner_volume: 246.8 * units.cc,
        intake_runner_cross_section_area: 11.2 * units.cm2,
        exhaust_runner_volume: 82.3 * units.cc,
        exhaust_runner_cross_section_area: 12.4 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

public node crank {
    input phasing;
    input var;

    output c: _c;
    output rj0: _rj0;
    output rj1: _rj1;
    output rj2: _rj2;
    output rj3: _rj3;
    output rj4: _rj4;

    label crank_moment(disk_moment_of_inertia(mass: crank_mass * units.kg, radius: (stroke / 2.0) * units.mm))
    label flywheel_moment(disk_moment_of_inertia(mass: flywheel_mass * units.kg, radius: (stroke * 3.5) * units.mm))
    label other_moment(disk_moment_of_inertia(mass: 1 * units.kg, radius: 1.0 * units.cm))

    crankshaft _c(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: flywheel_mass * units.kg,
        mass: crank_mass * units.kg,
        friction_torque: 4.5 * units.Nm,
        moment_of_inertia: crank_moment + flywheel_moment + other_moment,
        tdc: 0.0
    )

    rod_journal _rj0(angle: rot_base * var.s1 + rot90 + vee / 2)
    rod_journal _rj1(angle: rot_base * var.s2 + rot90 + vee / 2)
    rod_journal _rj2(angle: rot_base * var.s3 + rot90 + vee / 2)
    rod_journal _rj3(angle: rot_base * var.s4 + rot90 + vee / 2)
    rod_journal _rj4(angle: rot_base * var.s5 + rot90 + vee / 2)

    _c
        .add_rod_journal(_rj0)
        .add_rod_journal(_rj1)
        .add_rod_journal(_rj2)
        .add_rod_journal(_rj3)
        .add_rod_journal(_rj4)
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "S85B50",
        starter_torque: 150 * units.Nm,
        starter_speed: 400 * units.rpm,
        redline: redline * units.rpm,
        fuel: r80(),
        throttle_gamma: 1.5,
        jitter: 0.5,
        noise: 0.1,
        hf_gain: 0.005,
        simulation_frequency: 8500,
        dyno_min_speed: 750 * units.rpm
    )

    variant_0 variant()
    em_0 ems()

    crank c0(vee / 2 + rot90, variant)

    bank_builder bank0(
        var: variant,
        crankshaft: c0,
        incline: vee / 2,
        phasing: 0,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust0,
        primaries: ems,
        flip: true
    )

    bank_builder bank1(
        var: variant,
        crankshaft: c0,
        incline: -vee / 2,
        phasing: vee - comp,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust1,
        primaries: ems
    )

    ignition_module ignition(timing_curve: timing_curve, rev_limit: redline * 1.05 * units.rpm, limiter_duration: 0.06)

    label collector_cross_section_area(31.1)

    label exhaust_pipe_length_0(353.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 100.0) // Litres
    
    label exhaust_pipe_length_1(350.0) // cm
    label exhaust_volume_1(collector_cross_section_area * exhaust_pipe_length_1 / 100.0) // Litres

    intake intake(
        plenum_volume: 4.2 * units.L,
        plenum_cross_section_area: 56.1 * units.cm2,
        runner_length: 22.0 * units.cm,
        intake_flow_rate: k_carb(898.9),
        idle_flow_rate: k_carb(0.01),
        idle_throttle_plate_position: 0.998,
        runner_flow_rate: k_carb(215.7),
        velocity_decay: 0.03
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(824.0),
        collector_cross_section_area: 31.1 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 14.7 * units.cm,
        primary_flow_rate: k_carb(362.6),
        velocity_decay: 1.0,
        volume: (exhaust_volume_0 + 3.0) * units.L
    )

    exhaust_system_parameters es_params1(
        outlet_flow_rate: k_carb(824.0),
        collector_cross_section_area: 31.1 * units.cm2,
        length: exhaust_pipe_length_1 * units.cm,
        primary_tube_length: 14.7 * units.cm,
        primary_flow_rate: k_carb(362.6),
        velocity_decay: 1.0,
        volume: (exhaust_volume_1 + 3.0) * units.L
    )
    
    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_52.wav", volume: 0.0007))
    exhaust_system exhaust1(es_params1, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_52.wav", volume: 0.0005))

    engine
        .add_crankshaft(c0.c)
        .add_cylinder_bank(bank0.bank)
        .add_cylinder_bank(bank1.bank)
        .add_ignition_module(ignition)

    function timing_curve(1000.0 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 0.0 * units.deg)
        .add_sample(1000.0 * units.rpm, 8.5 * units.deg)
        .add_sample(2000.0 * units.rpm, 19.7 * units.deg)
        .add_sample(3000.0 * units.rpm, 27.3 * units.deg)
        .add_sample(4000.0 * units.rpm, 32.4 * units.deg)
        .add_sample(5000.0 * units.rpm, 35.8 * units.deg)
        .add_sample(6000.0 * units.rpm, 38.2 * units.deg)
        .add_sample(7000.0 * units.rpm, 39.7 * units.deg)
        .add_sample(8000.0 * units.rpm, 40.8 * units.deg)
        .add_sample(9000.0 * units.rpm, 41.5 * units.deg)
        .add_sample(10000.0 * units.rpm, 42.0 * units.deg)
        .add_sample(11000.0 * units.rpm, 42.0 * units.deg)
        .add_sample(12000.0 * units.rpm, 42.0 * units.deg)
        .add_sample(13000.0 * units.rpm, 42.0 * units.deg)
        .add_sample(14000.0 * units.rpm, 42.0 * units.deg)
        .add_sample(15000.0 * units.rpm, 42.0 * units.deg)
        .add_sample(16000.0 * units.rpm, -15.0 * units.deg)
        .add_sample(17000.0 * units.rpm, -45.0 * units.deg)
}

label spacing_factor(1.15)
label flange_density(2.0 * exhaust_delay_coeff)
label em_unit(bore * spacing_factor / flange_density)

private node em_0{
    output pl0: (2.0 * em_unit) * units.mm;
    output pl1: (1.5 * em_unit) * units.mm;
    output pl2: (1.0 * em_unit) * units.mm;
    output pl3: (1.0 * em_unit) * units.mm;
    output pl4: (1.5 * em_unit) * units.mm;
}

// E39
public node veh {
    alias output __out: vehicle;

    label car_mass(2000) // 1795

    vehicle vehicle(
        mass: car_mass * units.kg,
        drag_coefficient: 0.27,
        cross_sectional_area: (1801 * units.mm) * (1412 * units.mm),
        diff_ratio: 3.15,
        tire_radius: (649.7 / 2) * units.mm, // 9.35 x 15
        rolling_resistance: 0.012 * car_mass * 9.81
        )
}

// E39
private node trn {
    alias output __out:
        transmission(
            max_clutch_torque: 550 * units.Nm,
            max_clutch_flex: 5 * units.deg,
            limit_clutch_flex: true,
            clutch_stiffness: 20 * units.Nm / units.deg,
            clutch_damping: 1.0,
            simulate_flex: true
        )
        .add_gear(4.23)
        .add_gear(2.53)
        .add_gear(1.67)
        .add_gear(1.23)
        .add_gear(1.00)
        .add_gear(0.83);
}

run(
    engine: eng(),
    transmission: trn(),
    vehicle: veh()
)

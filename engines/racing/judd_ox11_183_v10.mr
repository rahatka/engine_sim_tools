import "engine_sim.mr"
import "../../fuel/racing.mr"

units units()
constants constants()

label bore(90.0)
label stroke(47.1)
label compression_ratio(12.0)
label con_rod(100.0) // ~ 2.2
label compression_height(21.0 * units.mm)
label intake_valve_diameter(37.5)
label exhaust_valve_diameter(30.5)
label intake_valves(2.0)
label exhaust_valves(2.0)

label cyl(10.0)
label row(2.0)
label cycle(720.0 * units.deg)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label vee(rot)

// label comp(vee - rot)
label comp(0)

label redline(15000.0) // RPM

label IVL(9.0)
label EVL(7.0)
label IVO(29.0 * units.deg) //BTDC
label IVC(59.0 * units.deg) //ABDC
label EVO(65.0 * units.deg) //BBDC
label EVC(15.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label piston_mass(258.0) // g
label con_rod_mass(250.0) // g
label crank_mass(15.0) // kg
label flywheel_mass(6.0) // kg

label exhaust_delay_coeff(1.5)


private node trn {
    alias output __out:
        transmission(
            max_clutch_torque: 10000 * units.Nm,
            max_clutch_flex: 2.0 * units.deg,
            limit_clutch_flex: true,
            clutch_stiffness: 5000.0 * units.Nm / units.deg,
            clutch_damping: 1.0,
            simulate_flex: true
        )
        .add_gear(7.14)
        .add_gear(3.53)
        .add_gear(1.88)
        .add_gear(1.00)
        .add_gear(0.72);
}

run(
    engine: eng(),
    transmission: trn(),
    vehicle: veh()
)

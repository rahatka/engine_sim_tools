private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node head {
    // port_flow.py v1.7
    // intake port area: 84.4 cm²; saturated lift: 25.24 mm
    // exhaust port area: 126.8 cm²; saturated lift: 15.47 mm
    // cylinder volume: 17126.1 cm³ (1045.10 CI); engine volume: 171.261 L (10451.00 CI)
    // chamber volume: 1070.4 cm³ (65.32 CI); conrod/stroke 2.500; N.A.C.C. 532.26
    // 32 harmonic intake runner length: 61.3 cm; diameter: 8.7 cm
    // primary length: 168.5 cm, area: 126.8 cm², diameter: 12.7 cm
    // collector diameter: 31.1 cm, area: 760.6 cm²


    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 1070.382 * units.cc,
        intake_runner_volume: 3658.5 * units.cc,
        intake_runner_cross_section_area: 59.7 * units.cm2,
        exhaust_runner_volume: 1219.5 * units.cc,
        exhaust_runner_cross_section_area: 126.8 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

public node crank {
    input phasing;
    input var;

    output c: _c;
    output rj0: _rj0;
    output rj1: _rj1;
    output rj2: _rj2;
    output rj3: _rj3;
    output rj4: _rj4;
    output rj5: _rj5;
    output rj6: _rj6;
    output rj7: _rj7;
    output rj8: _rj8;
    output rj9: _rj9;

    label crank_moment(disk_moment_of_inertia(mass: crank_mass * units.kg, radius: (stroke / 2.0) * units.mm))
    label flywheel_moment(disk_moment_of_inertia(mass: flywheel_mass * units.kg, radius: (stroke * 3.5) * units.mm))
    label other_moment(disk_moment_of_inertia(mass: 1 * units.kg, radius: 1.0 * units.cm))

    crankshaft _c(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: flywheel_mass * units.kg,
        mass: crank_mass * units.kg,
        friction_torque: 256.9 * units.Nm,
        moment_of_inertia: crank_moment + flywheel_moment + other_moment,
        tdc: 0.0
    )

    rod_journal _rj0(angle: rot_base * var.s1 + rot90)
    rod_journal _rj1(angle: rot_base * var.s2 + rot90)
    rod_journal _rj2(angle: rot_base * var.s3 + rot90)
    rod_journal _rj3(angle: rot_base * var.s4 + rot90)
    rod_journal _rj4(angle: rot_base * var.s5 + rot90)
    rod_journal _rj5(angle: rot_base * var.s6 + rot90)
    rod_journal _rj6(angle: rot_base * var.s7 + rot90)
    rod_journal _rj7(angle: rot_base * var.s8 + rot90)
    rod_journal _rj8(angle: rot_base * var.s9 + rot90)
    rod_journal _rj9(angle: rot_base * var.s10 + rot90)

    _c
        .add_rod_journal(_rj0)
        .add_rod_journal(_rj1)
        .add_rod_journal(_rj2)
        .add_rod_journal(_rj3)
        .add_rod_journal(_rj4)
        .add_rod_journal(_rj5)
        .add_rod_journal(_rj6)
        .add_rod_journal(_rj7)
        .add_rod_journal(_rj8)
        .add_rod_journal(_rj9)
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "YaAZ-206",
        starter_torque: 8000 * units.Nm,
        starter_speed: 150 * units.rpm,
        redline: redline * units.rpm,
        fuel: diesel(),
        throttle_gamma: 1.5,
        jitter: 0.5,
        noise: 0.04,
        hf_gain: 0.001,
        simulation_frequency: 8000,
        dyno_min_speed: 500 * units.rpm
    )

    variant_0 variant()
    es0 es()

    crank c0(rot90, variant)

    bank_builder bank0(
        var: variant,
        crankshaft: c0,
        incline: 0,
        phasing: 0,
        ignition: ignition,
        intake: intake,
        exhaust: es,
        flip: true
    )

    ignition_module ignition(timing_curve: timing_curve, rev_limit: redline * 1.5 * units.rpm, limiter_duration: 0.03)

    intake intake(
        plenum_volume: 22.6 * units.L,
        plenum_cross_section_area: 298.3 * units.cm2,
        runner_length: 61.3 * units.cm,
        intake_flow_rate: k_carb(3353.9),
        idle_flow_rate: k_carb(0.05),
        idle_throttle_plate_position: 0.9945,
        runner_flow_rate: k_carb(788.2),
        velocity_decay: 0.05
    )

    function timing_curve(66.7 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 0.0 * units.deg)
        .add_sample(66.7 * units.rpm, 0.0 * units.deg)
        .add_sample(133.3 * units.rpm, 3.1 * units.deg)
        .add_sample(200.0 * units.rpm, 6.7 * units.deg)
        .add_sample(266.7 * units.rpm, 9.5 * units.deg)
        .add_sample(333.3 * units.rpm, 11.8 * units.deg)
        .add_sample(400.0 * units.rpm, 13.6 * units.deg)
        .add_sample(466.7 * units.rpm, 15.1 * units.deg)
        .add_sample(533.3 * units.rpm, 16.3 * units.deg)
        .add_sample(600.0 * units.rpm, 17.2 * units.deg)
        .add_sample(666.7 * units.rpm, 18.0 * units.deg)
        .add_sample(733.3 * units.rpm, 18.6 * units.deg)
        .add_sample(800.0 * units.rpm, 19.1 * units.deg)
        .add_sample(866.7 * units.rpm, 19.4 * units.deg)
        .add_sample(933.3 * units.rpm, 19.8 * units.deg)
        .add_sample(1000.0 * units.rpm, 20.0 * units.deg)
        .add_sample(1066.7 * units.rpm, -12.5 * units.deg)
        .add_sample(1133.3 * units.rpm, -25.0 * units.deg)

    engine
        .add_crankshaft(c0.c)
        .add_cylinder_bank(bank0.bank)
        .add_ignition_module(ignition)
}

label spacing_factor(1.2)
label flange_density(1.0 * exhaust_delay_coeff)
label em_unit(bore * spacing_factor / flange_density)

// std1
private node es0{
    output es0: exhaust0; output es1: exhaust0; output es2: exhaust0;
    output es3: exhaust0; output es4: exhaust0; output es5: exhaust0;
    output es6: exhaust0; output es7: exhaust0; output es8: exhaust0; output es9: exhaust0;
    output pl0: _pl0; output pl1: _pl1; output pl2: _pl2; output pl3: _pl3;
    output pl4: _pl4; output pl5: _pl5; output pl6: _pl6; output pl7: _pl7;
    output pl8: _pl8; output pl9: _pl9;

    label collector_cross_section_area(760.6)
    label exhaust_pipe_length_0(300.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 1000.0) // Litres

    float _pl0 ((1.0 * em_unit) * units.mm)
    float _pl1 ((2.0 * em_unit) * units.mm)
    float _pl2 ((3.0 * em_unit) * units.mm)
    float _pl3 ((4.0 * em_unit) * units.mm)
    float _pl4 ((5.0 * em_unit) * units.mm)
    float _pl5 ((6.0 * em_unit) * units.mm)
    float _pl6 ((7.0 * em_unit) * units.mm)
    float _pl7 ((8.0 * em_unit) * units.mm)
    float _pl8 ((9.0 * em_unit) * units.mm)
    float _pl9 ((10.0 * em_unit) * units.mm)

    // float _pl0 ((5.0 * em_unit) * units.mm)
    // float _pl1 ((4.0 * em_unit) * units.mm)
    // float _pl2 ((3.0 * em_unit) * units.mm)
    // float _pl3 ((2.0 * em_unit) * units.mm)
    // float _pl4 ((1.0 * em_unit) * units.mm)
    // float _pl5 ((1.0 * em_unit) * units.mm)
    // float _pl6 ((2.0 * em_unit) * units.mm)
    // float _pl7 ((3.0 * em_unit) * units.mm)
    // float _pl8 ((4.0 * em_unit) * units.mm)
    // float _pl9 ((5.0 * em_unit) * units.mm)

    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(6078.9),
        collector_cross_section_area: 760.6 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 168.5 * units.cm,
        primary_flow_rate: k_carb(1322.2),
        velocity_decay: 1.0,
        volume: (exhaust_volume_0 + 100.0) * units.L
    )

    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_60.wav", volume: 0.005))
}


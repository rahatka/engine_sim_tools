import "engine_sim.mr"
import "_diesel.mr"

units units()
constants constants()

label bore(293.0) // 207
label stroke(254.0)
label compression_ratio(17.0)
label con_rod(635.0) // 686 + 584.2 / 2
label compression_height(89.3 * units.mm)
label intake_valve_diameter(117.47343) // 722.566 / 2 * 30 area
label exhaust_valve_diameter(72.0) // ~
label intake_valves(1.0)
label exhaust_valves(4.0)

label cyl(10.0)
label row(1.0)
label cycle(rot360)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)

label redline(1000.0) // RPM

//
label IVL(25.0) // no intake valves
label EVL(15.0)
label IVO(40.0 * units.deg) //BBDC 44
label IVC(10.0 * units.deg) //ABDC 44
label EVO(60.0 * units.deg) //BBDC 84
label EVC(-20.0 * units.deg) //ABDC 52

label intake_duration(IVO + IVC)
label exhaust_duration(EVO + EVC)

label ILC(rot180 + IVC - IVO)
label ELC(rot180 + EVC - EVO)

label con_rod_mass(50.0 * units.kg)
label piston_mass(50.0 * units.kg)
label crank_mass(1000.0) // kg
label flywheel_mass(100.0) // kg

label exhaust_delay_coeff(1.5)


import "engine_sim.mr"
import "_diesel.mr"

units units()
constants constants()

label bore(318.0)
label stroke(330.0)
label compression_ratio(11.0)
label con_rod(710.0)
label compression_height(302.0 * units.mm)
label intake_valve_diameter(102.0)
label exhaust_valve_diameter(102.0)
label intake_valves(2.0)
label exhaust_valves(2.0)

label redline(800.0) // RPM

label cyl(6.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// Д-50 Тепловозные дизели 1980
label IVL(19.5)
label EVL(19.5)
label IVO(80.0 * units.deg) //BTDC
label IVC(35.0 * units.deg) //ABDC
label EVO(52.0 * units.deg) //BBDC
label EVC(54.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(61.0) // kg
label crank_mass(1900.0) // kg
label flywheel_mass(200.0) // kg

label exhaust_delay_coeff(1.5)

public node eng_distributor {
    input wires;
    input timing_curve;
    input rev_limit: 1000 * units.rpm;
    input limiter_duration: 0.001;
    alias output __out:
        ignition_module(timing_curve: timing_curve, rev_limit: rev_limit, limiter_duration: limiter_duration)
            .connect_wire(wires.wire1, rot * 0)
            .connect_wire(wires.wire3, rot * 1)
            .connect_wire(wires.wire5, rot * 2)
            .connect_wire(wires.wire6, rot * 3)
            .connect_wire(wires.wire4, rot * 4)
            .connect_wire(wires.wire2, rot * 5);
}

private node wires {
    output wire1: ignition_wire();
    output wire2: ignition_wire();
    output wire3: ignition_wire();
    output wire4: ignition_wire();
    output wire5: ignition_wire();
    output wire6: ignition_wire();
}


public node camshaft_builder {
    output intake_cam_0: _intake_cam_0;
    output exhaust_cam_0: _exhaust_cam_0;

    camshaft _intake_cam_0(base_radius: 37.4 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 37.4 * units.mm, lobe_profile: exhaust_lobe_profile)

    _intake_cam_0
        .add_lobe(ILC + 0 * rot)
        .add_lobe(ILC + 5 * rot)
        .add_lobe(ILC + 1 * rot)
        .add_lobe(ILC + 4 * rot)
        .add_lobe(ILC + 2 * rot)
        .add_lobe(ILC + 3 * rot)
    _exhaust_cam_0
        .add_lobe(ELC + 0 * rot)
        .add_lobe(ELC + 5 * rot)
        .add_lobe(ELC + 1 * rot)
        .add_lobe(ELC + 4 * rot)
        .add_lobe(ELC + 2 * rot)
        .add_lobe(ELC + 3 * rot)
}

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node head {
    // port_flow.py v1.7
    // intake port area: 127.2 cm²; saturated lift: 21.92 mm
    // exhaust port area: 127.2 cm²; saturated lift: 21.92 mm
    // cylinder volume: 26209.5 cm³ (1599.40 CI); engine volume: 157.257 L (9596.40 CI)
    // chamber volume: 2620.9 cm³ (159.94 CI); conrod/stroke 2.152; N.A.C.C. 376.18
    // 32 harmonic intake runner length: 53.2 cm; diameter: 11.0 cm
    // primary length: 173.0 cm, area: 127.2 cm², diameter: 12.7 cm
    // collector diameter: 22.0 cm, area: 381.6 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 2620.946 * units.cc,
        intake_runner_volume: 5051.5 * units.cc,
        intake_runner_cross_section_area: 94.9 * units.cm2,
        exhaust_runner_volume: 1683.8 * units.cc,
        exhaust_runner_cross_section_area: 127.2 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "D50",
        starter_torque: 6000 * units.Nm,
        starter_speed: 50 * units.rpm,
        redline: redline * units.rpm,
        fuel: diesel(),
        throttle_gamma: 2.0,
        jitter: 0.4,
        noise: 0.01,
        hf_gain: 0.001,
        simulation_frequency: 10000,
        dyno_min_speed: 300 * units.rpm
    )

    wires wires()

    connecting_rod_parameters cr_params(
        mass: con_rod_mass * units.kg,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.kg,
            length: con_rod * units.mm
        ),
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    label crank_moment(
         disk_moment_of_inertia(mass: crank_mass * units.kg, radius: (stroke / 2.0) * units.mm)
    )
    label flywheel_moment(
        disk_moment_of_inertia(mass: flywheel_mass * units.kg, radius: (stroke * 2.5) * units.mm)
    )
    label other_moment(
        disk_moment_of_inertia(mass: 1 * units.kg, radius: 1.0 * units.cm)
    )

    crankshaft c0(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: flywheel_mass * units.kg,
        mass: crank_mass * units.kg,
        friction_torque: 235.9 * units.Nm,
        moment_of_inertia: crank_moment + flywheel_moment + other_moment,
        tdc: 0.0
    )

    rod_journal rj0(angle: rot * 0 + rot90)
    rod_journal rj1(angle: rot * 5 + rot90)
    rod_journal rj2(angle: rot * 1 + rot90)
    rod_journal rj3(angle: rot * 4 + rot90)
    rod_journal rj4(angle: rot * 2 + rot90)
    rod_journal rj5(angle: rot * 3 + rot90)
    
    c0
        .add_rod_journal(rj0)
        .add_rod_journal(rj1)
        .add_rod_journal(rj2)
        .add_rod_journal(rj3)
        .add_rod_journal(rj4)
        .add_rod_journal(rj5)

    piston_parameters piston_params(
        mass: 64.43 * units.kg,
        blowby: k_28inH2O(0.100),
        compression_height: compression_height,
        wrist_pin_position: 0.0,
        displacement: 0.0
    )

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    label collector_cross_section_area(381.6)

    label exhaust_pipe_length_0(400.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 1000.0) // Litres
    
    label spacing_factor(1.2)
    label flange_density(1.0 * exhaust_delay_coeff)
    

    intake intake(
        plenum_volume: 17.7 * units.L,
        plenum_cross_section_area: 284.7 * units.cm2,
        runner_length: 53.2 * units.cm,
        intake_flow_rate: k_carb(2096.8),
        idle_flow_rate: k_carb(0.03),
        idle_throttle_plate_position: 0.994,
        runner_flow_rate: k_carb(698.9),
        velocity_decay: 0.05
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(3494.6),
        collector_cross_section_area: 381.6 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 173.0 * units.cm,
        primary_flow_rate: k_carb(1164.9),
        velocity_decay: 1.0,
        volume: (exhaust_volume_0 + 50.0) * units.L
    )

    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_55.wav", volume: 0.005))
 
    cylinder_bank b0(bank_params, angle: 0)
    
    label pl0((6.5 * bore * spacing_factor / flange_density) * units.mm)
    label pl1((5.5 * bore * spacing_factor / flange_density) * units.mm)
    label pl2((4.5 * bore * spacing_factor / flange_density) * units.mm)
    label pl3((3.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl4((2.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl5((1.0 * bore * spacing_factor / flange_density) * units.mm)

    b0
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj0,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl0,
            ignition_wire: wires.wire1
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj1,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl1,
            ignition_wire: wires.wire2
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj2,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl2,
            ignition_wire: wires.wire3
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj3,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl3,
            ignition_wire: wires.wire4
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj4,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl4,
            ignition_wire: wires.wire5
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj5,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl5,
            ignition_wire: wires.wire6
        )

    engine
        .add_cylinder_bank(b0)

    engine.add_crankshaft(c0)

    camshaft_builder camshaft()

    b0.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_0,
            exhaust_camshaft: camshaft.exhaust_cam_0,
            flip_display: true
        )
    )

    // ignition_timing.py v0.2 a = 4.0, m = 1.2, r = 800
    function timing_curve(80.0 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 0.0 * units.deg)
        .add_sample(80.0 * units.rpm, 4.0 * units.deg)
        .add_sample(160.0 * units.rpm, 4.8 * units.deg)
        .add_sample(240.0 * units.rpm, 5.8 * units.deg)
        .add_sample(320.0 * units.rpm, 6.9 * units.deg)
        .add_sample(400.0 * units.rpm, 8.3 * units.deg)
        .add_sample(480.0 * units.rpm, 10.0 * units.deg)
        .add_sample(560.0 * units.rpm, 11.9 * units.deg)
        .add_sample(640.0 * units.rpm, 14.3 * units.deg)
        .add_sample(720.0 * units.rpm, 17.2 * units.deg)
        .add_sample(800.0 * units.rpm, 20.6 * units.deg)
        .add_sample(880.0 * units.rpm, -30.0 * units.deg)
        .add_sample(960.0 * units.rpm, -90.0 * units.deg)

    engine.add_ignition_module(
        eng_distributor(
            wires: wires,
            timing_curve: timing_curve
        )
    )
}

// МАЗ-525
public node veh {
    alias output __out: vehicle;

    label car_mass(80000) // 24380 empty; 49520 full

    vehicle vehicle(
        mass: car_mass * units.kg,
        drag_coefficient: 0.63,
        cross_sectional_area: (3220 * units.mm) * (3675 * units.mm),
        diff_ratio: 5.0,
        tire_radius: (1676.4 / 2) * units.mm, // 17,00 - 32
        rolling_resistance: 0.012 * car_mass * 9.81
        )
}

// МАЗ-525
private node trn {
    alias output __out:
        transmission(
            max_clutch_torque: 10000 * units.Nm,
            max_clutch_flex: 2.0 * units.deg,
            limit_clutch_flex: true,
            clutch_stiffness: 5000.0 * units.Nm / units.deg,
            clutch_damping: 1.0,
            simulate_flex: true
        )
        .add_gear(7.14)
        .add_gear(3.53)
        .add_gear(1.88)
        .add_gear(1.00)
        .add_gear(0.72);
}

run(
    engine: eng(),
    transmission: trn(),
    vehicle: veh()
)

import "engine_sim.mr"
import "_diesel.mr"

units units()
constants constants()

// Engine Sim V0.1.14A
// Д-35 40 HP @ 1500
// d35
// Created by oror 2023
// 1-2-4-3

label bore(100.0)
label stroke(130.0)
label compression_ratio(17.6) // 17,3—17,9
label con_rod(260.0)
label compression_height(95.0 * units.mm)
label intake_valve_diameter(38.0)
label exhaust_valve_diameter(34.0)
label intake_valves(1.0)
label exhaust_valves(1.0)

label cyl(4.0)
label row(1.0)
label cycle(360.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

label redline(1600.0) // RPM

// D-35
label IVL(12.243)
label EVL(12.243)
label IVO(46.0 * units.deg) //BTDC
label IVC(46.0 * units.deg) //ABDC
label EVO(88.0 * units.deg) //BBDC
label EVC(58.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC))
label ELC((rot180 - EVO + EVC))

label con_rod_mass(1200.0) // ? g
label crank_mass(50.0) // ? kg
label flywheel_mass(12.0) // kg

label exhaust_delay_coeff(1.5)

public node eng_distributor {
    input wires;
    input timing_curve;
    input rev_limit: 2000 * units.rpm;
    input limiter_duration: 0.0001;
    alias output __out:
        ignition_module(timing_curve: timing_curve, rev_limit: rev_limit, limiter_duration: limiter_duration)
            .connect_wire(wires.wire1, rot * 0)
            .connect_wire(wires.wire3, rot * 1)
            .connect_wire(wires.wire4, rot * 2)
            .connect_wire(wires.wire2, rot * 3)
            .connect_wire(wires.wire1, rot * 4)
            .connect_wire(wires.wire3, rot * 5)
            .connect_wire(wires.wire4, rot * 6)
            .connect_wire(wires.wire2, rot * 7);
}

private node wires {
    output wire1: ignition_wire();
    output wire2: ignition_wire();
    output wire3: ignition_wire();
    output wire4: ignition_wire();
}


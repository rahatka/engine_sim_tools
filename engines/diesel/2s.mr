public node camshaft_builder {
    output intake_cam_0: _intake_cam_0;
    output exhaust_cam_0: _exhaust_cam_0;

    camshaft _intake_cam_0(base_radius: 22.4 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 22.4 * units.mm, lobe_profile: exhaust_lobe_profile)

    _intake_cam_0
        .add_lobe(ILC + 0 * rot)
        .add_lobe(ILC + 3 * rot)
        .add_lobe(ILC + 1 * rot)
        .add_lobe(ILC + 2 * rot)
    _exhaust_cam_0
        .add_lobe(ELC + 0 * rot)
        .add_lobe(ELC + 3 * rot)
        .add_lobe(ELC + 1 * rot)
        .add_lobe(ELC + 2 * rot)
}

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node head {
    // port_flow.py v1.2
    // intake port area: 8.7 cm²; saturated lift: 8.11 mm
    // exhaust port area: 7.0 cm²; saturated lift: 7.26 mm
    // cylinder volume: 1021.0 cm³ (62.3 CI); engine volume: 4.084 L (249.2 CI)
    // 32 harmonic intake runner length: 30.3 cm; diameter: 3.1 cm
    // primary length: 107.5 cm, area: 7.0 cm², diameter: 3.0 cm
    // collector diameter: 3.3 cm, area: 8.4 cm²
    // target power: 1500 RPM, power factor 1.00

    input intake_camshaft;
    input exhaust_camshaft;

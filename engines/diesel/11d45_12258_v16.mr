public node rods {
    input crank;
    output cr0_0: mcr0; output cr1_0: mcr1; output cr2_0: mcr2; output cr3_0: mcr3; output cr4_0: mcr4; output cr5_0: mcr5; output cr6_0: mcr6; output cr7_0: mcr7;
    output rj0_0: crank.rj0; output rj1_0: crank.rj1; output rj2_0: crank.rj2; output rj3_0: crank.rj3; output rj4_0: crank.rj4; output rj5_0: crank.rj5; output rj6_0: crank.rj6; output rj7_0: crank.rj7;
    output rj0_1: sj0_0; output rj1_1: sj1_0; output rj2_1: sj2_0; output rj3_1: sj3_0; output rj4_1: sj4_0; output rj5_1: sj5_0; output rj6_1: sj6_0; output rj7_1: sj7_0;
    output cr0_1: sr0_0; output cr1_1: sr1_0; output cr2_1: sr2_0; output cr3_1: sr3_0; output cr4_1: sr4_0; output cr5_1: sr5_0; output cr6_1: sr6_0; output cr7_1: sr7_0;

    connecting_rod_parameters master_rod_params(
        mass: con_rod_mass,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass,
            length: con_rod * units.mm
        ),
        slave_throw: R1 * units.mm,
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    connecting_rod_parameters slave_rod_params(
        mass: slave_rod_mass,
        moment_of_inertia: rod_moment_of_inertia(
            mass: slave_rod_mass,
            length: L1 * units.mm
        ),
        center_of_mass: 0.0,
        length: L1 * units.mm
    )

    connecting_rod mcr0(master_rod_params)
    connecting_rod mcr1(master_rod_params)
    connecting_rod mcr2(master_rod_params)
    connecting_rod mcr3(master_rod_params)
    connecting_rod mcr4(master_rod_params)
    connecting_rod mcr5(master_rod_params)
    connecting_rod mcr6(master_rod_params)
    connecting_rod mcr7(master_rod_params)

    connecting_rod sr0_0(slave_rod_params)
    connecting_rod sr1_0(slave_rod_params)
    connecting_rod sr2_0(slave_rod_params)
    connecting_rod sr3_0(slave_rod_params)
    connecting_rod sr4_0(slave_rod_params)
    connecting_rod sr5_0(slave_rod_params)
    connecting_rod sr6_0(slave_rod_params)
    connecting_rod sr7_0(slave_rod_params)

    rod_journal sj0_0(angle: -psi)
    rod_journal sj1_0(angle: -psi)
    rod_journal sj2_0(angle: -psi)
    rod_journal sj3_0(angle: -psi)
    rod_journal sj4_0(angle: -psi)
    rod_journal sj5_0(angle: -psi)
    rod_journal sj6_0(angle: -psi)
    rod_journal sj7_0(angle: -psi)

    mcr0.add_slave_journal(sj0_0)
    mcr1.add_slave_journal(sj1_0)
    mcr2.add_slave_journal(sj2_0)
    mcr3.add_slave_journal(sj3_0)
    mcr4.add_slave_journal(sj4_0)
    mcr5.add_slave_journal(sj5_0)
    mcr6.add_slave_journal(sj6_0)
    mcr7.add_slave_journal(sj7_0)
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "11D45",
        starter_torque: 8000 * units.Nm,
        starter_speed: 100 * units.rpm,
        redline: redline * units.rpm,
        fuel: diesel(),
        throttle_gamma: 1.5,
        jitter: 0.5,
        noise: 0.04,
        hf_gain: 0.001,
        simulation_frequency: 7000,
        dyno_min_speed: 500 * units.rpm
    )

    variant_0 variant()

    crank c0(vee / 2 + rot90, variant)
    rods r(c0)

    ignition_module ignition(timing_curve: timing_curve, rev_limit: redline * 1.25 * units.rpm, limiter_duration: 0.05)

    bank_builder bank0(
        var: variant,
        chamber_volume: 997.142,
        rods: r0(r),
        incline: vee / 2,
        phasing: 0,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust0,
        flip: true
    )

    bank_builder bank1(
        var: variant,
        chamber_volume: 994.061,
        rods: r1(r),
        incline: -vee / 2,
        phasing: vee,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust0
    )

    label collector_cross_section_area(1216.9)

    label exhaust_pipe_length_0(500.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 1000.0) // Litres

    intake intake(
        plenum_volume: 26.4 * units.L,
        plenum_cross_section_area: 347.3 * units.cm2,
        runner_length: 60.8 * units.cm,
        intake_flow_rate: k_carb(3905.5),
        idle_flow_rate: k_carb(0.03),
        idle_throttle_plate_position: 0.993,
        runner_flow_rate: k_carb(573.6),
        velocity_decay: 0.05
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(7078.7),
        collector_cross_section_area: 1216.9 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 168.5 * units.cm,
        primary_flow_rate: k_carb(962.3),
        velocity_decay: 0.9,
        volume: (exhaust_volume_0 + 20) * units.L
    )

    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_60.wav", volume: 0.005))
  
    function timing_curve(70.0 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 0.0 * units.deg)
        .add_sample(70.0 * units.rpm, 0.0 * units.deg)
        .add_sample(140.0 * units.rpm, 4.2 * units.deg)
        .add_sample(210.0 * units.rpm, 8.8 * units.deg)
        .add_sample(280.0 * units.rpm, 12.4 * units.deg)
        .add_sample(350.0 * units.rpm, 15.3 * units.deg)
        .add_sample(420.0 * units.rpm, 17.5 * units.deg)
        .add_sample(490.0 * units.rpm, 19.3 * units.deg)
        .add_sample(560.0 * units.rpm, 20.7 * units.deg)
        .add_sample(630.0 * units.rpm, 21.8 * units.deg)
        .add_sample(700.0 * units.rpm, 22.7 * units.deg)
        .add_sample(770.0 * units.rpm, 23.4 * units.deg)
        .add_sample(840.0 * units.rpm, 24.0 * units.deg)
        .add_sample(910.0 * units.rpm, 24.4 * units.deg)
        .add_sample(980.0 * units.rpm, 24.7 * units.deg)
        .add_sample(1050.0 * units.rpm, 25.0 * units.deg)
        .add_sample(1120.0 * units.rpm, -12.5 * units.deg)
        .add_sample(1190.0 * units.rpm, -25.0 * units.deg)

    engine
        .add_crankshaft(c0.c)
        .add_cylinder_bank(bank0.bank)
        .add_cylinder_bank(bank1.bank)
        .add_ignition_module(ignition)
}


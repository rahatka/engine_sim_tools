import "engine_sim.mr"
import "_diesel.mr"

units units()
constants constants()

label vee(45.0 * units.deg)
label bore(250.0)
label stroke(270.0)
label compression_ratio(12.8)
label con_rod(640.0) // ~
label compression_height(175.0 * units.mm) // ~
label intake_valve_diameter(76.0)
label exhaust_valve_diameter(72.0)
label intake_valves(2.0)
label exhaust_valves(2.0)

label redline(1050.0) // RPM
label intake_cfm((volume * redline) / 56.6) // CFM
label exhaust_cfm(intake_cfm * 1.75) // CFM

label cyl(16.0)
label row(2.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// Д-70 Тепловозные дизели 1980
label IVL(20.0)
label EVL(20.0)
label IVO(64.0 * units.deg) //BTDC
label IVC(46.0 * units.deg) //ABDC
label EVO(49.0 * units.deg) //BBDC
label EVC(61.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(93.0) // kg
label crank_mass(2500.0) // ? kg
label flywheel_mass(200.0) // kg

label exhaust_delay_coeff(1.5)

public node eng_distributor {
    input wires;
    input timing_curve;
    input rev_limit: 1500 * units.rpm;
    input limiter_duration: 0.001;
    alias output __out:
        ignition_module(timing_curve: timing_curve, rev_limit: rev_limit, limiter_duration: limiter_duration)
            .connect_wire(wires.wire1, rot * 0)
            .connect_wire(wires.wire6, rot * 2)
            .connect_wire(wires.wire7, rot * 4)
            .connect_wire(wires.wire4, rot * 6)
            .connect_wire(wires.wire8, rot * 8)
            .connect_wire(wires.wire3, rot * 10)
            .connect_wire(wires.wire2, rot * 12)
            .connect_wire(wires.wire5, rot * 14)
            .connect_wire(wires.wire9, rot * 0 + vee + rot360)
            .connect_wire(wires.wire14, rot * 2 + vee + rot360)
            .connect_wire(wires.wire15, rot * 4 + vee + rot360)
            .connect_wire(wires.wire12, rot * 6 + vee + rot360)
            .connect_wire(wires.wire16, rot * 8 + vee + rot360)
            .connect_wire(wires.wire11, rot * 10 + vee + rot360)
            .connect_wire(wires.wire10, rot * 12 + vee + rot360)
            .connect_wire(wires.wire13, rot * 14 + vee + rot360);
}

private node wires {
    output wire1: ignition_wire();
    output wire2: ignition_wire();
    output wire3: ignition_wire();
    output wire4: ignition_wire();
    output wire5: ignition_wire();
    output wire6: ignition_wire();
    output wire7: ignition_wire();
    output wire8: ignition_wire();
    output wire9: ignition_wire();
    output wire10: ignition_wire();
    output wire11: ignition_wire();
    output wire12: ignition_wire();
    output wire13: ignition_wire();
    output wire14: ignition_wire();
    output wire15: ignition_wire();
    output wire16: ignition_wire();
}


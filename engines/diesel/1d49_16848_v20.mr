import "engine_sim.mr"
import "_diesel.mr"

units units()
constants constants()

label vee(42.0 * units.deg)
label bore(260.0)
label stroke(260.0)
label compression_ratio(13.5)
label con_rod(600.0) // ~
label compression_height(162.0 * units.mm) // ~
label intake_valve_diameter(90.0)
label exhaust_valve_diameter(80.0)
label intake_valves(2.0)
label exhaust_valves(2.0)

label redline(1150.0) // RPM

label cyl(20.0)
label row(2.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// Д-49 Тепловозные дизели 1980
label IVL(22.0)
label EVL(22.0)
label IVO(57.0 * units.deg) //BTDC
label IVC(28.0 * units.deg) //ABDC
label EVO(59.5 * units.deg) //BBDC
label EVC(40.5 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(93.0) // kg
label crank_mass(1750.0) // ? kg
label flywheel_mass(200.0) // kg

label exhaust_delay_coeff(1.5)

public node eng_distributor {
    input wires;
    input timing_curve;
    input rev_limit: 1500 * units.rpm;
    input limiter_duration: 0.001;
    alias output __out:
        ignition_module(timing_curve: timing_curve, rev_limit: rev_limit, limiter_duration: limiter_duration)
            .connect_wire(wires.wire1, rot * 0)
            .connect_wire(wires.wire7, rot * 2)
            .connect_wire(wires.wire2, rot * 4)
            .connect_wire(wires.wire6, rot * 6)
            .connect_wire(wires.wire3, rot * 8)
            .connect_wire(wires.wire10, rot * 10)
            .connect_wire(wires.wire4, rot * 12)
            .connect_wire(wires.wire9, rot * 14)
            .connect_wire(wires.wire5, rot * 16)
            .connect_wire(wires.wire8, rot * 18)
            .connect_wire(wires.wire11, rot * 0 + vee + rot360)
            .connect_wire(wires.wire17, rot * 2 + vee + rot360)
            .connect_wire(wires.wire12, rot * 4 + vee + rot360)
            .connect_wire(wires.wire16, rot * 6 + vee + rot360)
            .connect_wire(wires.wire13, rot * 8 + vee + rot360)
            .connect_wire(wires.wire20, rot * 10 + vee + rot360)
            .connect_wire(wires.wire14, rot * 12 + vee + rot360)
            .connect_wire(wires.wire19, rot * 14 + vee + rot360)
            .connect_wire(wires.wire15, rot * 16 + vee + rot360)
            .connect_wire(wires.wire18, rot * 18 + vee + rot360);
}


public node camshaft_builder {
    output intake_cam_0: _intake_cam_0;
    output exhaust_cam_0: _exhaust_cam_0;

    camshaft _intake_cam_0(base_radius: 17.2 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 17.2 * units.mm, lobe_profile: exhaust_lobe_profile)

    _intake_cam_0
        .add_lobe(ILC + 0 * rot)
        .add_lobe(ILC + -0.5 * rot)
    _exhaust_cam_0
        .add_lobe(ELC + 0 * rot)
        .add_lobe(ELC + -0.5 * rot)
}

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node head {
    // port_flow.py v1.7
    // intake port area: 9.2 cm²; saturated lift: 8.32 mm
    // exhaust port area: 7.8 cm²; saturated lift: 7.68 mm
    // cylinder volume: 850.6 cm³ (51.91 CI); engine volume: 1.701 L (103.81 CI)
    // chamber volume: 50.0 cm³ (3.05 CI); conrod/stroke 1.750; N.A.C.C. 11.19
    // 32 harmonic intake runner length: 26.7 cm; diameter: 2.9 cm
    // primary length: 78.5 cm, area: 7.8 cm², diameter: 3.2 cm
    // collector diameter: 3.2 cm, area: 7.8 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 50.034 * units.cc,
        intake_runner_volume: 172.5 * units.cc,
        intake_runner_cross_section_area: 6.5 * units.cm2,
        exhaust_runner_volume: 57.5 * units.cc,
        exhaust_runner_cross_section_area: 7.8 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "D-16",
        starter_torque: 300 * units.Nm,
        starter_speed: 100 * units.rpm,
        redline: redline * units.rpm,
        fuel: gasoil(),
        throttle_gamma: 1.5,
        jitter: 0.3,
        noise: 0.1,
        hf_gain: 0.003,
        simulation_frequency: 12000,
        dyno_min_speed: 800 * units.rpm
    )

    wires wires()

    connecting_rod_parameters cr_params(
        mass: con_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.g,
            length: con_rod * units.mm
        ),
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    label crank_moment(disk_moment_of_inertia(mass: crank_mass * units.kg, radius: (stroke / 2.0) * units.mm))
    label flywheel_moment(disk_moment_of_inertia(mass: flywheel_mass * units.kg, radius: (stroke * 3.0) * units.mm))
    label other_moment(disk_moment_of_inertia(mass: 1 * units.kg, radius: 1.0 * units.cm))

    crankshaft c0(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: flywheel_mass * units.kg,
        mass: crank_mass * units.kg,
        friction_torque: 2.6 * units.Nm,
        moment_of_inertia: crank_moment + flywheel_moment + other_moment,
        tdc: 0.0
    )

    rod_journal rj0(angle: rot * 0 + rot90)
    rod_journal rj1(angle: rot * -0.5 + rot90)
    
    c0
        .add_rod_journal(rj0)
        .add_rod_journal(rj1)

    piston_parameters piston_params(
        mass: 750.0 * units.g,
        blowby: k_28inH2O(0.030),
        compression_height: compression_height,
        wrist_pin_position: 0.0,
        displacement: 0.0
    )

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    label collector_cross_section_area(7.8)

    label exhaust_pipe_length_0(120.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 100.0) // Litres
    
    label spacing_factor(1.16)
    label flange_density(1.2 * exhaust_delay_coeff)

    intake intake(
        plenum_volume: 0.4 * units.L,
        plenum_cross_section_area: 6.5 * units.cm2,
        runner_length: 26.7 * units.cm,
        intake_flow_rate: k_carb(51.0),
        idle_flow_rate: k_carb(0.05),
        idle_throttle_plate_position: 0.980,
        runner_flow_rate: k_carb(51.0),
        velocity_decay: 0.05
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(85.1),
        collector_cross_section_area: 7.8 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 78.5 * units.cm,
        primary_flow_rate: k_carb(85.1),
        velocity_decay: 0.9,
        volume: exhaust_volume_0 * units.L
    )

    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_51.wav", volume: 0.001))
 
    cylinder_bank b0(bank_params, angle: 0)

    label pl0((1.5 * bore * spacing_factor / flange_density) * units.mm)
    label pl1((1.0 * bore * spacing_factor / flange_density) * units.mm)

    b0
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj0,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl0,
            ignition_wire: wires.wire1
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj1,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl1,
            ignition_wire: wires.wire2
        )

    engine
        .add_cylinder_bank(b0)

    engine.add_crankshaft(c0)

    camshaft_builder camshaft()

    b0.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_0,
            exhaust_camshaft: camshaft.exhaust_cam_0
        )
    )

    // ignition_timing.py v0.2 a = 8.0, m = 1.11, r = 1800
    function timing_curve(180.0 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 0.0 * units.deg)
        .add_sample(180.0 * units.rpm, 8.0 * units.deg)
        .add_sample(360.0 * units.rpm, 8.9 * units.deg)
        .add_sample(540.0 * units.rpm, 9.9 * units.deg)
        .add_sample(720.0 * units.rpm, 10.9 * units.deg)
        .add_sample(900.0 * units.rpm, 12.1 * units.deg)
        .add_sample(1080.0 * units.rpm, 13.5 * units.deg)
        .add_sample(1260.0 * units.rpm, 15.0 * units.deg)
        .add_sample(1440.0 * units.rpm, 16.6 * units.deg)
        .add_sample(1620.0 * units.rpm, 18.4 * units.deg)
        .add_sample(1800.0 * units.rpm, 20.5 * units.deg)
        .add_sample(1980.0 * units.rpm, -15.0 * units.deg)
        .add_sample(2160.0 * units.rpm, -60.0 * units.deg)

    engine.add_ignition_module(
        eng_distributor(
            wires: wires,
            timing_curve: timing_curve
        )
    )
}

// ЗИС-8
public node veh {
    alias output __out: vehicle;

    label car_mass(5000) // 4200 empty; 7200 full

    vehicle vehicle(
        mass: car_mass * units.kg,
        drag_coefficient: 0.63,
        cross_sectional_area: (2300 * units.mm) * (2750 * units.mm),
        diff_ratio: 3.33, // custom
        tire_radius: (863.6 / 2) * units.mm, // 34x7
        rolling_resistance: 0.012 * car_mass * 9.81
        )
}

// ЗИС-5 / ЗИС-8
private node trn {
    alias output __out:
        transmission(
            max_clutch_torque: 350 * units.Nm,
            max_clutch_flex: 12 * units.deg,
            limit_clutch_flex: true,
            clutch_stiffness: 20 * units.Nm / units.deg,
            clutch_damping: 1.0,
            simulate_flex: true
        )
        .add_gear(6.6)
        .add_gear(3.74)
        .add_gear(1.84)
        .add_gear(1.0);
}

run(
    engine: eng(),
    transmission: trn(),
    vehicle: veh()
)
import "engine_sim.mr"
import "_diesel.mr"

units units()
constants constants()

label bore(95.0)
label stroke(120.0)
label compression_ratio(18.0)
label con_rod(210.0)
label compression_height(65.0 * units.mm) // ~
label intake_valve_diameter(39.0)
label exhaust_valve_diameter(36.0)
label intake_valves(1.0)
label exhaust_valves(1.0)

label cyl(2.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

label redline(1800.0) // RPM

// Д-16
label IVL(9.375) // 7.5 * 1.25
label EVL(9.375) // 7.5 * 1.25
label IVO(16.0 * units.deg) //BTDC
label IVC(40.0 * units.deg) //ABDC
label EVO(40.0 * units.deg) //BBDC
label EVC(16.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(1100.0) // ? g
label crank_mass(10.0) // ? kg
label flywheel_mass(8.0) // kg

label exhaust_delay_coeff(1.5)

public node eng_distributor {
    input wires;
    input timing_curve;
    input rev_limit: 2000 * units.rpm;
    input limiter_duration: 0.0001;
    alias output __out:
        ignition_module(timing_curve: timing_curve, rev_limit: rev_limit, limiter_duration: limiter_duration)
            .connect_wire(wires.wire1, rot * 0)
            .connect_wire(wires.wire2, rot * -0.5);
}

private node wires {
    output wire1: ignition_wire();
    output wire2: ignition_wire();
}


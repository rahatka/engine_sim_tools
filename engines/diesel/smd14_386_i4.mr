import "engine_sim.mr"
import "../../fuel/diesel.mr"

units units()
constants constants()

label bore(120.0)
label stroke(140.0)
label compression_ratio(16.0)
label con_rod(250.0)
label compression_height(78.5 * units.mm)
label intake_valve_diameter(52.0) // 47.0 clear
label exhaust_valve_diameter(45.0) // 40.0 clear
label intake_valves(1.0)
label exhaust_valves(1.0)

label cyl(4.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)

label redline(2000.0) // RPM

// СМД-14
label IVL(12.4275) // 7.5 lobe, 1.657 mult
label EVL(12.4275) // 7.5 lobe, 1.657 mult
label IVO(10.0 * units.deg) //BTDC
label IVC(46.0 * units.deg) //ABDC
label EVO(56.0 * units.deg) //BBDC
label EVC(10.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label piston_mass(1500.0) // ?
label con_rod_mass(2000.0) // ? g
label crank_mass(25.0) // ? kg
label flywheel_mass(6.0) // kg

label exhaust_delay_coeff(1.5)


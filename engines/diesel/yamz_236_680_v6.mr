import "engine_sim.mr"
import "../../fuel/diesel.mr"

units units()
constants constants()

label bore(130.0)
label stroke(140.0)
label compression_ratio(16.5)
label con_rod(266.0)
label compression_height(85.0 * units.mm) // or 100
label intake_valve_diameter(61.0)
label exhaust_valve_diameter(48.0)
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(2150.0) // RPM

label cyl(6.0)
label row(2.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label vee(90.0 * units.deg)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

label IVL(13.5)
label EVL(13.5)
label IVO(20.0 * units.deg) //BTDC
label IVC(56.0 * units.deg) //ABDC
label EVO(56.0 * units.deg) //BBDC
label EVC(20.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(4210.0) // g
label crank_mass(50.0) // ? kg
label flywheel_mass(10.0) // kg

label exhaust_delay_coeff(1.5)

public node eng_distributor {
    input wires;
    input timing_curve;
    input rev_limit: 3000 * units.rpm;
    input limiter_duration: 0.08;
    alias output __out:
        ignition_module(timing_curve: timing_curve, rev_limit: rev_limit, limiter_duration: limiter_duration)
            .connect_wire(wires.wire1, rot * 0)
            .connect_wire(wires.wire2, rot * 2)
            .connect_wire(wires.wire3, rot * 4)
            .connect_wire(wires.wire4, rot * 0 + vee)
            .connect_wire(wires.wire5, rot * 2 + vee)
            .connect_wire(wires.wire6, rot * 4 + vee);
}

private node wires {
    output wire1: ignition_wire();
    output wire2: ignition_wire();
    output wire3: ignition_wire();
    output wire4: ignition_wire();
    output wire5: ignition_wire();
    output wire6: ignition_wire();
}


units units()
constants constants()

label vee(45.0 * units.deg)
label bore(215.9) // 8 1/2
label stroke(254.0) // 10
label compression_ratio(16.0)
label con_rod(584.2) // 23
label compression_height(157.48 * units.mm) // ~ 6.2
label intake_valve_diameter(158.0) // 678.27 / 2 * x.x area
label exhaust_valve_diameter(63.5) // 2 1/2
label intake_valves(1.0)
label exhaust_valves(4.0)

label redline(1000.0) // RPM

label cyl(16.0)
label row(2.0)
label cycle(rot360)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)

label IVL(35.0) // no intake valves
label EVL(17.4244) // .686
label IVO(40.0 * units.deg) //BBDC 44
label IVC(10.0 * units.deg) //ABDC 44
label EVO(60.0 * units.deg) //BBDC 84
label EVC(-20.0 * units.deg) //ABDC 52

label ILC(rot180 + IVC - IVO)
label ELC(rot180 + EVC - EVO)

label con_rod_mass(17.0 * units.kg) // 23 / 11
label piston_mass(17.0 * units.kg)
label crank_mass(1406.0) // kg
label flywheel_mass(130.0) // kg harmonic balancer

label exhaust_delay_coeff(1.5)


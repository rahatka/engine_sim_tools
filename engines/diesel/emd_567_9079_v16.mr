private node add_lobes {
    input base;
    input shift: 0.0;
    input var;
    input this;
    alias output __out: this;

    this.add_lobe(base + var.s1 * rot_base + shift)
    this.add_lobe(base + var.s2 * rot_base + shift)
    this.add_lobe(base + var.s3 * rot_base + shift)
    this.add_lobe(base + var.s4 * rot_base + shift)
    this.add_lobe(base + var.s5 * rot_base + shift)
    this.add_lobe(base + var.s6 * rot_base + shift)
    this.add_lobe(base + var.s7 * rot_base + shift)
    this.add_lobe(base + var.s8 * rot_base + shift)
}

public node bank_builder {
    input var;
    input crankshaft;
    input incline;
    input phasing;
    input ignition;
    input intake;
    input exhaust0;
    input exhaust1;
    input pls;
    input flip: false;

    output bank: _b0;

    ignition_wire _wire1()
    ignition_wire _wire2()
    ignition_wire _wire3()
    ignition_wire _wire4()
    ignition_wire _wire5()
    ignition_wire _wire6()
    ignition_wire _wire7()
    ignition_wire _wire8()

    piston_parameters piston_params(
        mass: piston_mass,
        blowby: k_28inH2O(0.068),
        compression_height: compression_height,
        wrist_pin_position: 0.0,
        displacement: 0.0
    )

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    connecting_rod_parameters cr_params(
        mass: con_rod_mass,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass,
            length: con_rod * units.mm
        ),
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    camshaft _intake_cam_0(base_radius: 27.7 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 27.7 * units.mm, lobe_profile: exhaust_lobe_profile)

    _intake_cam_0.add_lobes(ILC, phasing, var)
    _exhaust_cam_0.add_lobes(ELC, phasing, var)

    ignition.connect_wire(_wire1, rot_base * var.s1 + phasing)
    ignition.connect_wire(_wire2, rot_base * var.s2 + phasing)
    ignition.connect_wire(_wire3, rot_base * var.s3 + phasing)
    ignition.connect_wire(_wire4, rot_base * var.s4 + phasing)
    ignition.connect_wire(_wire5, rot_base * var.s5 + phasing)
    ignition.connect_wire(_wire6, rot_base * var.s6 + phasing)
    ignition.connect_wire(_wire7, rot_base * var.s7 + phasing)
    ignition.connect_wire(_wire8, rot_base * var.s8 + phasing)
    ignition.connect_wire(_wire1, rot_base * var.s1 + phasing + rot360)
    ignition.connect_wire(_wire2, rot_base * var.s2 + phasing + rot360)
    ignition.connect_wire(_wire3, rot_base * var.s3 + phasing + rot360)
    ignition.connect_wire(_wire4, rot_base * var.s4 + phasing + rot360)
    ignition.connect_wire(_wire5, rot_base * var.s5 + phasing + rot360)
    ignition.connect_wire(_wire6, rot_base * var.s6 + phasing + rot360)
    ignition.connect_wire(_wire7, rot_base * var.s7 + phasing + rot360)
    ignition.connect_wire(_wire8, rot_base * var.s8 + phasing + rot360)

    cylinder_bank _b0(bank_params, angle: incline)

    _b0
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj0,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pls.pl0,
            ignition_wire: _wire1
        )
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj1,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pls.pl1,
            ignition_wire: _wire2
        )
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj2,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pls.pl2,
            ignition_wire: _wire3
        )
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj3,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pls.pl3,
            ignition_wire: _wire4
        )
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj4,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pls.pl4,
            ignition_wire: _wire5
        )
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj5,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pls.pl5,
            ignition_wire: _wire6
        )
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj6,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pls.pl6,
            ignition_wire: _wire7
        )
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj7,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pls.pl7,
            ignition_wire: _wire8
        )

    _b0.set_cylinder_head(
        head(
            intake_camshaft: _intake_cam_0,
            exhaust_camshaft: _exhaust_cam_0,
            flip_display: flip
        )
    )
}

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node head {
    // port_flow.py v1.7
    // intake port area: 85.9 cm²; saturated lift: 25.46 mm
    // exhaust port area: 96.1 cm²; saturated lift: 13.46 mm
    // cylinder volume: 9298.8 cm³ (567.45 CI); engine volume: 148.781 L (9079.20 CI)
    // chamber volume: 619.9 cm³ (37.83 CI); conrod/stroke 2.300; N.A.C.C. 462.40
    // 32 harmonic intake runner length: 61.3 cm; diameter: 6.4 cm
    // primary length: 168.5 cm, area: 96.1 cm², diameter: 11.1 cm
    // collector diameter: 34.3 cm, area: 922.2 cm²


    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 619.923 * units.cc,
        intake_runner_volume: 1985.7 * units.cc,
        intake_runner_cross_section_area: 32.4 * units.cm2,
        exhaust_runner_volume: 661.9 * units.cc,
        exhaust_runner_cross_section_area: 96.1 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

public node crank {
    input phasing;
    input var;

    output c: _c;
    output rj0: _rj0;
    output rj1: _rj1;
    output rj2: _rj2;
    output rj3: _rj3;
    output rj4: _rj4;
    output rj5: _rj5;
    output rj6: _rj6;
    output rj7: _rj7;

    label crank_moment(disk_moment_of_inertia(mass: crank_mass * units.kg, radius: (stroke / 2.0) * units.mm))
    label flywheel_moment(disk_moment_of_inertia(mass: flywheel_mass * units.kg, radius: (stroke * 2.5) * units.mm))
    label other_moment(disk_moment_of_inertia(mass: 1 * units.kg, radius: 1.0 * units.cm))

    crankshaft _c(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: flywheel_mass * units.kg,
        mass: crank_mass * units.kg,
        friction_torque: 223.2 * units.Nm,
        moment_of_inertia: crank_moment + flywheel_moment + other_moment,
        tdc: 0.0
    )

    rod_journal _rj0(angle: rot_base * var.s1 + rot90 + vee / 2)
    rod_journal _rj1(angle: rot_base * var.s2 + rot90 + vee / 2)
    rod_journal _rj2(angle: rot_base * var.s3 + rot90 + vee / 2)
    rod_journal _rj3(angle: rot_base * var.s4 + rot90 + vee / 2)
    rod_journal _rj4(angle: rot_base * var.s5 + rot90 + vee / 2)
    rod_journal _rj5(angle: rot_base * var.s6 + rot90 + vee / 2)
    rod_journal _rj6(angle: rot_base * var.s7 + rot90 + vee / 2)
    rod_journal _rj7(angle: rot_base * var.s8 + rot90 + vee / 2)

    _c
        .add_rod_journal(_rj0)
        .add_rod_journal(_rj1)
        .add_rod_journal(_rj2)
        .add_rod_journal(_rj3)
        .add_rod_journal(_rj4)
        .add_rod_journal(_rj5)
        .add_rod_journal(_rj6)
        .add_rod_journal(_rj7)
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "16-567",
        starter_torque: 8000 * units.Nm,
        starter_speed: 100 * units.rpm,
        redline: redline * units.rpm,
        fuel: diesel(),
        throttle_gamma: 1.5,
        jitter: 0.5,
        noise: 0.04,
        hf_gain: 0.001,
        simulation_frequency: 6500,
        dyno_min_speed: 250 * units.rpm
    )

    ////////////////////////////////////////
    ///////// change variants here /////////
    ////////////////////////////////////////
    variant_0 variant()

    crank c0(vee / 2 + rot90, variant)
    pl_0 pls() 

    ignition_module ignition(timing_curve: timing_curve, rev_limit: redline * 1.5 * units.rpm, limiter_duration: 0.05)

    bank_builder bank0(
        var: variant,
        crankshaft: c0,
        incline: vee / 2,
        phasing: 0,
        ignition: ignition,
        intake: intake,
        exhaust0: exhaust0,
        exhaust1: exhaust1,
        pls: pls,
        flip: true
    )

    bank_builder bank1(
        var: variant,
        crankshaft: c0,
        incline: -vee / 2,
        phasing: vee + rot360,
        ignition: ignition,
        intake: intake,
        exhaust0: exhaust0,
        exhaust1: exhaust1,
        pls: pls
    )

    label collector_cross_section_area(461.1)

    label exhaust_pipe_length_0(200.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 1000.0) // Litres

    label exhaust_pipe_length_1(250.0) // cm
    label exhaust_volume_1(collector_cross_section_area * exhaust_pipe_length_0 / 1000.0) // Litres

    intake intake(
        plenum_volume: 16.7 * units.L,
        plenum_cross_section_area: 259.1 * units.cm2,
        runner_length: 61.3 * units.cm,
        intake_flow_rate: k_carb(2479.7),
        idle_flow_rate: k_carb(0.05),
        idle_throttle_plate_position: 0.993,
        runner_flow_rate: k_carb(310.0),
        velocity_decay: 0.05
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(2066.4),
        collector_cross_section_area: 461.1 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 168.5 * units.cm,
        primary_flow_rate: k_carb(516.6),
        velocity_decay: 1.0,
        volume: (exhaust_volume_0 + 120.0) * units.L
    )

    exhaust_system_parameters es_params1(
        outlet_flow_rate: k_carb(2066.4),
        collector_cross_section_area: 461.1 * units.cm2,
        length: exhaust_pipe_length_1 * units.cm,
        primary_tube_length: 168.5 * units.cm,
        primary_flow_rate: k_carb(516.6),
        velocity_decay: 1.0,
        volume: (exhaust_volume_1 + 120.0) * units.L
    )

    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_56.wav", volume: 0.00025))
    exhaust_system exhaust1(es_params1, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_57.wav", volume: 0.00023))

    function timing_curve(66.7 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 5.0 * units.deg)
        .add_sample(66.7 * units.rpm, 5.5 * units.deg)
        .add_sample(133.3 * units.rpm, 6.5 * units.deg)
        .add_sample(200.0 * units.rpm, 7.5 * units.deg)
        .add_sample(266.7 * units.rpm, 8.4 * units.deg)
        .add_sample(333.3 * units.rpm, 9.3 * units.deg)
        .add_sample(400.0 * units.rpm, 10.1 * units.deg)
        .add_sample(466.7 * units.rpm, 10.8 * units.deg)
        .add_sample(533.3 * units.rpm, 11.5 * units.deg)
        .add_sample(600.0 * units.rpm, 12.1 * units.deg)
        .add_sample(666.7 * units.rpm, 12.7 * units.deg)
        .add_sample(733.3 * units.rpm, 13.2 * units.deg)
        .add_sample(800.0 * units.rpm, 13.7 * units.deg)
        .add_sample(866.7 * units.rpm, 14.2 * units.deg)
        .add_sample(933.3 * units.rpm, 14.6 * units.deg)
        .add_sample(1000.0 * units.rpm, 15.0 * units.deg)
        .add_sample(1066.7 * units.rpm, -12.5 * units.deg)
        .add_sample(1133.3 * units.rpm, -25.0 * units.deg)

    engine
        .add_crankshaft(c0.c)
        .add_cylinder_bank(bank0.bank)
        .add_cylinder_bank(bank1.bank)
        .add_ignition_module(ignition)
}


label spacing_factor(1.2)
label flange_density(1.0 * exhaust_delay_coeff)

private node pl_0 {
    output pl0: _pl0;
    output pl1: _pl1;
    output pl2: _pl2;
    output pl3: _pl3;
    output pl4: _pl4;
    output pl5: _pl5;
    output pl6: _pl6;
    output pl7: _pl7;

    float _pl0((2.0 * bore * spacing_factor / flange_density) * units.mm)
    float _pl1((1.0 * bore * spacing_factor / flange_density) * units.mm)
    float _pl2((2.2 * bore * spacing_factor / flange_density) * units.mm)
    float _pl3((3.2 * bore * spacing_factor / flange_density) * units.mm)
    float _pl4((3.2 * bore * spacing_factor / flange_density) * units.mm)
    float _pl5((2.2 * bore * spacing_factor / flange_density) * units.mm)
    float _pl6((1.0 * bore * spacing_factor / flange_density) * units.mm)
    float _pl7((2.0 * bore * spacing_factor / flange_density) * units.mm)
}


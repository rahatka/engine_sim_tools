public node rods {
    input c;
    output cr0_0: mcr0; output cr1_0: mcr1; output cr2_0: mcr2; output cr3_0: mcr3;
    output rj0_0: c.rj0; output rj1_0: c.rj1; output rj2_0: c.rj2; output rj3_0: c.rj3;
    output cr0_1: sr0_0; output cr1_1: sr1_0; output cr2_1: sr2_0; output cr3_1: sr3_0;
    output rj0_1: sj0_0; output rj1_1: sj1_0; output rj2_1: sj2_0; output rj3_1: sj3_0;
    output cr0_2: sr0_1; output cr1_2: sr1_1; output cr2_2: sr2_1; output cr3_2: sr3_1;
    output rj0_2: sj0_1; output rj1_2: sj1_1; output rj2_2: sj2_1; output rj3_2: sj3_1;
    output cr0_3: sr0_2; output cr1_3: sr1_2; output cr2_3: sr2_2; output cr3_3: sr3_2;
    output rj0_3: sj0_2; output rj1_3: sj1_2; output rj2_3: sj2_2; output rj3_3: sj3_2;

    connecting_rod_parameters master_rod_params(
        mass: con_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.g,
            length: con_rod * units.mm
        ),
        slave_throw: R1 * units.mm,
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    label slave_rod_mass(850)
    connecting_rod_parameters slave_rod_params(
        mass: slave_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: slave_rod_mass * units.g,
            length: L1 * units.mm
        ),
        center_of_mass: 0.0,
        length: L1 * units.mm
    )

    connecting_rod mcr0(master_rod_params)
    connecting_rod mcr1(master_rod_params)
    connecting_rod mcr2(master_rod_params)
    connecting_rod mcr3(master_rod_params)

    connecting_rod sr0_0(slave_rod_params)
    connecting_rod sr1_0(slave_rod_params)
    connecting_rod sr2_0(slave_rod_params)
    connecting_rod sr3_0(slave_rod_params)

    connecting_rod sr0_1(slave_rod_params)
    connecting_rod sr1_1(slave_rod_params)
    connecting_rod sr2_1(slave_rod_params)
    connecting_rod sr3_1(slave_rod_params)

    connecting_rod sr0_2(slave_rod_params)
    connecting_rod sr1_2(slave_rod_params)
    connecting_rod sr2_2(slave_rod_params)
    connecting_rod sr3_2(slave_rod_params)

    rod_journal sj0_0(angle: psi0)
    rod_journal sj1_0(angle: psi0)
    rod_journal sj2_0(angle: psi0)
    rod_journal sj3_0(angle: psi0)

    rod_journal sj0_1(angle: psi1)
    rod_journal sj1_1(angle: psi1)
    rod_journal sj2_1(angle: psi1)
    rod_journal sj3_1(angle: psi1)

    rod_journal sj0_2(angle: psi2)
    rod_journal sj1_2(angle: psi2)
    rod_journal sj2_2(angle: psi2)
    rod_journal sj3_2(angle: psi2)

    mcr0.add_slave_journal(sj0_0)
    mcr1.add_slave_journal(sj1_0)
    mcr2.add_slave_journal(sj2_0)
    mcr3.add_slave_journal(sj3_0)
    mcr0.add_slave_journal(sj0_1)
    mcr1.add_slave_journal(sj1_1)
    mcr2.add_slave_journal(sj2_1)
    mcr3.add_slave_journal(sj3_1)
    mcr0.add_slave_journal(sj0_2)
    mcr1.add_slave_journal(sj1_2)
    mcr2.add_slave_journal(sj2_2)
    mcr3.add_slave_journal(sj3_2)
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "Sla 16",
        starter_torque: 700 * units.Nm,
        starter_speed: 200 * units.rpm,
        redline: redline * units.rpm,
        fuel: diesel(),
        throttle_gamma: 1.75,
        jitter: 0.5,
        noise: 0.1,
        hf_gain: 0.003,
        simulation_frequency: 8000,
        dyno_min_speed: 500 * units.rpm,
        fluid_simulation_steps: 6,
        max_sle_solver_steps: 32
    )

    ignition_module ignition(timing_curve: timing_curve, rev_limit: redline * 1.3 * units.rpm, limiter_duration: 0.05)

    crank c0()
    rods r(c0)

    block_builder block0(
        phase: 0,        
        cv0: 169.646, // 0
        cv1: 169.646, // 180
        angle: vee / 2,
        rods0: r0(r),
        rods1: r2(r),
        ignition: ignition,
        intake: intake,
        exhaust0: exhaust0,
        exhaust1: exhaust1,
        flip_display: true
    )

    block_builder block1(
        phase: vee,
        cv0: 175.357, // 135
        cv1: 174.171, // 315
        angle: -vee / 2,
        rods0: r1(r),
        rods1: r3(r),
        ignition: ignition,
        intake: intake,
        exhaust0: exhaust1,
        exhaust1: exhaust0,
        flip_display: false
    )

    label collector_cross_section_area(68.8)

    label exhaust_pipe_length_0(200.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 1000.0) // Litres
    
    label exhaust_pipe_length_1(200.0) // cm
    label exhaust_volume_1(collector_cross_section_area * exhaust_pipe_length_1 / 1000.0) // Litres
    
    label spacing_factor(1.1)
    label flange_density(1.2)
    
    intake intake(
        plenum_volume: 11.0 * units.L,
        plenum_cross_section_area: 163.6 * units.cm2,
        runner_length: 20.8 * units.cm,
        intake_flow_rate: k_carb(1374.1),
        idle_flow_rate: k_carb(0.05),
        idle_throttle_plate_position: 0.994,
        runner_flow_rate: k_carb(171.8),
        velocity_decay: 0.05
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(1145.1),
        collector_cross_section_area: 68.8 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 65.4 * units.cm,
        primary_flow_rate: k_carb(286.3),
        velocity_decay: 0.8,
        volume: (exhaust_volume_0 + 5) * units.L
    )
    
    exhaust_system_parameters es_params1(
        outlet_flow_rate: k_carb(1145.1),
        collector_cross_section_area: 68.8 * units.cm2,
        length: exhaust_pipe_length_1 * units.cm,
        primary_tube_length: 65.4 * units.cm,
        primary_flow_rate: k_carb(286.3),
        velocity_decay: 0.8,
        volume: (exhaust_volume_1 + 5) * units.L
    )

    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_53.wav", volume: 0.001))
    exhaust_system exhaust1(es_params1, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_58.wav", volume: 0.001))

    engine
        .add_cylinder_bank(block0.b0)
        .add_cylinder_bank(block0.b1)
        .add_cylinder_bank(block1.b0)
        .add_cylinder_bank(block1.b1)
        .add_crankshaft(c0.c)
        .add_ignition_module(ignition)

    function timing_curve(150.0 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 0.0 * units.deg)
        .add_sample(150.0 * units.rpm, 0.0 * units.deg)
        .add_sample(300.0 * units.rpm, 1.2 * units.deg)
        .add_sample(450.0 * units.rpm, 3.0 * units.deg)
        .add_sample(600.0 * units.rpm, 4.9 * units.deg)
        .add_sample(750.0 * units.rpm, 6.7 * units.deg)
        .add_sample(900.0 * units.rpm, 8.5 * units.deg)
        .add_sample(1050.0 * units.rpm, 10.4 * units.deg)
        .add_sample(1200.0 * units.rpm, 12.2 * units.deg)
        .add_sample(1350.0 * units.rpm, 14.0 * units.deg)
        .add_sample(1500.0 * units.rpm, 15.9 * units.deg)
        .add_sample(1650.0 * units.rpm, 17.7 * units.deg)
        .add_sample(1800.0 * units.rpm, 19.5 * units.deg)
        .add_sample(1950.0 * units.rpm, 21.3 * units.deg)
        .add_sample(2100.0 * units.rpm, 23.2 * units.deg)
        .add_sample(2250.0 * units.rpm, 25.0 * units.deg)
        .add_sample(2400.0 * units.rpm, -12.5 * units.deg)
        .add_sample(2550.0 * units.rpm, -25.0 * units.deg)
}

// Pz V
public node veh {
    alias output __out: vehicle;

    label car_mass(45000) // 45.0 t panther

    vehicle vehicle(
        mass: car_mass * units.kg,
        drag_coefficient: 0.6,
        cross_sectional_area: (2830 * units.mm) * (2680 * units.mm),
        diff_ratio: 3.5 * 2,
        tire_radius: (821.77 / 2) * units.mm,
        rolling_resistance: 0.06 * car_mass * 9.81
        )
}


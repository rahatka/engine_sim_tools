public node block_builder {
    input phase;
    input cv0;
    input cv1;
    input angle;
    input rods0;
    input rods1;
    input ignition;
    input intake;
    input exhaust0;
    input exhaust1;

    input flip_display: false;

    output b0: _b0;
    output b1: _b1;

    ignition_wire _wire1()
    ignition_wire _wire2()
    ignition_wire _wire3()
    ignition_wire _wire4()
    ignition_wire _wire5()
    ignition_wire _wire6()
    ignition_wire _wire7()
    ignition_wire _wire8()

    camshaft _intake_cam_0(base_radius: 17.9 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 17.9 * units.mm, lobe_profile: exhaust_lobe_profile)
    
    camshaft _intake_cam_1(base_radius: 17.9 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_1(base_radius: 17.9 * units.mm, lobe_profile: exhaust_lobe_profile)

    _intake_cam_0
        .add_lobe(ILC + var.s1_0 * rot_base + phase)
        .add_lobe(ILC + var.s2_0 * rot_base + phase)
        .add_lobe(ILC + var.s3_0 * rot_base + phase)
        .add_lobe(ILC + var.s4_0 * rot_base + phase)
    _exhaust_cam_0
        .add_lobe(ELC + var.s1_0 * rot_base + phase)
        .add_lobe(ELC + var.s2_0 * rot_base + phase)
        .add_lobe(ELC + var.s3_0 * rot_base + phase)
        .add_lobe(ELC + var.s4_0 * rot_base + phase)
        
    _intake_cam_1
        .add_lobe(ILC + var.s1_1 * rot_base + phase)
        .add_lobe(ILC + var.s2_1 * rot_base + phase)
        .add_lobe(ILC + var.s3_1 * rot_base + phase)
        .add_lobe(ILC + var.s4_1 * rot_base + phase)
    _exhaust_cam_1
        .add_lobe(ELC + var.s1_1 * rot_base + phase)
        .add_lobe(ELC + var.s2_1 * rot_base + phase)
        .add_lobe(ELC + var.s3_1 * rot_base + phase)
        .add_lobe(ELC + var.s4_1 * rot_base + phase)

    ignition.connect_wire(_wire1, rot_base * var.s1_0 + phase)
    ignition.connect_wire(_wire2, rot_base * var.s2_0 + phase)
    ignition.connect_wire(_wire3, rot_base * var.s3_0 + phase)
    ignition.connect_wire(_wire4, rot_base * var.s4_0 + phase)

    ignition.connect_wire(_wire5, rot_base * var.s1_1 + phase)
    ignition.connect_wire(_wire6, rot_base * var.s2_1 + phase)
    ignition.connect_wire(_wire7, rot_base * var.s3_1 + phase)
    ignition.connect_wire(_wire8, rot_base * var.s4_1 + phase)

    piston_parameters piston_params(
        mass: piston_mass * units.g,
        blowby: k_28inH2O(0.042),
        compression_height: compression_height,
        wrist_pin_position: 0.0,
        displacement: 0.0
    )

    connecting_rod_parameters cr_params(
        mass: con_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.g,
            length: con_rod * units.mm
        ),
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    cylinder_bank _b0(bank_params, angle: angle)
    cylinder_bank _b1(bank_params, angle: angle + rot180)

    label spacing_factor(1.1)
    label flange_density(1.0 * exhaust_delay_coeff)

    label sh((15.0 / exhaust_delay_coeff) * units.cm)
    label pl0 ((1.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl1 ((2.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl2 ((3.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl3 ((4.0 * bore * spacing_factor / flange_density) * units.mm)

    _b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: rods0.cr0,
            rod_journal: rods0.rj0,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl0 + sh,
            ignition_wire: _wire1
        )
    _b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: rods0.cr1,
            rod_journal: rods0.rj1,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl1 + sh,
            ignition_wire: _wire2
        )
    _b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: rods0.cr2,
            rod_journal: rods0.rj2,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl2 + sh,
            ignition_wire: _wire3
        )
    _b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: rods0.cr3,
            rod_journal: rods0.rj3,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl3 + sh,
            ignition_wire: _wire4
        )

    _b1.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: rods1.cr0,
            rod_journal: rods1.rj0,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl0,
            ignition_wire: _wire5
        )
    _b1.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: rods1.cr1,
            rod_journal: rods1.rj1,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl1,
            ignition_wire: _wire6
        )
    _b1.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: rods1.cr2,
            rod_journal: rods1.rj2,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl2,
            ignition_wire: _wire7
        )
    _b1.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: rods1.cr3,
            rod_journal: rods1.rj3,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl3,
            ignition_wire: _wire8
        )

    _b0.set_cylinder_head(
        head(
            chamber_volume: cv0,
            intake_camshaft: _intake_cam_0,
            exhaust_camshaft: _exhaust_cam_0,
            flip_display: flip_display
        )
    )
    _b1.set_cylinder_head(
        head(
            chamber_volume: cv1,
            intake_camshaft: _intake_cam_1,
            exhaust_camshaft: _exhaust_cam_1,
            flip_display: flip_display
        )
    )
}

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node head {
    // port_flow.py v1.7
    // intake port area: 26.9 cm²; saturated lift: 14.27 mm
    // exhaust port area: 23.4 cm²; saturated lift: 13.31 mm
    // cylinder volume: 2290.2 cm³ (139.76 CI); engine volume: 36.644 L (2236.13 CI)
    // chamber volume: 169.6 cm³ (10.35 CI); conrod/stroke 2.000; N.A.C.C. 180.79
    // 32 harmonic intake runner length: 20.8 cm; diameter: 5.1 cm
    // primary length: 65.4 cm, area: 24.6 cm², diameter: 5.6 cm
    // collector diameter: 13.2 cm, area: 137.7 cm²

    input chamber_volume;
    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: chamber_volume * units.cc,
        intake_runner_volume: 425.5 * units.cc,
        intake_runner_cross_section_area: 20.4 * units.cm2,
        exhaust_runner_volume: 170.2 * units.cc,
        exhaust_runner_cross_section_area: 24.6 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

public node crank {
    output c: _c;
    output rj0: _rj0;
    output rj1: _rj1;
    output rj2: _rj2;
    output rj3: _rj3;

    label crank_moment(disk_moment_of_inertia(mass: crank_mass * units.kg, radius: (stroke / 2.0) * units.mm))
    label flywheel_moment(disk_moment_of_inertia(mass: flywheel_mass * units.kg, radius: (stroke * 3.5) * units.mm))
    label other_moment(disk_moment_of_inertia(mass: 1 * units.kg, radius: 1.0 * units.cm))

    crankshaft _c(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: flywheel_mass * units.kg,
        mass: crank_mass * units.kg,
        friction_torque: 55.0 * units.Nm,
        moment_of_inertia: crank_moment + flywheel_moment + other_moment,
        tdc: 0.0
    )

    rod_journal _rj0(angle: rot_base * var.s1_0 + rot90 + vee / 2)
    rod_journal _rj1(angle: rot_base * var.s2_0 + rot90 + vee / 2)
    rod_journal _rj2(angle: rot_base * var.s3_0 + rot90 + vee / 2)
    rod_journal _rj3(angle: rot_base * var.s4_0 + rot90 + vee / 2)

    _c
        .add_rod_journal(_rj0)
        .add_rod_journal(_rj1)
        .add_rod_journal(_rj2)
        .add_rod_journal(_rj3)
}


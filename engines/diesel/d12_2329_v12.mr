label bore(150.0)
label stroke(180.0) // 186.7 right bank
label compression_ratio(14.0) // 14 left bank 15 right bank
label con_rod(320.0)
label L1(238.4)
label R1(82.0)
label psi(67.0 * units.deg)
label compression_height(75.3 * units.mm)
label intake_valve_diameter(54.0)
label exhaust_valve_diameter(50.0)
label intake_valves(2.0)
label exhaust_valves(2.0)

label cyl(12.0)
label row(2.0)
label cycle(720.0 * units.deg)
label vee(60.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)

label redline(1700.0) // RPM

label IVL(13.0)
label EVL(13.0)
label IVO(20.0 * units.deg) //BTDC
label IVC(48.0 * units.deg) //ABDC
label EVO(48.0 * units.deg) //BBDC
label EVC(20.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180) // 270
label exhaust_duration(EVO + EVC + rot180) // 262

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(5450.0) // g
label slave_rod_mass(2013.0)
label piston_mass(2000.0) // ?
label crank_mass(87.5) // kg
label flywheel_mass(20.0) // kg

label exhaust_delay_coeff(1.5)


import "engine_sim.mr"
import "_diesel.mr"

units units()
constants constants()

label bore(130.0)
label stroke(140.0)
label compression_ratio(16.0)
label con_rod(265.0)
label compression_height(75.3 * units.mm) // ?
label intake_valve_diameter(61.0) // 58.0 калибр
label exhaust_valve_diameter(48.0) // 46.0 калибр
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(1800.0)

label cyl(6.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// А-01
label IVL(13.5)
label EVL(13.5)
label IVO(20.0 * units.deg) //BTDC 12
label IVC(50.0 * units.deg) //ABDC 54
label EVO(50.0 * units.deg) //BBDC 56
label EVC(20.0 * units.deg) //ATDC 14

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label piston_mass(4150.0 / cyl)
label con_rod_mass(4620.0 / cyl)
label crank_mass(50.0) // ?
label flywheel_mass(3.0) // kg

label exhaust_delay_coeff(1.5)


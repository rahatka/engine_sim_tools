public node block_builder {
    input variant;
    input phase;
    input angle;
    input crankshaft;
    input ignition;
    input intake;
    input exhaust;

    output b0: _b0;
    output b1: _b1;

    ignition_wire _wire1()
    ignition_wire _wire2()
    ignition_wire _wire3()
    ignition_wire _wire4()
    ignition_wire _wire5()
    ignition_wire _wire6()
    ignition_wire _wire7()
    ignition_wire _wire8()

    camshaft _intake_cam_0(base_radius: 20.0 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 20.0 * units.mm, lobe_profile: exhaust_lobe_profile)
    
    camshaft _intake_cam_1(base_radius: 20.0 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_1(base_radius: 20.0 * units.mm, lobe_profile: exhaust_lobe_profile)

    _intake_cam_0
        .add_lobe(ILC + variant.s1 * rot_base + phase)
        .add_lobe(ILC + variant.s2 * rot_base + phase)
        .add_lobe(ILC + variant.s3 * rot_base + phase)
        .add_lobe(ILC + variant.s4 * rot_base + phase)
    _exhaust_cam_0
        .add_lobe(ELC + variant.s1 * rot_base + phase)
        .add_lobe(ELC + variant.s2 * rot_base + phase)
        .add_lobe(ELC + variant.s3 * rot_base + phase)
        .add_lobe(ELC + variant.s4 * rot_base + phase)
        
    _intake_cam_1
        .add_lobe(ILC + variant.s1 * rot_base + phase - rot + rot360)
        .add_lobe(ILC + variant.s2 * rot_base + phase - rot + rot360)
        .add_lobe(ILC + variant.s3 * rot_base + phase - rot + rot360)
        .add_lobe(ILC + variant.s4 * rot_base + phase - rot + rot360)
    _exhaust_cam_1
        .add_lobe(ELC + variant.s1 * rot_base + phase - rot + rot360)
        .add_lobe(ELC + variant.s2 * rot_base + phase - rot + rot360)
        .add_lobe(ELC + variant.s3 * rot_base + phase - rot + rot360)
        .add_lobe(ELC + variant.s4 * rot_base + phase - rot + rot360)

    ignition.connect_wire(_wire1, rot_base * variant.s1 + phase)
    ignition.connect_wire(_wire2, rot_base * variant.s2 + phase)
    ignition.connect_wire(_wire3, rot_base * variant.s3 + phase)
    ignition.connect_wire(_wire4, rot_base * variant.s4 + phase)

    ignition.connect_wire(_wire5, rot_base * variant.s1 + phase - rot + rot360)
    ignition.connect_wire(_wire6, rot_base * variant.s2 + phase - rot + rot360)
    ignition.connect_wire(_wire7, rot_base * variant.s3 + phase - rot + rot360)
    ignition.connect_wire(_wire8, rot_base * variant.s4 + phase - rot + rot360)

    piston_parameters piston_params(
        mass: piston_mass * units.g,
        blowby: k_28inH2O(0.047),
        compression_height: compression_height,
        wrist_pin_position: 0.0,
        displacement: 0.0
    )

    connecting_rod_parameters cr_params(
        mass: con_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.g,
            length: con_rod * units.mm
        ),
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    cylinder_bank _b0(bank_params, angle: angle)
    cylinder_bank _b1(bank_params, angle: angle + rot)

    label spacing_factor(1.1)
    label flange_density(1.0 * exhaust_delay_coeff)

    label pl0 ((4.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl1 ((3.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl2 ((2.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl3 ((1.0 * bore * spacing_factor / flange_density) * units.mm)

    _b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj0,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl0,
            ignition_wire: _wire1
        )
    _b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj1,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl1,
            ignition_wire: _wire2
        )
    _b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj2,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl2,
            ignition_wire: _wire3
        )
    _b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj3,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl3,
            ignition_wire: _wire4
        )

    _b1.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj0,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl0,
            ignition_wire: _wire5
        )
    _b1.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj1,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl1,
            ignition_wire: _wire6
        )
    _b1.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj2,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl2,
            ignition_wire: _wire7
        )
    _b1.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj3,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl3,
            ignition_wire: _wire8
        )

    _b0.set_cylinder_head(
        head(
            intake_camshaft: _intake_cam_0,
            exhaust_camshaft: _exhaust_cam_0,
            flip_display: true
        )
    )
    _b1.set_cylinder_head(
        head(
            intake_camshaft: _intake_cam_1,
            exhaust_camshaft: _exhaust_cam_1,
            flip_display: false
        )
    )
}

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node head {
    // port_flow.py v1.7
    // intake port area: 41.7 cm²; saturated lift: 12.55 mm
    // exhaust port area: 33.5 cm²; saturated lift: 11.25 mm
    // cylinder volume: 2827.4 cm³ (172.54 CI); engine volume: 45.239 L (2760.65 CI)
    // chamber volume: 202.0 cm³ (12.32 CI); conrod/stroke 1.750; N.A.C.C. 223.20
    // 32 harmonic intake runner length: 22.1 cm; diameter: 5.9 cm
    // primary length: 63.3 cm, area: 35.2 cm², diameter: 6.7 cm
    // collector diameter: 16.9 cm, area: 225.0 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 201.960 * units.cc,
        intake_runner_volume: 603.0 * units.cc,
        intake_runner_cross_section_area: 27.3 * units.cm2,
        exhaust_runner_volume: 201.0 * units.cc,
        exhaust_runner_cross_section_area: 35.2 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

public node crank {
    input var;
    output c: _c;
    output rj0_0: _rj0_0;
    output rj1_0: _rj1_0;
    output rj2_0: _rj2_0;
    output rj3_0: _rj3_0;
    output rj0_1: _rj0_1;
    output rj1_1: _rj1_1;
    output rj2_1: _rj2_1;
    output rj3_1: _rj3_1;

    label crank_moment(disk_moment_of_inertia(mass: crank_mass * units.kg, radius: (stroke / 2.0) * units.mm))
    label flywheel_moment(disk_moment_of_inertia(mass: flywheel_mass * units.kg, radius: (stroke * 3.5) * units.mm))
    label other_moment(disk_moment_of_inertia(mass: 1 * units.kg, radius: 1.0 * units.cm))

    crankshaft _c(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: flywheel_mass * units.kg,
        mass: crank_mass * units.kg,
        friction_torque: 67.9 * units.Nm,
        moment_of_inertia: crank_moment + flywheel_moment + other_moment,
        tdc: 0.0
    )

    rod_journal _rj0_0(angle: rot_base * var.s1 + rot90 + vee / 2)
    rod_journal _rj1_0(angle: rot_base * var.s2 + rot90 + vee / 2)
    rod_journal _rj2_0(angle: rot_base * var.s3 + rot90 + vee / 2)
    rod_journal _rj3_0(angle: rot_base * var.s4 + rot90 + vee / 2)
    rod_journal _rj0_1(angle: rot_base * var.s1 + rot90 + vee / 2 + sb_phase)
    rod_journal _rj1_1(angle: rot_base * var.s2 + rot90 + vee / 2 + sb_phase)
    rod_journal _rj2_1(angle: rot_base * var.s3 + rot90 + vee / 2 + sb_phase)
    rod_journal _rj3_1(angle: rot_base * var.s4 + rot90 + vee / 2 + sb_phase)

    _c
        .add_rod_journal(_rj0_0)
        .add_rod_journal(_rj0_1)
        .add_rod_journal(_rj1_0)
        .add_rod_journal(_rj1_1)
        .add_rod_journal(_rj2_0)
        .add_rod_journal(_rj2_1)
        .add_rod_journal(_rj3_0)
        .add_rod_journal(_rj3_1)
}

public node c0_1 {
    input crank;
    output rj0: crank.rj0_0;
    output rj1: crank.rj1_0;
    output rj2: crank.rj2_0;
    output rj3: crank.rj3_0;
}

public node c1_1 {
    input crank;
    output rj0: crank.rj0_1;
    output rj1: crank.rj1_1;
    output rj2: crank.rj2_1;
    output rj3: crank.rj3_1;
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "X16",
        starter_torque: 700 * units.Nm,
        starter_speed: 200 * units.rpm,
        redline: redline * units.rpm,
        fuel: diesel(),
        throttle_gamma: 1.75,
        jitter: 0.8,
        noise: 0.1,
        hf_gain: 0.006,
        simulation_frequency: 7000,
        dyno_min_speed: 500 * units.rpm,
        fluid_simulation_steps: 6,
        max_sle_solver_steps: 32
    )

    ignition_module ignition(timing_curve: timing_curve, rev_limit: redline * 1.3 * units.rpm, limiter_duration: 0.05)

    variant_0 var()
    crank c0(var)

    block_builder block0(
        variant: var,
        phase: 0,
        angle: vee / 2,
        crankshaft: c0_1(c0),
        ignition: ignition,
        intake: intake,
        exhaust: exhaust0
    )

    block_builder block1(
        variant: var,
        phase: vee + sb_phase + rot,
        angle: vee / 2 + rot180,
        crankshaft: c1_1(c0),
        ignition: ignition,
        intake: intake,
        exhaust: exhaust1
    )

    label collector_cross_section_area(112.5)

    label exhaust_pipe_length_0(200.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 100.0) // Litres
    
    label exhaust_pipe_length_1(200.0) // cm
    label exhaust_volume_1(collector_cross_section_area * exhaust_pipe_length_1 / 100.0) // Litres

    intake intake(
        plenum_volume: 13.6 * units.L,
        plenum_cross_section_area: 218.4 * units.cm2,
        runner_length: 22.1 * units.cm,
        intake_flow_rate: k_carb(1696.5),
        idle_flow_rate: k_carb(0.05),
        idle_throttle_plate_position: 0.994,
        runner_flow_rate: k_carb(212.1),
        velocity_decay: 0.05
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(1413.7),
        collector_cross_section_area: 112.5 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 63.3 * units.cm,
        primary_flow_rate: k_carb(353.4),
        velocity_decay: 0.8,
        volume: (exhaust_volume_0 + 2) * units.L
    )
    
    exhaust_system_parameters es_params1(
        outlet_flow_rate: k_carb(1413.7),
        collector_cross_section_area: 112.5 * units.cm2,
        length: exhaust_pipe_length_1 * units.cm,
        primary_tube_length: 63.3 * units.cm,
        primary_flow_rate: k_carb(353.4),
        velocity_decay: 0.8,
        volume: (exhaust_volume_1 + 2) * units.L
    )

    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_53.wav", volume: 0.001))
    exhaust_system exhaust1(es_params1, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_58.wav", volume: 0.001))

    engine
        .add_cylinder_bank(block0.b0)
        .add_cylinder_bank(block0.b1)
        .add_cylinder_bank(block1.b0)
        .add_cylinder_bank(block1.b1)
        .add_crankshaft(c0.c)
        .add_ignition_module(ignition)

    function timing_curve(150.0 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 0.0 * units.deg)
        .add_sample(150.0 * units.rpm, 0.0 * units.deg)
        .add_sample(300.0 * units.rpm, 1.2 * units.deg)
        .add_sample(450.0 * units.rpm, 3.0 * units.deg)
        .add_sample(600.0 * units.rpm, 4.9 * units.deg)
        .add_sample(750.0 * units.rpm, 6.7 * units.deg)
        .add_sample(900.0 * units.rpm, 8.5 * units.deg)
        .add_sample(1050.0 * units.rpm, 10.4 * units.deg)
        .add_sample(1200.0 * units.rpm, 12.2 * units.deg)
        .add_sample(1350.0 * units.rpm, 14.0 * units.deg)
        .add_sample(1500.0 * units.rpm, 15.9 * units.deg)
        .add_sample(1650.0 * units.rpm, 17.7 * units.deg)
        .add_sample(1800.0 * units.rpm, 19.5 * units.deg)
        .add_sample(1950.0 * units.rpm, 21.3 * units.deg)
        .add_sample(2100.0 * units.rpm, 23.2 * units.deg)
        .add_sample(2250.0 * units.rpm, 25.0 * units.deg)
        .add_sample(2400.0 * units.rpm, -15.0 * units.deg)
        .add_sample(2550.0 * units.rpm, -45.0 * units.deg)
}

// Pz V
public node veh {
    alias output __out: vehicle;

    label car_mass(45000) // 45.0 t panther

    vehicle vehicle(
        mass: car_mass * units.kg,
        drag_coefficient: 0.6,
        cross_sectional_area: (2830 * units.mm) * (2680 * units.mm),
        diff_ratio: 3.5 * 2,
        tire_radius: (821.77 / 2) * units.mm,
        rolling_resistance: 0.06 * car_mass * 9.81
        )
}

// Pz V
private node trn {
    alias output __out:
        transmission(
            max_clutch_torque: 3200 * units.Nm,
            max_clutch_flex: 15 * units.deg,
            limit_clutch_flex: true,
            clutch_stiffness: 50 * units.Nm / units.deg,
            clutch_damping: 1.0,
            simulate_flex: true
        )
        .add_gear(10.8)  // 3.7 @ 2600
        .add_gear(5.26)  // 7.6
        .add_gear(3.33)  // 12.0
        .add_gear(2.13)  // 18.8
        .add_gear(1.48)  // 27.0
        .add_gear(1.06)  // 37.7
        .add_gear(0.85); // 47.3
}

run(
    engine: eng(),
    transmission: trn(),
    vehicle: veh()
)

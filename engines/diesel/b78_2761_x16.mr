import "engine_sim.mr"
import "_diesel.mr"

units units()
constants constants()

label bore(150.0)
label stroke(160.0)
label compression_ratio(15.0)
label con_rod(280.0)
label compression_height(80.0 * units.mm)
label intake_valve_diameter(58.0)
label exhaust_valve_diameter(52.0)
label intake_valves(2.0)
label exhaust_valves(2.0)

label redline(2250.0) // RPM

label cyl(16.0)
label row(4.0)
label cycle(720.0 * units.deg)
label vee(135.0 * units.deg)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label rot(cycle / cyl)

label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)

label sb_phase(rot * 4)

label IVL(12.0)
label EVL(12.0)
label IVO(15.0 * units.deg) //BTDC
label IVC(55.0 * units.deg) //ABDC
label EVO(55.0 * units.deg) //BBDC
label EVC(15.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(1500.0) // ?
label piston_mass(1500.0) // ?
label crank_mass(32.0) // ?
label flywheel_mass(15.0) // kg

label exhaust_delay_coeff(1.5)


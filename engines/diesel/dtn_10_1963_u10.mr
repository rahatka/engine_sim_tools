import "engine_sim.mr"
import "_diesel.mr"

units units()
constants constants()

label bore(160.0)
label stroke(160.0)
label compression_ratio(13.5)
label con_rod(275.0) // ~
label compression_height(75.0 * units.mm) // ~
label intake_valve_diameter(54.0) // ?
label exhaust_valve_diameter(50.0) // ?
label intake_valves(2.0) // ?
label exhaust_valves(2.0) // ?

label redline(2750.0) // RPM

label cyl(10.0)
label row(2.0)
label cyl_base(cyl / row)
label cycle(720.0 * units.deg)
label separation(320.0 / 2.0 * units.mm)
label rot(cycle / cyl)
label rot_base(cycle / cyl_base)
label rot45(45.0 * units.deg)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

label sb_pos(1)
label sb_shift(rot + rot * 2 * sb_pos)

label IVL(13.0)
label EVL(13.0)
label IVO(20.0 * units.deg) //BTDC
label IVC(48.0 * units.deg) //ABDC
label EVO(48.0 * units.deg) //BBDC
label EVC(20.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label exhaust_delay_coeff(1.5)


private node add_lobes {
    input base;
    input shift: 0.0;
    input this;
    alias output __out: this;

    this.add_lobe(base + 0 * rot_base + shift)
    this.add_lobe(base + 1 * rot_base + shift)
    this.add_lobe(base + 4 * rot_base + shift)
    this.add_lobe(base + 2 * rot_base + shift)
    this.add_lobe(base + 3 * rot_base + shift)
}

public node block_builder {
    input phasing;
    input intake;
    input exhaust;
    input ignition;
    input separation;
    input sh;
    input flip: false;

    output bank: _b0;
    output crank: _c0;

    ignition_wire _wire1()
    ignition_wire _wire2()
    ignition_wire _wire3()
    ignition_wire _wire4()
    ignition_wire _wire5()

    piston_parameters piston_params(
        mass: 1700 * units.g,
        blowby: k_28inH2O(0.050),
        compression_height: compression_height,
        wrist_pin_position: 0.0,
        displacement: 0.0
    )

    connecting_rod_parameters cr_params(
        mass: 2300.0 * units.g,
        moment_of_inertia: 0.007,
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    camshaft _intake_cam_0(base_radius: 20.6 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 20.6 * units.mm, lobe_profile: exhaust_lobe_profile)

    _intake_cam_0.add_lobes(ILC, phasing)
    _exhaust_cam_0.add_lobes(ELC, phasing)

    rod_journal _rj0(angle: rot_base * 0 + phasing + rot90)
    rod_journal _rj1(angle: rot_base * 1 + phasing + rot90)
    rod_journal _rj2(angle: rot_base * 4 + phasing + rot90)
    rod_journal _rj3(angle: rot_base * 2 + phasing + rot90)
    rod_journal _rj4(angle: rot_base * 3 + phasing + rot90)

    crankshaft _c0(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: 25.0 * units.kg,
        mass: 42.0 * units.kg,
        friction_torque: 48.3 * units.Nm,
        moment_of_inertia: 2.0,
        position_x: separation,
        position_y: 0.0,
        tdc: 0.0
    )

    _c0
        .add_rod_journal(_rj0)
        .add_rod_journal(_rj1)
        .add_rod_journal(_rj2)
        .add_rod_journal(_rj3)
        .add_rod_journal(_rj4)

    ignition.connect_wire(_wire1, rot_base * 0 + phasing)
    ignition.connect_wire(_wire2, rot_base * 1 + phasing)
    ignition.connect_wire(_wire4, rot_base * 2 + phasing)
    ignition.connect_wire(_wire5, rot_base * 3 + phasing)
    ignition.connect_wire(_wire3, rot_base * 4 + phasing)

    label spacing_factor(1.2)
    label flange_density(1.0 * exhaust_delay_coeff)

    label pl0 ((5.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl1 ((4.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl2 ((3.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl3 ((2.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl4 ((1.0 * bore * spacing_factor / flange_density) * units.mm)

    cylinder_bank _b0(bank_params, angle: 0, position_x: separation)
    _b0
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: _rj0,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl0 + sh,
            ignition_wire: _wire1
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: _rj1,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl1 + sh,
            ignition_wire: _wire2
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: _rj2,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl2 + sh,
            ignition_wire: _wire3
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: _rj3,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl3 + sh,
            ignition_wire: _wire4
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: _rj4,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl4 + sh,
            ignition_wire: _wire5
        )
        .set_cylinder_head (
            head(
                intake_camshaft: _intake_cam_0,
                exhaust_camshaft: _exhaust_cam_0,
                flip_display: flip
            )
    )
    
}

public node head {
    // port_flow.py v1.7
    // intake port area: 35.7 cm²; saturated lift: 11.60 mm
    // exhaust port area: 30.6 cm²; saturated lift: 10.74 mm
    // cylinder volume: 3217.0 cm³ (196.31 CI); engine volume: 32.170 L (1963.13 CI)
    // chamber volume: 257.4 cm³ (15.71 CI); conrod/stroke 1.719; N.A.C.C. 158.72
    // 32 harmonic intake runner length: 17.8 cm; diameter: 6.8 cm
    // primary length: 51.5 cm, area: 33.6 cm², diameter: 6.5 cm
    // collector diameter: 14.6 cm, area: 168.1 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 257.359 * units.cc,
        intake_runner_volume: 637.7 * units.cc,
        intake_runner_cross_section_area: 35.9 * units.cm2,
        exhaust_runner_volume: 212.6 * units.cc,
        exhaust_runner_cross_section_area: 33.6 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "DTN-10",
        starter_torque: 800 * units.Nm,
        starter_speed: 150 * units.rpm,
        redline: redline * units.rpm,
        fuel: diesel(),
        throttle_gamma: 1.5,
        jitter: 0.7,
        noise: 0.05,
        hf_gain: 0.005,
        simulation_frequency: 10000,
        dyno_min_speed: 300 * units.rpm,
        fluid_simulation_steps: 6,
        max_sle_solver_steps: 32
    )

    block_builder block0(
        phasing: 0,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust0,
        separation: -separation,
        sh: 0,
        flip: true
    )

    block_builder block1(
        phasing: sb_shift,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust0,
        sh: 0,
        separation: separation
    )

    ignition_module ignition(timing_curve: timing_curve, rev_limit: 3500 * units.rpm, limiter_duration: 0.01)

    label collector_cross_section_area(168.1)

    label exhaust_pipe_length_0(250.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 100.0) // Litres

    intake intake(
        plenum_volume: 12.7 * units.L,
        plenum_cross_section_area: 179.5 * units.cm2,
        runner_length: 17.8 * units.cm,
        intake_flow_rate: k_carb(1548.2),
        idle_flow_rate: k_carb(0.02),
        idle_throttle_plate_position: 0.995,
        runner_flow_rate: k_carb(325.1),
        velocity_decay: 0.05
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(2644.8),
        collector_cross_section_area: 168.1 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 51.5 * units.cm,
        primary_flow_rate: k_carb(542.2),
        velocity_decay: 1.0,
        volume: exhaust_volume_0 * units.L
    )

    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_58.wav", volume: 0.001))

    engine
        .add_cylinder_bank(block0.bank)
        .add_cylinder_bank(block1.bank)

        .add_crankshaft(block0.crank)
        .add_crankshaft(block1.crank)

        .add_ignition_module(ignition)

    // ignition_timing.py v0.2 a = 4.0, m = 1.18, r = 2750
    function timing_curve(275.0 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 0.0 * units.deg)
        .add_sample(275.0 * units.rpm, 4.0 * units.deg)
        .add_sample(550.0 * units.rpm, 4.7 * units.deg)
        .add_sample(825.0 * units.rpm, 5.6 * units.deg)
        .add_sample(1100.0 * units.rpm, 6.6 * units.deg)
        .add_sample(1375.0 * units.rpm, 7.8 * units.deg)
        .add_sample(1650.0 * units.rpm, 9.2 * units.deg)
        .add_sample(1925.0 * units.rpm, 10.8 * units.deg)
        .add_sample(2200.0 * units.rpm, 12.7 * units.deg)
        .add_sample(2475.0 * units.rpm, 15.0 * units.deg)
        .add_sample(2750.0 * units.rpm, 17.7 * units.deg)
        .add_sample(3025.0 * units.rpm, -30.0 * units.deg)
        .add_sample(3300.0 * units.rpm, -90.0 * units.deg)
}

// Pz V
public node veh {
    alias output __out: vehicle;

    label car_mass(45000) // 45.0 t panther

    vehicle vehicle(
        mass: car_mass * units.kg,
        drag_coefficient: 0.6,
        cross_sectional_area: (2830 * units.mm) * (2680 * units.mm),
        diff_ratio: 3.5 * 3,
        tire_radius: (821.77 / 2) * units.mm,
        rolling_resistance: 0.06 * car_mass * 9.81
        )
}

// Pz V
private node trn {
    alias output __out:
        transmission(
            max_clutch_torque: 2200 * units.Nm,
            max_clutch_flex: 15 * units.deg,
            limit_clutch_flex: true,
            clutch_stiffness: 50 * units.Nm / units.deg,
            clutch_damping: 1.0,
            simulate_flex: true
        )
        .add_gear(10.8)  // 3.7 @ 2600
        .add_gear(5.26)  // 7.6
        .add_gear(3.33)  // 12.0
        .add_gear(2.13)  // 18.8
        .add_gear(1.48)  // 27.0
        .add_gear(1.06)  // 37.7
        .add_gear(0.85); // 47.3
}

run(
    engine: eng(),
    transmission: trn(),
    vehicle: veh()
)

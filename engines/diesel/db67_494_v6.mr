import "engine_sim.mr"
import "../../fuel/diesel.mr"

units units()
constants constants()

label bore(125.0)
label stroke(110.0)
label compression_ratio(16.0)
label con_rod(230.0) // ~
label compression_height(80.0 * units.mm) // ~
label intake_valve_diameter(43.5) // ~
label exhaust_valve_diameter(43.5) // ~
label intake_valves(2.0)
label exhaust_valves(2.0)

label cyl(6.0)
label row(2.0)
label cycle(720.0 * units.deg)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label vee(80.0 * units.deg)

label redline(3200.0) // RPM

// ЗиС Д7
label IVL(9.5)
label EVL(9.5)
label IVO(10.0 * units.deg) //BTDC
label IVC(46.0 * units.deg) //ABDC
label EVO(56.0 * units.deg) //BBDC
label EVC(10.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(1255.0) // ? g
label piston_mass(700.0) // ?
label crank_mass(40.0) // ? kg
label flywheel_mass(10.0) // kg

label exhaust_delay_coeff(1.5)


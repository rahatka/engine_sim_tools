public node camshaft_builder {
    output intake_cam_0: _intake_cam_0;
    output exhaust_cam_0: _exhaust_cam_0;
    
    output intake_cam_1: _intake_cam_1;
    output exhaust_cam_1: _exhaust_cam_1;

    camshaft _intake_cam_0(base_radius: 22.0 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 22.0 * units.mm, lobe_profile: exhaust_lobe_profile)
    
    camshaft _intake_cam_1(base_radius: 22.0 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_1(base_radius: 22.0 * units.mm, lobe_profile: exhaust_lobe_profile)

    _intake_cam_0
        .add_lobe(ILC + 0 * rot)
        .add_lobe(ILC + 2 * rot)
        .add_lobe(ILC + 4 * rot)
    _exhaust_cam_0
        .add_lobe(ELC + 0 * rot)
        .add_lobe(ELC + 2 * rot)
        .add_lobe(ELC + 4 * rot)
        
    _intake_cam_1
        .add_lobe(ILC + 0 * rot + vee)
        .add_lobe(ILC + 2 * rot + vee)
        .add_lobe(ILC + 4 * rot + vee)
    _exhaust_cam_1
        .add_lobe(ELC + 0 * rot + vee)
        .add_lobe(ELC + 2 * rot + vee)
        .add_lobe(ELC + 4 * rot + vee)
}

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node head {
    // port_flow.py v1.7
    // intake port area: 19.2 cm²; saturated lift: 12.03 mm
    // exhaust port area: 12.9 cm²; saturated lift: 9.88 mm
    // cylinder volume: 1526.4 cm³ (93.15 CI); engine volume: 9.159 L (558.89 CI)
    // chamber volume: 109.0 cm³ (6.65 CI); conrod/stroke 1.826; N.A.C.C. 62.87
    // 32 harmonic intake runner length: 23.1 cm; diameter: 4.3 cm
    // primary length: 61.2 cm, area: 13.6 cm², diameter: 4.2 cm
    // collector diameter: 5.6 cm, area: 24.4 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 109.030 * units.cc,
        intake_runner_volume: 339.8 * units.cc,
        intake_runner_cross_section_area: 14.7 * units.cm2,
        exhaust_runner_volume: 113.3 * units.cc,
        exhaust_runner_cross_section_area: 13.6 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "SMD-60",
        starter_torque: 700 * units.Nm,
        starter_speed: 100 * units.rpm,
        redline: redline * units.rpm,
        fuel: diesel(),
        throttle_gamma: 2.0,
        jitter: 0.8,
        noise: 0.5,
        hf_gain: 0.008,
        simulation_frequency: 10000,
        dyno_min_speed: 500 * units.rpm
    )

    wires wires()
    
    connecting_rod_parameters cr_params(
        mass: con_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.g,
            length: con_rod * units.mm
        ),
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    label crank_moment(disk_moment_of_inertia(mass: crank_mass * units.kg, radius: (stroke / 2.0) * units.mm))
    label flywheel_moment(disk_moment_of_inertia(mass: flywheel_mass * units.kg, radius: (stroke * 3.5) * units.mm))
    label other_moment(disk_moment_of_inertia(mass: 1 * units.kg, radius: 1.0 * units.cm))

    crankshaft c0(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: flywheel_mass * units.kg,
        mass: crank_mass * units.kg,
        friction_torque: 13.7 * units.Nm,
        moment_of_inertia: crank_moment + flywheel_moment + other_moment,
        tdc: 0.0
    )

    rod_journal rj0(angle: rot * 0 + rot90 + vee / 2)
    rod_journal rj1(angle: rot * 2 + rot90 + vee / 2)
    rod_journal rj2(angle: rot * 4 + rot90 + vee / 2)
    
    c0
        .add_rod_journal(rj0)
        .add_rod_journal(rj1)
        .add_rod_journal(rj2)

    piston_parameters piston_params(
        mass: piston_mass * units.g,
        blowby: k_28inH2O(0.041),
        compression_height: compression_height,
        wrist_pin_position: 0.0,
        displacement: 0.0
    )

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    label collector_cross_section_area(24.4)

    label exhaust_pipe_length_0(300.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 100.0) // Litres
    
    label spacing_factor(1.25)
    label flange_density(1.0 * exhaust_delay_coeff)

    intake intake(
        plenum_volume: 2.7 * units.L,
        plenum_cross_section_area: 44.2 * units.cm2,
        runner_length: 23.1 * units.cm,
        intake_flow_rate: k_carb(335.8),
        idle_flow_rate: k_carb(0.05),
        idle_throttle_plate_position: 0.985,
        runner_flow_rate: k_carb(111.9),
        velocity_decay: 0.05
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(559.7),
        collector_cross_section_area: 24.4 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 61.2 * units.cm,
        primary_flow_rate: k_carb(186.6),
        velocity_decay: 1.0,
        volume: (exhaust_volume_0 + 10.0) * units.L
    )

    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_55.wav", volume: 0.001))
 
    cylinder_bank b0(bank_params, angle: vee / 2)
    cylinder_bank b1(bank_params, angle: -vee / 2)
    
    label sh((15.0 / exhaust_delay_coeff) * units.cm)
    label pl0 ((2.7 * bore * spacing_factor / flange_density) * units.mm)
    label pl1 ((1.7 * bore * spacing_factor / flange_density) * units.mm)
    label pl2 ((1.3 * bore * spacing_factor / flange_density) * units.mm)

    b0
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj0,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl0,
            ignition_wire: wires.wire1
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj1,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl1,
            ignition_wire: wires.wire2
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj2,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl2,
            ignition_wire: wires.wire3
        )
        
    b1
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj0,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl0 + sh,
            ignition_wire: wires.wire4
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj1,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl1 + sh,
            ignition_wire: wires.wire5
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj2,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl2 + sh,
            ignition_wire: wires.wire6
        )

    engine
        .add_cylinder_bank(b0)
        .add_cylinder_bank(b1)

    engine.add_crankshaft(c0)

    camshaft_builder camshaft()

    engine.add_ignition_module(
        eng_distributor(
            wires: wires,
            timing_curve: timing_curve
        )
    )

    b0.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_0,
            exhaust_camshaft: camshaft.exhaust_cam_0,
            flip_display: true
        )
    )
    b1.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_1,
            exhaust_camshaft: camshaft.exhaust_cam_1
        )
    )

    function timing_curve(153.3 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 0.0 * units.deg)
        .add_sample(153.3 * units.rpm, 0.0 * units.deg)
        .add_sample(306.7 * units.rpm, 1.2 * units.deg)
        .add_sample(460.0 * units.rpm, 2.9 * units.deg)
        .add_sample(613.3 * units.rpm, 4.6 * units.deg)
        .add_sample(766.7 * units.rpm, 6.3 * units.deg)
        .add_sample(920.0 * units.rpm, 8.0 * units.deg)
        .add_sample(1073.3 * units.rpm, 9.7 * units.deg)
        .add_sample(1226.7 * units.rpm, 11.4 * units.deg)
        .add_sample(1380.0 * units.rpm, 13.1 * units.deg)
        .add_sample(1533.3 * units.rpm, 14.8 * units.deg)
        .add_sample(1686.7 * units.rpm, 16.5 * units.deg)
        .add_sample(1840.0 * units.rpm, 18.2 * units.deg)
        .add_sample(1993.3 * units.rpm, 19.9 * units.deg)
        .add_sample(2146.7 * units.rpm, 20.0 * units.deg)
        .add_sample(2300.0 * units.rpm, 20.0 * units.deg)
        .add_sample(2453.3 * units.rpm, -12.5 * units.deg)
        .add_sample(2606.7 * units.rpm, -25.0 * units.deg)
}

// МАЗ-504
public node veh {
    alias output __out: vehicle;

    label car_mass(10000) // 7000 empty; 13975 full

    vehicle vehicle(
        mass: car_mass * units.kg,
        drag_coefficient: 0.63,
        cross_sectional_area: (2500 * units.mm) * (2620 * units.mm),
        diff_ratio: 7.73,
        tire_radius: (1082.0 / 2) * units.mm, // 11,00 - 20
        rolling_resistance: 0.012 * car_mass * 9.81
        )
}

// МАЗ-504
private node trn {
    alias output __out:
        transmission(
            max_clutch_torque: 1000 * units.Nm,
            max_clutch_flex: 8 * units.deg,
            limit_clutch_flex: true,
            clutch_stiffness: 70 * units.Nm / units.deg,
            clutch_damping: 1.0,
            simulate_flex: true
        )
        .add_gear(6.17)
        .add_gear(3.40)
        .add_gear(1.79)
        .add_gear(1.0)
        .add_gear(0.73);
}

run(
    engine: eng(),
    transmission: trn(),
    vehicle: veh()
)
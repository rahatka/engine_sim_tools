private node add_lobes {
    input base;
    input phasing: 0.0;
    input this;
    alias output __out: this;

    this.add_lobe(base + 0 * rot * row + phasing)
    this.add_lobe(base + 4 * rot * row + phasing)
    this.add_lobe(base + 2 * rot * row + phasing)
    this.add_lobe(base + 5 * rot * row + phasing)
    this.add_lobe(base + 1 * rot * row + phasing)
    this.add_lobe(base + 3 * rot * row + phasing)
}

public node bank_builder {
    input phasing;
    input ignition;
    input intake;
    input exhaust;

    input rj0;
    input rj1;
    input rj2;
    input rj3;
    input rj4;
    input rj5;

    input pl0;
    input pl1;
    input pl2;
    input pl3;
    input pl4;
    input pl5;

    output b0: _b0;

    ignition_wire _wire1()
    ignition_wire _wire2()
    ignition_wire _wire3()
    ignition_wire _wire4()
    ignition_wire _wire5()
    ignition_wire _wire6()

    connecting_rod_parameters cr_params(
        mass: con_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.g,
            length: con_rod * units.mm
        ),
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    piston_parameters piston_params(
        mass: 2000 * units.g,
        blowby: k_28inH2O(0.050),
        compression_height: compression_height,
        wrist_pin_position: 0.0,
        displacement: 0.0
    )

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    camshaft _intake_cam_0(base_radius: 33.8 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 33.8 * units.mm, lobe_profile: exhaust_lobe_profile)

    _intake_cam_0.add_lobes(ILC, -phasing)
    _exhaust_cam_0.add_lobes(ELC, -phasing)

    cylinder_bank _b0(bank_params, angle: phasing)

    _b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj0,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl0,
            ignition_wire: _wire1
        )
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj1,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl1,
            ignition_wire: _wire2
        )
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj2,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl2,
            ignition_wire: _wire3
        )
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj3,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl1,
            ignition_wire: _wire4
        )
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj4,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl2,
            ignition_wire: _wire5
        )
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj5,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl2,
            ignition_wire: _wire6
        )
    .set_cylinder_head(
        head(
            intake_camshaft: _intake_cam_0,
            exhaust_camshaft: _exhaust_cam_0
        )
    )

    ignition
        .connect_wire(_wire1, rot_base * 0 - phasing)
        .connect_wire(_wire5, rot_base * 1 - phasing)
        .connect_wire(_wire3, rot_base * 2 - phasing)
        .connect_wire(_wire6, rot_base * 3 - phasing)
        .connect_wire(_wire2, rot_base * 4 - phasing)
        .connect_wire(_wire4, rot_base * 5 - phasing)
}

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node head {
    // port_flow.py v1.7
    // intake port area: 51.7 cm²; saturated lift: 13.97 mm
    // exhaust port area: 44.0 cm²; saturated lift: 12.89 mm
    // cylinder volume: 3418.1 cm³ (208.58 CI); engine volume: 143.558 L (8760.46 CI)
    // chamber volume: 284.8 cm³ (17.38 CI); conrod/stroke 2.147; N.A.C.C. 666.63
    // 32 harmonic intake runner length: 21.1 cm; diameter: 6.6 cm
    // primary length: 61.6 cm, area: 48.4 cm², diameter: 7.9 cm
    // collector diameter: 39.4 cm, area: 1220.1 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 284.838 * units.cc,
        intake_runner_volume: 732.4 * units.cc,
        intake_runner_cross_section_area: 34.7 * units.cm2,
        exhaust_runner_volume: 293.0 * units.cc,
        exhaust_runner_cross_section_area: 48.4 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "Zvezda M-501",
        starter_torque: 2000 * units.Nm,
        starter_speed: 150 * units.rpm,
        redline: redline * units.rpm,
        fuel: diesel(),
        throttle_gamma: 2.0,
        jitter: 0.5,
        noise: 0.1,
        hf_gain: 0.001,
        fluid_simulation_steps: 4,
        max_sle_solver_steps: 16,
        simulation_frequency: 2250,
        dyno_min_speed: 500 * units.rpm
    )

    label crank_moment(disk_moment_of_inertia(mass: crank_mass * units.kg, radius: (stroke / 2.0) * units.mm))
    label flywheel_moment(disk_moment_of_inertia(mass: flywheel_mass * units.kg, radius: (stroke * 3.5) * units.mm))
    label other_moment(disk_moment_of_inertia(mass: 1 * units.kg, radius: 1.0 * units.cm))

    crankshaft c0(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: flywheel_mass * units.kg,
        mass: crank_mass * units.kg,
        friction_torque: 215.3 * units.Nm,
        moment_of_inertia: crank_moment + flywheel_moment + other_moment,
        tdc: 0.0
    )

    bank_builder bank0(
        phasing: 0.0 * units.deg,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust,
        rj0, rj1, rj2, rj3, rj4, rj5,
        pl0 + sh0, pl1 + sh0, pl2 + sh0, pl3 + sh0, pl4 + sh0, pl5 + sh0
    )
    bank_builder bank1(
        phasing: vee * 1 + rot360,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust,
        rj0, rj1, rj2, rj3, rj4, rj5,
        pl0 + sh1, pl1 + sh1, pl2 + sh1, pl3 + sh1, pl4 + sh1, pl5 + sh1
    )
    bank_builder bank2(
        phasing: vee * 2,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust,
        rj0, rj1, rj2, rj3, rj4, rj5,
        pl0 + sh2, pl1 + sh2, pl2 + sh2, pl3 + sh2, pl4 + sh2, pl5 + sh2
    )
    bank_builder bank3(
        phasing: vee * 3 + rot360,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust,
        rj0, rj1, rj2, rj3, rj4, rj5,
        pl0 + sh3, pl1 + sh3, pl2 + sh3, pl3 + sh3, pl4 + sh3, pl5 + sh3
    )
    bank_builder bank4(
        phasing: vee * 4,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust,
        rj0, rj1, rj2, rj3, rj4, rj5,
        pl0 + sh4, pl1 + sh4, pl2 + sh4, pl3 + sh4, pl4 + sh4, pl5 + sh4
    )
    bank_builder bank5(
        phasing: vee * 5 + rot360,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust,
        rj0, rj1, rj2, rj3, rj4, rj5,
        pl0 + sh5, pl1 + sh5, pl2 + sh5, pl3 + sh5, pl4 + sh5, pl5 + sh5
    )
    bank_builder bank6(
        phasing: vee * 6,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust,
        rj0, rj1, rj2, rj3, rj4, rj5,
        pl0 + sh6, pl1 + sh6, pl2 + sh6, pl3 + sh6, pl4 + sh6, pl5 + sh6
    )

    ignition_module ignition(timing_curve: timing_curve, rev_limit: 5000 * units.rpm, limiter_duration: 0.001)
    
    rod_journal rj0(angle: rot_base * 0 + rot90)
    rod_journal rj1(angle: rot_base * 4 + rot90)
    rod_journal rj2(angle: rot_base * 2 + rot90)
    rod_journal rj3(angle: rot_base * 5 + rot90)
    rod_journal rj4(angle: rot_base * 1 + rot90)
    rod_journal rj5(angle: rot_base * 3 + rot90)
    
    c0
        .add_rod_journal(rj0)
        .add_rod_journal(rj1)
        .add_rod_journal(rj2)
        .add_rod_journal(rj3)
        .add_rod_journal(rj4)
        .add_rod_journal(rj5)

    label collector_cross_section_area(1220.1)

    label exhaust_pipe_length_0(300.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 100.0) // Litres
    
    label spacing_factor(1.15)
    label flange_density(1.0 * exhaust_delay_coeff)
    label radial_density(10.0 * exhaust_delay_coeff)
    

    intake intake(
        plenum_volume: 50.9 * units.L,
        plenum_cross_section_area: 727.7 * units.cm2,
        runner_length: 21.1 * units.cm,
        intake_flow_rate: k_carb(6056.4),
        idle_flow_rate: k_carb(0.03),
        idle_throttle_plate_position: 0.996,
        runner_flow_rate: k_carb(324.4),
        velocity_decay: 0.05
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(10724.8),
        collector_cross_section_area: 1220.1 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 61.6 * units.cm,
        primary_flow_rate: k_carb(542.6),
        velocity_decay: 0.7,
        volume: exhaust_volume_0 * units.L
    )

    exhaust_system exhaust(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_40.wav", volume: 0.001))

    label sh0 ((1.0 * bore * spacing_factor / radial_density) * units.mm)
    label sh1 ((4.0 * bore * spacing_factor / radial_density) * units.mm)
    label sh2 ((7.0 * bore * spacing_factor / radial_density) * units.mm)
    label sh3 ((10.0 * bore * spacing_factor / radial_density) * units.mm)
    label sh4 ((8.0 * bore * spacing_factor / radial_density) * units.mm)
    label sh5 ((5.0 * bore * spacing_factor / radial_density) * units.mm)
    label sh6 ((2.0 * bore * spacing_factor / radial_density) * units.mm)
    
    label pl0 ((6.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl1 ((5.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl2 ((4.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl3 ((3.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl4 ((2.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl5 ((1.0 * bore * spacing_factor / flange_density) * units.mm)

    engine
        .add_cylinder_bank(bank0.b0)
        .add_cylinder_bank(bank1.b0)
        .add_cylinder_bank(bank2.b0)
        .add_cylinder_bank(bank3.b0)
        .add_cylinder_bank(bank4.b0)
        .add_cylinder_bank(bank5.b0)
        .add_cylinder_bank(bank6.b0)

        .add_crankshaft(c0)
        .add_ignition_module(ignition)

    // ignition_timing.py v0.2 a = 5.0, m = 1.2, r = 2250
    function timing_curve(225.0 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 0.0 * units.deg)
        .add_sample(225.0 * units.rpm, 5.0 * units.deg)
        .add_sample(450.0 * units.rpm, 6.0 * units.deg)
        .add_sample(675.0 * units.rpm, 7.2 * units.deg)
        .add_sample(900.0 * units.rpm, 8.6 * units.deg)
        .add_sample(1125.0 * units.rpm, 10.4 * units.deg)
        .add_sample(1350.0 * units.rpm, 12.4 * units.deg)
        .add_sample(1575.0 * units.rpm, 14.9 * units.deg)
        .add_sample(1800.0 * units.rpm, 17.9 * units.deg)
        .add_sample(2025.0 * units.rpm, 21.5 * units.deg)
        .add_sample(2250.0 * units.rpm, 25.8 * units.deg)
        .add_sample(2475.0 * units.rpm, -12.5 * units.deg)
        .add_sample(2700.0 * units.rpm, -25.0 * units.deg)
}

// Bugatti Type 41
public node veh {
    alias output __out: vehicle;
    vehicle vehicle(
        mass: 3250  * units.kg,
        drag_coefficient: 0.5,
        cross_sectional_area: (1320 * units.mm) * (1050 * units.mm),
        diff_ratio: 3.6,
        tire_radius: (914.4 / 2) * units.mm, // 36 x 6.75
        rolling_resistance: 20
        )
}

// Bugatti Type 41
private node trn {
    alias output __out:
        transmission(
            max_clutch_torque: 3000 * units.Nm,
            max_clutch_flex: 5 * units.deg,
            limit_clutch_flex: true,
            clutch_stiffness: 100 * units.Nm / units.deg,
            clutch_damping: 1.0,
            simulate_flex: true
        )
        .add_gear(7.50)
        .add_gear(1.0)
        .add_gear(0.37);
}

run(
    engine: eng(),
    transmission: trn(),
    vehicle: veh()
)

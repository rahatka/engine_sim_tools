private node add_lobes {
    input base;
    input shift: 0.0;
    input this;
    alias output __out: this;

    this.add_lobe(base + 0 * rot * row + shift)
    this.add_lobe(base + 1 * rot * row + shift)
    this.add_lobe(base + 3 * rot * row + shift)
    this.add_lobe(base + 2 * rot * row + shift)
}

public node camshaft_builder {
    output intake_cam_0: _intake_cam_0;
    output exhaust_cam_0: _exhaust_cam_0;

    output intake_cam_1: _intake_cam_1;
    output exhaust_cam_1: _exhaust_cam_1;

    camshaft _intake_cam_0(base_radius: 42.2 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 42.2 * units.mm, lobe_profile: exhaust_lobe_profile)
    
    camshaft _intake_cam_1(base_radius: 42.2 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_1(base_radius: 42.2 * units.mm, lobe_profile: exhaust_lobe_profile)

    _intake_cam_0.add_lobes(ILC)
    _exhaust_cam_0.add_lobes(ELC)

    _intake_cam_1.add_lobes(ILC, vee + rot360)
    _exhaust_cam_1.add_lobes(ELC, vee + rot360)
}

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node head {
    // port_flow.py v1.5
    // intake port area: 99.0 cm²; saturated lift: 19.34 mm
    // exhaust port area: 78.2 cm²; saturated lift: 17.19 mm
    // cylinder volume: 13804.2 cm³ (842.4 CI); engine volume: 110.433 L (6739.1 CI)
    // chamber volume: 1104.3 cm³ (67.4 CI)
    // 32 harmonic intake runner length: 42.7 cm; diameter: 9.0 cm
    // primary length: 160.3 cm, area: 78.2 cm², diameter: 10.0 cm
    // collector diameter: 21.9 cm, area: 375.6 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 1104.333 * units.cc,
        intake_runner_volume: 2740.0 * units.cc,
        intake_runner_cross_section_area: 64.1 * units.cm2,
        exhaust_runner_volume: 913.3 * units.cc,
        exhaust_runner_cross_section_area: 78.2 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "6D49",
        starter_torque: 3750 * units.Nm,
        starter_speed: 100 * units.rpm,
        redline: redline * units.rpm,
        fuel: diesel(),
        throttle_gamma: 2.0,
        jitter: 0.5,
        noise: 0.1,
        hf_gain: 0.005,
        simulation_frequency: 8000,
        dyno_min_speed: 300 * units.rpm
    )

    wires wires()

    connecting_rod_parameters cr_params(
        mass: con_rod_mass * units.kg,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.kg,
            length: con_rod * units.mm
        ),
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    label crank_moment(
         disk_moment_of_inertia(mass: crank_mass * units.kg, radius: (stroke / 2.0) * units.mm)
    )
    label flywheel_moment(
        disk_moment_of_inertia(mass: flywheel_mass * units.kg, radius: (stroke * 2.5) * units.mm)
    )
    label other_moment(
        disk_moment_of_inertia(mass: 1 * units.kg, radius: 1.0 * units.cm)
    )

    crankshaft c0(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: flywheel_mass * units.kg,
        mass: crank_mass * units.kg,
        friction_torque: 165.6 * units.Nm,
        moment_of_inertia: crank_moment + flywheel_moment + other_moment,
        position_x: 0.0,
        position_y: 0.0,
        tdc: 0.0
    )

    rod_journal rj0(angle: rot * 0 + rot90 + vee / 2)
    rod_journal rj1(angle: rot * 2 + rot90 + vee / 2)
    rod_journal rj2(angle: rot * 6 + rot90 + vee / 2)
    rod_journal rj3(angle: rot * 4 + rot90 + vee / 2)
    
    c0
        .add_rod_journal(rj0)
        .add_rod_journal(rj1)
        .add_rod_journal(rj2)
        .add_rod_journal(rj3)

    piston_parameters piston_params(
        mass: 37.0 * units.kg,
        blowby: k_28inH2O(0.082),
        compression_height: compression_height,
        wrist_pin_position: 0.0,
        displacement: 0.0
    )

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    label collector_cross_section_area(375.6)

    label exhaust_pipe_length_0(400.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 100.0) // Litres
    
    label spacing_factor(1.2)
    label flange_density(1.0 * exhaust_delay_coeff)
    

    intake intake(
        plenum_volume: 16.6 * units.L,
        plenum_cross_section_area: 256.4 * units.cm2,
        runner_length: 42.7 * units.cm,
        intake_flow_rate: k_carb(1932.6),
        idle_flow_rate: k_carb(0.1),
        idle_throttle_plate_position: 0.995,
        runner_flow_rate: k_carb(483.1),
        velocity_decay: 0.05
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(3221.0),
        collector_cross_section_area: 375.6 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 160.3 * units.cm,
        primary_flow_rate: k_carb(805.2),
        velocity_decay: 1.0,
        volume: (exhaust_volume_0 + 50.0) * units.L
    )

    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_53.wav", volume: 0.01))
 
    cylinder_bank b0(bank_params, angle: vee / 2)
    cylinder_bank b1(bank_params, angle: -vee / 2)
    
    label pl0((4.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl1((3.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl2((2.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl3((1.0 * bore * spacing_factor / flange_density) * units.mm)

    b0
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj0,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl0,
            ignition_wire: wires.wire1
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj1,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl1,
            ignition_wire: wires.wire2
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj2,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl2,
            ignition_wire: wires.wire3
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj3,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl3,
            ignition_wire: wires.wire4
        )
        
    b1
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj0,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl0,
            ignition_wire: wires.wire5
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj1,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl1,
            ignition_wire: wires.wire6
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj2,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl2,
            ignition_wire: wires.wire7
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj3,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl3,
            ignition_wire: wires.wire8
        )

    engine
        .add_cylinder_bank(b0)
        .add_cylinder_bank(b1)

    engine.add_crankshaft(c0)

    camshaft_builder camshaft()

    b0.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_0,
            exhaust_camshaft: camshaft.exhaust_cam_0,
            flip_display: true
        )
    )
    b1.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_1,
            exhaust_camshaft: camshaft.exhaust_cam_1
        )
    )

    // ignition_timing.py v0.2 a = 5.0, m = 1.2, r = 1050
    function timing_curve(105.0 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 0.0 * units.deg)
        .add_sample(105.0 * units.rpm, 5.0 * units.deg)
        .add_sample(210.0 * units.rpm, 6.0 * units.deg)
        .add_sample(315.0 * units.rpm, 7.2 * units.deg)
        .add_sample(420.0 * units.rpm, 8.6 * units.deg)
        .add_sample(525.0 * units.rpm, 10.4 * units.deg)
        .add_sample(630.0 * units.rpm, 12.4 * units.deg)
        .add_sample(735.0 * units.rpm, 14.9 * units.deg)
        .add_sample(840.0 * units.rpm, 17.9 * units.deg)
        .add_sample(945.0 * units.rpm, 21.5 * units.deg)
        .add_sample(1050.0 * units.rpm, 25.8 * units.deg)
        .add_sample(1155.0 * units.rpm, -15.0 * units.deg)
        .add_sample(1260.0 * units.rpm, -60.0 * units.deg)

    engine.add_ignition_module(
        eng_distributor(
            wires: wires,
            timing_curve: timing_curve
        )
    )
}

// МАЗ-525
public node veh {
    alias output __out: vehicle;

    label car_mass(80000) // 24380 empty; 49520 full

    vehicle vehicle(
        mass: car_mass * units.kg,
        drag_coefficient: 0.63,
        cross_sectional_area: (3220 * units.mm) * (3675 * units.mm),
        diff_ratio: 2.0,
        tire_radius: (1676.4 / 2) * units.mm, // 17,00 - 32
        rolling_resistance: 0.012 * car_mass * 9.81
        )
}

// МАЗ-525
private node trn {
    alias output __out:
        transmission(
            max_clutch_torque: 7500 * units.Nm,
            max_clutch_flex: 1.0 * units.deg,
            limit_clutch_flex: true,
            clutch_stiffness: 150.0 * units.Nm / units.deg,
            clutch_damping: 1.0,
            simulate_flex: false
        )
        .add_gear(7.14)
        .add_gear(3.53)
        .add_gear(1.88)
        .add_gear(1.00)
        .add_gear(0.72);
}

run(
    engine: eng(),
    transmission: trn(),
    vehicle: veh()
)

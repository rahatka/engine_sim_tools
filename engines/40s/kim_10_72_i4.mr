public node camshaft_builder {
    output intake_cam_0: _intake_cam_0;
    output exhaust_cam_0: _exhaust_cam_0;

    camshaft _intake_cam_0(base_radius: 12.4 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 12.4 * units.mm, lobe_profile: exhaust_lobe_profile)

    _intake_cam_0
        .add_lobe(ILC + 0 * rot)
        .add_lobe(ILC + 1 * rot)
        .add_lobe(ILC + 3 * rot)
        .add_lobe(ILC + 2 * rot)
    _exhaust_cam_0
        .add_lobe(ELC + 0 * rot)
        .add_lobe(ELC + 1 * rot)
        .add_lobe(ELC + 3 * rot)
        .add_lobe(ELC + 2 * rot)
}

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node head {
    // port_flow.py v1.7
    // intake port area: 5.1 cm²; saturated lift: 6.23 mm
    // exhaust port area: 4.3 cm²; saturated lift: 5.69 mm
    // cylinder volume: 292.9 cm³ (17.88 CI); engine volume: 1.172 L (71.51 CI)
    // chamber volume: 61.7 cm³ (3.76 CI); conrod/stroke 1.762; N.A.C.C. 10.00
    // 16 harmonic intake runner length: 23.5 cm; diameter: 2.5 cm
    // primary length: 32.9 cm, area: 4.7 cm², diameter: 2.5 cm
    // collector diameter: 2.7 cm, area: 5.7 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 61.672 * units.cc,
        intake_runner_volume: 116.9 * units.cc,
        intake_runner_cross_section_area: 5.0 * units.cm2,
        exhaust_runner_volume: 39.0 * units.cc,
        exhaust_runner_cross_section_area: 4.7 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "KIM-10",
        starter_torque: 70 * units.Nm,
        starter_speed: 300 * units.rpm,
        redline: redline * units.rpm,
        fuel: s30(),
        throttle_gamma: 1.5,
        jitter: 0.8,
        noise: 0.2,
        hf_gain: 0.006,
        simulation_frequency: 12000,
        dyno_min_speed: 500 * units.rpm
    )

    wires wires()

    label crank_moment(disk_moment_of_inertia(mass: crank_mass * units.kg, radius: (stroke / 2.0) * units.mm))
    label flywheel_moment(disk_moment_of_inertia(mass: flywheel_mass * units.kg, radius: (stroke * 2.0) * units.mm))
    label other_moment(disk_moment_of_inertia(mass: 1 * units.kg, radius: 1.0 * units.cm))

    crankshaft c0(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: 17.8602 * units.kg,
        mass: 12.0 * units.kg,
        friction_torque: 1.8 * units.Nm,
        moment_of_inertia: crank_moment + flywheel_moment + other_moment,
        position_x: -3.0 * units.mm, // ~
        position_y: 0.0,
        tdc: 0.0
    )

    label tdc_comp(-0.8215 * units.deg) // 90 - math.acos(x_shift / (stroke / 2 + con_rod))

    rod_journal rj0(angle: rot * 0 + rot90 + tdc_comp)
    rod_journal rj1(angle: rot * 1 + rot90 + tdc_comp)
    rod_journal rj2(angle: rot * 3 + rot90 + tdc_comp)
    rod_journal rj3(angle: rot * 2 + rot90 + tdc_comp)
    
    c0
        .add_rod_journal(rj0)
        .add_rod_journal(rj1)
        .add_rod_journal(rj2)
        .add_rod_journal(rj3)

    piston_parameters piston_params(
        mass: 578.0 * units.g,
        blowby: k_28inH2O(0.020),
        compression_height: compression_height,
        wrist_pin_position: 0.0,
        displacement: 0.0
    )

    connecting_rod_parameters cr_params(
        mass: 750.0 * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.g,
            length: con_rod * units.mm
        ),
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    label collector_cross_section_area(5.7)

    label exhaust_pipe_length_0(250.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 1000.0) // Litres
    
    label spacing_factor(1.16)
    label flange_density(1.0 * exhaust_delay_coeff)

    intake intake(
        plenum_volume: 0.7 * units.L,
        plenum_cross_section_area: 9.9 * units.cm2,
        runner_length: 23.5 * units.cm,
        intake_flow_rate: k_carb(80.1),
        idle_flow_rate: k_carb(0.01),
        idle_throttle_plate_position: 0.994,
        runner_flow_rate: k_carb(40.0),
        velocity_decay: 0.05
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(133.5),
        collector_cross_section_area: 5.7 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 32.9 * units.cm,
        primary_flow_rate: k_carb(66.7),
        velocity_decay: 1.0,
        volume: exhaust_volume_0 * units.L
    )

    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_58.wav", volume: 0.00025))
 
    cylinder_bank b0(bank_params, angle: 0)

    // ГАЗ М
    label pl0((3.3 * bore * spacing_factor / flange_density) * units.mm)
    label pl1((1.5 * bore * spacing_factor / flange_density) * units.mm)
    label pl2((1.5 * bore * spacing_factor / flange_density) * units.mm)
    label pl3((3.3 * bore * spacing_factor / flange_density) * units.mm)

    b0
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj0,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl0,
            ignition_wire: wires.wire1
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj1,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl1,
            ignition_wire: wires.wire2
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj2,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl2,
            ignition_wire: wires.wire3
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj3,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl3,
            ignition_wire: wires.wire4
        )

    engine
        .add_cylinder_bank(b0)

    engine.add_crankshaft(c0)

    camshaft_builder camshaft()

    b0.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_0,
            exhaust_camshaft: camshaft.exhaust_cam_0
        )
    )

    function timing_curve(283.3 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 3.0 * units.deg)
        .add_sample(283.3 * units.rpm, 3.0 * units.deg)
        .add_sample(566.7 * units.rpm, 4.0 * units.deg)
        .add_sample(850.0 * units.rpm, 5.6 * units.deg)
        .add_sample(1133.3 * units.rpm, 7.3 * units.deg)
        .add_sample(1416.7 * units.rpm, 8.9 * units.deg)
        .add_sample(1700.0 * units.rpm, 10.5 * units.deg)
        .add_sample(1983.3 * units.rpm, 12.2 * units.deg)
        .add_sample(2266.7 * units.rpm, 13.8 * units.deg)
        .add_sample(2550.0 * units.rpm, 15.5 * units.deg)
        .add_sample(2833.3 * units.rpm, 17.1 * units.deg)
        .add_sample(3116.7 * units.rpm, 18.8 * units.deg)
        .add_sample(3400.0 * units.rpm, 20.4 * units.deg)
        .add_sample(3683.3 * units.rpm, 21.0 * units.deg)
        .add_sample(3966.7 * units.rpm, 21.0 * units.deg)
        .add_sample(4250.0 * units.rpm, 21.0 * units.deg)
        .add_sample(4533.3 * units.rpm, -12.5 * units.deg)
        .add_sample(4816.7 * units.rpm, -25.0 * units.deg)

    engine.add_ignition_module(
        eng_distributor(
            wires: wires,
            timing_curve: timing_curve
        )
    )
}


label bore(101.6)
label stroke(114.3)
label compression_ratio(6.0) // 6.2 ZIL-158 / 164
label con_rod(217.0)
label compression_height(56.0 * units.mm) // ZIS-120
label intake_valve_diameter(48.5) // ZIS-120 / ZIL-164 150-В-1007010-Б
label exhaust_valve_diameter(44.0) // ZIS-120 / ZIL-164 150-В-1007015-Б
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(3000.0) // RPM

label cyl(6.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// ZIL-124 valve timings
label IVL(10.42) // ZIS-120
label EVL(10.42) // ZIS-120
label IVO(20.0 * units.deg) //BTDC
label IVC(69.0 * units.deg) //ABDC
label EVO(67.0 * units.deg) //BBDC
label EVC(22.0 * units.deg) //ATDC

// ZIL-164 valve timings
// label IVL(10.42) // ZIS-120
// label EVL(10.42) // ZIS-120
// label IVO(15.0 * units.deg) //BTDC
// label IVC(59.3 * units.deg) //ABDC
// label EVO(44.3 * units.deg) //BBDC
// label EVC(27.3 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(1485.0) // g
label crank_mass(28.0) // ? kg
label flywheel_mass(15.0) // kg

label exhaust_delay_coeff(1.5)

public node eng_distributor {
    input wires;
    input timing_curve;
    input rev_limit: 3500 * units.rpm;
    input limiter_duration: 0.01;
    alias output __out:
        ignition_module(timing_curve: timing_curve, rev_limit: rev_limit, limiter_duration: limiter_duration)
            .connect_wire(wires.wire1, rot * 0)
            .connect_wire(wires.wire5, rot * 1)
            .connect_wire(wires.wire3, rot * 2)
            .connect_wire(wires.wire6, rot * 3)
            .connect_wire(wires.wire2, rot * 4)
            .connect_wire(wires.wire4, rot * 5);
}

private node wires {
    output wire1: ignition_wire();
    output wire2: ignition_wire();
    output wire3: ignition_wire();
    output wire4: ignition_wire();
    output wire5: ignition_wire();
    output wire6: ignition_wire();
}


import "engine_sim.mr"
import "../../fuel/30.mr"

units units()
constants constants()

label bore(87.3125)
label stroke(114.3)
label compression_ratio(6.2)
label con_rod(200.025) // 7 7/8
label compression_height(47.4 * units.mm) // ?
label intake_valve_diameter(43.6563) // 1 23/32
label exhaust_valve_diameter(38.8937) // 1 17/32
label intake_valves(1.0)
label exhaust_valves(1.0)

label cyl(30.0)
label row(5.0)
label cyl_base(cyl / row)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

label redline(2750.0) // RPM

label IVL(9.6266) // 251 cam
label EVL(9.6266) // 251 cam

// 251 cam
label IVO(12.0 * units.deg) //BTDC
label IVC(44.0 * units.deg) //ABDC
label EVO(50.0 * units.deg) //BBDC
label EVC(6.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(850.0) // ? g
label crank_mass(22.0) // ? kg
label flywheel_mass(6.0) // kg

label exhaust_delay_coeff(1.5)

// The firing order is progressive throughout all the engines and whilst the firing order of each block is 142635
// i.e. the reverse of normal due to the reverse rotation, the overall firing order is
// 1 on eng.1, 5 on eng.3, 3 on eng.5, 6 on eng.2, 2 on eng.4 and 4 on eng.1 and so on.
// This gives a firing impulse every 24 degrees making it very smooth.
// Source https://hmvf.co.uk/topic/7836-m4a4-restoration/

label rot_base(cycle / cyl_base)

label incline_0(0.0 * units.deg)            // engine 1
label phasing_0(rot_base * 0 + rot * 0)

label incline_1((-90.0 + 27.0) * units.deg) // engine 2
label phasing_1(rot_base * 3 + rot * 3)

label incline_2((-90.0 + 7.5) * units.deg)  // engine 3
label phasing_2(rot_base * 1 + rot * 1)

label incline_3((90.0 - 7.5) * units.deg)   // engine 4
label phasing_3(rot_base * 4 + rot * 4)

label incline_4((90.0 - 27.0) * units.deg)  // engine 5
label phasing_4(rot_base * 2 + rot * 2)


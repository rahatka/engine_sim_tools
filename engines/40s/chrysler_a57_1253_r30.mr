private node add_lobes {
    input base;
    input shift: 0.0;
    input this;
    alias output __out: this;

    this.add_lobe(base + 0 * rot * row + shift)
    this.add_lobe(base + 2 * rot * row + shift)
    this.add_lobe(base + 4 * rot * row + shift)
    this.add_lobe(base + 1 * rot * row + shift)
    this.add_lobe(base + 5 * rot * row + shift)
    this.add_lobe(base + 3 * rot * row + shift)
}

public node bank_builder {
    input incline;
    input phasing;
    input intake;
    input exhaust;
    input x;
    input y;

    input exhaust_l;

    output wire1: _wire1;
    output wire2: _wire2;
    output wire3: _wire3;
    output wire4: _wire4;
    output wire5: _wire5;
    output wire6: _wire6;

    output b0: _b0;
    output c0: _c0;

    ignition_wire _wire1()
    ignition_wire _wire2()
    ignition_wire _wire3()
    ignition_wire _wire4()
    ignition_wire _wire5()
    ignition_wire _wire6()

    connecting_rod_parameters cr_params(
        mass: con_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.g,
            length: con_rod * units.mm
        ),
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    piston_parameters piston_params(
        mass: 750 * units.g,
        blowby: k_28inH2O(0.027),
        compression_height: compression_height,
        wrist_pin_position: 0.0,
        displacement: 0.0
    )

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    label crank_moment(disk_moment_of_inertia(mass: crank_mass * units.kg, radius: (stroke / 2.0) * units.mm))
    label flywheel_moment(disk_moment_of_inertia(mass: flywheel_mass * units.kg, radius: (stroke * 3.5) * units.mm))
    label other_moment(disk_moment_of_inertia(mass: 1 * units.kg, radius: 1.0 * units.cm))

    label spacing_factor(1.15)
    label flange_density(1.0 * exhaust_delay_coeff)

    crankshaft _c0(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: flywheel_mass * units.kg,
        mass: crank_mass * units.kg,
        friction_torque: 30.8 * units.Nm,
        moment_of_inertia: crank_moment + flywheel_moment + other_moment,
        position_x: x,
        position_y: y,
        tdc: 0.0
    )

    rod_journal rj0(angle: (0.0 / cyl * row) * cycle + rot90 + incline + phasing)
    rod_journal rj1(angle: (2.0 / cyl * row) * cycle + rot90 + incline + phasing)
    rod_journal rj2(angle: (4.0 / cyl * row) * cycle + rot90 + incline + phasing)
    rod_journal rj3(angle: (1.0 / cyl * row) * cycle + rot90 + incline + phasing)
    rod_journal rj4(angle: (5.0 / cyl * row) * cycle + rot90 + incline + phasing)
    rod_journal rj5(angle: (3.0 / cyl * row) * cycle + rot90 + incline + phasing)
    
    _c0
        .add_rod_journal(rj0)
        .add_rod_journal(rj1)
        .add_rod_journal(rj2)
        .add_rod_journal(rj3)
        .add_rod_journal(rj4)
        .add_rod_journal(rj5)

    camshaft _intake_cam_0(base_radius: 18.5 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 18.5 * units.mm, lobe_profile: exhaust_lobe_profile)

    _intake_cam_0.add_lobes(ILC, phasing)
    _exhaust_cam_0.add_lobes(ELC, phasing)

    label pl0 ((6.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl1 ((5.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl2 ((4.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl3 ((3.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl4 ((2.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl5 ((1.0 * bore * spacing_factor / flange_density) * units.mm)

    cylinder_bank _b0(bank_params, angle: incline, position_x: x, position_y: y)

    _b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj0,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl0 + exhaust_l,
            ignition_wire: _wire1
        )
    _b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj1,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl1 + exhaust_l,
            ignition_wire: _wire2
        )
    _b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj2,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl2 + exhaust_l,
            ignition_wire: _wire3
        )
    _b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj3,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl3 + exhaust_l,
            ignition_wire: _wire4
        )
    _b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj4,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl4 + exhaust_l,
            ignition_wire: _wire5
        )
    _b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj5,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl5 + exhaust_l,
            ignition_wire: _wire6
        )

    _b0.set_cylinder_head(
        head(
            intake_camshaft: _intake_cam_0,
            exhaust_camshaft: _exhaust_cam_0
        )
    )
}

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node head {
    // port_flow.py v1.5
    // intake port area: 11.4 cm²; saturated lift: 9.25 mm
    // exhaust port area: 9.0 cm²; saturated lift: 8.24 mm
    // cylinder volume: 684.4 cm³ (41.76 CI); engine volume: 20.531 L (1252.88 CI)
    // chamber volume: 131.6 cm³ (8.03 CI)
    // 32 harmonic intake runner length: 18.9 cm; diameter: 3.1 cm
    // primary length: 67.8 cm, area: 9.9 cm², diameter: 3.6 cm
    // collector diameter: 12.3 cm, area: 118.9 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 131.609 * units.cc,
        intake_runner_volume: 138.8 * units.cc,
        intake_runner_cross_section_area: 7.3 * units.cm2,
        exhaust_runner_volume: 46.3 * units.cc,
        exhaust_runner_cross_section_area: 9.9 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

private node add_wires {
    input bank;
    input phasing: 0.0;
    input this;
    alias output __out: this;

    this.connect_wire(bank.wire1, rot_base * 0 + phasing)
    this.connect_wire(bank.wire4, rot_base * 1 + phasing)
    this.connect_wire(bank.wire2, rot_base * 2 + phasing)
    this.connect_wire(bank.wire6, rot_base * 3 + phasing)
    this.connect_wire(bank.wire3, rot_base * 4 + phasing)
    this.connect_wire(bank.wire5, rot_base * 5 + phasing)
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "Chrysler A57",
        starter_torque: 500 * units.Nm,
        starter_speed: 250 * units.rpm,
        redline: redline * units.rpm,
        fuel: s30(),
        throttle_gamma: 2.0,
        jitter: 0.7,
        noise: 0.2,
        hf_gain: 0.007,
        fluid_simulation_steps: 4,
        max_sle_solver_steps: 32,
        simulation_frequency: 3500,
        dyno_min_speed: 500 * units.rpm
    )

    bank_builder bank0(
        incline: incline_0,
        phasing: phasing_0,
        intake: intake,
        exhaust: exhaust0,
        x: 0.0 * units.mm,
        y: 228.0 * units.mm,
        exhaust_l: 49.0 / exhaust_delay_coeff * units.cm
    )

    bank_builder bank1(
        incline: incline_1,
        phasing: phasing_1,
        intake: intake,
        exhaust: exhaust0,
        x: 203.15 * units.mm,
        y: 103.51 * units.mm,
        exhaust_l: 67.0 / exhaust_delay_coeff * units.cm
    )

    bank_builder bank2(
        incline: incline_2,
        phasing: phasing_2,
        intake: intake,
        exhaust: exhaust0,
        x: 114.0 * units.mm,
        y: -197.45 * units.mm,
        exhaust_l: 114.0 / exhaust_delay_coeff * units.cm
    )

    bank_builder bank3(
        incline: incline_3,
        phasing: phasing_3,
        intake: intake,
        exhaust: exhaust1,
        x: -114.0 * units.mm,
        y: -197.45 * units.mm,
        exhaust_l: 75.0 / exhaust_delay_coeff * units.cm
    )

    bank_builder bank4(
        incline: incline_4,
        phasing: phasing_4,
        intake: intake,
        exhaust: exhaust1,
        x: -203.15 * units.mm,
        y: 103.51 * units.mm,
        exhaust_l: 42.0 / exhaust_delay_coeff * units.cm
    )

    ignition_module ignition(timing_curve: timing_curve, rev_limit: 3250 * units.rpm, limiter_duration: 0.001)
    ignition.add_wires(bank0, phasing_0)
    ignition.add_wires(bank1, phasing_1)
    ignition.add_wires(bank2, phasing_2)
    ignition.add_wires(bank3, phasing_3)
    ignition.add_wires(bank4, phasing_4)

    label collector_cross_section_area_0(59.5 / 5 * 3)
    label collector_cross_section_area_1(59.5 / 5 * 2)
    label exhaust_pipe_length_0(260.0) // cm
    label exhaust_volume_0(collector_cross_section_area_0 * exhaust_pipe_length_0 / 100.0) // Litres

    label exhaust_pipe_length_1(250.0) // cm
    label exhaust_volume_1(collector_cross_section_area_1 * exhaust_pipe_length_1 / 100.0) // Litres

    intake intake(
        plenum_volume: 7.4 * units.L,
        plenum_cross_section_area: 110.0 * units.cm2,
        runner_length: 18.9 * units.cm,
        intake_flow_rate: k_carb(941.0),
        idle_flow_rate: k_carb(0.03),
        idle_throttle_plate_position: 0.993,
        runner_flow_rate: k_carb(62.7),
        velocity_decay: 0.05
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(784.2),
        collector_cross_section_area: 59.5 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 67.8 * units.cm,
        primary_flow_rate: k_carb(104.6),
        velocity_decay: 0.9,
        volume: (exhaust_volume_0 + 3.0) * units.L
    )

    exhaust_system_parameters es_params1(
        outlet_flow_rate: k_carb(784.2),
        collector_cross_section_area: 59.5 * units.cm2,
        length: exhaust_pipe_length_1 * units.cm,
        primary_tube_length: 67.8 * units.cm,
        primary_flow_rate: k_carb(104.6),
        velocity_decay: 0.9,
        volume: (exhaust_volume_0 + 3.0) * units.L
    )

    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_57.wav", volume: 0.00025))
    exhaust_system exhaust1(es_params1, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_58.wav", volume: 0.00025))

    engine
        .add_cylinder_bank(bank0.b0)
        .add_cylinder_bank(bank1.b0)
        .add_cylinder_bank(bank2.b0)
        .add_cylinder_bank(bank3.b0)
        .add_cylinder_bank(bank4.b0)

    engine
        .add_crankshaft(bank0.c0)
        .add_crankshaft(bank1.c0)
        .add_crankshaft(bank2.c0)
        .add_crankshaft(bank3.c0)
        .add_crankshaft(bank4.c0)

    function timing_curve(183.3 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 0.0 * units.deg)
        .add_sample(183.3 * units.rpm, 0.0 * units.deg)
        .add_sample(366.7 * units.rpm, 0.0 * units.deg)
        .add_sample(550.0 * units.rpm, 2.1 * units.deg)
        .add_sample(733.3 * units.rpm, 5.7 * units.deg)
        .add_sample(916.7 * units.rpm, 8.5 * units.deg)
        .add_sample(1100.0 * units.rpm, 10.7 * units.deg)
        .add_sample(1283.3 * units.rpm, 12.4 * units.deg)
        .add_sample(1466.7 * units.rpm, 13.8 * units.deg)
        .add_sample(1650.0 * units.rpm, 14.9 * units.deg)
        .add_sample(1833.3 * units.rpm, 15.8 * units.deg)
        .add_sample(2016.7 * units.rpm, 16.4 * units.deg)
        .add_sample(2200.0 * units.rpm, 17.0 * units.deg)
        .add_sample(2383.3 * units.rpm, 17.4 * units.deg)
        .add_sample(2566.7 * units.rpm, 17.7 * units.deg)
        .add_sample(2750.0 * units.rpm, 18.0 * units.deg)
        .add_sample(2933.3 * units.rpm, -15.0 * units.deg)
        .add_sample(3116.7 * units.rpm, -45.0 * units.deg)

    engine.add_ignition_module(ignition)
}

// Pz IV
public node veh {
    alias output __out: vehicle;

    label car_mass(19000) // 18.5 t combat weight

    vehicle vehicle(
        mass: car_mass * units.kg,
        drag_coefficient: 0.6,
        cross_sectional_area: (2830 * units.mm) * (2680 * units.mm),
        diff_ratio: 3.23 * 3.75, // 3.23 diff
        tire_radius: (860.0 / 2) * units.mm,
        rolling_resistance: 0.07 * car_mass * 9.81
        )
}

// Pz IV
private node trn {
    alias output __out:
        transmission(
            max_clutch_torque: 1200 * units.Nm,
            max_clutch_flex: 15 * units.deg,
            limit_clutch_flex: true,
            clutch_stiffness: 50 * units.Nm / units.deg,
            clutch_damping: 1.0,
            simulate_flex: true
        )
        .add_gear(9.05)  // 4.2
        .add_gear(4.69)  // 8.1
        .add_gear(2.75)  // 13.8
        .add_gear(1.83)  // 20.8
        .add_gear(1.29)  // 29.5
        .add_gear(1.00); // 38
}

run(
    engine: eng(),
    transmission: trn(),
    vehicle: veh()
)

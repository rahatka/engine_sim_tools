units units()
constants constants()

label bore(82.55) // 3 1/4
label stroke(111.125) // 4 3/8
label compression_ratio(6.65) // also 7.0, 7.25
label con_rod(177.8) // 7
label compression_height(92.075 / 2 * units.mm) // 3 5/8 L
label intake_valve_diameter(38.4969) // 1 33/64 ; .341 stem
label exhaust_valve_diameter(33.7344) // 1 21/64 ; .341 stem
label intake_valves(1.0)
label exhaust_valves(1.0)

label cyl(6.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)

label redline(4000.0) // RPM

// 1941 
label IVL(7.9375) // 5/16
label EVL(7.9375) // 5/16
label IVO(4.5 * units.deg) //BTDC
label IVC(47.5 * units.deg) //ABDC ?
label EVO(47.5 * units.deg) //BBDC ?
label EVC(4.5 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label piston_mass(326.02) // 14 1/8
label con_rod_mass(765.437) // 27
label crank_mass(12.0) // ? kg
label flywheel_mass(3.0) // kg

label exhaust_delay_coeff(1.5)


public node camshaft_builder {
    output intake_cam_0: _intake_cam_0;
    output exhaust_cam_0: _exhaust_cam_0;

    camshaft _intake_cam_0(base_radius: 16.1 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 16.1 * units.mm, lobe_profile: exhaust_lobe_profile)

    _intake_cam_0
        .add_lobe(ILC + 0 * rot)
        .add_lobe(ILC + 4 * rot)
        .add_lobe(ILC + 2 * rot)
        .add_lobe(ILC + 5 * rot)
        .add_lobe(ILC + 1 * rot)
        .add_lobe(ILC + 3 * rot)
    _exhaust_cam_0
        .add_lobe(ELC + 0 * rot)
        .add_lobe(ELC + 4 * rot)
        .add_lobe(ELC + 2 * rot)
        .add_lobe(ELC + 5 * rot)
        .add_lobe(ELC + 1 * rot)
        .add_lobe(ELC + 3 * rot)
}

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node head {
    // port_flow.py v1.7
    // intake port area: 9.6 cm²; saturated lift: 8.51 mm
    // exhaust port area: 8.1 cm²; saturated lift: 7.86 mm
    // cylinder volume: 581.0 cm³ (35.45 CI); engine volume: 3.486 L (212.72 CI)
    // chamber volume: 101.9 cm³ (6.22 CI); conrod/stroke 1.836; N.A.C.C. 25.01
    // 16 harmonic intake runner length: 25.1 cm; diameter: 3.6 cm
    // primary length: 35.5 cm, area: 9.0 cm², diameter: 3.4 cm
    // collector diameter: 4.5 cm, area: 16.1 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 101.927 * units.cc,
        intake_runner_volume: 253.1 * units.cc,
        intake_runner_cross_section_area: 10.1 * units.cm2,
        exhaust_runner_volume: 84.4 * units.cc,
        exhaust_runner_cross_section_area: 9.0 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "GAZ-12 ZIM",
        starter_torque: 250 * units.Nm,
        starter_speed: 300 * units.rpm,
        redline: redline * units.rpm,
        fuel: s30(),
        throttle_gamma: 1.5,
        jitter: 0.7,
        noise: 0.2,
        hf_gain: 0.007,
        simulation_frequency: 12000
    )

    wires wires()

    connecting_rod_parameters cr_params(
        mass: con_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.g,
            length: con_rod * units.mm
        ),
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    label crank_moment(disk_moment_of_inertia(mass: crank_mass * units.kg, radius: (stroke / 2.0) * units.mm))
    label flywheel_moment(disk_moment_of_inertia(mass: flywheel_mass * units.kg, radius: (stroke * 3.5) * units.mm))
    label other_moment(disk_moment_of_inertia(mass: 1 * units.kg, radius: 1.0 * units.cm))

    crankshaft c0(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: flywheel_mass * units.kg,
        mass: crank_mass * units.kg,
        friction_torque: 5.2 * units.Nm,
        moment_of_inertia: crank_moment + flywheel_moment + other_moment,
        position_x: -offset * units.mm,
        position_y: 0.0,
        tdc: 0.0
    )

    label tdc_comp(-0.669 * units.deg) // 90 - math.acos(x_shift / (stroke / 2 + con_rod))

    rod_journal rj0(angle: rot * 0 + rot90 + tdc_comp)
    rod_journal rj1(angle: rot * 4 + rot90 + tdc_comp)
    rod_journal rj2(angle: rot * 2 + rot90 + tdc_comp)
    rod_journal rj3(angle: rot * 5 + rot90 + tdc_comp)
    rod_journal rj4(angle: rot * 1 + rot90 + tdc_comp)
    rod_journal rj5(angle: rot * 3 + rot90 + tdc_comp)
    
    c0
        .add_rod_journal(rj0)
        .add_rod_journal(rj1)
        .add_rod_journal(rj2)
        .add_rod_journal(rj3)
        .add_rod_journal(rj4)
        .add_rod_journal(rj5)

    piston_parameters piston_params(
        mass: piston_mass * units.g,
        blowby: k_28inH2O(0.026),
        compression_height: compression_height,
        wrist_pin_position: 0.0,
        displacement: 0.0
    )

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    label collector_cross_section_area(16.1)

    label exhaust_pipe_length_0(400.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 1000.0) // Litres
    
    label spacing_factor(1.1)
    label flange_density(1.0 * exhaust_delay_coeff)
    

    intake intake(
        plenum_volume: 1.9 * units.L,
        plenum_cross_section_area: 30.3 * units.cm2,
        runner_length: 25.1 * units.cm,
        intake_flow_rate: k_carb(232.4),
        idle_flow_rate: k_carb(0.02),
        idle_throttle_plate_position: 0.995,
        runner_flow_rate: k_carb(77.5),
        velocity_decay: 0.05
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(387.3),
        collector_cross_section_area: 16.1 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 35.5 * units.cm,
        primary_flow_rate: k_carb(129.1),
        velocity_decay: 1.0,
        volume: (exhaust_volume_0 + 5.0) * units.L
    )

    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_55.wav", volume: 0.00025))
 
    cylinder_bank b0(bank_params, angle: 0)
    
    label pl0((4.44 * bore * spacing_factor / flange_density) * units.mm)
    label pl1((2.33 * bore * spacing_factor / flange_density) * units.mm)
    label pl2((1.77 * bore * spacing_factor / flange_density) * units.mm)
    label pl3((1.33 * bore * spacing_factor / flange_density) * units.mm)
    label pl4((1.00 * bore * spacing_factor / flange_density) * units.mm)
    label pl5((1.55 * bore * spacing_factor / flange_density) * units.mm)

    b0
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj0,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl0,
            ignition_wire: wires.wire1
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj1,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl1,
            ignition_wire: wires.wire2
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj2,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl2,
            ignition_wire: wires.wire3
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj3,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl3,
            ignition_wire: wires.wire4
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj4,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl4,
            ignition_wire: wires.wire5
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj5,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl5,
            ignition_wire: wires.wire6
        )

    engine
        .add_cylinder_bank(b0)

    engine.add_crankshaft(c0)

    camshaft_builder camshaft()

    b0.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_0,
            exhaust_camshaft: camshaft.exhaust_cam_0,
            flip_display: true
        )
    )

    function timing_curve(266.7 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 1.0 * units.deg)
        .add_sample(266.7 * units.rpm, 2.0 * units.deg)
        .add_sample(533.3 * units.rpm, 5.6 * units.deg)
        .add_sample(800.0 * units.rpm, 8.5 * units.deg)
        .add_sample(1066.7 * units.rpm, 10.9 * units.deg)
        .add_sample(1333.3 * units.rpm, 12.8 * units.deg)
        .add_sample(1600.0 * units.rpm, 14.4 * units.deg)
        .add_sample(1866.7 * units.rpm, 15.6 * units.deg)
        .add_sample(2133.3 * units.rpm, 16.6 * units.deg)
        .add_sample(2400.0 * units.rpm, 17.5 * units.deg)
        .add_sample(2666.7 * units.rpm, 18.1 * units.deg)
        .add_sample(2933.3 * units.rpm, 18.7 * units.deg)
        .add_sample(3200.0 * units.rpm, 19.1 * units.deg)
        .add_sample(3466.7 * units.rpm, 19.5 * units.deg)
        .add_sample(3733.3 * units.rpm, 19.8 * units.deg)
        .add_sample(4000.0 * units.rpm, 20.0 * units.deg)
        .add_sample(4266.7 * units.rpm, -12.5 * units.deg)
        .add_sample(4533.3 * units.rpm, -25.0 * units.deg)

    engine.add_ignition_module(
        eng_distributor(
            wires: wires,
            timing_curve: timing_curve
        )
    )
}

// ГАЗ 12 ЗИМ
public node veh {
    alias output __out: vehicle;
    vehicle vehicle(
        mass: 1800  * units.kg,
        drag_coefficient: 0.45,
        cross_sectional_area: (1900 * units.mm) * (1660 * units.mm),
        diff_ratio: 4.55,
        tire_radius: (736.6 / 2) * units.mm,
        rolling_resistance: 20
        )
}

// ГАЗ 12 ЗИМ
private node trn {
    alias output __out:
        transmission(
            max_clutch_torque: 300 * units.Nm,
            max_clutch_flex: 30 * units.deg,
            limit_clutch_flex: true,
            clutch_stiffness: 20 * units.Nm / units.deg,
            clutch_damping: 1.0,
            simulate_flex: true
        )
        .add_gear(3.115)
        .add_gear(1.772)
        .add_gear(1.0);
}

run(
    engine: eng(),
    transmission: trn(),
    vehicle: veh()
)

import "engine_sim.mr"
import "_fuel_10.mr"

units units()
constants constants()

label bore(79.375) // 3 1/8
label stroke(107.95) // 4 1/4
label compression_ratio(4.0) // ?
label con_rod(206.375) // 8 1/8
label compression_height(92.075 / 2 * units.mm) // ?
label intake_valve_diameter(34.0614) // 1.341
label exhaust_valve_diameter(34.0614) // 1.341
label intake_valves(1.0)
label exhaust_valves(1.0)

label cyl(8.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

label redline(3200.0) // RPM

// falls
label IVL(7.14375) // 9/32
label EVL(7.14375) // 9/32
label IVO(-10.0 * units.deg) //BTDC
label IVC(46.0 * units.deg) //ABDC
label EVO(49.0 * units.deg) //BBDC
label EVC(0.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(921.36) // 32 1/2
label piston_mass(566.99) // 20
label crank_mass(25.0) // ?
label flywheel_mass(5.0) // kg

label exhaust_delay_coeff(1.5)

public node variant {
    output s1: 0;
    output s6: 1;
    output s2: 2;
    output s5: 3;
    output s8: 4;
    output s3: 5;
    output s7: 6;
    output s4: 7;
}


import "engine_sim.mr"
import "_fuel_10.mr"

units units()
constants constants()

label bore(85.725) // 3 3/8
label stroke(101.6) // 4
label compression_ratio(4.94)
label con_rod(204.787) // 8 1/16
label compression_height(60.325 * units.mm) // 2 3/8
label intake_valve_diameter(36.5125) // 1 7/16 ; 1 5/16
label exhaust_valve_diameter(33.3375) // 1 5/16 ; 1 3/16
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(3300.0) // RPM

label cyl(6.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// 1927 Moon
label IVL(7.9375) // 5/16
label EVL(7.9375) // 5/16
label IVO(-5.0 * units.deg) //BTDC
label IVC(45.0 * units.deg) //ABDC
label EVO(45.0 * units.deg) //BBDC
label EVC(5.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(935.534) // 33
label piston_mass(595.34) // 21
label crank_mass(30.0) // ?
label flywheel_mass(5.0) // kg

label exhaust_delay_coeff(1.5)

public node variant {
    output s1: 0;
    output s5: 1;
    output s3: 2;
    output s6: 3;
    output s2: 4;
    output s4: 5;
}


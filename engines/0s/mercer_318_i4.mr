import "engine_sim.mr"
import "_fuel_10.mr"

units units()
constants constants()

label bore(114.3) // 4 1/2
label stroke(127.0) // 5
label compression_ratio(4.0) // ?
label con_rod(317.5) // 12 1/2
label compression_height(146.05 / 2 * units.mm) // 5 3/4 length
label intake_valve_diameter(57.15) // 2 1/4
label exhaust_valve_diameter(57.15) // 2 1/4
label intake_valves(1.0)
label exhaust_valves(1.0)

label cyl(4.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

label redline(2000.0) // RPM

// 1913 Mercer 230.6 / 286.6 @ 0.0 ; -2 ADV 110 LSA
label IVL(11.1125) // 7/16
label EVL(11.1125) // 7/16
label IVO(3.3 * units.deg) //BTDC
label IVC(47.3 * units.deg) //ABDC
label EVO(71.3 * units.deg) //BBDC
label EVC(35.3 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(1500.0) // ?
label crank_mass(20.0) // ? kg
label flywheel_mass(22.0) // kg

label exhaust_delay_coeff(1.5)

public node eng_distributor {
    input wires;
    input timing_curve;
    input rev_limit: 4000 * units.rpm;
    input limiter_duration: 0.0001;
    alias output __out:
        ignition_module(timing_curve: timing_curve, rev_limit: rev_limit, limiter_duration: limiter_duration)
            .connect_wire(wires.wire1, rot * 0)
            .connect_wire(wires.wire3, rot * 1)
            .connect_wire(wires.wire4, rot * 2)
            .connect_wire(wires.wire2, rot * 3);
}

private node wires {
    output wire1: ignition_wire();
    output wire2: ignition_wire();
    output wire3: ignition_wire();
    output wire4: ignition_wire();
}


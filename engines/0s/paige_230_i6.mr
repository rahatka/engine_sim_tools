import "engine_sim.mr"
import "_fuel_10.mr"

units units()
constants constants()

label bore(79.375) // 3 1/8
label stroke(127.0) // 5
label compression_ratio(5.0)
label con_rod(266.7) // 10 1/2
label compression_height(bore / 1.66 * units.mm) // ?
label intake_valve_diameter(39.6875) // 1 9/16
label exhaust_valve_diameter(39.6875) // 1 9/16
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(3000.0) // RPM

label cyl(6.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// 1921 Paige
label IVL(7.8486) // .309
label EVL(7.9756) // .314
label IVO(-15.0 * units.deg) //BTDC
label IVC(50.0 * units.deg) //ABDC
label EVO(45.0 * units.deg) //BBDC
label EVC(10.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(1332.42) // 2 lb 15 oz
label piston_mass(815.0) // 28 3/4 oz
label crank_mass(31.3) // 69 lb
label flywheel_mass(5.0) // kg

label exhaust_delay_coeff(1.5)


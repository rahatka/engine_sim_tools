import "engine_sim.mr"
import "_fuel_10.mr"

units units()
constants constants()

label bore(80.0)
label stroke(119.5)
label compression_ratio(4.5) // ?
label con_rod(225.0) // ~
label compression_height(51.0 * units.mm) // ~
label intake_valve_diameter(40.0) // ~
label exhaust_valve_diameter(40.0) // ~
label intake_valves(1.0)
label exhaust_valves(1.0)
label offset(20.4) // 20x 4y

label cyl(12.0)
label row(2.0)
label cycle(720.0 * units.deg)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label vee(22.5 * units.deg)

label comp(vee - rot)
label tdc_comp(4.108 * units.deg)

label redline(3000.0) // RPM

// ?
label IVL(9.0) // ~
label EVL(9.0) // ~
label IVO(-6.0 * units.deg) //BTDC
label IVC(46.0 * units.deg) //ABDC
label EVO(45.0 * units.deg) //BBDC
label EVC(05.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(800.0) // g
label crank_mass(25.0) // kg
label flywheel_mass(8.0) // kg

label exhaust_delay_coeff(1.5)


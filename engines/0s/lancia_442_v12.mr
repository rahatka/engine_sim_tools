private node add_lobes {
    input base;
    input shift: 0.0;
    input var;
    input this;
    alias output __out: this;

    this.add_lobe(base + var.s1 * rot_base + shift)
    this.add_lobe(base + var.s2 * rot_base + shift)
    this.add_lobe(base + var.s3 * rot_base + shift)
    this.add_lobe(base + var.s4 * rot_base + shift)
    this.add_lobe(base + var.s5 * rot_base + shift)
    this.add_lobe(base + var.s6 * rot_base + shift)
}

public node bank_builder {
    input var;
    input crankshaft;
    input incline;
    input phasing;
    input ignition;
    input intake;
    input exhaust;
    input primaries;
    input x;
    input y;
    input sh;
    input flip: false;

    output bank: _b0;

    ignition_wire _wire1()
    ignition_wire _wire2()
    ignition_wire _wire3()
    ignition_wire _wire4()
    ignition_wire _wire5()
    ignition_wire _wire6()

    piston_parameters piston_params(
        mass: 545 * units.g,
        blowby: k_28inH2O(0.025),
        compression_height: compression_height
    )

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    connecting_rod_parameters cr_params(
        mass: con_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.g,
            length: con_rod * units.mm
        ),
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    camshaft _intake_cam_0(base_radius: 16.5 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 16.5 * units.mm, lobe_profile: exhaust_lobe_profile)

    _intake_cam_0.add_lobes(ILC, phasing, var)
    _exhaust_cam_0.add_lobes(ELC, phasing, var)

    ignition.connect_wire(_wire1, rot_base * var.s1 + phasing)
    ignition.connect_wire(_wire2, rot_base * var.s2 + phasing)
    ignition.connect_wire(_wire3, rot_base * var.s3 + phasing)
    ignition.connect_wire(_wire4, rot_base * var.s4 + phasing)
    ignition.connect_wire(_wire5, rot_base * var.s5 + phasing)
    ignition.connect_wire(_wire6, rot_base * var.s6 + phasing)

    cylinder_bank _b0(bank_params, angle: incline, position_x: x, position_y: y) 

    _b0
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj0,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: primaries.pl0 + sh,
            ignition_wire: _wire1
        )
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj1,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: primaries.pl1 + sh,
            ignition_wire: _wire2
        )
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj2,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: primaries.pl2 + sh,
            ignition_wire: _wire3
        )
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj3,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: primaries.pl3 + sh,
            ignition_wire: _wire4
        )
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj4,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: primaries.pl4 + sh,
            ignition_wire: _wire5
        )
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj5,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: primaries.pl5 + sh,
            ignition_wire: _wire6
        )

    _b0.set_cylinder_head(
        head(
            intake_camshaft: _intake_cam_0,
            exhaust_camshaft: _exhaust_cam_0,
            flip_display: flip
        )
    )
}

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node head {
    // port_flow.py v1.5
    // intake port area: 9.8 cm²; saturated lift: 8.61 mm
    // exhaust port area: 9.8 cm²; saturated lift: 8.61 mm
    // cylinder volume: 603.3 cm³ (36.8 CI); engine volume: 7.240 L (441.8 CI)
    // chamber volume: 172.4 cm³ (10.5 CI)
    // 32 harmonic intake runner length: 16.8 cm; diameter: 3.1 cm
    // primary length: 58.8 cm, area: 9.8 cm², diameter: 3.5 cm
    // collector diameter: 7.2 cm, area: 41.2 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 172.385 * units.cc,
        intake_runner_volume: 126.5 * units.cc,
        intake_runner_cross_section_area: 7.5 * units.cm2,
        exhaust_runner_volume: 42.2 * units.cc,
        exhaust_runner_cross_section_area: 9.8 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

public node crank {
    input phasing;
    input var;

    output c: _c;
    output rj0: _rj0;
    output rj1: _rj1;
    output rj2: _rj2;
    output rj3: _rj3;
    output rj4: _rj4;
    output rj5: _rj5;
    output rj0_1: _rj0_1;
    output rj1_1: _rj1_1;
    output rj2_1: _rj2_1;
    output rj3_1: _rj3_1;
    output rj4_1: _rj4_1;
    output rj5_1: _rj5_1;

    label crank_moment(disk_moment_of_inertia(mass: crank_mass * units.kg, radius: (stroke / 2.0) * units.mm))
    label flywheel_moment(disk_moment_of_inertia(mass: flywheel_mass * units.kg, radius: (stroke * 3.5) * units.mm))
    label other_moment(disk_moment_of_inertia(mass: 1 * units.kg, radius: 1.0 * units.cm))

    crankshaft _c(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: flywheel_mass * units.kg,
        mass: crank_mass * units.kg,
        friction_torque: 10.9 * units.Nm,
        moment_of_inertia: crank_moment + flywheel_moment + other_moment,
        tdc: 0.0
    )

    rod_journal _rj0(angle: rot_base * var.s1 + rot90 + vee / 2 + tdc_comp)
    rod_journal _rj1(angle: rot_base * var.s2 + rot90 + vee / 2 + tdc_comp)
    rod_journal _rj2(angle: rot_base * var.s3 + rot90 + vee / 2 + tdc_comp)
    rod_journal _rj3(angle: rot_base * var.s4 + rot90 + vee / 2 + tdc_comp)
    rod_journal _rj4(angle: rot_base * var.s5 + rot90 + vee / 2 + tdc_comp)
    rod_journal _rj5(angle: rot_base * var.s6 + rot90 + vee / 2 + tdc_comp)

    rod_journal _rj0_1(angle: rot_base * var.s1 + rot90 + vee / 2 - comp - tdc_comp)
    rod_journal _rj1_1(angle: rot_base * var.s2 + rot90 + vee / 2 - comp - tdc_comp)
    rod_journal _rj2_1(angle: rot_base * var.s3 + rot90 + vee / 2 - comp - tdc_comp)
    rod_journal _rj3_1(angle: rot_base * var.s4 + rot90 + vee / 2 - comp - tdc_comp)
    rod_journal _rj4_1(angle: rot_base * var.s5 + rot90 + vee / 2 - comp - tdc_comp)
    rod_journal _rj5_1(angle: rot_base * var.s6 + rot90 + vee / 2 - comp - tdc_comp)

    _c
        .add_rod_journal(_rj0_1)
        .add_rod_journal(_rj0)
        .add_rod_journal(_rj1_1)
        .add_rod_journal(_rj1)
        .add_rod_journal(_rj2_1)
        .add_rod_journal(_rj2)
        .add_rod_journal(_rj3_1)
        .add_rod_journal(_rj3)
        .add_rod_journal(_rj4_1)
        .add_rod_journal(_rj4)
        .add_rod_journal(_rj5_1)
        .add_rod_journal(_rj5)
}

public node c0_1 {
    input crank;
    output rj0: crank.rj0;
    output rj1: crank.rj1;
    output rj2: crank.rj2;
    output rj3: crank.rj3;
    output rj4: crank.rj4;
    output rj5: crank.rj5;
}

public node c1_1 {
    input crank;
    output rj0: crank.rj0_1;
    output rj1: crank.rj1_1;
    output rj2: crank.rj2_1;
    output rj3: crank.rj3_1;
    output rj4: crank.rj4_1;
    output rj5: crank.rj5_1;
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "Lancia 22 1/2 V12",
        starter_torque: 350 * units.Nm,
        starter_speed: 150 * units.rpm,
        redline: redline * units.rpm,
        fuel: s10(),
        throttle_gamma: 1.5,
        jitter: 1.0,
        noise: 0.1,
        hf_gain: 0.01,
        simulation_frequency: 8500,
        dyno_min_speed: 750 * units.rpm
    )

    variant_0 variant()

    crank c0(vee / 2 + rot90, variant)

    bank_builder bank0(
        var: variant,
        crankshaft: c0_1(c0),
        incline: vee / 2,
        phasing: 0,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust0,
        primaries: em_0(),
        x: -offset * units.mm,
        y: -4.0 * units.mm,
        sh: 0
    )

    bank_builder bank1(
        var: variant,
        crankshaft: c1_1(c0),
        incline: -vee / 2,
        phasing: vee - comp + rot360,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust0,
        primaries: em_0(),
        sh: 8.0 * units.cm,
        x: offset * units.mm,
        y: -4.0 * units.mm,
        flip: true
    )

    ignition_module ignition(timing_curve: timing_curve, rev_limit: redline * 1.15 * units.rpm, limiter_duration: 0.28)

    label collector_cross_section_area(41.2)

    label exhaust_pipe_length_0(350.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 100.0) // Litres

    intake intake(
        plenum_volume: 3.2 * units.L,
        plenum_cross_section_area: 45.2 * units.cm2,
        runner_length: 16.8 * units.cm,
        intake_flow_rate: k_carb(380.1),
        idle_flow_rate: k_carb(0.02),
        idle_throttle_plate_position: 0.995,
        runner_flow_rate: k_carb(66.5),
        velocity_decay: 0.03
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(649.4),
        collector_cross_section_area: 41.2 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 58.8 * units.cm,
        primary_flow_rate: k_carb(110.9),
        velocity_decay: 1.0,
        volume: (exhaust_volume_0 + 5.0) * units.L
    )
    
    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_56.wav", volume: 0.00025))

    engine
        .add_crankshaft(c0.c)
        .add_cylinder_bank(bank0.bank)
        .add_cylinder_bank(bank1.bank)
        .add_ignition_module(ignition)

    function timing_curve(200.0 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 2.0 * units.deg)
        .add_sample(200.0 * units.rpm, 2.0 * units.deg)
        .add_sample(400.0 * units.rpm, 2.0 * units.deg)
        .add_sample(600.0 * units.rpm, 4.8 * units.deg)
        .add_sample(800.0 * units.rpm, 9.5 * units.deg)
        .add_sample(1000.0 * units.rpm, 13.1 * units.deg)
        .add_sample(1200.0 * units.rpm, 15.9 * units.deg)
        .add_sample(1400.0 * units.rpm, 18.2 * units.deg)
        .add_sample(1600.0 * units.rpm, 19.9 * units.deg)
        .add_sample(1800.0 * units.rpm, 21.2 * units.deg)
        .add_sample(2000.0 * units.rpm, 22.3 * units.deg)
        .add_sample(2200.0 * units.rpm, 23.1 * units.deg)
        .add_sample(2400.0 * units.rpm, 23.7 * units.deg)
        .add_sample(2600.0 * units.rpm, 24.0 * units.deg)
        .add_sample(2800.0 * units.rpm, 24.0 * units.deg)
        .add_sample(3000.0 * units.rpm, 24.0 * units.deg)
        .add_sample(3200.0 * units.rpm, -15.0 * units.deg)
        .add_sample(3400.0 * units.rpm, -45.0 * units.deg)
}

label spacing_factor(1.5)
label flange_density(1.0 * exhaust_delay_coeff)
label em_unit(bore * spacing_factor / flange_density)

private node em_0{
    output pl0: (6.0 * em_unit) * units.mm;
    output pl1: (5.0 * em_unit) * units.mm;
    output pl2: (4.0 * em_unit) * units.mm;
    output pl3: (3.0 * em_unit) * units.mm;
    output pl4: (2.0 * em_unit) * units.mm;
    output pl5: (1.0 * em_unit) * units.mm;
}

// Dilambda
public node veh {
    alias output __out: vehicle;
    vehicle vehicle(
        mass: 2100 * units.kg,
        drag_coefficient: 0.47,
        cross_sectional_area: (1747 * units.mm) * (1970 * units.mm),
        diff_ratio: 3.0, // custom
        tire_radius: (792.0 / 2) * units.mm, // 16x50
        rolling_resistance: 20
        )
}

// Dilambda
private node trn {
    alias output __out:
        transmission(
            max_clutch_torque: 400 * units.Nm,
            max_clutch_flex: 30 * units.deg,
            limit_clutch_flex: true,
            clutch_stiffness: 10 * units.Nm / units.deg,
            clutch_damping: 1.0,
            simulate_flex: true
        )
        .add_gear(2.80)
        .add_gear(2.20)
        .add_gear(1.488)
        .add_gear(1.0);
}

run(
    engine: eng(),
    transmission: trn(),
    vehicle: veh()
)

import "engine_sim.mr"
import "_fuel_10.mr"

units units()
constants constants()

label bore(82.55) // 3 1/4
label stroke(114.3) // 4 1/2
label compression_ratio(5.0)
label con_rod(234.95) // 9 1/4
label compression_height(61.9125 * units.mm) // 2 7/16
label intake_valve_diameter(40.4812) // 1 19/32
label exhaust_valve_diameter(40.4812) // 1 19/32
label intake_valves(1.0)
label exhaust_valves(1.0)

label cyl(8.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

label redline(3400.0) // RPM

// 1927 stutz
label IVL(8.73125) // 11/32
label EVL(8.73125) // 11/32
label IVO(-10.0 * units.deg) //BTDC
label IVC(50.0 * units.deg) //ABDC
label EVO(46.0 * units.deg) //BBDC
label EVC(10.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(850.486) // 30
label piston_mass(623.69) // 22
label crank_mass(30.0) // ?
label flywheel_mass(7.0) // kg

label exhaust_delay_coeff(1.5)


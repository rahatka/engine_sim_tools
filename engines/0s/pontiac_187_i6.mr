units units()
constants constants()

label bore(82.55) // 3 1/4
label stroke(95.25) // 3 3/4
label compression_ratio(4.6)
label con_rod(185.737) // 7 5/16
label compression_height(55.5625 * units.mm) // 2.1875
label intake_valve_diameter(30.1625) // 1 3/16 ; 1928 1 11/32
label exhaust_valve_diameter(30.1625) // 1 3/16 ; 1928 1 11/32
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(2600.0) // RPM

label cyl(6.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// 1927 Pontiac ; 1928 6/32	6/32 -7	39	42	7
label IVL(7.14375) // 9/32
label EVL(7.14375) // 9/32
label IVO(-7.0 * units.deg) //BTDC
label IVC(33.0 * units.deg) //ABDC
label EVO(42.0 * units.deg) //BBDC
label EVC(0.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(700.0) // ?
label piston_mass(600.0) // ?
label crank_mass(18.0) // ?
label flywheel_mass(5.0) // kg

label exhaust_delay_coeff(1.5)


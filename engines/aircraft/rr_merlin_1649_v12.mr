units units()
constants constants()

label bore(137.16) // 5.4
label stroke(152.4) // 6
label compression_ratio(6.0)
label con_rod(256.54) // 10.1 or 258
label compression_height(102.0 / 2 * units.mm) // 102 PH
label intake_valve_diameter(52.07) // max valve seat dia
label exhaust_valve_diameter(51.44) // max valve seat dia
label intake_valves(2.0)
label exhaust_valves(2.0)

label cyl(12.0)
label row(2.0)
label cycle(720.0 * units.deg)
label vee(60.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

label redline(3000.0)

// Packard Merlin
label IVL(14.986)
label EVL(14.986)
label IVO(31.0 * units.deg) //BTDC
label IVC(52.0 * units.deg) //ABDC
label EVO(72.0 * units.deg) //BBDC
label EVC(12.0 * units.deg) //ATDC


import "engine_sim.mr"
import "_fuel_10.mr"

units units()
constants constants()

label bore(107.95) // 4 1/4
label stroke(133.35) // 5 1/4
label compression_ratio(5.14)
label con_rod(247.65) // 9 3/4
label compression_height(57.0 * units.mm) // ?
label intake_valve_diameter(52.3875) // 2 1/16
label exhaust_valve_diameter(52.3875) // 2 1/16
label intake_valves(1.0)
label exhaust_valves(1.0)

label cyl(3.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

label redline(1800.0)

// L-2
label IVL(11.938) // 0.47
label EVL(11.938) // 0.47
label IVO(42.0 * units.deg) //BTDC
label IVC(90.0 * units.deg) //ABDC
label EVO(92.0 * units.deg) //BBDC
label EVC(46.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label piston_mass(900.0) // ?
label con_rod_mass(1300.0) // ? g
label crank_mass(8.0) // ? kg
label flywheel_mass(5.0) // kg

label exhaust_delay_coeff(1.5)

public node eng_distributor {
    input wires;
    input timing_curve;
    input rev_limit: 4000 * units.rpm;
    input limiter_duration: 0.0001;
    alias output __out:
        ignition_module(timing_curve: timing_curve, rev_limit: rev_limit, limiter_duration: limiter_duration)
            .connect_wire(wires.wire1, rot * 0)
            .connect_wire(wires.wire3, rot * 1)
            .connect_wire(wires.wire2, rot * 2);
}

private node wires {
    output wire1: ignition_wire();
    output wire2: ignition_wire();
    output wire3: ignition_wire();
}


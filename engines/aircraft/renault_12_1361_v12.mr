import "engine_sim.mr"
import "_fuel_10.mr"

units units()
constants constants()

label bore(130.0)
label stroke(140.0)
label compression_ratio(5.0)
label con_rod(300.0) // ?
label compression_height(57.5 * units.mm) // ~
label intake_valve_diameter(40.0) // 37 c
label exhaust_valve_diameter(40.0) // 37 c
label intake_valves(2.0)
label exhaust_valves(2.0)

label redline(2250.0) // RPM

label cyl(12.0)
label row(2.0)
label cycle(720.0 * units.deg)
label vee(60.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)

// Renault Cannon
label IVL(10.2)
label EVL(10.2)
label IVO(-10.0 * units.deg) //BTDC
label IVC(50.0 * units.deg) //ABDC
label EVO(56.0 * units.deg) //BBDC
label EVC(10.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(1300.0) // ? g
label piston_mass(800.0) // ?
label crank_mass(50.0) // ? kg
label flywheel_mass(20.0) // kg

label exhaust_delay_coeff(1.5)

public node variant_0 {
    output s1: 0;
    output s5: 1;
    output s3: 2;
    output s6: 3;
    output s2: 4;
    output s4: 5;
}


units units()
constants constants()

label bore(127.0) // 5
label stroke(177.8) // 7
label compression_ratio(5.42)
label compression_height(88.1062 * units.mm) // 3 15/32
label con_rod(304.8) // 12
label intake_valve_diameter(69.85) // 2 1/2 port dia
label exhaust_valve_diameter(69.85) // 2 1/2 port dia
label intake_valves(1.0)
label exhaust_valves(1.0)

label cyl(12.0)
label row(2.0)
label cyl_base(cyl / row)
label cycle(720.0 * units.deg)
label vee(45.0 * units.deg)
label rot(cycle / cyl)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

label redline(2000.0) // RPM

// Liberty L-12
label IVL(11.1125) // 7/16
label EVL(9.525) // 3/8
label IVO(10.0 * units.deg) //BTDC
label IVC(45.0 * units.deg) //ABDC
label EVO(48.0 * units.deg) //BBDC
label EVC(8.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(4036.97) // 1 conrod inc. bearing, bolts, 5.7lb Forked end, 3.2lb plain end (six of each)

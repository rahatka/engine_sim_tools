public node rods {
    input crank;
    output cr0_0: mcr0; output cr1_0: mcr1;
    output rj0_0: crank.rj0; output rj1_0: crank.rj1;
    output rj0_1: sj0_0; output rj1_1: sj1_0;
    output cr0_1: sr0_0; output cr1_1: sr1_0;
    output cr0_2: sr0_1; output cr1_2: sr1_1;
    output rj0_2: sj0_1; output rj1_2: sj1_1;
    output cr0_3: sr0_2; output cr1_3: sr1_2;
    output rj0_3: sj0_2; output rj1_3: sj1_2;
    output cr0_4: sr0_3; output cr1_4: sr1_3;
    output rj0_4: sj0_3; output rj1_4: sj1_3;
    output cr0_5: sr0_4; output cr1_5: sr1_4;
    output rj0_5: sj0_4; output rj1_5: sj1_4;
    output cr0_6: sr0_5; output cr1_6: sr1_5;
    output rj0_6: sj0_5; output rj1_6: sj1_5;
    output cr0_7: sr0_6; output cr1_7: sr1_6;
    output rj0_7: sj0_6; output rj1_7: sj1_6;
    output cr0_8: sr0_7; output cr1_8: sr1_7;
    output rj0_8: sj0_7; output rj1_8: sj1_7;

    connecting_rod_parameters master_rod_params(
        mass: con_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.g,
            length: con_rod * units.mm
        ),
        slave_throw: R1 * units.mm,
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    connecting_rod_parameters slave_rod_params(
        mass: slave_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: slave_rod_mass * units.g,
            length: L1 * units.mm
        ),
        center_of_mass: 0.0,
        length: L1 * units.mm
    )

    connecting_rod mcr0(master_rod_params)
    connecting_rod mcr1(master_rod_params)

    connecting_rod sr0_0(slave_rod_params)
    connecting_rod sr1_0(slave_rod_params)

    connecting_rod sr0_1(slave_rod_params)
    connecting_rod sr1_1(slave_rod_params)

    connecting_rod sr0_2(slave_rod_params)
    connecting_rod sr1_2(slave_rod_params)

    connecting_rod sr0_3(slave_rod_params)
    connecting_rod sr1_3(slave_rod_params)

    connecting_rod sr0_4(slave_rod_params)
    connecting_rod sr1_4(slave_rod_params)

    connecting_rod sr0_5(slave_rod_params)
    connecting_rod sr1_5(slave_rod_params)

    connecting_rod sr0_6(slave_rod_params)
    connecting_rod sr1_6(slave_rod_params)

    connecting_rod sr0_7(slave_rod_params)
    connecting_rod sr1_7(slave_rod_params)

    label psi0(-rot_base * 0.5)
    label psi1(-rot_base * 1.0)
    label psi2(-rot_base * 1.5)
    label psi3(-rot_base * 2.0)
    label psi4(-rot_base * 2.5)
    label psi5(-rot_base * 3.0)
    label psi6(-rot_base * 3.5)
    label psi7(-rot_base * 4.0)

    rod_journal sj0_0(angle: psi0)
    rod_journal sj1_0(angle: psi0)

    rod_journal sj0_1(angle: psi1)
    rod_journal sj1_1(angle: psi1)

    rod_journal sj0_2(angle: psi2)
    rod_journal sj1_2(angle: psi2)

    rod_journal sj0_3(angle: psi3)
    rod_journal sj1_3(angle: psi3)

    rod_journal sj0_4(angle: psi4)
    rod_journal sj1_4(angle: psi4)

    rod_journal sj0_5(angle: psi5)
    rod_journal sj1_5(angle: psi5)

    rod_journal sj0_6(angle: psi6)
    rod_journal sj1_6(angle: psi6)

    rod_journal sj0_7(angle: psi7)
    rod_journal sj1_7(angle: psi7)

    mcr0.add_slave_journal(sj0_0)
    mcr1.add_slave_journal(sj1_0)

    mcr0.add_slave_journal(sj0_1)
    mcr1.add_slave_journal(sj1_1)

    mcr0.add_slave_journal(sj0_2)
    mcr1.add_slave_journal(sj1_2)

    mcr0.add_slave_journal(sj0_3)
    mcr1.add_slave_journal(sj1_3)

    mcr0.add_slave_journal(sj0_4)
    mcr1.add_slave_journal(sj1_4)

    mcr0.add_slave_journal(sj0_5)
    mcr1.add_slave_journal(sj1_5)

    mcr0.add_slave_journal(sj0_6)
    mcr1.add_slave_journal(sj1_6)

    mcr0.add_slave_journal(sj0_7)
    mcr1.add_slave_journal(sj1_7)
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "Salmson 18 CMb",
        starter_torque: 300 * units.Nm,
        starter_speed: 100 * units.rpm,
        redline: redline * units.rpm,
        fuel: a10(),
        throttle_gamma: 2.0,
        jitter: 0.7,
        noise: 0.1,
        hf_gain: 0.005,
        simulation_frequency: 7000,
        dyno_min_speed: 500 * units.rpm,
        fluid_simulation_steps: 6,
        max_sle_solver_steps: 32
    )

    variant_0 variant()
    crank c0(variant)
    rods r(c0)

    ignition_module ignition(timing_curve: timing_curve, rev_limit: 2500 * units.rpm, limiter_duration: 0.001)

    bank_builder bank0(
        var: variant,
        incline: 0,
        rods: r0(r),
        phasing: 0.0 * units.deg,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust0,
        sh: bore * 1.5 * units.cm,
        flip_display: true
    )

    bank_builder bank1(
        var: variant,
        incline: -rot_base * 0.5,
        phasing: rot_base * 0.5 + rot360,
        rods: r1(r),
        ignition: ignition,
        intake: intake,
        exhaust: exhaust0,
        sh: bore * 3.5 * units.cm,
        flip_display: false
    )

    bank_builder bank2(
        var: variant,
        incline: -rot_base * 1.0,
        phasing: rot_base * 1.0,
        rods: r2(r),
        ignition: ignition,
        intake: intake,
        exhaust: exhaust0,
        sh: bore * 5.5 * units.cm,
        flip_display: true
    )

    bank_builder bank3(
        var: variant,
        incline: -rot_base * 1.5,
        phasing: rot_base * 1.5 + rot360,
        rods: r3(r),
        ignition: ignition,
        intake: intake,
        exhaust: exhaust0,
        sh: bore * 7.5 * units.cm,
        flip_display: false
    )

    bank_builder bank4(
        var: variant,
        incline: -rot_base * 2.0,
        phasing: rot_base * 2.0,
        rods: r4(r),
        ignition: ignition,
        intake: intake,
        exhaust: exhaust0,
        sh: bore * 9 * units.cm,
        flip_display: false
    )

    bank_builder bank5(
        var: variant,
        incline: -rot_base * 2.5,
        phasing: rot_base * 2.5 + rot360,
        rods: r5(r),
        ignition: ignition,
        intake: intake,
        exhaust: exhaust0,
        sh: bore * 7 * units.cm,
        flip_display: false
    )

    bank_builder bank6(
        var: variant,
        incline: -rot_base * 3.0,
        phasing: rot_base * 3.0,
        rods: r6(r),
        ignition: ignition,
        intake: intake,
        exhaust: exhaust0,
        sh: bore * 5 * units.cm,
        flip_display: false
    )

    bank_builder bank7(
        var: variant,
        incline: -rot_base * 3.5,
        phasing: rot_base * 3.5 + rot360,
        rods: r7(r),
        ignition: ignition,
        intake: intake,
        exhaust: exhaust0,
        sh: bore * 3 * units.cm,
        flip_display: false
    )

    bank_builder bank8(
        var: variant,
        incline: -rot_base * 4.0,
        phasing: rot_base * 4.0,
        rods: r8(r),
        ignition: ignition,
        intake: intake,
        exhaust: exhaust0,
        sh: bore * 1 * units.cm,
        flip_display: false
    )

    label collector_cross_section_area(318.4)

    label exhaust_pipe_length_0(150.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 100.0) // Litres

    intake intake(
        plenum_volume: 9.6 * units.L,
        plenum_cross_section_area: 148.2 * units.cm2,
        runner_length: 25.4 * units.cm,
        intake_flow_rate: k_carb(1251.7),
        idle_flow_rate: k_carb(0.05),
        idle_throttle_plate_position: 0.9965,
        runner_flow_rate: k_carb(139.1),
        velocity_decay: 0.05
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(2086.2),
        collector_cross_section_area: 318.4 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 71.0 * units.cm,
        primary_flow_rate: k_carb(231.8),
        velocity_decay: 0.8,
        volume: exhaust_volume_0 * units.L
    )

    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_58.wav", volume: 0.001))

    engine
        .add_crankshaft(c0.c)
        .add_cylinder_bank(bank0.b0)
        .add_cylinder_bank(bank1.b0)
        .add_cylinder_bank(bank2.b0)
        .add_cylinder_bank(bank3.b0)
        .add_cylinder_bank(bank4.b0)
        .add_cylinder_bank(bank5.b0)
        .add_cylinder_bank(bank6.b0)
        .add_cylinder_bank(bank7.b0)
        .add_cylinder_bank(bank8.b0)
        .add_ignition_module(ignition)

    function timing_curve(133.3 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 5.0 * units.deg)
        .add_sample(133.3 * units.rpm, 5.0 * units.deg)
        .add_sample(266.7 * units.rpm, 6.4 * units.deg)
        .add_sample(400.0 * units.rpm, 9.1 * units.deg)
        .add_sample(533.3 * units.rpm, 11.4 * units.deg)
        .add_sample(666.7 * units.rpm, 13.5 * units.deg)
        .add_sample(800.0 * units.rpm, 15.4 * units.deg)
        .add_sample(933.3 * units.rpm, 17.0 * units.deg)
        .add_sample(1066.7 * units.rpm, 18.5 * units.deg)
        .add_sample(1200.0 * units.rpm, 19.8 * units.deg)
        .add_sample(1333.3 * units.rpm, 20.9 * units.deg)
        .add_sample(1466.7 * units.rpm, 21.9 * units.deg)
        .add_sample(1600.0 * units.rpm, 22.9 * units.deg)
        .add_sample(1733.3 * units.rpm, 23.7 * units.deg)
        .add_sample(1866.7 * units.rpm, 24.4 * units.deg)
        .add_sample(2000.0 * units.rpm, 25.0 * units.deg)
        .add_sample(2133.3 * units.rpm, -15.0 * units.deg)
        .add_sample(2266.7 * units.rpm, -45.0 * units.deg)
}

// Pz V
public node veh {
    alias output __out: vehicle;

    label car_mass(45000) // 45.0 t panther

    vehicle vehicle(
        mass: car_mass * units.kg,
        drag_coefficient: 0.6,
        cross_sectional_area: (2830 * units.mm) * (2680 * units.mm),
        diff_ratio: 3.5 * 2,
        tire_radius: (821.77 / 2) * units.mm,
        rolling_resistance: 0.06 * car_mass * 9.81
        )
}

// Pz V
private node trn {
    alias output __out:
        transmission(
            max_clutch_torque: 2700 * units.Nm,
            max_clutch_flex: 15 * units.deg,
            limit_clutch_flex: true,
            clutch_stiffness: 50 * units.Nm / units.deg,
            clutch_damping: 1.0,
            simulate_flex: true
        )
        .add_gear(10.8)  // 3.7 @ 2600
        .add_gear(5.26)  // 7.6
        .add_gear(3.33)  // 12.0
        .add_gear(2.13)  // 18.8
        .add_gear(1.48)  // 27.0
        .add_gear(1.06)  // 37.7
        .add_gear(0.85); // 47.3
}

run(
    engine: eng(),
    transmission: trn(),
    vehicle: veh()
)

import "engine_sim.mr"
import "../../fuel/10.mr"

units units()
constants constants()

label bore(140.0)
label stroke(150.0)
label compression_ratio(5.32)
label con_rod(263.5) // 245 Neuman ; 10 3/8 US army rep
label compression_height(75.0 * units.mm) // ~ 51 ?
label intake_valve_diameter(62.0) // 56.0 clear // 70/63.5 clear 1920 US army rep
label exhaust_valve_diameter(62.0) // 56.0 clear // 70/63.5 clear 1920 US army rep
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(2250.0) // RPM

label cyl(8.0)
label row(2.0)
label cycle(720.0 * units.deg)
label vee(90.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// Hispano-Suiza 8Fb
label IVL(10.0) // 13 H
label EVL(10.0) // 13 H
label IVO(-8.0 * units.deg) //BTDC -10 H
label IVC(52.0 * units.deg) //ABDC 62 H
label EVO(43.0 * units.deg) //BBDC 62.5 H
label EVC(10.0 * units.deg) //ATDC 29.5 H

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(3000.0) // 5.65 + 3.05 = 4.35
label piston_mass(1800.0) // 5.95
label crank_mass(40.0) // ? kg
label flywheel_mass(10.0) // kg

label exhaust_delay_coeff(1.5)

public node eng_distributor {
    input wires;
    input timing_curve;
    input rev_limit: 3000 * units.rpm;
    input limiter_duration: 0.0001;
    alias output __out:
        ignition_module(timing_curve: timing_curve, rev_limit: rev_limit, limiter_duration: limiter_duration)
            .connect_wire(wires.wire1, rot * 0)
            .connect_wire(wires.wire3, rot * 2)
            .connect_wire(wires.wire4, rot * 4)
            .connect_wire(wires.wire2, rot * 6)
            .connect_wire(wires.wire5, rot * 0 + vee + rot360)
            .connect_wire(wires.wire7, rot * 2 + vee + rot360)
            .connect_wire(wires.wire8, rot * 4 + vee + rot360)
            .connect_wire(wires.wire6, rot * 6 + vee + rot360);
}

private node wires {
    output wire1: ignition_wire();
    output wire2: ignition_wire();
    output wire3: ignition_wire();
    output wire4: ignition_wire();
    output wire5: ignition_wire();
    output wire6: ignition_wire();
    output wire7: ignition_wire();
    output wire8: ignition_wire();
}


public node eng {
    alias output __out: engine;

    engine engine(
        name: "Wright R-2160",
        starter_torque: 1000 * units.Nm,
        starter_speed: 250 * units.rpm,
        redline: redline * units.rpm,
        fuel: a30(),
        throttle_gamma: 2.0,
        jitter: 0.8,
        noise: 0.1,
        hf_gain: 0.008,
        fluid_simulation_steps: 4,
        max_sle_solver_steps: 16,
        simulation_frequency: 2750,
        dyno_min_speed: 500 * units.rpm
    )

    ////////////////////////////////////////
    ///////// change variants here /////////
    ////////////////////////////////////////
    variant_0 variant()

    label crank_moment(disk_moment_of_inertia(mass: crank_mass * units.kg, radius: (stroke / 2.0) * units.mm))
    label flywheel_moment(disk_moment_of_inertia(mass: flywheel_mass * units.kg, radius: (stroke * 3.5) * units.mm))
    label other_moment(disk_moment_of_inertia(mass: 1 * units.kg, radius: 1.0 * units.cm))

    crankshaft c0(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: flywheel_mass * units.kg,
        mass: crank_mass * units.kg,
        friction_torque: 53.1 * units.Nm,
        moment_of_inertia: crank_moment + flywheel_moment + other_moment,
        tdc: 0.0
    )

    rod_journal rj0(angle: rot_base * variant.s1 + rot90)
    rod_journal rj1(angle: rot_base * variant.s2 + rot90)
    rod_journal rj2(angle: rot_base * variant.s3 + rot90)
    rod_journal rj3(angle: rot_base * variant.s4 + rot90)
    rod_journal rj4(angle: rot_base * variant.s5 + rot90)
    rod_journal rj5(angle: rot_base * variant.s6 + rot90)

    rods r(rj0, rj1, rj2, rj3, rj4, rj5)

    bank_builder bank0( // A
        var: variant,
        chamber_volume: 140.451,
        rods: r0(r),
        angle: 0,
        phasing: 0.0 * units.deg,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust,
        pl0 + sh0, pl1 + sh0, pl2 + sh0, pl3 + sh0, pl4 + sh0, pl5 + sh0
    )
    bank_builder bank1( // G
        var: variant,
        chamber_volume: 142.816,
        rods: r1(r),
        angle: -vee,
        phasing: vee * 1 + rot360,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust,
        pl0 + sh1, pl1 + sh1, pl2 + sh1, pl3 + sh1, pl4 + sh1, pl5 + sh1
    )
    bank_builder bank2( // F
        var: variant,
        chamber_volume: 144.344,
        rods: r2(r),
        angle: -vee * 2,
        phasing: vee * 2,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust,
        pl0 + sh2, pl1 + sh2, pl2 + sh2, pl3 + sh2, pl4 + sh2, pl5 + sh2
    )
    bank_builder bank3( // E
        var: variant,
        chamber_volume: 141.317,
        rods: r3(r),
        angle: -vee * 3,
        phasing: vee * 3 + rot360,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust,
        pl0 + sh3, pl1 + sh3, pl2 + sh3, pl3 + sh3, pl4 + sh3, pl5 + sh3
    )
    bank_builder bank4( // D
        var: variant,
        chamber_volume: 141.317,
        rods: r4(r),
        angle: -vee * 4,
        phasing: vee * 4,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust,
        pl0 + sh4, pl1 + sh4, pl2 + sh4, pl3 + sh4, pl4 + sh4, pl5 + sh4
    )
    bank_builder bank5( // C
        var: variant,
        chamber_volume: 144.344,
        rods: r5(r),
        angle: -vee * 5,
        phasing: vee * 5 + rot360,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust,
        pl0 + sh5, pl1 + sh5, pl2 + sh5, pl3 + sh5, pl4 + sh5, pl5 + sh5
    )
    bank_builder bank6( // B
        var: variant,
        chamber_volume: 142.816,
        rods: r6(r),
        angle: -vee * 6,
        phasing: vee * 6,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust,
        pl0 + sh6, pl1 + sh6, pl2 + sh6, pl3 + sh6, pl4 + sh6, pl5 + sh6
    )

    ignition_module ignition(timing_curve: timing_curve, rev_limit: 5000 * units.rpm, limiter_duration: 0.001)
    
    c0
        .add_rod_journal(rj0)
        .add_rod_journal(rj1)
        .add_rod_journal(rj2)
        .add_rod_journal(rj3)
        .add_rod_journal(rj4)
        .add_rod_journal(rj5)

    label collector_cross_section_area(412.7)

    label exhaust_pipe_length_0(400.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 1000.0) // Litres
    
    label spacing_factor(1.2)
    label flange_density(1.0 * exhaust_delay_coeff)
    label radial_density(3.0 * exhaust_delay_coeff)

    intake intake(
        plenum_volume: 19.9 * units.L,
        plenum_cross_section_area: 296.2 * units.cm2,
        runner_length: 21.9 * units.cm,
        intake_flow_rate: k_carb(2507.1),
        idle_flow_rate: k_carb(0.02),
        idle_throttle_plate_position: 0.996,
        runner_flow_rate: k_carb(119.4),
        velocity_decay: 0.05
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(4178.4),
        collector_cross_section_area: 412.7 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 30.9 * units.cm,
        primary_flow_rate: k_carb(199.0),
        velocity_decay: 1.0,
        volume: exhaust_volume_0 * units.L
    )

    exhaust_system exhaust(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_56.wav", volume: 0.001))

    label sh0 ((1.0 * bore * spacing_factor / radial_density) * units.mm)
    label sh1 ((4.0 * bore * spacing_factor / radial_density) * units.mm)
    label sh2 ((7.0 * bore * spacing_factor / radial_density) * units.mm)
    label sh3 ((10.0 * bore * spacing_factor / radial_density) * units.mm)
    label sh4 ((8.0 * bore * spacing_factor / radial_density) * units.mm)
    label sh5 ((5.0 * bore * spacing_factor / radial_density) * units.mm)
    label sh6 ((2.0 * bore * spacing_factor / radial_density) * units.mm)
    
    label pl0 ((1.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl1 ((2.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl2 ((3.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl3 ((4.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl4 ((5.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl5 ((6.0 * bore * spacing_factor / flange_density) * units.mm)

    engine
        .add_cylinder_bank(bank0.b0)
        .add_cylinder_bank(bank1.b0)
        .add_cylinder_bank(bank2.b0)
        .add_cylinder_bank(bank3.b0)
        .add_cylinder_bank(bank4.b0)
        .add_cylinder_bank(bank5.b0)
        .add_cylinder_bank(bank6.b0)

    engine.add_crankshaft(c0)

    // ignition_timing.py v0.2 a = 7.0, m = 1.15, r = 4250
    function timing_curve(425.0 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 0.0 * units.deg)
        .add_sample(425.0 * units.rpm, 7.0 * units.deg)
        .add_sample(850.0 * units.rpm, 8.0 * units.deg)
        .add_sample(1275.0 * units.rpm, 9.3 * units.deg)
        .add_sample(1700.0 * units.rpm, 10.6 * units.deg)
        .add_sample(2125.0 * units.rpm, 12.2 * units.deg)
        .add_sample(2550.0 * units.rpm, 14.1 * units.deg)
        .add_sample(2975.0 * units.rpm, 16.2 * units.deg)
        .add_sample(3400.0 * units.rpm, 18.6 * units.deg)
        .add_sample(3825.0 * units.rpm, 21.4 * units.deg)
        .add_sample(4250.0 * units.rpm, 24.6 * units.deg)
        .add_sample(4675.0 * units.rpm, -12.5 * units.deg)
        .add_sample(5100.0 * units.rpm, -25.0 * units.deg)

    engine.add_ignition_module(ignition)
}

// Bugatti Type 41
public node veh {
    alias output __out: vehicle;
    vehicle vehicle(
        mass: 3250  * units.kg,
        drag_coefficient: 0.5,
        cross_sectional_area: (1320 * units.mm) * (1050 * units.mm),
        diff_ratio: 3.6,
        tire_radius: (914.4 / 2) * units.mm, // 36 x 6.75
        rolling_resistance: 20
        )
}

// Bugatti Type 41
private node trn {
    alias output __out:
        transmission(
            max_clutch_torque: 12000 * units.Nm,
            max_clutch_flex: 5 * units.deg,
            limit_clutch_flex: true,
            clutch_stiffness: 100 * units.Nm / units.deg,
            clutch_damping: 1.0,
            simulate_flex: true
        )
        .add_gear(7.50)
        .add_gear(1.0)
        .add_gear(0.37);
}

run(
    engine: eng(),
    transmission: trn(),
    vehicle: veh()
)

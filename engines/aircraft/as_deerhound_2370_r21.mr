    input rj0; input rj1; input rj2;
    output cr0_0: mcr0; output cr1_0: mcr1; output cr2_0: mcr2;
    output rj0_0: rj0; output rj1_0: rj1; output rj2_0: rj2;

    output rj0_1: sj0_0; output rj1_1: sj1_0; output rj2_1: sj2_0;
    output cr0_1: sr0_0; output cr1_1: sr1_0; output cr2_1: sr2_0;

    output cr0_2: sr0_1; output cr1_2: sr1_1; output cr2_2: sr2_1;
    output rj0_2: sj0_1; output rj1_2: sj1_1; output rj2_2: sj2_1;

    output cr0_3: sr0_2; output cr1_3: sr1_2; output cr2_3: sr2_2;
    output rj0_3: sj0_2; output rj1_3: sj1_2; output rj2_3: sj2_2;

    output cr0_4: sr0_3; output cr1_4: sr1_3; output cr2_4: sr2_3;
    output rj0_4: sj0_3; output rj1_4: sj1_3; output rj2_4: sj2_3;

    output cr0_5: sr0_4; output cr1_5: sr1_4; output cr2_5: sr2_4;
    output rj0_5: sj0_4; output rj1_5: sj1_4; output rj2_5: sj2_4;

    output cr0_6: sr0_5; output cr1_6: sr1_5; output cr2_6: sr2_5;
    output rj0_6: sj0_5; output rj1_6: sj1_5; output rj2_6: sj2_5;

    connecting_rod_parameters master_rod_params(
        mass: con_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.g,
            length: con_rod * units.mm
        ),
        slave_throw: R1 * units.mm,
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    label slave_rod_mass(850)
    connecting_rod_parameters slave_rod_params(
        mass: slave_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: slave_rod_mass * units.g,
            length: L1 * units.mm
        ),
        center_of_mass: 0.0,
        length: L1 * units.mm
    )

    connecting_rod mcr0(master_rod_params)
    connecting_rod mcr1(master_rod_params)
    connecting_rod mcr2(master_rod_params)

    connecting_rod sr0_0(slave_rod_params)
    connecting_rod sr1_0(slave_rod_params)
    connecting_rod sr2_0(slave_rod_params)

    connecting_rod sr0_1(slave_rod_params)
    connecting_rod sr1_1(slave_rod_params)
    connecting_rod sr2_1(slave_rod_params)

    connecting_rod sr0_2(slave_rod_params)
    connecting_rod sr1_2(slave_rod_params)
    connecting_rod sr2_2(slave_rod_params)

    connecting_rod sr0_3(slave_rod_params)
    connecting_rod sr1_3(slave_rod_params)
    connecting_rod sr2_3(slave_rod_params)

    connecting_rod sr0_4(slave_rod_params)
    connecting_rod sr1_4(slave_rod_params)
    connecting_rod sr2_4(slave_rod_params)

    connecting_rod sr0_5(slave_rod_params)
    connecting_rod sr1_5(slave_rod_params)
    connecting_rod sr2_5(slave_rod_params)

    label psi0(57.1 * units.deg)
    label psi1(109.9 * units.deg)
    label psi2(157.4 * units.deg)
    label psi3(202.6 * units.deg)
    label psi4(250.1 * units.deg)
    label psi5(302.9 * units.deg)

    rod_journal sj0_0(angle: psi0)
    rod_journal sj1_0(angle: psi0)
    rod_journal sj2_0(angle: psi0)

    rod_journal sj0_1(angle: psi1)
    rod_journal sj1_1(angle: psi1)
    rod_journal sj2_1(angle: psi1)

    rod_journal sj0_2(angle: psi2)
    rod_journal sj1_2(angle: psi2)
    rod_journal sj2_2(angle: psi2)

    rod_journal sj0_3(angle: psi3)
    rod_journal sj1_3(angle: psi3)
    rod_journal sj2_3(angle: psi3)

    rod_journal sj0_4(angle: psi4)
    rod_journal sj1_4(angle: psi4)
    rod_journal sj2_4(angle: psi4)

    rod_journal sj0_5(angle: psi5)
    rod_journal sj1_5(angle: psi5)
    rod_journal sj2_5(angle: psi5)

    mcr0.add_slave_journal(sj0_0)
    mcr1.add_slave_journal(sj1_0)
    mcr2.add_slave_journal(sj2_0)

    mcr0.add_slave_journal(sj0_1)
    mcr1.add_slave_journal(sj1_1)
    mcr2.add_slave_journal(sj2_1)

    mcr0.add_slave_journal(sj0_2)
    mcr1.add_slave_journal(sj1_2)
    mcr2.add_slave_journal(sj2_2)

    mcr0.add_slave_journal(sj0_3)
    mcr1.add_slave_journal(sj1_3)
    mcr2.add_slave_journal(sj2_3)

    mcr0.add_slave_journal(sj0_4)
    mcr1.add_slave_journal(sj1_4)
    mcr2.add_slave_journal(sj2_4)

    mcr0.add_slave_journal(sj0_5)
    mcr1.add_slave_journal(sj1_5)
    mcr2.add_slave_journal(sj2_5)
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "A. S. Deerhound",
        starter_torque: 500 * units.Nm,
        starter_speed: 150 * units.rpm,
        redline: redline * units.rpm,
        fuel: a30(),
        throttle_gamma: 2.0,
        jitter: 0.8,
        noise: 0.1,
        hf_gain: 0.008,
        fluid_simulation_steps: 4,
        max_sle_solver_steps: 32,
        simulation_frequency: 5500,
        dyno_min_speed: 500 * units.rpm
    )

    label crank_moment(disk_moment_of_inertia(mass: crank_mass * units.kg, radius: (stroke / 2.0) * units.mm))
    label flywheel_moment(disk_moment_of_inertia(mass: flywheel_mass * units.kg, radius: (stroke * 3.5) * units.mm))
    label other_moment(disk_moment_of_inertia(mass: 1 * units.kg, radius: 1.0 * units.cm))

    crankshaft c0(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: flywheel_mass * units.kg,
        mass: crank_mass * units.kg,
        friction_torque: 57.3 * units.Nm,
        moment_of_inertia: crank_moment + flywheel_moment + other_moment,
        tdc: 0.0
    )

    rod_journal rj0(angle: rot_base * 0 + rot90)
    rod_journal rj1(angle: rot_base * 2 + rot90)
    rod_journal rj2(angle: rot_base * 1 + rot90)

    rods r(rj0, rj1, rj2)
    
    c0
        .add_rod_journal(rj0)
        .add_rod_journal(rj1)
        .add_rod_journal(rj2)

    bank_builder bank0(
        shift: 0.0 * units.deg,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust,
        rods: r0(r),
        pl0 + sh0, pl1 + sh0, pl2 + sh0
    )
    bank_builder bank1(
        shift: vee * 1 + rot360,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust,
        rods: r1(r),
        pl0 + sh1, pl1 + sh1, pl2 + sh1
    )
    bank_builder bank2(
        shift: vee * 2,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust,
        rods: r2(r),
        pl0 + sh2, pl1 + sh2, pl2 + sh2
    )
    bank_builder bank3(
        shift: vee * 3 + rot360,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust,
        rods: r3(r),
        pl0 + sh3, pl1 + sh3, pl2 + sh3
    )
    bank_builder bank4(
        shift: vee * 4,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust,
        rods: r4(r),
        pl0 + sh4, pl1 + sh4, pl2 + sh4
    )
    bank_builder bank5(
        shift: vee * 5 + rot360,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust,
        rods: r5(r),
        pl0 + sh5, pl1 + sh5, pl2 + sh5
    )
    bank_builder bank6(
        shift: vee * 6,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust,
        rods: r6(r),
        pl0 + sh6, pl1 + sh6, pl2 + sh6
    )

    ignition_module ignition(timing_curve: timing_curve, rev_limit: 10000 * units.rpm, limiter_duration: 0.001)

    label collector_cross_section_area(392.3)

    label exhaust_pipe_length_0(100.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 100.0) // Litres
    
    label spacing_factor(1.15)
    label flange_density(1.0 * exhaust_delay_coeff)
    label radial_density(1.0 * exhaust_delay_coeff)

    intake intake(
        plenum_volume: 17.2 * units.L,
        plenum_cross_section_area: 258.0 * units.cm2,
        runner_length: 16.0 * units.cm,
        intake_flow_rate: k_carb(2051.9),
        idle_flow_rate: k_carb(0.03),
        idle_throttle_plate_position: 0.994,
        runner_flow_rate: k_carb(210.1),
        velocity_decay: 0.05
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(3548.1),
        collector_cross_section_area: 392.3 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 56.9 * units.cm,
        primary_flow_rate: k_carb(350.6),
        velocity_decay: 0.75,
        volume: exhaust_volume_0 * units.L
    )

    exhaust_system exhaust(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_58.wav", volume: 0.0001))

    label sh0 ((1.0 * bore * spacing_factor / radial_density) * units.mm)
    label sh1 ((4.0 * bore * spacing_factor / radial_density) * units.mm)
    label sh2 ((7.0 * bore * spacing_factor / radial_density) * units.mm)
    label sh3 ((10.0 * bore * spacing_factor / radial_density) * units.mm)
    label sh4 ((8.0 * bore * spacing_factor / radial_density) * units.mm)
    label sh5 ((5.0 * bore * spacing_factor / radial_density) * units.mm)
    label sh6 ((2.0 * bore * spacing_factor / radial_density) * units.mm)
    
    label pl0 ((1.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl1 ((2.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl2 ((3.0 * bore * spacing_factor / flange_density) * units.mm)

    engine
        .add_cylinder_bank(bank0.b0)
        .add_cylinder_bank(bank1.b0)
        .add_cylinder_bank(bank2.b0)
        .add_cylinder_bank(bank3.b0)
        .add_cylinder_bank(bank4.b0)
        .add_cylinder_bank(bank5.b0)
        .add_cylinder_bank(bank6.b0)

        .add_crankshaft(c0)
        .add_ignition_module(ignition)

    function timing_curve(200.0 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 0.0 * units.deg)
        .add_sample(200.0 * units.rpm, 1.0 * units.deg)
        .add_sample(400.0 * units.rpm, 10.3 * units.deg)
        .add_sample(600.0 * units.rpm, 17.1 * units.deg)
        .add_sample(800.0 * units.rpm, 22.2 * units.deg)
        .add_sample(1000.0 * units.rpm, 25.8 * units.deg)
        .add_sample(1200.0 * units.rpm, 28.5 * units.deg)
        .add_sample(1400.0 * units.rpm, 30.5 * units.deg)
        .add_sample(1600.0 * units.rpm, 32.0 * units.deg)
        .add_sample(1800.0 * units.rpm, 33.0 * units.deg)
        .add_sample(2000.0 * units.rpm, 33.8 * units.deg)
        .add_sample(2200.0 * units.rpm, 34.4 * units.deg)
        .add_sample(2400.0 * units.rpm, 34.8 * units.deg)
        .add_sample(2600.0 * units.rpm, 35.0 * units.deg)
        .add_sample(2800.0 * units.rpm, 35.0 * units.deg)
        .add_sample(3000.0 * units.rpm, 35.0 * units.deg)
        .add_sample(3200.0 * units.rpm, -15.0 * units.deg)
        .add_sample(3400.0 * units.rpm, -45.0 * units.deg)
}

// Pz V
public node veh {
    alias output __out: vehicle;

    label car_mass(45000) // 45.0 t panther

    vehicle vehicle(
        mass: car_mass * units.kg,
        drag_coefficient: 0.6,
        cross_sectional_area: (2830 * units.mm) * (2680 * units.mm),
        diff_ratio: 2.5 * 3,
        tire_radius: (821.77 / 2) * units.mm,
        rolling_resistance: 0.06 * car_mass * 9.81
        )
}

// Pz V
private node trn {
    alias output __out:
        transmission(
            max_clutch_torque: 3250 * units.Nm,
            max_clutch_flex: 15 * units.deg,
            limit_clutch_flex: true,
            clutch_stiffness: 50 * units.Nm / units.deg,
            clutch_damping: 1.0,
            simulate_flex: true
        )
        .add_gear(10.8)  // 3.7 @ 2600
        .add_gear(5.26)  // 7.6
        .add_gear(3.33)  // 12.0
        .add_gear(2.13)  // 18.8
        .add_gear(1.48)  // 27.0
        .add_gear(1.06)  // 37.7
        .add_gear(0.85); // 47.3
}

run(
    engine: eng(),
    transmission: trn(),
    vehicle: veh()
)

units units()
constants constants()

label bore(105.0)
label stroke(130.0)
label compression_ratio(4.0)
label con_rod(260.0)
label L1(216.0) // ?
label R1(45.0)
label psi(90.0 * units.deg)
label compression_height(53.0 * units.mm) // ~
label intake_valve_diameter(45.0) // ~ 40 c
label exhaust_valve_diameter(45.0) // ~ 40 c
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(2000.0) // RPM

label cyl(12.0)
label row(2.0)
label cycle(720.0 * units.deg)
label vee(90.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)

// 80HP
label IVL(10.0)
label EVL(10.0)
label IVO(-6.5 * units.deg) //BTDC
label IVC(51.5 * units.deg) //ABDC
label EVO(59.0 * units.deg) //BBDC
label EVC(18.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(1300.0) // ? g
label piston_mass(800.0) // ?
label crank_mass(50.0) // ? kg
label flywheel_mass(20.0) // kg

label exhaust_delay_coeff(1.5)


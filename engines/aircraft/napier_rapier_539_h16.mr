public node block_builder {
    input phase;
    input rods0;
    input rods1;
    input separation;
    input ignition;
    input intake;
    input exhaust0;
    input exhaust1;

    input flip_display_0: false;
    input flip_display_1: false;

    output b0: _b0;
    output b1: _b1;

    ignition_wire _wire1()
    ignition_wire _wire2()
    ignition_wire _wire3()
    ignition_wire _wire4()
    ignition_wire _wire5()
    ignition_wire _wire6()
    ignition_wire _wire7()
    ignition_wire _wire8()

    camshaft _intake_cam_0(base_radius: 16.0 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 16.0 * units.mm, lobe_profile: exhaust_lobe_profile)
    
    camshaft _intake_cam_1(base_radius: 16.0 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_1(base_radius: 16.0 * units.mm, lobe_profile: exhaust_lobe_profile)

    _intake_cam_0
        .add_lobe(ILC + 0.0 * rot_base + phase)
        .add_lobe(ILC + 1.0 * rot_base + phase)
        .add_lobe(ILC + 3.5 * rot_base + phase)
        .add_lobe(ILC + 2.5 * rot_base + phase)
    _exhaust_cam_0
        .add_lobe(ELC + 0.0 * rot_base + phase)
        .add_lobe(ELC + 1.0 * rot_base + phase)
        .add_lobe(ELC + 3.5 * rot_base + phase)
        .add_lobe(ELC + 2.5 * rot_base + phase)
        
    _intake_cam_1
        .add_lobe(ILC + 3.0 * rot_base + phase)
        .add_lobe(ILC + 2.0 * rot_base + phase)
        .add_lobe(ILC + 0.5 * rot_base + phase)
        .add_lobe(ILC + 1.5 * rot_base + phase)
    _exhaust_cam_1
        .add_lobe(ELC + 3.0 * rot_base + phase)
        .add_lobe(ELC + 2.0 * rot_base + phase)
        .add_lobe(ELC + 0.5 * rot_base + phase)
        .add_lobe(ELC + 1.5 * rot_base + phase)

    ignition.connect_wire(_wire1, rot_base * 0.0 + phase)
    ignition.connect_wire(_wire2, rot_base * 1.0 + phase)
    ignition.connect_wire(_wire3, rot_base * 3.5 + phase)
    ignition.connect_wire(_wire4, rot_base * 2.5 + phase)

    ignition.connect_wire(_wire5, rot_base * 3.0 + phase)
    ignition.connect_wire(_wire6, rot_base * 2.0 + phase)
    ignition.connect_wire(_wire7, rot_base * 0.5 + phase)
    ignition.connect_wire(_wire8, rot_base * 1.5 + phase)

    piston_parameters piston_params(
        mass: piston_mass * units.g,
        blowby: k_28inH2O(0.028),
        compression_height: compression_height,
        wrist_pin_position: 0.0,
        displacement: 0.0
    )

    connecting_rod_parameters cr_params(
        mass: con_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.g,
            length: con_rod * units.mm
        ),
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    cylinder_bank _b0(bank_params, angle: 0, position_x: separation)
    cylinder_bank _b1(bank_params, angle: vee, position_x: separation)

    label spacing_factor(1.1)
    label flange_density(1.0 * exhaust_delay_coeff)

    label sh ((0.5 * bore * spacing_factor / flange_density) * units.mm)
    label pl0 ((5.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl1 ((4.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl2 ((3.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl3 ((2.0 * bore * spacing_factor / flange_density) * units.mm)

    _b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: rods0.cr0,
            rod_journal: rods0.rj0,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl0 + sh,
            ignition_wire: _wire1
        )
    _b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: rods0.cr1,
            rod_journal: rods0.rj1,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl1 + sh,
            ignition_wire: _wire2
        )
    _b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: rods0.cr2,
            rod_journal: rods0.rj2,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl2 + sh,
            ignition_wire: _wire3
        )
    _b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: rods0.cr3,
            rod_journal: rods0.rj3,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl3 + sh,
            ignition_wire: _wire4
        )

    _b1.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: rods1.cr0,
            rod_journal: rods1.rj0,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl0 - sh,
            ignition_wire: _wire5
        )
    _b1.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: rods1.cr1,
            rod_journal: rods1.rj1,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl1 - sh,
            ignition_wire: _wire6
        )
    _b1.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: rods1.cr2,
            rod_journal: rods1.rj2,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl2 - sh,
            ignition_wire: _wire7
        )
    _b1.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: rods1.cr3,
            rod_journal: rods1.rj3,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl3 - sh,
            ignition_wire: _wire8
        )

    _b0.set_cylinder_head(
        head(
            intake_camshaft: _intake_cam_0,
            exhaust_camshaft: _exhaust_cam_0,
            flip_display: flip_display_0
        )
    )
    _b1.set_cylinder_head(
        head(
            intake_camshaft: _intake_cam_1,
            exhaust_camshaft: _exhaust_cam_1,
            flip_display: flip_display_1
        )
    )
}

public node head {
    // port_flow.py v1.7
    // intake port area: 10.0 cm²; saturated lift: 8.66 mm
    // exhaust port area: 10.0 cm²; saturated lift: 8.66 mm
    // cylinder volume: 551.8 cm³ (33.67 CI); engine volume: 8.829 L (538.78 CI)
    // chamber volume: 110.4 cm³ (6.73 CI); conrod/stroke 2.000; N.A.C.C. 78.40
    // 16 harmonic intake runner length: 23.7 cm; diameter: 3.5 cm
    // primary length: 33.4 cm, area: 10.0 cm², diameter: 3.6 cm
    // collector diameter: 14.3 cm, area: 160.7 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 110.363 * units.cc,
        intake_runner_volume: 221.8 * units.cc,
        intake_runner_cross_section_area: 9.4 * units.cm2,
        exhaust_runner_volume: 73.9 * units.cc,
        exhaust_runner_cross_section_area: 10.0 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node crank {
    input phasing;
    input separation;

    output c: _c;
    output rj0: _rj0;
    output rj1: _rj1;
    output rj2: _rj2;
    output rj3: _rj3;

    label crank_moment(disk_moment_of_inertia(mass: crank_mass * units.kg, radius: (stroke / 2.0) * units.mm))
    label flywheel_moment(disk_moment_of_inertia(mass: flywheel_mass * units.kg, radius: (stroke * 3.5) * units.mm))
    label other_moment(disk_moment_of_inertia(mass: 1 * units.kg, radius: 1.0 * units.cm))

    crankshaft _c(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: flywheel_mass * units.kg,
        mass: crank_mass * units.kg,
        friction_torque: 6.6 * units.Nm,
        moment_of_inertia: crank_moment + flywheel_moment + other_moment,
        position_x: separation,
        position_y: 0,
        tdc: 0.0
    )

    rod_journal _rj0(angle: rot_base * 0.0 + phasing)
    rod_journal _rj1(angle: rot_base * 1.0 + phasing)
    rod_journal _rj2(angle: rot_base * 3.5 + phasing)
    rod_journal _rj3(angle: rot_base * 2.5 + phasing)

    _c
        .add_rod_journal(_rj0)
        .add_rod_journal(_rj1)
        .add_rod_journal(_rj2)
        .add_rod_journal(_rj3)
}

public node r0 {
    input rods;
    output cr0: rods.cr0_0;
    output cr1: rods.cr1_0;
    output cr2: rods.cr2_0;
    output cr3: rods.cr3_0;

    output rj0: rods.rj0_0;
    output rj1: rods.rj1_0;
    output rj2: rods.rj2_0;
    output rj3: rods.rj3_0;
}

public node r1 {
    input rods;
    output cr0: rods.cr0_1;
    output cr1: rods.cr1_1;
    output cr2: rods.cr2_1;
    output cr3: rods.cr3_1;

    output rj0: rods.rj0_1;
    output rj1: rods.rj1_1;
    output rj2: rods.rj2_1;
    output rj3: rods.rj3_1;
}

public node rods {
    input crank;
    output cr0_0: mcr0; output cr1_0: mcr1; output cr2_0: mcr2; output cr3_0: mcr3;
    output rj0_0: crank.rj0; output rj1_0: crank.rj1; output rj2_0: crank.rj2; output rj3_0: crank.rj3;
    output rj0_1: sj0_0; output rj1_1: sj1_0; output rj2_1: sj2_0; output rj3_1: sj3_0;
    output cr0_1: sr0_0; output cr1_1: sr1_0; output cr2_1: sr2_0; output cr3_1: sr3_0;

    connecting_rod_parameters master_rod_params(
        mass: con_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.g,
            length: con_rod * units.mm
        ),
        slave_throw: R1 * units.mm,
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    connecting_rod_parameters slave_rod_params(
        mass: slave_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: slave_rod_mass * units.g,
            length: L1 * units.mm
        ),
        center_of_mass: 0.0,
        length: L1 * units.mm
    )

    connecting_rod mcr0(master_rod_params)
    connecting_rod mcr1(master_rod_params)
    connecting_rod mcr2(master_rod_params)
    connecting_rod mcr3(master_rod_params)

    connecting_rod sr0_0(slave_rod_params)
    connecting_rod sr1_0(slave_rod_params)
    connecting_rod sr2_0(slave_rod_params)
    connecting_rod sr3_0(slave_rod_params)

    rod_journal sj0_0(angle: psi)
    rod_journal sj1_0(angle: psi)
    rod_journal sj2_0(angle: psi)
    rod_journal sj3_0(angle: psi)

    mcr0.add_slave_journal(sj0_0)
    mcr1.add_slave_journal(sj1_0)
    mcr2.add_slave_journal(sj2_0)
    mcr3.add_slave_journal(sj3_0)
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "Napier Rapier",
        starter_torque: 150 * units.Nm,
        starter_speed: 300 * units.rpm,
        redline: redline * units.rpm,
        fuel: a10(),
        throttle_gamma: 1.5,
        jitter: 0.7,
        noise: 0.1,
        hf_gain: 0.007,
        simulation_frequency: 8000,
        dyno_min_speed: 500 * units.rpm,
        fluid_simulation_steps: 6,
        max_sle_solver_steps: 32
    )

    ignition_module ignition(timing_curve: timing_curve, rev_limit: redline * 2.0 * units.rpm, limiter_duration: 0.01)

    crank c0(rot90, -separation)
    rods rb0(c0)

    crank c1(rot90 + rot180, separation)
    rods rb1(c1)

    block_builder block0(
        phase: 0,
        rods0: r1(rb0),
        rods1: r0(rb0),
        separation: -separation,
        ignition: ignition,
        intake: intake,
        exhaust0: exhaust0,
        exhaust1: exhaust1,
        flip_display_0: true,
        flip_display_1: false
    )

    block_builder block1(
        phase: rot180,
        rods0: r1(rb1),
        rods1: r0(rb1),
        separation: separation,
        ignition: ignition,
        intake: intake,
        exhaust0: exhaust2,
        exhaust1: exhaust3,
        flip_display_0: false,
        flip_display_1: true
    )

    label collector_cross_section_area(40.2)

    label exhaust_pipe_length_0(100.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 1000.0) // Litres
    
    intake intake(
        plenum_volume: 5.0 * units.L,
        plenum_cross_section_area: 74.9 * units.cm2,
        intake_flow_rate: k_carb(588.6),
        idle_flow_rate: k_carb(0.01),
        idle_throttle_plate_position: 0.996,
        runner_flow_rate: k_carb(73.6),
        runner_length: 23.7 * units.cm,
        velocity_decay: 0.05
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(245.3),
        collector_cross_section_area: 40.2 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 33.4 * units.cm,
        primary_flow_rate: k_carb(122.6),
        velocity_decay: 0.75,
        volume: exhaust_volume_0 * units.L
    )

    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_45.wav", volume: 0.00025))
    exhaust_system exhaust1(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_45.wav", volume: 0.00024))

    exhaust_system exhaust2(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_57.wav", volume: 0.00020))
    exhaust_system exhaust3(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_57.wav", volume: 0.00020))

    engine
        .add_cylinder_bank(block0.b0)
        .add_cylinder_bank(block0.b1)
        .add_cylinder_bank(block1.b0)
        .add_cylinder_bank(block1.b1)
        .add_crankshaft(c0.c)
        .add_crankshaft(c1.c)
        .add_ignition_module(ignition)

    // ignition_timing.py v0.3 from = 3.0, to = 25.0, i = 500, r = 4000
    function timing_curve(400.0 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 0.0 * units.deg)
        .add_sample(400.0 * units.rpm, 1.5 * units.deg)
        .add_sample(800.0 * units.rpm, 3.0 * units.deg)
        .add_sample(1200.0 * units.rpm, 8.8 * units.deg)
        .add_sample(1600.0 * units.rpm, 13.3 * units.deg)
        .add_sample(2000.0 * units.rpm, 16.7 * units.deg)
        .add_sample(2400.0 * units.rpm, 19.3 * units.deg)
        .add_sample(2800.0 * units.rpm, 21.4 * units.deg)
        .add_sample(3200.0 * units.rpm, 22.9 * units.deg)
        .add_sample(3600.0 * units.rpm, 24.1 * units.deg)
        .add_sample(4000.0 * units.rpm, 25.0 * units.deg)
        .add_sample(4400.0 * units.rpm, -12.5 * units.deg)
        .add_sample(4800.0 * units.rpm, -25.0 * units.deg)
}


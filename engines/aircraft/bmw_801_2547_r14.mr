        .add_lobe(ILC + rot360 + 0 * rot)
    _exhaust_cam_7
        .add_lobe(ELC + rot360 + 0 * rot)

    _intake_cam_8
        .add_lobe(ILC + rot360 + 8 * rot)
    _exhaust_cam_8
        .add_lobe(ELC + rot360 + 8 * rot)
    
    _intake_cam_9
        .add_lobe(ILC + rot360 + 2 * rot)
    _exhaust_cam_9
        .add_lobe(ELC + rot360 + 2 * rot)
    
    _intake_cam_10
        .add_lobe(ILC + rot360 + 10 * rot)
    _exhaust_cam_10
        .add_lobe(ELC + rot360 + 10 * rot)
    
    _intake_cam_11
        .add_lobe(ILC + rot360 + 4 * rot)
    _exhaust_cam_11
        .add_lobe(ELC + rot360 + 4 * rot)
        
    _intake_cam_12
        .add_lobe(ILC + rot360 + 12 * rot)
    _exhaust_cam_12
        .add_lobe(ELC + rot360 + 12 * rot)
        
    _intake_cam_13
        .add_lobe(ILC + rot360 + 6 * rot)
    _exhaust_cam_13
        .add_lobe(ELC + rot360 + 6 * rot)
}

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node head {
    // port_flow.py v1.5
    // intake port area: 36.6 cm²; saturated lift: 16.64 mm
    // exhaust port area: 31.1 cm²; saturated lift: 15.36 mm
    // cylinder volume: 2981.7 cm³ (182.0 CI); engine volume: 41.744 L (2547.4 CI)
    // chamber volume: 542.1 cm³ (33.1 CI)
    // 32 harmonic intake runner length: 18.4 cm; diameter: 6.4 cm
    // primary length: 69.7 cm, area: 34.2 cm², diameter: 6.6 cm
    // collector diameter: 21.4 cm, area: 359.4 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 542.127 * units.cc,
        intake_runner_volume: 582.2 * units.cc,
        intake_runner_cross_section_area: 31.7 * units.cm2,
        exhaust_runner_volume: 232.9 * units.cc,
        exhaust_runner_cross_section_area: 34.2 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "BMW 801",
        starter_torque: 500 * units.Nm,
        starter_speed: 300 * units.rpm,
        redline: redline * units.rpm,
        fuel: a30(),
        throttle_gamma: 2.0,
        jitter: 0.7,
        noise: 0.1,
        hf_gain: 0.006,
        simulation_frequency: 8000,
        dyno_min_speed: 500 * units.rpm,
        fluid_simulation_steps: 6,
        max_sle_solver_steps: 32
    )

    wires wires()

    label crank_moment(disk_moment_of_inertia(mass: crank_mass * units.kg, radius: (stroke / 2.0) * units.mm))
    label flywheel_moment(disk_moment_of_inertia(mass: flywheel_mass * units.kg, radius: (stroke * 3.5) * units.mm))
    label other_moment(disk_moment_of_inertia(mass: 1 * units.kg, radius: 1.0 * units.cm))

    crankshaft c0(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: flywheel_mass * units.kg,
        mass: crank_mass * units.kg,
        friction_torque: 62.6 * units.Nm,
        moment_of_inertia: crank_moment + flywheel_moment + other_moment,
        tdc: 0.0
    )

    rod_journal rj0(angle: rot90)
    rod_journal rj1(angle: rot180 + rot90)
    
    c0
        .add_rod_journal(rj0)
        .add_rod_journal(rj1)

    piston_parameters piston_params(
        mass: 1700 * units.g,
        blowby: k_28inH2O(0.049),
        compression_height: compression_height,
        wrist_pin_position: 0.0,
        displacement: 0.0
    )

    connecting_rod_parameters cr_params(
        mass: con_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.g,
            length: con_rod * units.mm
        ),
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    label collector_cross_section_area(119.8)

    label exhaust_pipe_length_0(100.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 100.0) // Litres

    label exhaust_pipe_length_1(100.0) // cm
    label exhaust_volume_1(collector_cross_section_area * exhaust_pipe_length_1 / 100.0) // Litres

    label exhaust_pipe_length_2(75.0) // cm
    label exhaust_volume_2(collector_cross_section_area * exhaust_pipe_length_2 / 100.0) // Litres

    intake intake(
        plenum_volume: 15.5 * units.L,
        plenum_cross_section_area: 221.7 * units.cm2,
        intake_flow_rate: k_carb(2152.4),
        idle_flow_rate: k_carb(0.002),
        idle_throttle_plate_position: 0.9977,
        runner_flow_rate: k_carb(345.9),
        runner_length: 18.4 * units.cm,
        velocity_decay: 0.05
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(1270.5),
        collector_cross_section_area: 119.8 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 69.7 * units.cm,
        primary_flow_rate: k_carb(578.5),
        velocity_decay: 0.5,
        volume: exhaust_volume_0 * units.L
    )
    
    exhaust_system_parameters es_params1(
        outlet_flow_rate: k_carb(1270.5),
        collector_cross_section_area: 119.8 * units.cm2,
        length: exhaust_pipe_length_1 * units.cm,
        primary_tube_length: 69.7 * units.cm,
        primary_flow_rate: k_carb(578.5),
        velocity_decay: 0.5,
        volume: exhaust_volume_1 * units.L
    )

    exhaust_system_parameters es_params2(
        outlet_flow_rate: k_carb(1270.5),
        collector_cross_section_area: 119.8 * units.cm2,
        length: exhaust_pipe_length_2 * units.cm,
        primary_tube_length: 69.7 * units.cm,
        primary_flow_rate: k_carb(578.5),
        velocity_decay: 0.5,
        volume: exhaust_volume_2 * units.L
    )

    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_45.wav", volume: 0.001))
    exhaust_system exhaust1(es_params1, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_45.wav", volume: 0.001))
    exhaust_system exhaust2(es_params2, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_58.wav", volume: 0.001))

    cylinder_bank b0(bank_params, angle: -(0.0 / cyl) * rot360)
    cylinder_bank b1(bank_params, angle: -(2.0 / cyl) * rot360)
    cylinder_bank b2(bank_params, angle: -(4.0 / cyl) * rot360)
    cylinder_bank b3(bank_params, angle: -(6.0 / cyl) * rot360)
    cylinder_bank b4(bank_params, angle: -(8.0 / cyl) * rot360)
    cylinder_bank b5(bank_params, angle: -(10.0 / cyl) * rot360)
    cylinder_bank b6(bank_params, angle: -(12.0 / cyl) * rot360)
    
    cylinder_bank b7(bank_params, angle: -(0.0 / cyl) * rot360 + rot180)
    cylinder_bank b8(bank_params, angle: -(2.0 / cyl) * rot360 + rot180)
    cylinder_bank b9(bank_params, angle: -(4.0 / cyl) * rot360 + rot180)
    cylinder_bank b10(bank_params, angle: -(6.0 / cyl) * rot360 + rot180)
    cylinder_bank b11(bank_params, angle: -(8.0 / cyl) * rot360 + rot180)
    cylinder_bank b12(bank_params, angle: -(10.0 / cyl) * rot360 + rot180)
    cylinder_bank b13(bank_params, angle: -(12.0 / cyl) * rot360 + rot180)
    
    label sh(50.0 * units.cm)
    label spacing_factor(4.0)
    label flange_density(1.0 * exhaust_delay_coeff)
    label pl0 ((3.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl1 ((2.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl2 ((1.0 * bore * spacing_factor / flange_density) * units.mm)
    
    b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj0,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl0 + sh,
            ignition_wire: wires.wire1
        )
    b1.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj0,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl1 + sh,
            ignition_wire: wires.wire2
        )
    b2.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj0,
            intake: intake,
            exhaust_system: exhaust2,
            primary_length: pl0 + sh,
            ignition_wire: wires.wire3
        )
    b3.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj0,
            intake: intake,
            exhaust_system: exhaust2,
            primary_length: pl1 + sh,
            ignition_wire: wires.wire4
        )
    b4.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj0,
            intake: intake,
            exhaust_system: exhaust2,
            primary_length: pl2 + sh,
            ignition_wire: wires.wire5
        )
    b5.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj0,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl0 + sh,
            ignition_wire: wires.wire6
        )
    b6.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj0,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl1 + sh,
            ignition_wire: wires.wire7
        )

    b7.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj1,
            intake: intake,
            exhaust_system: exhaust2,
            primary_length: pl1,
            ignition_wire: wires.wire8
        )
    b8.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj1,
            intake: intake,
            exhaust_system: exhaust2,
            primary_length: pl2,
            ignition_wire: wires.wire9
        )
    b9.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj1,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl0,
            ignition_wire: wires.wire10
        )
    b10.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj1,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl1,
            ignition_wire: wires.wire11
        )
    b11.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj1,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl0,
            ignition_wire: wires.wire12
        )
    b12.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj1,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl1,
            ignition_wire: wires.wire13
        )
    b13.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj1,
            intake: intake,
            exhaust_system: exhaust2,
            primary_length: pl0,
            ignition_wire: wires.wire14
        )

    engine
        .add_cylinder_bank(b0)
        .add_cylinder_bank(b1)
        .add_cylinder_bank(b2)
        .add_cylinder_bank(b3)
        .add_cylinder_bank(b4)
        .add_cylinder_bank(b5)
        .add_cylinder_bank(b6)
        
        .add_cylinder_bank(b7)
        .add_cylinder_bank(b8)
        .add_cylinder_bank(b9)
        .add_cylinder_bank(b10)
        .add_cylinder_bank(b11)
        .add_cylinder_bank(b12)
        .add_cylinder_bank(b13)

    engine.add_crankshaft(c0)

    camshaft_builder camshaft()

    b0.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_0,
            exhaust_camshaft: camshaft.exhaust_cam_0
        )
    )
    b1.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_1,
            exhaust_camshaft: camshaft.exhaust_cam_1
        )
    )
    b2.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_2,
            exhaust_camshaft: camshaft.exhaust_cam_2
        )
    )
    b3.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_3,
            exhaust_camshaft: camshaft.exhaust_cam_3
        )
    )
    b4.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_4,
            exhaust_camshaft: camshaft.exhaust_cam_4
        )
    )
    b5.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_5,
            exhaust_camshaft: camshaft.exhaust_cam_5
        )
    )
    b6.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_6,
            exhaust_camshaft: camshaft.exhaust_cam_6
        )
    )
    
    b7.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_7,
            exhaust_camshaft: camshaft.exhaust_cam_7
        )
    )
    b8.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_8,
            exhaust_camshaft: camshaft.exhaust_cam_8
        )
    )
    b9.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_9,
            exhaust_camshaft: camshaft.exhaust_cam_9
        )
    )
    b10.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_10,
            exhaust_camshaft: camshaft.exhaust_cam_10
        )
    )
    b11.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_11,
            exhaust_camshaft: camshaft.exhaust_cam_11
        )
    )
    b12.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_12,
            exhaust_camshaft: camshaft.exhaust_cam_12
        )
    )
    b13.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_13,
            exhaust_camshaft: camshaft.exhaust_cam_13
        )
    )

    // ignition_timing.py v0.3 from = 5.0, to = 25.0, i = 500, r = 2750
    function timing_curve(275.0 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 0.0 * units.deg)
        .add_sample(275.0 * units.rpm, 2.5 * units.deg)
        .add_sample(550.0 * units.rpm, 5.0 * units.deg)
        .add_sample(825.0 * units.rpm, 9.6 * units.deg)
        .add_sample(1100.0 * units.rpm, 13.3 * units.deg)
        .add_sample(1375.0 * units.rpm, 16.3 * units.deg)
        .add_sample(1650.0 * units.rpm, 18.8 * units.deg)
        .add_sample(1925.0 * units.rpm, 20.9 * units.deg)
        .add_sample(2200.0 * units.rpm, 22.5 * units.deg)
        .add_sample(2475.0 * units.rpm, 23.9 * units.deg)
        .add_sample(2750.0 * units.rpm, 25.0 * units.deg)
        .add_sample(3025.0 * units.rpm, -15.0 * units.deg)
        .add_sample(3300.0 * units.rpm, -60.0 * units.deg)

    engine.add_ignition_module(
        eng_distributor(
            wires: wires,
            timing_curve: timing_curve
        ))
}

// Bugatti Type 41
public node veh {
    alias output __out: vehicle;
    vehicle vehicle(
        mass: 3250  * units.kg,
        drag_coefficient: 0.5,
        cross_sectional_area: (1320 * units.mm) * (1050 * units.mm),
        diff_ratio: 3.6,
        tire_radius: (914.4 / 2) * units.mm, // 36 x 6.75
        rolling_resistance: 20
        )
}

// Bugatti Type 41
private node trn {
    alias output __out:
        transmission(
            max_clutch_torque: 5000 * units.Nm,
            max_clutch_flex: 15 * units.deg,
            limit_clutch_flex: true,
            clutch_stiffness: 100 * units.Nm / units.deg,
            clutch_damping: 1.0,
            simulate_flex: true
        )
        .add_gear(7.50)
        .add_gear(1.0)
        .add_gear(0.37);
}

run(
    engine: eng(),
    transmission: trn(),
    vehicle: veh()
)

import "../../fuel/10.mr"

units units()
constants constants()

label bore(125.0)
label stroke(150.0)
label compression_ratio(5.0)
label con_rod(300.0)
label L1(238.0)
label R1(64.0) // ~
label psi(50.0 * units.deg) // ~
label compression_height(57.5 * units.mm) // ~
label intake_valve_diameter(66.0) // 61 c
label exhaust_valve_diameter(66.0) // 61 c
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(1800.0) // RPM

label cyl(12.0)
label row(2.0)
label cycle(720.0 * units.deg)
label vee(50.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)

// Renault 300 H.P.
label IVL(10.0)
label EVL(10.0)
label IVO(-5.55 * units.deg) //BTDC
label IVC(45.43 * units.deg) //ABDC
label EVO(48.30 * units.deg) //BBDC
label EVC(18.50 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(4250 - 850) // 4250 bielle et biellette
label slave_rod_mass(850) // ?
label piston_mass(1640.0)
label crank_mass(50.0) // ? kg
label flywheel_mass(20.0) // kg

label exhaust_delay_coeff(1.5)


public node rods {
    input rj0; input rj1; input rj2; input rj3;
    output cr0_0: sr0_0; output cr1_0: sr1_0; output cr2_0: sr2_0; output cr3_0: sr3_0;
    output rj0_0: sj0_0; output rj1_0: sj1_0; output rj2_0: sj2_0; output rj3_0: sj3_0;
    output cr0_1: mcr0; output cr1_1: mcr1; output cr2_1: mcr2; output cr3_1: mcr3;
    output rj0_1: rj0; output rj1_1: rj1; output rj2_1: rj2; output rj3_1: rj3;
    output cr0_2: sr0_1; output cr1_2: sr1_1; output cr2_2: sr2_1; output cr3_2: sr3_1;
    output rj0_2: sj0_1; output rj1_2: sj1_1; output rj2_2: sj2_1; output rj3_2: sj3_1;
    output cr0_3: sr0_2; output cr1_3: sr1_2; output cr2_3: sr2_2; output cr3_3: sr3_2;
    output rj0_3: sj0_2; output rj1_3: sj1_2; output rj2_3: sj2_2; output rj3_3: sj3_2;

    connecting_rod_parameters master_rod_params(
        mass: con_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.g,
            length: con_rod * units.mm
        ),
        slave_throw: R1 * units.mm,
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    label slave_rod_mass(850)
    connecting_rod_parameters slave_rod_params(
        mass: slave_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: slave_rod_mass * units.g,
            length: L1 * units.mm
        ),
        center_of_mass: 0.0,
        length: L1 * units.mm
    )

    connecting_rod mcr0(master_rod_params)
    connecting_rod mcr1(master_rod_params)
    connecting_rod mcr2(master_rod_params)
    connecting_rod mcr3(master_rod_params)

    connecting_rod sr0_0(slave_rod_params)
    connecting_rod sr1_0(slave_rod_params)
    connecting_rod sr2_0(slave_rod_params)
    connecting_rod sr3_0(slave_rod_params)

    connecting_rod sr0_1(slave_rod_params)
    connecting_rod sr1_1(slave_rod_params)
    connecting_rod sr2_1(slave_rod_params)
    connecting_rod sr3_1(slave_rod_params)

    connecting_rod sr0_2(slave_rod_params)
    connecting_rod sr1_2(slave_rod_params)
    connecting_rod sr2_2(slave_rod_params)
    connecting_rod sr3_2(slave_rod_params)

    rod_journal sj0_0(angle: psi0)
    rod_journal sj1_0(angle: psi0)
    rod_journal sj2_0(angle: psi0)
    rod_journal sj3_0(angle: psi0)

    rod_journal sj0_1(angle: psi1)
    rod_journal sj1_1(angle: psi1)
    rod_journal sj2_1(angle: psi1)
    rod_journal sj3_1(angle: psi1)

    rod_journal sj0_2(angle: psi2)
    rod_journal sj1_2(angle: psi2)
    rod_journal sj2_2(angle: psi2)
    rod_journal sj3_2(angle: psi2)

    mcr0.add_slave_journal(sj0_0)
    mcr1.add_slave_journal(sj1_0)
    mcr2.add_slave_journal(sj2_0)
    mcr3.add_slave_journal(sj3_0)
    mcr0.add_slave_journal(sj0_1)
    mcr1.add_slave_journal(sj1_1)
    mcr2.add_slave_journal(sj2_1)
    mcr3.add_slave_journal(sj3_1)
    mcr0.add_slave_journal(sj0_2)
    mcr1.add_slave_journal(sj1_2)
    mcr2.add_slave_journal(sj2_2)
    mcr3.add_slave_journal(sj3_2)
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "Peugeot 16 X",
        starter_torque: 500 * units.Nm,
        starter_speed: 100 * units.rpm,
        redline: redline * units.rpm,
        fuel: a10(),
        throttle_gamma: 2.0,
        jitter: 0.8,
        noise: 0.1,
        hf_gain: 0.008,
        simulation_frequency: 7000,
        dyno_min_speed: 500 * units.rpm,
        fluid_simulation_steps: 6,
        max_sle_solver_steps: 32
    )

    label crank_moment(disk_moment_of_inertia(mass: crank_mass * units.kg, radius: (stroke / 2.0) * units.mm))
    label flywheel_moment(disk_moment_of_inertia(mass: flywheel_mass * units.kg, radius: (stroke * 3.5) * units.mm))
    label other_moment(disk_moment_of_inertia(mass: 1 * units.kg, radius: 1.0 * units.cm))

    crankshaft c0(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: flywheel_mass * units.kg,
        mass: crank_mass * units.kg,
        friction_torque: 54.2 * units.Nm,
        moment_of_inertia: crank_moment + flywheel_moment + other_moment,
        tdc: 0.0
    )

    ignition_module ignition(timing_curve: timing_curve, rev_limit: 2500 * units.rpm, limiter_duration: 0.001)

    rods r(rj0, rj1, rj2, rj3)

    bank_builder bank0(
        incline: vee / 2.0,
        chamber_volume: 544.485, // NW
        rods: r0(r),
        phasing: 0.0 * units.deg,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust0
    )

    bank_builder bank1(
        incline: -vee / 2.0,
        chamber_volume: 524.756, // NE M
        rods: r1(r),
        phasing: vee + rot360,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust2
    )

    bank_builder bank2(
        incline: -vee / 2.0 - rot90,
        chamber_volume: 544.485, // SE
        rods: r2(r),
        phasing: vee + rot90,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust3
    )

    bank_builder bank3(
        incline: vee / 2.0 + rot90,
        chamber_volume: 524.756, // SW
        rods: r3(r),
        phasing: -rot90 + rot360,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust1
    )
    
    rod_journal rj0(angle: rot * 8 + rot90 + vee / 2.0)
    rod_journal rj1(angle: rot * 4 + rot90 + vee / 2.0)
    rod_journal rj2(angle: rot * 12 + rot90 + vee / 2.0)
    rod_journal rj3(angle: rot * 0 + rot90 + vee / 2.0)
    
    c0
        .add_rod_journal(rj0)
        .add_rod_journal(rj1)
        .add_rod_journal(rj2)
        .add_rod_journal(rj3)

    
    rod_journal sj0(angle: -(1.0 / cyl) * rot360)
    rod_journal sj1(angle: -(2.0 / cyl) * rot360)

    label collector_cross_section_area(90.0)

    label exhaust_pipe_length_0(50.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 1000.0) // Litres

    intake intake(
        plenum_volume: 10.8 * units.L,
        plenum_cross_section_area: 161.2 * units.cm2,
        runner_length: 23.7 * units.cm,
        intake_flow_rate: k_carb(1263.6),
        idle_flow_rate: k_carb(0.03),
        idle_throttle_plate_position: 0.996,
        runner_flow_rate: k_carb(158.0),
        velocity_decay: 0.05
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(526.5),
        collector_cross_section_area: 90.0 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 63.8 * units.cm,
        primary_flow_rate: k_carb(263.3),
        velocity_decay: 0.7,
        volume: exhaust_volume_0 * units.L
    )

    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_45.wav", volume: 0.0012))
    exhaust_system exhaust1(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_45.wav", volume: 0.0012))
    exhaust_system exhaust2(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_58.wav", volume: 0.0010))
    exhaust_system exhaust3(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_58.wav", volume: 0.0010))
    engine
        .add_crankshaft(c0)
        .add_cylinder_bank(bank0.b0)
        .add_cylinder_bank(bank1.b0)
        .add_cylinder_bank(bank2.b0)
        .add_cylinder_bank(bank3.b0)
        .add_ignition_module(ignition)

    function timing_curve(140.0 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 2.0 * units.deg)
        .add_sample(140.0 * units.rpm, 2.0 * units.deg)
        .add_sample(280.0 * units.rpm, 2.0 * units.deg)
        .add_sample(420.0 * units.rpm, 2.8 * units.deg)
        .add_sample(560.0 * units.rpm, 7.6 * units.deg)
        .add_sample(700.0 * units.rpm, 11.5 * units.deg)
        .add_sample(840.0 * units.rpm, 14.6 * units.deg)
        .add_sample(980.0 * units.rpm, 17.2 * units.deg)
        .add_sample(1120.0 * units.rpm, 19.2 * units.deg)
        .add_sample(1260.0 * units.rpm, 20.9 * units.deg)
        .add_sample(1400.0 * units.rpm, 22.2 * units.deg)
        .add_sample(1540.0 * units.rpm, 23.3 * units.deg)
        .add_sample(1680.0 * units.rpm, 24.2 * units.deg)
        .add_sample(1820.0 * units.rpm, 24.9 * units.deg)
        .add_sample(1960.0 * units.rpm, 25.5 * units.deg)
        .add_sample(2100.0 * units.rpm, 26.0 * units.deg)
        .add_sample(2240.0 * units.rpm, -12.5 * units.deg)
        .add_sample(2380.0 * units.rpm, -25.0 * units.deg)
}

// Pz V
public node veh {
    alias output __out: vehicle;

    label car_mass(45000) // 45.0 t panther

    vehicle vehicle(
        mass: car_mass * units.kg,
        drag_coefficient: 0.6,
        cross_sectional_area: (2830 * units.mm) * (2680 * units.mm),
        diff_ratio: 2.5 * 3,
        tire_radius: (821.77 / 2) * units.mm,
        rolling_resistance: 0.06 * car_mass * 9.81
        )
}

// Pz V
private node trn {
    alias output __out:
        transmission(
            max_clutch_torque: 4500 * units.Nm,
            max_clutch_flex: 15 * units.deg,
            limit_clutch_flex: true,
            clutch_stiffness: 50 * units.Nm / units.deg,
            clutch_damping: 1.0,
            simulate_flex: true
        )
        .add_gear(10.8)  // 3.7 @ 2600
        .add_gear(5.26)  // 7.6
        .add_gear(3.33)  // 12.0
        .add_gear(2.13)  // 18.8
        .add_gear(1.48)  // 27.0
        .add_gear(1.06)  // 37.7
        .add_gear(0.85); // 47.3
}

run(
    engine: eng(),
    transmission: trn(),
    vehicle: veh()
)

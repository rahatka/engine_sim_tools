units units()
constants constants()

label bore(120.0)
label stroke(180.0)
label compression_ratio(6.0)
label con_rod(300.0) // ~
label L1(236.0) // ~
label R1(66.0) // ~
label psi(40.0 * units.deg)
label compression_height(56.0 * units.mm) // ~
label intake_valve_diameter(61.2) // 2.41 W12
label exhaust_valve_diameter(60.0) // 2.36 W12
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(2200.0)

label cyl(18.0)
label row(3.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label vee(rot)

// Lorraine-Dietrich 18Kd
label IVL(8.71) // .343
label EVL(8.71) // ?
label IVO(-2.0 * units.deg) //BTDC
label IVC(50.0 * units.deg) //ABDC
label EVO(50.0 * units.deg) //BBDC
label EVC(04.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(3500.0) // ?
label slave_rod_mass(600)
label piston_mass(1000.0) // ?
label crank_mass(65.0) // ?
label flywheel_mass(20.0) // kg

label exhaust_delay_coeff(1.5)


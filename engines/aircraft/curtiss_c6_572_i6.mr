units units()
constants constants()

label bore(114.3) // 4 1/2
label stroke(152.4) // 6
label compression_ratio(5.4)
label con_rod(254.0) // 10
label compression_height(46.0 * units.mm) // ~
label intake_valve_diameter(41.275) // 1 5/8
label exhaust_valve_diameter(41.275) // 1 5/8
label intake_valves(2.0)
label exhaust_valves(2.0)

label redline(2000.0)

label cyl(6.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// C-6
label IVL(10.3187) // 0.406 ?
label EVL(10.3187) // 0.406 ?
label IVO(15.0 * units.deg) //BTDC
label IVC(26.0 * units.deg) //ABDC
label EVO(50.0 * units.deg) //BBDC
label EVC(05.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(1542.21) // 3.4 p
label piston_mass(1043.26) // 2.3 p
label crank_mass(26.5) // 58.5 p
label flywheel_mass(8.0) // kg

label exhaust_delay_coeff(1.5)


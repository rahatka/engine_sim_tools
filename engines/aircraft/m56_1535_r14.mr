label EVO(74.0 * units.deg) //BBDC
label EVC(25.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(1800.0) // ? g
label crank_mass(20.0) // ? kg
label flywheel_mass(20.0) // kg

label exhaust_delay_coeff(1.5)

public node eng_distributor {
    input wires;
    input timing_curve;
    input rev_limit: 3500 * units.rpm;
    input limiter_duration: 0.02;
    alias output __out:
        ignition_module(timing_curve: timing_curve, rev_limit: rev_limit, limiter_duration: limiter_duration)
            .connect_wire(wires.wire1, rot * 0)
            .connect_wire(wires.wire3, rot * 2 + 4.2 * units.deg)
            .connect_wire(wires.wire5, rot * 4 - 2.5 * units.deg)
            .connect_wire(wires.wire7, rot * 6 - 1.1 * units.deg)
            .connect_wire(wires.wire2, rot * 8 + 1.1 * units.deg)
            .connect_wire(wires.wire4, rot * 10 + 2.5 * units.deg)
            .connect_wire(wires.wire6, rot * 12 - 4.2 * units.deg)

            .connect_wire(wires.wire8, rot * 0 + rot360)
            .connect_wire(wires.wire10, rot * 2 + rot360 + 4.2 * units.deg)
            .connect_wire(wires.wire12, rot * 4 + rot360 - 2.5 * units.deg)
            .connect_wire(wires.wire14, rot * 6 + rot360 - 1.1 * units.deg)
            .connect_wire(wires.wire9, rot * 8 + rot360 + 1.1 * units.deg)
            .connect_wire(wires.wire11, rot * 10 + rot360 + 2.5 * units.deg)
            .connect_wire(wires.wire13, rot * 12 + rot360 - 4.2 * units.deg);
}


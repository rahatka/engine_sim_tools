units units()
constants constants()

label bore(170.0)
label stroke(210.0)
label compression_ratio(4.85)
label con_rod(365.0) // ~
label compression_height(62.0 * units.mm) // ~
label intake_valve_diameter(71.0) // 2.61 ; 71 mm Aviation Week 1920-01-01: Vol 7 Iss 11 ; 66.5 clear
label exhaust_valve_diameter(71.0) // 2.61 ; 71 mm Aviation Week 1920-01-01: Vol 7 Iss 11 ; 66.5 clear
label intake_valves(2.0)
label exhaust_valves(2.0)

label redline(1800.0) // RPM

label cyl(12.0)
label row(2.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label vee(60.0 * units.deg)

label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)

// Fiat A.14
label IVL(11.91) // .469
label EVL(11.23) // .442
label IVO(-8.0 * units.deg) //BTDC
label IVC(45.0 * units.deg) //ABDC
label EVO(45.0 * units.deg) //BBDC 42 Airplane Engine Encyclopedia
label EVC(5.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(2000.0) // ? g
label piston_mass(2950.0) // 6 1/2 lb
label crank_mass(96.1) // 212 lb
label flywheel_mass(30.0) // kg

label exhaust_delay_coeff(1.5)


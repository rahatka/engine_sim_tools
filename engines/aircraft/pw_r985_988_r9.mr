import "engine_sim.mr"
import "../../fuel/30.mr"

units units()
constants constants()

label bore(131.762) // 5 3/16
label stroke(131.762) // 5 3/16
label compression_ratio(6.0)
label con_rod(296.862) // ~ 11 11/16
label L1(228.36) // L / 1.3 ?
label R1(69.7) // ?
label compression_height(bore / 2.5 * units.mm) // ?
label intake_valve_diameter(56.2102) // 2.213 ; 1.963 seat
label exhaust_valve_diameter(56.2102) // 2.213 ; 1.963 seat
label intake_valves(1.0)
label exhaust_valves(1.0)

label cyl(9.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label rot(cycle / cyl)

label redline(2500.0) // RPM

// R-985
label IVL(11.9063) // 15/32
label EVL(14.2875) // 9/16
label IVO(26.0 * units.deg) //BTDC
label IVC(76.0 * units.deg) //ABDC
label EVO(71.0 * units.deg) //BBDC
label EVC(31.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(1800.0) // ? g
label crank_mass(20.0) // ? kg
label flywheel_mass(15.0) // kg

label exhaust_delay_coeff(1.5)

public node eng_distributor {
    input wires;
    input timing_curve;
    input rev_limit: 3000 * units.rpm;
    input limiter_duration: 0.0001;
    alias output __out:
        ignition_module(timing_curve: timing_curve, rev_limit: rev_limit, limiter_duration: limiter_duration)
            .connect_wire(wires.wire1, rot * 0)
            .connect_wire(wires.wire3, rot * 1 + 2.6 * units.deg)
            .connect_wire(wires.wire5, rot * 2 + 1.8 * units.deg)
            .connect_wire(wires.wire7, rot * 3 - 3.8 * units.deg)
            .connect_wire(wires.wire9, rot * 4 - 0.5 * units.deg)
            .connect_wire(wires.wire2, rot * 5 + 0.5 * units.deg)
            .connect_wire(wires.wire4, rot * 6 + 3.8 * units.deg)
            .connect_wire(wires.wire6, rot * 7 - 1.8 * units.deg)
            .connect_wire(wires.wire8, rot * 8 - 2.6 * units.deg);
}

private node wires {
    output wire1: ignition_wire();
    output wire2: ignition_wire();
    output wire3: ignition_wire();
    output wire4: ignition_wire();
    output wire5: ignition_wire();
    output wire6: ignition_wire();
    output wire7: ignition_wire();
    output wire8: ignition_wire();
    output wire9: ignition_wire();
}


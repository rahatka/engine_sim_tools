units units()
constants constants()

label bore(140.0)
label stroke(180.0)
label compression_ratio(5.1)
label con_rod(325.0) // 12.795
label compression_height(71.0 * units.mm) // 2.795
label intake_valve_diameter(65.126) // 2.564 ; 2.375 clear
label exhaust_valve_diameter(65.126) // 2.564; 2.375 clear
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(1800.0)

label cyl(6.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// Isotta Fraschini V.6
label IVL(11.0) // 0.433
label EVL(12.5) // 0.4921
label IVO(-8.0 * units.deg) //BTDC
label IVC(48.0 * units.deg) //ABDC
label EVO(40.0 * units.deg) //BBDC
label EVC(14.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(2585.48) // 5.7
label piston_mass(2404.04) // 5.3
label crank_mass(55.34) // 122
label flywheel_mass(15.0) // kg

label exhaust_delay_coeff(1.5)


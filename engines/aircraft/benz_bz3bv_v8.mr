private node add_lobes {
    input base;
    input shift: 0.0;
    input var;
    input this;
    alias output __out: this;

    this.add_lobe(base + var.s1 * rot_base + shift)
    this.add_lobe(base + var.s2 * rot_base + shift)
    this.add_lobe(base + var.s3 * rot_base + shift)
    this.add_lobe(base + var.s4 * rot_base + shift)
}

public node bank_builder {
    input var;
    input crankshaft;
    input incline;
    input phasing;
    input ignition;
    input intake;
    input exhaust;
    input primaries;
    input sh;
    input flip: false;

    output bank: _b0;

    ignition_wire _wire1()
    ignition_wire _wire2()
    ignition_wire _wire3()
    ignition_wire _wire4()

    piston_parameters piston_params(
        mass: piston_mass * units.g,
        blowby: k_28inH2O(0.042),
        compression_height: compression_height,
        wrist_pin_position: 0.0,
        displacement: 0.0
    )

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    connecting_rod_parameters cr_params(
        mass: con_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.g,
            length: con_rod * units.mm
        ),
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    camshaft _intake_cam_0(base_radius: 20.0 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 20.0 * units.mm, lobe_profile: exhaust_lobe_profile)

    _intake_cam_0.add_lobes(ILC, phasing, var)
    _exhaust_cam_0.add_lobes(ELC, phasing, var)

    ignition.connect_wire(_wire1, rot_base * var.s1 + phasing)
    ignition.connect_wire(_wire2, rot_base * var.s2 + phasing)
    ignition.connect_wire(_wire3, rot_base * var.s3 + phasing)
    ignition.connect_wire(_wire4, rot_base * var.s4 + phasing)

    cylinder_bank _b0(bank_params, angle: incline)

    _b0
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj0,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: primaries.pl0 + sh,
            ignition_wire: _wire1
        )
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj1,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: primaries.pl1 + sh,
            ignition_wire: _wire2
        )
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj2,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: primaries.pl2 + sh,
            ignition_wire: _wire3
        )
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj3,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: primaries.pl3 + sh,
            ignition_wire: _wire4
        )

    _b0.set_cylinder_head(
        head(
            intake_camshaft: _intake_cam_0,
            exhaust_camshaft: _exhaust_cam_0,
            flip_display: flip
        )
    )
}

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node head {
    // port_flow.py v1.5
    // intake port area: 21.4 cm²; saturated lift: 12.72 mm
    // exhaust port area: 19.1 cm²; saturated lift: 8.48 mm
    // cylinder volume: 1932.4 cm³ (117.9 CI); engine volume: 15.459 L (943.4 CI)
    // chamber volume: 411.1 cm³ (25.1 CI)
    // 32 harmonic intake runner length: 22.4 cm; diameter: 4.8 cm
    // primary length: 78.5 cm, area: 20.0 cm², diameter: 5.0 cm
    // collector diameter: 14.3 cm, area: 160.1 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 411.143 * units.cc,
        intake_runner_volume: 402.1 * units.cc,
        intake_runner_cross_section_area: 17.9 * units.cm2,
        exhaust_runner_volume: 134.0 * units.cc,
        exhaust_runner_cross_section_area: 20.0 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

public node crank {
    input phasing;
    input var;

    output c: _c;
    output rj0: _rj0;
    output rj1: _rj1;
    output rj2: _rj2;
    output rj3: _rj3;

    label crank_moment(disk_moment_of_inertia(mass: crank_mass * units.kg, radius: (stroke / 2.0) * units.mm))
    label flywheel_moment(disk_moment_of_inertia(mass: flywheel_mass * units.kg, radius: (stroke * 3.5) * units.mm))
    label other_moment(disk_moment_of_inertia(mass: 1 * units.kg, radius: 1.0 * units.cm))

    crankshaft _c(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: flywheel_mass * units.kg,
        mass: crank_mass * units.kg,
        friction_torque: 23.2 * units.Nm,
        moment_of_inertia: crank_moment + flywheel_moment + other_moment,
        tdc: 0.0
    )

    rod_journal _rj0(angle: rot_base * var.s1 + rot90 + vee / 2)
    rod_journal _rj1(angle: rot_base * var.s2 + rot90 + vee / 2)
    rod_journal _rj2(angle: rot_base * var.s3 + rot90 + vee / 2)
    rod_journal _rj3(angle: rot_base * var.s4 + rot90 + vee / 2)

    _c
        .add_rod_journal(_rj0)
        .add_rod_journal(_rj1)
        .add_rod_journal(_rj2)
        .add_rod_journal(_rj3)
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "Benz BZ 3bV",
        starter_torque: 250 * units.Nm,
        starter_speed: 100 * units.rpm,
        redline: redline * units.rpm,
        fuel: a10(),
        throttle_gamma: 1.5,
        jitter: 0.7,
        noise: 0.0,
        hf_gain: 0.009,
        simulation_frequency: 7500,
        dyno_min_speed: 750 * units.rpm
    )

    variant_0 variant()

    crank c0(vee / 2 + rot90, variant)

    bank_builder bank0(
        var: variant,
        crankshaft: c0,
        incline: vee / 2,
        phasing: 0,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust0,
        primaries: em_0(),
        sh: 0.0 * units.cm,
        flip: true
    )

    bank_builder bank1(
        var: variant,
        crankshaft: c0,
        incline: -vee / 2,
        phasing: vee + rot360,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust1,
        sh: 0.0 * units.cm,
        primaries: em_0()
    )

    ignition_module ignition(timing_curve: timing_curve, rev_limit: redline * 1.05 * units.rpm, limiter_duration: 0.06)

    label collector_cross_section_area(80.0)

    label exhaust_pipe_length_0(100.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 100.0) // Litres

    intake intake(
        plenum_volume: 4.6 * units.L,
        plenum_cross_section_area: 71.8 * units.cm2,
        runner_length: 22.4 * units.cm,
        intake_flow_rate: k_carb(541.1),
        idle_flow_rate: k_carb(0.01),
        idle_throttle_plate_position: 0.995,
        runner_flow_rate: k_carb(135.3),
        velocity_decay: 0.03
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(450.9),
        collector_cross_section_area: 80.0 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 78.5 * units.cm,
        primary_flow_rate: k_carb(225.4),
        velocity_decay: 0.7,
        volume: exhaust_volume_0 * units.L
    )
    
    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_42.wav", volume: 0.0015))
    exhaust_system exhaust1(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_58.wav", volume: 0.0010))

    engine
        .add_crankshaft(c0.c)
        .add_cylinder_bank(bank0.bank)
        .add_cylinder_bank(bank1.bank)
        .add_ignition_module(ignition)

    function timing_curve(140.0 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 0.0 * units.deg)
        .add_sample(140.0 * units.rpm, 0.0 * units.deg)
        .add_sample(280.0 * units.rpm, 5.3 * units.deg)
        .add_sample(420.0 * units.rpm, 10.7 * units.deg)
        .add_sample(560.0 * units.rpm, 14.2 * units.deg)
        .add_sample(700.0 * units.rpm, 16.5 * units.deg)
        .add_sample(840.0 * units.rpm, 18.1 * units.deg)
        .add_sample(980.0 * units.rpm, 19.1 * units.deg)
        .add_sample(1120.0 * units.rpm, 19.7 * units.deg)
        .add_sample(1260.0 * units.rpm, 20.0 * units.deg)
        .add_sample(1400.0 * units.rpm, 20.0 * units.deg)
        .add_sample(1540.0 * units.rpm, 20.0 * units.deg)
        .add_sample(1680.0 * units.rpm, 20.0 * units.deg)
        .add_sample(1820.0 * units.rpm, 20.0 * units.deg)
        .add_sample(1960.0 * units.rpm, 20.0 * units.deg)
        .add_sample(2100.0 * units.rpm, 20.0 * units.deg)
        .add_sample(2240.0 * units.rpm, -15.0 * units.deg)
        .add_sample(2380.0 * units.rpm, -45.0 * units.deg)
}

label spacing_factor(1.1)
label flange_density(1.0 * exhaust_delay_coeff)
label em_unit(bore * spacing_factor / flange_density)

private node em_0{
    output pl0: (4.0 * em_unit) * units.mm;
    output pl1: (3.0 * em_unit) * units.mm;
    output pl2: (2.0 * em_unit) * units.mm;
    output pl3: (1.0 * em_unit) * units.mm;
}

// 1988 750iL
public node veh {
    alias output __out: vehicle;

    label car_mass(1960) // 1860 empty

    vehicle vehicle(
        mass: car_mass * units.kg,
        drag_coefficient: 0.34,
        cross_sectional_area: (1845  * units.mm) * (1400 * units.mm),
        diff_ratio: 3.15,
        tire_radius: (653.0 / 2) * units.mm, // 225/60 VR 15
        rolling_resistance: 0.012 * car_mass * 9.81
        )
}

// ZF 4HP24E
private node trn {
    alias output __out:
        transmission(
            max_clutch_torque: 675 * units.Nm,
            max_clutch_flex: 30 * units.deg,
            limit_clutch_flex: true,
            clutch_stiffness: 20 * units.Nm / units.deg,
            clutch_damping: 1.0,
            simulate_flex: true
        )
        .add_gear(2.479)
        .add_gear(1.479)
        .add_gear(1.000)
        .add_gear(0.728);
}

run(
    engine: eng(),
    transmission: trn(),
    vehicle: veh()
)

import "engine_sim.mr"
import "_fuel_10.mr"

units units()
constants constants()

label bore(135.0)
label stroke(135.0)
label compression_ratio(5.7) // 6.02 also
label con_rod(250.0) // ?
label compression_height(50.0 * units.mm) // ?
label intake_valve_diameter(60.0) // ?
label exhaust_valve_diameter(40.0) // ?
label intake_valves(1.0)
label exhaust_valves(2.0)

label redline(2100.0) // RPM

label cyl(8.0)
label row(2.0)
label cycle(720.0 * units.deg)
label vee(90.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)

// BZ 3bV
label IVL(11.7) // .462
label EVL(10.1) // .397
label IVO(3.0 * units.deg) //BTDC
label IVC(60.0 * units.deg) //ABDC
label EVO(62.0 * units.deg) //BBDC
label EVC(30.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(1564.894) // ?
label piston_mass(1905.09) // ?
label crank_mass(29.4) // ?
label flywheel_mass(20.0) // kg

label exhaust_delay_coeff(1.5)


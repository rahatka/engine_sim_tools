import "engine_sim.mr"
import "../../fuel/10.mr"

units units()
constants constants()

label bore(130.0)
label stroke(180.0)
label compression_ratio(4.7)
label con_rod(315.0) // ~
label compression_height(60.0 * units.mm) // ~
label intake_valve_diameter(61.5) // 2.42
label exhaust_valve_diameter(61.5) // 2.42
label intake_valves(1.0)
label exhaust_valves(1.0)
label offset(22.5)

label redline(1600.0)

label cyl(6.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// Benz Bz III
label IVL(11.8) // .465
label EVL(11.25) // .443
label IVO(0.0 * units.deg) //BTDC
label IVC(60.0 * units.deg) //ABDC
label EVO(60.0 * units.deg) //BBDC
label EVC(16.3 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(1500.0) // ?
label piston_mass(1000.0) // ?
label crank_mass(50.0) // ?
label flywheel_mass(15.0) // kg

label exhaust_delay_coeff(1.5)


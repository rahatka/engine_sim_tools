import "engine_sim.mr"
import "../../fuel/10.mr"

units units()
constants constants()

label bore(100.0)
label stroke(140.0)
label compression_ratio(4.3) // 4.7 RAF 4D
label con_rod(280.0) // ?
label compression_height(57.2 * units.mm) // ?
label intake_valve_diameter(44.0) // 1.73 ; 1.69 RAF 4D
label exhaust_valve_diameter(40.0) // 1.57
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(2000.0) // RPM

label cyl(12.0)
label row(2.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label vee(60.0 * units.deg)

label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)

// RAF 4A
label IVL(8.8) // .346 ; .394 RAF 4D
label EVL(6.5) // .256 ; .346 RAF 4D
label IVO(-12.3 * units.deg) //BTDC
label IVC(43.3 * units.deg) //ABDC
label EVO(61.0 * units.deg) //BBDC
label EVC(17.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(1500.0) // ? g
label piston_mass(800.0) // ?
label crank_mass(50.0) // ? kg
label flywheel_mass(20.0) // kg

label exhaust_delay_coeff(1.5)


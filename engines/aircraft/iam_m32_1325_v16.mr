import "engine_sim.mr"
import "../../fuel/30.mr"

units units()
constants constants()

label bore(120.0)
label stroke(120.0)
label compression_ratio(6.0)
label con_rod(200.0) // ~
label compression_height(41.0 * units.mm) // ~
label intake_valve_diameter(50.0) // ?
label exhaust_valve_diameter(50.0) // ?
label intake_valves(1.0)
label exhaust_valves(1.0)

label cyl(16.0)
label row(2.0)
label cycle(720.0 * units.deg)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label vee(60.0 * units.deg)

label redline(3500.0) // RPM

label IVL(12.0) // ?
label EVL(12.0) // ?
label IVO(20.0 * units.deg) //BTDC
label IVC(50.0 * units.deg) //ABDC
label EVO(50.0 * units.deg) //BBDC
label EVC(20.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(1500.0) // ? g
label crank_mass(58.0) // ? kg
label flywheel_mass(20.0) // kg

label exhaust_delay_coeff(1.5)


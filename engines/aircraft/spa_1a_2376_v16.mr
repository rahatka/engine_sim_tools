import "engine_sim.mr"
import "../../fuel/10.mr"

units units()
constants constants()

label bore(135.0)
label stroke(170.0)
label compression_ratio(5.19)
label con_rod(300.0) // 10.435 + 35mm
label compression_height(43.434 * units.mm) // 1.71
label intake_valve_diameter(58.7) // 2.312 ; 2.1875 clear
label exhaust_valve_diameter(58.7) // 2.312 ; 2.1875 clear
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(1900.0)

label cyl(16.0)
label row(2.0)
label cyl_base(cyl / row)
label cycle(720.0 * units.deg)
label vee(45.0 * units.deg)
label rot_base(cycle / cyl_base * 2)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// S.P.A. 6-A
label IVL(12.0) // .472
label EVL(12.0) // .472
label IVO(-8.0 * units.deg) //BTDC
label IVC(50.0 * units.deg) //ABDC
label EVO(62.0 * units.deg) //BBDC
label EVC(14.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(19057.17 / cyl) // ? / 6
label piston_mass(11475.89 / cyl) // 25.3 / 6
label crank_mass(44.0 * 1.33)
label flywheel_mass(15.0) // kg

label exhaust_delay_coeff(1.5)

label sb_rot(rot90)


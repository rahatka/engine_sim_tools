import "engine_sim.mr"
import "../../fuel/30.mr"

units units()
constants constants()

label bore(133.35) // 5 1/4
label stroke(127.0) // 5
label compression_ratio(7.0)
label con_rod(228.6) // ? 9
label separation((350.0 / 2.0) * units.mm) // ?
label compression_height(50.0 * units.mm) // ~
label intake_valve_diameter(63.5) // ? 2 1/2
label exhaust_valve_diameter(63.5) // ? 2 1/2
label intake_valves(1.0) // ?
label exhaust_valves(1.0) // ?

label redline(3000.0) // RPM

label cyl(24.0)
label cyl_base(cyl / 2.0)
label row(2.0)
label cycle(720.0 * units.deg)
label vee(180.0 * units.deg)
label rot_base(2 * (360.0 / cyl_base) * units.deg)
label rot45(45.0 * units.deg)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label sb_shift(rot_base / row * 13) // 390 deg phasing

// P&W R-1535
label IVL(15.0) // ?
label EVL(15.0) // ?
label IVO(20.0 * units.deg) //BTDC
label IVC(76.0 * units.deg) //ABDC
label EVO(76.0 * units.deg) //BBDC
label EVC(20.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(1200.0) // ?  g
label crank_mass(70.0) // ? kg
label flywheel_mass(10.0) // kg

label exhaust_delay_coeff(1.5)


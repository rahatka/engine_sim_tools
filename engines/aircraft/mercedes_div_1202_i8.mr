import "../../fuel/10.mr"

units units()
constants constants()

label bore(140.0)
label stroke(160.0)
label compression_ratio(4.73)
label con_rod(291.95)
label compression_height(57.5 * units.mm)
label intake_valve_diameter(67.818) // 2.67
label exhaust_valve_diameter(67.818) // 2.67
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(1800.0)

label cyl(8.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// Mercedes D.IV
label IVL(11.5) // .45
label EVL(10.5) // .412
label IVO(-2.0 * units.deg) //BTDC
label IVC(51.0 * units.deg) //ABDC
label EVO(52.0 * units.deg) //BBDC
label EVC(16.3 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(1200.0) // ?
label piston_mass(1000.0) // ?
label crank_mass(50.0) // ?
label flywheel_mass(15.0) // kg

label exhaust_delay_coeff(1.5)


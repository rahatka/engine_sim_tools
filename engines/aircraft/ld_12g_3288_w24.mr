public node rods {
    input crank;
    output cr0_0: sr0_0; output cr1_0: sr1_0; output cr2_0: sr2_0; output cr3_0: sr3_0; output cr4_0: sr4_0; output cr5_0: sr5_0; output cr6_0: sr6_0; output cr7_0: sr7_0;
    output rj0_0: sj0_0; output rj1_0: sj1_0; output rj2_0: sj2_0; output rj3_0: sj3_0; output rj4_0: sj4_0; output rj5_0: sj5_0; output rj6_0: sj6_0; output rj7_0: sj7_0;
    output cr0_1: mcr0; output cr1_1: mcr1; output cr2_1: mcr2; output cr3_1: mcr3; output cr4_1: mcr4; output cr5_1: mcr5; output cr6_1: mcr6; output cr7_1: mcr7;
    output rj0_1: crank.rj0; output rj1_1: crank.rj1; output rj2_1: crank.rj2; output rj3_1: crank.rj3; output rj4_1: crank.rj4; output rj5_1: crank.rj5; output rj6_1: crank.rj6; output rj7_1: crank.rj7;
    output cr0_2: sr0_1; output cr1_2: sr1_1; output cr2_2: sr2_1; output cr3_2: sr3_1; output cr4_2: sr4_1; output cr5_2: sr5_1; output cr6_2: sr6_1; output cr7_2: sr7_1;
    output rj0_2: sj0_1; output rj1_2: sj1_1; output rj2_2: sj2_1; output rj3_2: sj3_1; output rj4_2: sj4_1; output rj5_2: sj5_1; output rj6_2: sj6_1; output rj7_2: sj7_1;

    connecting_rod_parameters master_rod_params(
        mass: con_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.g,
            length: con_rod * units.mm
        ),
        slave_throw: R1 * units.mm,
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    label slave_rod_mass(850)
    connecting_rod_parameters slave_rod_params(
        mass: slave_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: slave_rod_mass * units.g,
            length: L1 * units.mm
        ),
        center_of_mass: 0.0,
        length: L1 * units.mm
    )

    connecting_rod mcr0(master_rod_params)
    connecting_rod mcr1(master_rod_params)
    connecting_rod mcr2(master_rod_params)
    connecting_rod mcr3(master_rod_params)
    connecting_rod mcr4(master_rod_params)
    connecting_rod mcr5(master_rod_params)
    connecting_rod mcr6(master_rod_params)
    connecting_rod mcr7(master_rod_params)

    connecting_rod sr0_0(slave_rod_params)
    connecting_rod sr1_0(slave_rod_params)
    connecting_rod sr2_0(slave_rod_params)
    connecting_rod sr3_0(slave_rod_params)
    connecting_rod sr4_0(slave_rod_params)
    connecting_rod sr5_0(slave_rod_params)
    connecting_rod sr6_0(slave_rod_params)
    connecting_rod sr7_0(slave_rod_params)

    connecting_rod sr0_1(slave_rod_params)
    connecting_rod sr1_1(slave_rod_params)
    connecting_rod sr2_1(slave_rod_params)
    connecting_rod sr3_1(slave_rod_params)
    connecting_rod sr4_1(slave_rod_params)
    connecting_rod sr5_1(slave_rod_params)
    connecting_rod sr6_1(slave_rod_params)
    connecting_rod sr7_1(slave_rod_params)

    rod_journal sj0_0(angle: psi)
    rod_journal sj1_0(angle: psi)
    rod_journal sj2_0(angle: psi)
    rod_journal sj3_0(angle: psi)
    rod_journal sj4_0(angle: psi)
    rod_journal sj5_0(angle: psi)
    rod_journal sj6_0(angle: psi)
    rod_journal sj7_0(angle: psi)

    rod_journal sj0_1(angle: -psi)
    rod_journal sj1_1(angle: -psi)
    rod_journal sj2_1(angle: -psi)
    rod_journal sj3_1(angle: -psi)
    rod_journal sj4_1(angle: -psi)
    rod_journal sj5_1(angle: -psi)
    rod_journal sj6_1(angle: -psi)
    rod_journal sj7_1(angle: -psi)

    mcr0.add_slave_journal(sj0_0)
    mcr1.add_slave_journal(sj1_0)
    mcr2.add_slave_journal(sj2_0)
    mcr3.add_slave_journal(sj3_0)
    mcr4.add_slave_journal(sj4_0)
    mcr5.add_slave_journal(sj5_0)
    mcr6.add_slave_journal(sj6_0)
    mcr7.add_slave_journal(sj7_0)

    mcr0.add_slave_journal(sj0_1)
    mcr1.add_slave_journal(sj1_1)
    mcr2.add_slave_journal(sj2_1)
    mcr3.add_slave_journal(sj3_1)
    mcr4.add_slave_journal(sj4_1)
    mcr5.add_slave_journal(sj5_1)
    mcr6.add_slave_journal(sj6_1)
    mcr7.add_slave_journal(sj7_1)
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "Lorraine-Dietrich 24",
        starter_torque: 700 * units.Nm,
        starter_speed: 100 * units.rpm,
        redline: redline * units.rpm,
        fuel: a10(),
        throttle_gamma: 2.0,
        jitter: 0.7,
        noise: 0.03,
        hf_gain: 0.005,
        simulation_frequency: 4500,
        dyno_min_speed: 500 * units.rpm,
        fluid_simulation_steps: 6,
        max_sle_solver_steps: 64
    )

    ////////////////////////////////////////
    ///////// change variants here /////////
    ////////////////////////////////////////
    variant_0 variant()

    bank_builder bank0(
        var: variant,
        incline: vee,
        phasing: 0,
        rods: r0(r),
        ignition: ignition,
        intake: intake,
        exhaust: exhaust0,
        flip: true
    )

    bank_builder bank1(
        var: variant,
        incline: 0,
        phasing: vee + rot360,
        rods: r1(r),
        ignition: ignition,
        intake: intake,
        exhaust: exhaust1,
        flip: true
    )

    bank_builder bank2(
        var: variant,
        incline: -vee,
        phasing: vee * 2,
        rods: r2(r),
        ignition: ignition,
        intake: intake,
        exhaust: exhaust2
    )

    crank c0(variant, vee + rot90)
    rods r(c0)

    ignition_module ignition(timing_curve: timing_curve, rev_limit: 7250 * units.rpm, limiter_duration: 0.03)

    label collector_cross_section_area(239.0)

    label exhaust_pipe_length_0(50.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 100.0) // Litres

    intake intake(
        plenum_volume: 14.2 * units.L,
        plenum_cross_section_area: 216.1 * units.cm2,
        runner_length: 28.8 * units.cm,
        intake_flow_rate: k_carb(1876.3),
        idle_flow_rate: k_carb(0.07),
        idle_throttle_plate_position: 0.996,
        runner_flow_rate: k_carb(172.0),
        velocity_decay: 0.05
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(1094.5),
        collector_cross_section_area: 239.0 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 80.6 * units.cm,
        primary_flow_rate: k_carb(287.3),
        velocity_decay: 0.5,
        volume: exhaust_volume_0 * units.L
    )

    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_45.wav", volume: 0.0012))
    exhaust_system exhaust1(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_45.wav", volume: 0.0011))
    exhaust_system exhaust2(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_58.wav", volume: 0.0010))

    engine
        .add_cylinder_bank(bank0.b0)
        .add_cylinder_bank(bank1.b0)
        .add_cylinder_bank(bank2.b0)

        .add_crankshaft(c0.c)
        .add_ignition_module(ignition)

    function timing_curve(126.7 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 5.0 * units.deg)
        .add_sample(126.7 * units.rpm, 5.0 * units.deg)
        .add_sample(253.3 * units.rpm, 6.1 * units.deg)
        .add_sample(380.0 * units.rpm, 8.4 * units.deg)
        .add_sample(506.7 * units.rpm, 10.5 * units.deg)
        .add_sample(633.3 * units.rpm, 12.4 * units.deg)
        .add_sample(760.0 * units.rpm, 14.1 * units.deg)
        .add_sample(886.7 * units.rpm, 15.6 * units.deg)
        .add_sample(1013.3 * units.rpm, 16.9 * units.deg)
        .add_sample(1140.0 * units.rpm, 18.1 * units.deg)
        .add_sample(1266.7 * units.rpm, 19.2 * units.deg)
        .add_sample(1393.3 * units.rpm, 20.1 * units.deg)
        .add_sample(1520.0 * units.rpm, 21.0 * units.deg)
        .add_sample(1646.7 * units.rpm, 21.7 * units.deg)
        .add_sample(1773.3 * units.rpm, 22.4 * units.deg)
        .add_sample(1900.0 * units.rpm, 23.0 * units.deg)
        .add_sample(2026.7 * units.rpm, -15.0 * units.deg)
        .add_sample(2153.3 * units.rpm, -45.0 * units.deg)
}


label con_rod(310.0) // 12Ew
label L1(254.0)
label R1(58.0)
label psi(60.0 * units.deg)
label intake_valve_diameter(65.0)
label exhaust_valve_diameter(65.0)
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(1900.0) // RPM

label cyl(24.0)
label row(3.0)
label cyl_base(cyl / row)
label cycle(720.0 * units.deg)
label vee(60.0 * units.deg)
label rot_base(cycle / cyl_base * 2)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label sb_rot(rot360 + rot90)

// 12E
label IVL(13.5)
label EVL(13.5)
label IVO(-3.0 * units.deg) //BTDC
label IVC(52.0 * units.deg) //ABDC
label EVO(51.0 * units.deg) //BBDC
label EVC(6.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label piston_mass(1500.0) // ?
label con_rod_mass(2500.0)
label crank_mass(90.0) // kg
label flywheel_mass(30.0) // kg

label exhaust_delay_coeff(1.5)


import "../../fuel/10.mr"

units units()
constants constants()

label bore(120.0)
label stroke(130.0)
label compression_ratio(5.33)
label con_rod(225.0) // 8.858
label compression_height(44.85 * units.mm) // 1 49/64
label intake_valve_diameter(56.0) // 56 ? 50 clear
label exhaust_valve_diameter(56.0) // 56 ? 50 clear
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(2000.0) // RPM

label cyl(8.0)
label row(2.0)
label cycle(720.0 * units.deg)
label vee(90.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)

// Hispano-Suiza 8E
label IVL(10.0)
label EVL(10.0)
label IVO(-10.0 * units.deg) //BTDC
label IVC(50.0 * units.deg) //ABDC
label EVO(45.0 * units.deg) //BBDC
label EVC(10.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(1496.85) // 4.1 + 2.5 = 3.3
label piston_mass(1632.93) // 3.6
label crank_mass(24.35) // 53.7
label flywheel_mass(10.0) // kg

label exhaust_delay_coeff(1.5)


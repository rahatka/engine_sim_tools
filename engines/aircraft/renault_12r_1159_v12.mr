import "../../fuel/30.mr"

units units()
constants constants()

label bore(120.0)
label stroke(140.0)
label compression_ratio(6.4)
label con_rod(265.0)
label compression_height(45.0 * units.mm) // ~
label intake_valve_diameter(56.0) // 52 clear
label exhaust_valve_diameter(56.0) // 52 ?
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(2800.0) // RPM

label cyl(12.0)
label cyl_base(cyl / row)
label row(2.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label rot_base(cycle / cyl_base)
label vee(60.0 * units.deg)

// Renault 12R-01
label IVL(12.8)
label EVL(12.8)
label IVO(14.0 * units.deg) //BTDC
label IVC(64.0 * units.deg) //ABDC
label EVO(64.0 * units.deg) //BBDC
label EVC(14.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(1300.0) // ? g
label piston_mass(900.0) // ?
label crank_mass(40.0) // ?
label flywheel_mass(15.0) // kg

label exhaust_delay_coeff(1.5)


import "engine_sim.mr"
import "../../fuel/30.mr"

units units()
constants constants()

label bore(120.0)
label stroke(140.0)
label compression_ratio(5.9)
label con_rod(220.0) // ~
label compression_height(50.0 * units.mm) // ~
label intake_valve_diameter(57.0) // ?
label exhaust_valve_diameter(48.0) // ?
label intake_valves(1.0)
label exhaust_valves(1.0)

label cyl(8.0)
label row(2.0)
label cycle(720.0 * units.deg)
label vee(90.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

label redline(2500.0) // RPM

// Argus As 10
label IVL(12.0) // ?
label EVL(12.0) // ?
label IVO(56.0 * units.deg) //BTDC
label IVC(92.0 * units.deg) //ABDC
label EVO(88.0 * units.deg) //BBDC
label EVC(48.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180) 
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label exhaust_delay_coeff(1.5)

public node distributor_0 {
    input wires;
    input timing_curve;
    input limiter_duration: 0.0001;
    alias output __out:
        ignition_module(timing_curve: timing_curve, rev_limit: redline * 1.05, limiter_duration: limiter_duration)
            .connect_wire(wires.wire1, rot * 0)
            .connect_wire(wires.wire5, rot * 1)
            .connect_wire(wires.wire6, rot * 2)
            .connect_wire(wires.wire3, rot * 3)
            .connect_wire(wires.wire7, rot * 4)
            .connect_wire(wires.wire2, rot * 5)
            .connect_wire(wires.wire4, rot * 6)
            .connect_wire(wires.wire8, rot * 7);
}

private node wires {
    output wire1: ignition_wire();
    output wire2: ignition_wire();
    output wire3: ignition_wire();
    output wire4: ignition_wire();
    output wire5: ignition_wire();
    output wire6: ignition_wire();
    output wire7: ignition_wire();
    output wire8: ignition_wire();
}


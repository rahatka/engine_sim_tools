import "engine_sim.mr"
import "../../fuel/30.mr"

units units()
constants constants()

label bore(170.0)
label stroke(200.0)
label compression_ratio(5.2)
label con_rod(345.0)
label compression_height(62.0 * units.mm)
label intake_valve_diameter(69.7) // 62 clear
label exhaust_valve_diameter(69.7) // 62 clear
label intake_valves(2.0)
label exhaust_valves(2.0)

label redline(2000.0) // RPM

label cyl(12.0)
label row(2.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label vee(60.0 * units.deg)

label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)

// Fiat A.25
label IVL(16.0)
label EVL(16.0)
label IVO(10.0 * units.deg) //BTDC
label IVC(50.0 * units.deg) //ABDC
label EVO(60.0 * units.deg) //BBDC
label EVC(10.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(2000.0) // ? g
label piston_mass(2950.0) // ? lb
label crank_mass(96.1) // ? lb
label flywheel_mass(30.0) // kg

label exhaust_delay_coeff(1.5)


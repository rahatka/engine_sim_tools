            .connect_wire(bank0.wire6, rot * 12)
            .connect_wire(bank0.wire5, rot * 16)
            .connect_wire(bank0.wire3, rot * 20)

            .connect_wire(bank1.wire1, rot * 0 + vee + rot360)
            .connect_wire(bank1.wire2, rot * 4 + vee + rot360)
            .connect_wire(bank1.wire4, rot * 8 + vee + rot360)
            .connect_wire(bank1.wire6, rot * 12 + vee + rot360)
            .connect_wire(bank1.wire5, rot * 16 + vee + rot360)
            .connect_wire(bank1.wire3, rot * 20 + vee + rot360)
            
            .connect_wire(bank2.wire1, rot * 0 + vee * 2.0)
            .connect_wire(bank2.wire2, rot * 4 + vee * 2.0)
            .connect_wire(bank2.wire4, rot * 8 + vee * 2.0)
            .connect_wire(bank2.wire6, rot * 12 + vee * 2.0)
            .connect_wire(bank2.wire5, rot * 16 + vee * 2.0)
            .connect_wire(bank2.wire3, rot * 20 + vee * 2.0)
            
            .connect_wire(bank3.wire1, rot * 0 + vee * 3.0 + rot360)
            .connect_wire(bank3.wire2, rot * 4 + vee * 3.0 + rot360)
            .connect_wire(bank3.wire4, rot * 8 + vee * 3.0 + rot360)
            .connect_wire(bank3.wire6, rot * 12 + vee * 3.0 + rot360)
            .connect_wire(bank3.wire5, rot * 16 + vee * 3.0 + rot360)
            .connect_wire(bank3.wire3, rot * 20 + vee * 3.0 + rot360);
}

public node head {
    // port_flow.py v1.7
    // intake port area: 44.0 cm²; saturated lift: 12.89 mm
    // exhaust port area: 34.3 cm²; saturated lift: 11.39 mm
    // cylinder volume: 1932.4 cm³ (117.92 CI); engine volume: 46.377 L (2830.10 CI)
    // chamber volume: 322.1 cm³ (19.65 CI); conrod/stroke 1.630; N.A.C.C. 271.19
    // 16 harmonic intake runner length: 27.0 cm; diameter: 5.7 cm
    // primary length: 40.0 cm, area: 34.3 cm², diameter: 6.6 cm
    // collector diameter: 32.4 cm, area: 824.2 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 322.062 * units.cc,
        intake_runner_volume: 698.2 * units.cc,
        intake_runner_cross_section_area: 25.9 * units.cm2,
        exhaust_runner_volume: 232.7 * units.cc,
        exhaust_runner_cross_section_area: 34.3 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "DB 604A",
        starter_torque: 600 * units.Nm,
        starter_speed: 300 * units.rpm,
        redline: redline * units.rpm,
        fuel: a30(),
        throttle_gamma: 1.5,
        jitter: 0.8,
        noise: 0.1,
        hf_gain: 0.007,
        fluid_simulation_steps: 6,
        max_sle_solver_steps: 16,
        simulation_frequency: 4500,
        dyno_min_speed: 500 * units.rpm
    )

    connecting_rod_parameters cr_params(
        mass: con_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.g,
            length: con_rod * units.mm
        ),
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    label crank_moment(disk_moment_of_inertia(mass: crank_mass * units.kg, radius: (stroke / 2.0) * units.mm))
    label flywheel_moment(disk_moment_of_inertia(mass: flywheel_mass * units.kg, radius: (stroke * 3.5) * units.mm))
    label other_moment(disk_moment_of_inertia(mass: 1 * units.kg, radius: 1.0 * units.cm))

    crankshaft c0(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: flywheel_mass * units.kg,
        mass: crank_mass * units.kg,
        friction_torque: 69.6 * units.Nm,
        moment_of_inertia: crank_moment + flywheel_moment + other_moment,
        tdc: 0.0
    )

    bank_builder bank0(
        shift: 0.0 * units.deg,
        piston_params: piston_params,
        cr_params: cr_params,
        bank_params: bank_params,
        bank_angle: vee / 2.0,
        intake: intake,
        exhaust: exhaust0,
        pl0,pl1,pl2,pl3,pl4,pl5,
        rj0,rj1,rj2,rj3,rj4,rj5,
        flip_display: true
    )

    bank_builder bank1(
        shift: vee + rot360,
        piston_params: piston_params,
        cr_params: cr_params,
        bank_params: bank_params,
        bank_angle: -vee / 2.0,
        intake: intake,
        exhaust: exhaust1,
        pl0,pl1,pl2,pl3,pl4,pl5,
        rj0,rj1,rj2,rj3,rj4,rj5
    )

    bank_builder bank2(
        shift: vee * 2.0,
        piston_params: piston_params,
        cr_params: cr_params,
        bank_params: bank_params,
        bank_angle: -vee / 2.0 - vee,
        intake: intake,
        exhaust: exhaust1,
        pl0,pl1,pl2,pl3,pl4,pl5,
        rj0,rj1,rj2,rj3,rj4,rj5,
        flip_display: true
    )

    bank_builder bank3(
        shift: vee * 3.0 + rot360,
        piston_params: piston_params,
        cr_params: cr_params,
        bank_params: bank_params,
        bank_angle: -vee / 2.0 - vee * 2.0,
        intake: intake,
        exhaust: exhaust0,
        pl0,pl1,pl2,pl3,pl4,pl5,
        rj0,rj1,rj2,rj3,rj4,rj5
    )

    rod_journal rj0(angle: rot * 0 + rot90 + vee / 2)
    rod_journal rj1(angle: rot * 16 + rot90 + vee / 2)
    rod_journal rj2(angle: rot * 8 + rot90 + vee / 2)
    rod_journal rj3(angle: rot * 20 + rot90 + vee / 2)
    rod_journal rj4(angle: rot * 4 + rot90 + vee / 2)
    rod_journal rj5(angle: rot * 12 + rot90 + vee / 2)

    common_ignition_0 ignition(bank0: bank0, bank1: bank1, bank2: bank2, bank3: bank3, timing_curve: timing_curve)
    
    c0
        .add_rod_journal(rj0)
        .add_rod_journal(rj1)
        .add_rod_journal(rj2)
        .add_rod_journal(rj3)
        .add_rod_journal(rj4)
        .add_rod_journal(rj5)

    piston_parameters piston_params(
        mass: 1500 * units.g,
        blowby: k_28inH2O(0.042),
        compression_height: compression_height,
        wrist_pin_position: 0.0,
        displacement: 0.0
    )

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    label collector_cross_section_area(412.1)

    label exhaust_pipe_length_0(50.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 1000.0) // Litres
    
    label spacing_factor(1.1)
    label flange_density(2.0 * exhaust_delay_coeff)

    intake intake(
        plenum_volume: 20.9 * units.L,
        plenum_cross_section_area: 310.5 * units.cm2,
        runner_length: 27.0 * units.cm,
        intake_flow_rate: k_carb(2512.1),
        idle_flow_rate: k_carb(0.005),
        idle_throttle_plate_position: 0.993,
        runner_flow_rate: k_carb(209.3),
        velocity_decay: 0.05
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(2093.4),
        collector_cross_section_area: 412.1 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 40.0 * units.cm,
        primary_flow_rate: k_carb(348.9),
        velocity_decay: 0.75,
        volume: exhaust_volume_0 * units.L
    )

    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_45.wav", volume: 0.001))
    exhaust_system exhaust1(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_54.wav", volume: 0.001))
 
    label pl0 ((6.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl1 ((5.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl2 ((4.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl3 ((3.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl4 ((2.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl5 ((1.0 * bore * spacing_factor / flange_density) * units.mm)

    engine
        .add_cylinder_bank(bank0.b0)
        .add_cylinder_bank(bank1.b0)
        .add_cylinder_bank(bank2.b0)
        .add_cylinder_bank(bank3.b0)

    engine.add_crankshaft(c0)

    // ignition_timing.py v0.1 r = 1.23
    function timing_curve(1000 * units.rpm)
    timing_curve
        .add_sample(0000 * units.rpm, 8.0 * units.deg)
        .add_sample(0500 * units.rpm, 9.8 * units.deg)
        .add_sample(1000 * units.rpm, 12.1 * units.deg)
        .add_sample(1500 * units.rpm, 14.9 * units.deg)
        .add_sample(2000 * units.rpm, 18.3 * units.deg)
        .add_sample(2500 * units.rpm, 22.5 * units.deg)
        .add_sample(3000 * units.rpm, 27.7 * units.deg)
        .add_sample(3500 * units.rpm, 34.1 * units.deg)
        .add_sample(4000 * units.rpm, 41.9 * units.deg)

    engine.add_ignition_module(ignition)
}

// Bugatti Type 41
public node veh {
    alias output __out: vehicle;
    vehicle vehicle(
        mass: 3250  * units.kg,
        drag_coefficient: 0.5,
        cross_sectional_area: (1320 * units.mm) * (1050 * units.mm),
        diff_ratio: 3.6,
        tire_radius: (914.4 / 2) * units.mm, // 36 x 6.75
        rolling_resistance: 20
        )
}

// Bugatti Type 41
private node trn {
    alias output __out:
        transmission(
            max_clutch_torque: 4000 * units.Nm,
            max_clutch_flex: 5 * units.deg,
            limit_clutch_flex: true,
            clutch_stiffness: 300 * units.Nm / units.deg,
            clutch_damping: 1.0,
            simulate_flex: true
        )
        .add_gear(7.50)
        .add_gear(1.0)
        .add_gear(0.37);
}

run(
    engine: eng(),
    transmission: trn(),
    vehicle: veh()
)


public node bank_builder {
    input shift;
    input piston_params;
    input cr_params;
    input bank_params;
    input bank_angle;
    input intake;
    input exhaust;

    input pl0;
    input pl1;
    input pl2;
    input pl3;
    input pl4;
    input pl5;

    input rj0;
    input rj1;
    input rj2;
    input rj3;
    input rj4;
    input rj5;

    input flip_display: false;

    output wire1: _wire1;
    output wire2: _wire2;
    output wire3: _wire3;
    output wire4: _wire4;
    output wire5: _wire5;
    output wire6: _wire6;

    output b0: _b0;

    ignition_wire _wire1()
    ignition_wire _wire2()
    ignition_wire _wire3()
    ignition_wire _wire4()
    ignition_wire _wire5()
    ignition_wire _wire6()

    camshaft _intake_cam_0(base_radius: 25.0 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 25.0 * units.mm, lobe_profile: exhaust_lobe_profile)

    _intake_cam_0
        .add_lobe(ILC + 0 * rot + shift)
        .add_lobe(ILC + 16 * rot + shift)
        .add_lobe(ILC + 8 * rot + shift)
        .add_lobe(ILC + 20 * rot + shift)
        .add_lobe(ILC + 4 * rot + shift)
        .add_lobe(ILC + 12 * rot + shift)
    _exhaust_cam_0
        .add_lobe(ELC + 0 * rot + shift)
        .add_lobe(ELC + 16 * rot + shift)
        .add_lobe(ELC + 8 * rot + shift)
        .add_lobe(ELC + 20 * rot + shift)
        .add_lobe(ELC + 4 * rot + shift)
        .add_lobe(ELC + 12 * rot + shift)

    // _intake_cam_0
    //     .add_lobe(ILC + 0 * rot + shift)
    //     .add_lobe(ILC + 4 * rot + shift)
    //     .add_lobe(ILC + 20 * rot + shift)
    //     .add_lobe(ILC + 8 * rot + shift)
    //     .add_lobe(ILC + 16 * rot + shift)
    //     .add_lobe(ILC + 12 * rot + shift)
    // _exhaust_cam_0
    //     .add_lobe(ELC + 0 * rot + shift)
    //     .add_lobe(ELC + 4 * rot + shift)
    //     .add_lobe(ELC + 20 * rot + shift)
    //     .add_lobe(ELC + 8 * rot + shift)
    //     .add_lobe(ELC + 16 * rot + shift)
    //     .add_lobe(ELC + 12 * rot + shift)

    cylinder_bank _b0(bank_params, angle: bank_angle)

    _b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj0,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl0,
            ignition_wire: _wire1
        )
    _b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj1,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl1,
            ignition_wire: _wire2
        )
    _b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj2,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl2,
            ignition_wire: _wire3
        )
    _b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj3,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl3,
            ignition_wire: _wire4
        )
    _b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj4,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl4,
            ignition_wire: _wire5
        )
    _b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj5,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl5,
            ignition_wire: _wire6
        )

    _b0.set_cylinder_head(
        head(
            intake_camshaft: _intake_cam_0,
            exhaust_camshaft: _exhaust_cam_0,
            flip_display: flip_display
        )
    )
}

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node common_ignition_0 {
    input bank0;
    input bank1;
    input bank2;
    input bank3;
    input timing_curve;
    alias output __out:
        ignition_module(timing_curve: timing_curve, rev_limit: 3500 * units.rpm, limiter_duration: 0.001)
            .connect_wire(bank0.wire1, rot * 0)
            .connect_wire(bank0.wire5, rot * 4)
            .connect_wire(bank0.wire3, rot * 8)
            .connect_wire(bank0.wire6, rot * 12)
            .connect_wire(bank0.wire2, rot * 16)
            .connect_wire(bank0.wire4, rot * 20)

            .connect_wire(bank1.wire1, rot * 0 + vee + rot360)
            .connect_wire(bank1.wire5, rot * 4 + vee + rot360)
            .connect_wire(bank1.wire3, rot * 8 + vee + rot360)
            .connect_wire(bank1.wire6, rot * 12 + vee + rot360)
            .connect_wire(bank1.wire2, rot * 16 + vee + rot360)
            .connect_wire(bank1.wire4, rot * 20 + vee + rot360)
            
            .connect_wire(bank2.wire1, rot * 0 + vee * 2.0)
            .connect_wire(bank2.wire5, rot * 4 + vee * 2.0)
            .connect_wire(bank2.wire3, rot * 8 + vee * 2.0)
            .connect_wire(bank2.wire6, rot * 12 + vee * 2.0)
            .connect_wire(bank2.wire2, rot * 16 + vee * 2.0)
            .connect_wire(bank2.wire4, rot * 20 + vee * 2.0)
            
            .connect_wire(bank3.wire1, rot * 0 + vee * 3.0 + rot360)
            .connect_wire(bank3.wire5, rot * 4 + vee * 3.0 + rot360)
            .connect_wire(bank3.wire3, rot * 8 + vee * 3.0 + rot360)
            .connect_wire(bank3.wire6, rot * 12 + vee * 3.0 + rot360)
            .connect_wire(bank3.wire2, rot * 16 + vee * 3.0 + rot360)
            .connect_wire(bank3.wire4, rot * 20 + vee * 3.0 + rot360);
}


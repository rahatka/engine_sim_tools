public node bank_builder {
    input rotation;
    input phasing;
    input ignition;
    input intake;
    input exhaust;

    input pl0;
    input pl1;
    input pl2;
    input pl3;
    input pl4;
    input pl5;
    input pl6;

    output rj0: _rj0;

    output b0: _b0;
    output b1: _b1;
    output b2: _b2;
    output b3: _b3;
    output b4: _b4;
    output b5: _b5;
    output b6: _b6;

    ignition_wire _wire1()
    ignition_wire _wire2()
    ignition_wire _wire3()
    ignition_wire _wire4()
    ignition_wire _wire5()
    ignition_wire _wire6()
    ignition_wire _wire7()

    connecting_rod_parameters cr_params(
        mass: con_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.g,
            length: con_rod * units.mm
        ),
        slave_throw: R1 * units.mm,
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    label slave_rod_mass(850)
    connecting_rod_parameters slave_rod_params(
        mass: slave_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: slave_rod_mass * units.g,
            length: L1 * units.mm
        ),
        center_of_mass: 0.0,
        length: L1 * units.mm
    )

    rod_journal _rj0(angle: rot90 - phasing + rotation)
    rod_journal sj0(angle: -rot_base * 0.5)
    rod_journal sj1(angle: -rot_base * 1.0)
    rod_journal sj2(angle: -rot_base * 1.5)
    rod_journal sj3(angle: -rot_base * 2.0)
    rod_journal sj4(angle: -rot_base * 2.5)
    rod_journal sj5(angle: -rot_base * 3.0)

    connecting_rod mcr(cr_params)
    mcr.add_slave_journal(sj0)
    mcr.add_slave_journal(sj1)
    mcr.add_slave_journal(sj2)
    mcr.add_slave_journal(sj3)
    mcr.add_slave_journal(sj4)
    mcr.add_slave_journal(sj5)

    piston p(
        piston_parameters(
            mass: piston_mass * units.g,
            blowby: k_28inH2O(0.049),
            compression_height: compression_height
        )
    )

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    camshaft _intake_cam_0(base_radius: 28.5 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 28.5 * units.mm, lobe_profile: exhaust_lobe_profile)

    camshaft _intake_cam_1(base_radius: 28.5 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_1(base_radius: 28.5 * units.mm, lobe_profile: exhaust_lobe_profile)
    
    camshaft _intake_cam_2(base_radius: 28.5 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_2(base_radius: 28.5 * units.mm, lobe_profile: exhaust_lobe_profile)
    
    camshaft _intake_cam_3(base_radius: 28.5 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_3(base_radius: 28.5 * units.mm, lobe_profile: exhaust_lobe_profile)
    
    camshaft _intake_cam_4(base_radius: 28.5 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_4(base_radius: 28.5 * units.mm, lobe_profile: exhaust_lobe_profile)
    
    camshaft _intake_cam_5(base_radius: 28.5 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_5(base_radius: 28.5 * units.mm, lobe_profile: exhaust_lobe_profile)
    
    camshaft _intake_cam_6(base_radius: 28.5 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_6(base_radius: 28.5 * units.mm, lobe_profile: exhaust_lobe_profile)

    _intake_cam_0
        .add_lobe(ILC + 0 * rot_base - phasing)
    _exhaust_cam_0
        .add_lobe(ELC + 0 * rot_base - phasing)

    _intake_cam_1
        .add_lobe(ILC + 4 * rot_base - phasing)
    _exhaust_cam_1
        .add_lobe(ELC + 4 * rot_base - phasing)
    
    _intake_cam_2
        .add_lobe(ILC + 1 * rot_base - phasing)
    _exhaust_cam_2
        .add_lobe(ELC + 1 * rot_base - phasing)
    
    _intake_cam_3
        .add_lobe(ILC + 5 * rot_base - phasing)
    _exhaust_cam_3
        .add_lobe(ELC + 5 * rot_base - phasing)
    
    _intake_cam_4
        .add_lobe(ILC + 2 * rot_base - phasing)
    _exhaust_cam_4
        .add_lobe(ELC + 2 * rot_base - phasing)
        
    _intake_cam_5
        .add_lobe(ILC + 6 * rot_base - phasing)
    _exhaust_cam_5
        .add_lobe(ELC + 6 * rot_base - phasing)
        
    _intake_cam_6
        .add_lobe(ILC + 3 * rot_base - phasing)
    _exhaust_cam_6
        .add_lobe(ELC + 3 * rot_base - phasing)

    cylinder_bank _b0(bank_params, angle: -rot_base * 0.0 + rotation)
    cylinder_bank _b1(bank_params, angle: -rot_base * 0.5 + rotation)
    cylinder_bank _b2(bank_params, angle: -rot_base * 1.0 + rotation)
    cylinder_bank _b3(bank_params, angle: -rot_base * 1.5 + rotation)
    cylinder_bank _b4(bank_params, angle: -rot_base * 2.0 + rotation)
    cylinder_bank _b5(bank_params, angle: -rot_base * 2.5 + rotation)
    cylinder_bank _b6(bank_params, angle: -rot_base * 3.0 + rotation)

    _b0.add_cylinder(
            piston: p,
            connecting_rod: mcr,
            rod_journal: _rj0,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl0,
            ignition_wire: _wire1
        )
    _b1.add_cylinder(
            piston: p,
            connecting_rod: connecting_rod(slave_rod_params),
            rod_journal: sj0,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl1,
            ignition_wire: _wire2
        )
    _b2.add_cylinder(
            piston: p,
            connecting_rod: connecting_rod(slave_rod_params),
            rod_journal: sj1,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl2,
            ignition_wire: _wire3
        )
    _b3.add_cylinder(
            piston: p,
            connecting_rod: connecting_rod(slave_rod_params),
            rod_journal: sj2,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl3,
            ignition_wire: _wire4
        )
    _b4.add_cylinder(
            piston: p,
            connecting_rod: connecting_rod(slave_rod_params),
            rod_journal: sj3,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl4,
            ignition_wire: _wire5
        )
    _b5.add_cylinder(
            piston: p,
            connecting_rod: connecting_rod(slave_rod_params),
            rod_journal: sj4,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl5,
            ignition_wire: _wire6
        )
    _b6.add_cylinder(
            piston: p,
            connecting_rod: connecting_rod(slave_rod_params),
            rod_journal: sj5,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl6,
            ignition_wire: _wire7
        )

    _b0.set_cylinder_head(
        head(
            intake_camshaft: _intake_cam_0,
            exhaust_camshaft: _exhaust_cam_0
        )
    )
    _b1.set_cylinder_head(
        head(
            intake_camshaft: _intake_cam_1,
            exhaust_camshaft: _exhaust_cam_1
        )
    )
    _b2.set_cylinder_head(
        head(
            intake_camshaft: _intake_cam_2,
            exhaust_camshaft: _exhaust_cam_2
        )
    )
    _b3.set_cylinder_head(
        head(
            intake_camshaft: _intake_cam_3,
            exhaust_camshaft: _exhaust_cam_3
        )
    )
    _b4.set_cylinder_head(
        head(
            intake_camshaft: _intake_cam_4,
            exhaust_camshaft: _exhaust_cam_4
        )
    )
    _b5.set_cylinder_head(
        head(
            intake_camshaft: _intake_cam_5,
            exhaust_camshaft: _exhaust_cam_5
        )
    )
    _b6.set_cylinder_head(
        head(
            intake_camshaft: _intake_cam_6,
            exhaust_camshaft: _exhaust_cam_6
        )
    )

    ignition
        .connect_wire(_wire1, rot_base * 0 - phasing)
        .connect_wire(_wire3, rot_base * 1 - phasing + 4.2 * units.deg)
        .connect_wire(_wire5, rot_base * 2 - phasing - 2.5 * units.deg)
        .connect_wire(_wire7, rot_base * 3 - phasing - 1.1 * units.deg)
        .connect_wire(_wire2, rot_base * 4 - phasing + 1.1 * units.deg)
        .connect_wire(_wire4, rot_base * 5 - phasing + 2.5 * units.deg)
        .connect_wire(_wire6, rot_base * 6 - phasing - 4.2 * units.deg)
}

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node head {
    // port_flow.py v1.5
    // intake port area: 36.7 cm²; saturated lift: 16.65 mm
    // exhaust port area: 36.7 cm²; saturated lift: 16.65 mm
    // cylinder volume: 2943.6 cm³ (179.6 CI); engine volume: 61.816 L (3772.3 CI)
    // chamber volume: 490.6 cm³ (29.9 CI)
    // 32 harmonic intake runner length: 18.4 cm; diameter: 6.2 cm
    // primary length: 65.2 cm, area: 38.6 cm², diameter: 7.0 cm
    // collector diameter: 25.9 cm, area: 526.2 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 490.604 * units.cc,
        intake_runner_volume: 556.7 * units.cc,
        intake_runner_cross_section_area: 30.2 * units.cm2,
        exhaust_runner_volume: 185.6 * units.cc,
        exhaust_runner_cross_section_area: 38.6 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "M-3",
        starter_torque: 500 * units.Nm,
        starter_speed: 150 * units.rpm,
        redline: redline * units.rpm,
        fuel: a30(),
        throttle_gamma: 2.0,
        jitter: 0.8,
        noise: 0.1,
        hf_gain: 0.008,
        fluid_simulation_steps: 6,
        max_sle_solver_steps: 32,
        simulation_frequency: 5000,
        dyno_min_speed: 500 * units.rpm
    )

    label crank_moment(disk_moment_of_inertia(mass: crank_mass * units.kg, radius: (stroke / 2.0) * units.mm))
    label flywheel_moment(disk_moment_of_inertia(mass: flywheel_mass * units.kg, radius: (stroke * 3.5) * units.mm))
    label other_moment(disk_moment_of_inertia(mass: 1 * units.kg, radius: 1.0 * units.cm))

    crankshaft c0(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: flywheel_mass * units.kg,
        mass: crank_mass * units.kg,
        friction_torque: 92.7 * units.Nm,
        moment_of_inertia: crank_moment + flywheel_moment + other_moment,
        tdc: 0.0
    )

    ignition_module ignition(timing_curve: timing_curve, rev_limit: redline * 1.1 * units.rpm, limiter_duration: 0.001)

    bank_builder bank0(
        rotation: 0,
        phasing: 0,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust0,
        pl0 + sh0, pl0 + sh1, pl0 + sh2, pl0 + sh3, pl0 + sh4, pl0 + sh5, pl0 + sh6
    )

    bank_builder bank1(
        rotation: rot / 2,
        phasing: rot * 8,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust0,
        pl1 + sh0, pl1 + sh1, pl1 + sh2, pl1 + sh3, pl1 + sh4, pl1 + sh5, pl1 + sh6
    )

    bank_builder bank2(
        rotation: rot,
        phasing: rot * 4,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust0,
        pl2 + sh0, pl2 + sh1, pl2 + sh2, pl2 + sh3, pl2 + sh4, pl2 + sh5, pl2 + sh6
    )
    
    c0
        .add_rod_journal(bank0.rj0)
        .add_rod_journal(bank1.rj0)
        .add_rod_journal(bank2.rj0)

    label collector_cross_section_area(526.2)

    label exhaust_pipe_length_0(100.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 100.0) // Litres
    
    label spacing_factor(1.5)
    label flange_density(1.0 * exhaust_delay_coeff)

    intake intake(
        plenum_volume: 24.0 * units.L,
        plenum_cross_section_area: 317.3 * units.cm2,
        runner_length: 18.4 * units.cm,
        intake_flow_rate: k_carb(2897.6),
        idle_flow_rate: k_carb(0.05),
        idle_throttle_plate_position: 0.996,
        runner_flow_rate: k_carb(310.5),
        velocity_decay: 0.05
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(5131.2),
        collector_cross_section_area: 526.2 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 65.2 * units.cm,
        primary_flow_rate: k_carb(519.2),
        velocity_decay: 0.75,
        volume: exhaust_volume_0 * units.L
    )

    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_56.wav", volume: 0.001))
    
    label pl0((1.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl1((3.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl2((5.0 * bore * spacing_factor / flange_density) * units.mm)

    label sh0((1.0 * bore * spacing_factor / flange_density) * units.mm)
    label sh1((4.0 * bore * spacing_factor / flange_density) * units.mm)
    label sh2((8.0 * bore * spacing_factor / flange_density) * units.mm)
    label sh3((12.0 * bore * spacing_factor / flange_density) * units.mm) // outmost mid point
    label sh4((9.0 * bore * spacing_factor / flange_density) * units.mm)
    label sh5((5.0 * bore * spacing_factor / flange_density) * units.mm)
    label sh6((3.0 * bore * spacing_factor / flange_density) * units.mm)

    engine
        .add_cylinder_bank(bank0.b0)
        .add_cylinder_bank(bank0.b1)
        .add_cylinder_bank(bank0.b2)
        .add_cylinder_bank(bank0.b3)
        .add_cylinder_bank(bank0.b4)
        .add_cylinder_bank(bank0.b5)
        .add_cylinder_bank(bank0.b6)
        
        .add_cylinder_bank(bank1.b0)
        .add_cylinder_bank(bank1.b1)
        .add_cylinder_bank(bank1.b2)
        .add_cylinder_bank(bank1.b3)
        .add_cylinder_bank(bank1.b4)
        .add_cylinder_bank(bank1.b5)
        .add_cylinder_bank(bank1.b6)

        .add_cylinder_bank(bank2.b0)
        .add_cylinder_bank(bank2.b1)
        .add_cylinder_bank(bank2.b2)
        .add_cylinder_bank(bank2.b3)
        .add_cylinder_bank(bank2.b4)
        .add_cylinder_bank(bank2.b5)
        .add_cylinder_bank(bank2.b6)

        .add_crankshaft(c0)
        .add_ignition_module(ignition)

    function timing_curve(166.7 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 2.0 * units.deg)
        .add_sample(166.7 * units.rpm, 2.0 * units.deg)
        .add_sample(333.3 * units.rpm, 5.6 * units.deg)
        .add_sample(500.0 * units.rpm, 9.2 * units.deg)
        .add_sample(666.7 * units.rpm, 12.2 * units.deg)
        .add_sample(833.3 * units.rpm, 14.5 * units.deg)
        .add_sample(1000.0 * units.rpm, 16.4 * units.deg)
        .add_sample(1166.7 * units.rpm, 17.9 * units.deg)
        .add_sample(1333.3 * units.rpm, 19.1 * units.deg)
        .add_sample(1500.0 * units.rpm, 20.1 * units.deg)
        .add_sample(1666.7 * units.rpm, 20.9 * units.deg)
        .add_sample(1833.3 * units.rpm, 21.5 * units.deg)
        .add_sample(2000.0 * units.rpm, 22.0 * units.deg)
        .add_sample(2166.7 * units.rpm, 22.0 * units.deg)
        .add_sample(2333.3 * units.rpm, 22.0 * units.deg)
        .add_sample(2500.0 * units.rpm, 22.0 * units.deg)
        .add_sample(2666.7 * units.rpm, -15.0 * units.deg)
        .add_sample(2833.3 * units.rpm, -45.0 * units.deg)
}

// Pz V
public node veh {
    alias output __out: vehicle;

    label car_mass(45000) // 45.0 t panther

    vehicle vehicle(
        mass: car_mass * units.kg,
        drag_coefficient: 0.6,
        cross_sectional_area: (2830 * units.mm) * (2680 * units.mm),
        diff_ratio: 2.5 * 3,
        tire_radius: (821.77 / 2) * units.mm,
        rolling_resistance: 0.06 * car_mass * 9.81
        )
}

// Pz V
private node trn {
    alias output __out:
        transmission(
            max_clutch_torque: 5000 * units.Nm,
            max_clutch_flex: 15 * units.deg,
            limit_clutch_flex: true,
            clutch_stiffness: 50 * units.Nm / units.deg,
            clutch_damping: 1.0,
            simulate_flex: true
        )
        .add_gear(10.8)  // 3.7 @ 2600
        .add_gear(5.26)  // 7.6
        .add_gear(3.33)  // 12.0
        .add_gear(2.13)  // 18.8
        .add_gear(1.48)  // 27.0
        .add_gear(1.06)  // 37.7
        .add_gear(0.85); // 47.3
}

run(
    engine: eng(),
    transmission: trn(),
    vehicle: veh()
)

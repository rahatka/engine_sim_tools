import "engine_sim.mr"
import "../../fuel/30.mr"

units units()
constants constants()

label bore(165.0)
label stroke(180.0)
label compression_ratio(8.5)
label con_rod(310.0) // ~
label compression_height(70.0 * units.mm) // ~
label intake_valve_diameter(66.0) // ~
label exhaust_valve_diameter(63.0) // ~
label intake_valves(2.0)
label exhaust_valves(2.0)

label cyl(16.0)
label row(2.0)
label cycle(720.0 * units.deg)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label vee(60.0 * units.deg)

label redline(3000.0)

// Daimler-Benz DB 603 valve timing
label IVL(14.1) // @0.012"
label EVL(14.1) // @0.024"
label IVO(62.0 * units.deg) //BTDC
label IVC(48.0 * units.deg) //ABDC
label EVO(67.0 * units.deg) //BBDC
label EVC(43.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(2000.0) // ? g
label crank_mass(75.0) // ? g
label flywheel_mass(15.0) // kg

label exhaust_delay_coeff(1.5)


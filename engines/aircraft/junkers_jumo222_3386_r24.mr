private node add_lobes {
    input base;
    input shift: 0.0;
    input var;
    input this;
    alias output __out: this;

    this.add_lobe(base + var.s1 * rot_base + shift)
    this.add_lobe(base + var.s2 * rot_base + shift)
    this.add_lobe(base + var.s3 * rot_base + shift)
    this.add_lobe(base + var.s4 * rot_base + shift)
}

public node bank_builder {
    input var;
    input incline;
    input phasing;
    input crankshaft;
    input ignition;
    input intake;
    input exhaust;
    input flip: false;

    output b0: _b0;

    ignition_wire _wire1()
    ignition_wire _wire2()
    ignition_wire _wire3()
    ignition_wire _wire4()

    piston_parameters piston_params(
        mass: 1580.0 * units.g,
        blowby: k_28inH2O(0.046),
        compression_height: compression_height,
        wrist_pin_position: 0.0,
        displacement: 0.0
    )

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    connecting_rod_parameters cr_params(
        mass: con_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.g,
            length: con_rod * units.mm
        ),
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    camshaft _intake_cam_0(base_radius: 22.2 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 22.2 * units.mm, lobe_profile: exhaust_lobe_profile)

    ignition.connect_wire(_wire1, rot_base * var.s1 + phasing)
    ignition.connect_wire(_wire2, rot_base * var.s2 + phasing)
    ignition.connect_wire(_wire3, rot_base * var.s3 + phasing)
    ignition.connect_wire(_wire4, rot_base * var.s4 + phasing)

    _intake_cam_0.add_lobes(ILC, phasing, var)
    _exhaust_cam_0.add_lobes(ELC, phasing, var)

    cylinder_bank _b0(bank_params, angle: incline)

    label spacing_factor(1.1)
    label flange_density(2.0 * exhaust_delay_coeff)
    label pl0 ((4.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl1 ((3.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl2 ((2.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl3 ((1.0 * bore * spacing_factor / flange_density) * units.mm)

    _b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj0,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl0,
            ignition_wire: _wire1
        )
    _b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj1,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl1,
            ignition_wire: _wire2
        )
    _b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj2,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl2,
            ignition_wire: _wire3
        )
    _b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj3,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl3,
            ignition_wire: _wire4
        )

    _b0.set_cylinder_head(
        head(
            intake_camshaft: _intake_cam_0,
            exhaust_camshaft: _exhaust_cam_0,
            flip_display: flip
        )
    )
}

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node head {
    // port_flow.py v1.7
    // intake port area: 37.4 cm²; saturated lift: 11.88 mm
    // exhaust port area: 24.6 cm²; saturated lift: 13.65 mm
    // cylinder volume: 2311.8 cm³ (141.08 CI); engine volume: 55.484 L (3385.82 CI)
    // chamber volume: 402.1 cm³ (24.53 CI); conrod/stroke 1.857; N.A.C.C. 312.85
    // 16 harmonic intake runner length: 27.5 cm; diameter: 6.5 cm
    // primary length: 35.5 cm, area: 27.1 cm², diameter: 5.9 cm
    // collector diameter: 28.8 cm, area: 650.8 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 402.056 * units.cc,
        intake_runner_volume: 920.7 * units.cc,
        intake_runner_cross_section_area: 33.5 * units.cm2,
        exhaust_runner_volume: 306.9 * units.cc,
        exhaust_runner_cross_section_area: 27.1 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

public node crank {
    input var;
    input phasing;

    output c: _c;
    output rj0: _rj0;
    output rj1: _rj1;
    output rj2: _rj2;
    output rj3: _rj3;

    label crank_moment(disk_moment_of_inertia(mass: crank_mass * units.kg, radius: (stroke / 2.0) * units.mm))
    label flywheel_moment(disk_moment_of_inertia(mass: flywheel_mass * units.kg, radius: (stroke * 3.5) * units.mm))
    label other_moment(disk_moment_of_inertia(mass: 1 * units.kg, radius: 1.0 * units.cm))

    crankshaft _c(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: flywheel_mass * units.kg,
        mass: crank_mass * units.kg,
        friction_torque: 83.2 * units.Nm,
        moment_of_inertia: crank_moment + flywheel_moment + other_moment,
        tdc: 0.0
    )

    rod_journal _rj0(angle: rot_base * var.s1 + phasing)
    rod_journal _rj1(angle: rot_base * var.s2 + phasing)
    rod_journal _rj2(angle: rot_base * var.s3 + phasing)
    rod_journal _rj3(angle: rot_base * var.s4 + phasing)

    _c
        .add_rod_journal(_rj0)
        .add_rod_journal(_rj1)
        .add_rod_journal(_rj2)
        .add_rod_journal(_rj3)
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "Junkers Jumo 222",
        starter_torque: 600 * units.Nm,
        starter_speed: 300 * units.rpm,
        redline: redline * units.rpm,
        fuel: a30(),
        throttle_gamma: 1.5,
        jitter: 0.8,
        noise: 0.1,
        hf_gain: 0.007,
        fluid_simulation_steps: 6,
        max_sle_solver_steps: 32,
        simulation_frequency: 4000,
        dyno_min_speed: 500 * units.rpm
    )

    v1342 v1342()
    v1243 v1243()

    crank c0(v1342, rot90 + vee / 2)
    ignition_module ignition(timing_curve: timing_curve, rev_limit: redline * 1.5 * units.rpm, limiter_duration: 0.03)

    bank_builder bank1(
        var: v1243,
        incline: vee / 2.0,
        phasing: 0,
        crankshaft: c0,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust0,
        flip: true
    )

    bank_builder bank2(
        var: v1342,
        incline: -vee / 2.0,
        phasing: vee + rot360,
        crankshaft: c0,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust1
    )

    bank_builder bank3(
        var: v1243,
        incline: -vee / 2.0 - vee,
        phasing: vee * 2.0 + rot360,
        crankshaft: c0,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust1,
        flip: true
    )

    bank_builder bank4(
        var: v1342,
        incline: -vee / 2.0 - vee * 2.0,
        phasing: vee * 3.0,
        crankshaft: c0,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust2
    )

    bank_builder bank5(
        var: v1243,
        incline: -vee / 2.0 - vee * 3.0,
        phasing: vee * 4.0,
        crankshaft: c0,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust2,
        flip: true
    )

    bank_builder bank6(
        var: v1342,
        incline: -vee / 2.0 - vee * 4.0,
        phasing: vee * 5.0 + rot360,
        crankshaft: c0,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust0
    )


    label collector_cross_section_area(216.9)

    label exhaust_pipe_length_0(100.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 100.0) // Litres

    intake intake(
        plenum_volume: 25.0 * units.L,
        plenum_cross_section_area: 401.8 * units.cm2,
        runner_length: 27.5 * units.cm,
        intake_flow_rate: k_carb(3005.4),
        idle_flow_rate: k_carb(0.08),
        idle_throttle_plate_position: 0.9975,
        runner_flow_rate: k_carb(250.4),
        velocity_decay: 0.05
    )

    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(1669.6),
        collector_cross_section_area: 216.9 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 35.5 * units.cm,
        primary_flow_rate: k_carb(417.4),
        velocity_decay: 0.9,
        volume: exhaust_volume_0 * units.L
    )

    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_45.wav", volume: 0.0012))
    exhaust_system exhaust1(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_45.wav", volume: 0.0010))
    exhaust_system exhaust2(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_58.wav", volume: 0.0010))

    engine
        .add_cylinder_bank(bank1.b0)
        .add_cylinder_bank(bank2.b0)
        .add_cylinder_bank(bank3.b0)
        .add_cylinder_bank(bank4.b0)
        .add_cylinder_bank(bank5.b0)
        .add_cylinder_bank(bank6.b0)
        .add_crankshaft(c0.c)
        .add_ignition_module(ignition)

    function timing_curve(216.7 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 0.0 * units.deg)
        .add_sample(216.7 * units.rpm, 1.6 * units.deg)
        .add_sample(433.3 * units.rpm, 8.0 * units.deg)
        .add_sample(650.0 * units.rpm, 12.7 * units.deg)
        .add_sample(866.7 * units.rpm, 16.2 * units.deg)
        .add_sample(1083.3 * units.rpm, 18.7 * units.deg)
        .add_sample(1300.0 * units.rpm, 20.6 * units.deg)
        .add_sample(1516.7 * units.rpm, 22.0 * units.deg)
        .add_sample(1733.3 * units.rpm, 23.1 * units.deg)
        .add_sample(1950.0 * units.rpm, 23.8 * units.deg)
        .add_sample(2166.7 * units.rpm, 24.4 * units.deg)
        .add_sample(2383.3 * units.rpm, 24.8 * units.deg)
        .add_sample(2600.0 * units.rpm, 25.0 * units.deg)
        .add_sample(2816.7 * units.rpm, 25.0 * units.deg)
        .add_sample(3033.3 * units.rpm, 25.0 * units.deg)
        .add_sample(3250.0 * units.rpm, 25.0 * units.deg)
        .add_sample(3466.7 * units.rpm, -12.5 * units.deg)
        .add_sample(3683.3 * units.rpm, -25.0 * units.deg)
}

// Pz V
public node veh {
    alias output __out: vehicle;

    label car_mass(45000) // 45.0 t panther

    vehicle vehicle(
        mass: car_mass * units.kg,
        drag_coefficient: 0.6,
        cross_sectional_area: (2830 * units.mm) * (2680 * units.mm),
        diff_ratio: 2.5 * 3,
        tire_radius: (821.77 / 2) * units.mm,
        rolling_resistance: 0.06 * car_mass * 9.81
        )
}

// Pz V
private node trn {
    alias output __out:
        transmission(
            max_clutch_torque: 4000 * units.Nm,
            max_clutch_flex: 15 * units.deg,
            limit_clutch_flex: true,
            clutch_stiffness: 50 * units.Nm / units.deg,
            clutch_damping: 1.0,
            simulate_flex: true
        )
        .add_gear(10.8)  // 3.7 @ 2600
        .add_gear(5.26)  // 7.6
        .add_gear(3.33)  // 12.0
        .add_gear(2.13)  // 18.8
        .add_gear(1.48)  // 27.0
        .add_gear(1.06)  // 37.7
        .add_gear(0.85); // 47.3
}

run(
    engine: eng(),
    transmission: trn(),
    vehicle: veh()
)

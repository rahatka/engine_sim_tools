private node add_lobes {
    input base;
    input phasing: 0.0;
    input var;
    input this;
    alias output __out: this;

    this.add_lobe(base + var.s1 * rot_base + phasing)
    this.add_lobe(base + var.s2 * rot_base + phasing)
    this.add_lobe(base + var.s3 * rot_base + phasing)
    this.add_lobe(base + var.s4 * rot_base + phasing)
    this.add_lobe(base + var.s5 * rot_base + phasing)
    this.add_lobe(base + var.s6 * rot_base + phasing)
}

public node bank_builder {
    input var;
    input angle;
    input phasing;
    input ignition;
    input intake;
    input exhaust;

    input rj0;
    input rj1;
    input rj2;
    input rj3;
    input rj4;
    input rj5;

    input pl0;
    input pl1;
    input pl2;
    input pl3;
    input pl4;
    input pl5;

    output b0: _b0;

    ignition_wire _wire1()
    ignition_wire _wire2()
    ignition_wire _wire3()
    ignition_wire _wire4()
    ignition_wire _wire5()
    ignition_wire _wire6()

    piston_parameters piston_params(
        mass: 1200 * units.g,
        blowby: k_28inH2O(0.034),
        compression_height: compression_height,
        wrist_pin_position: 0.0,
        displacement: 0.0
    )

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    connecting_rod_parameters cr_params(
        mass: con_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.g,
            length: con_rod * units.mm
        ),
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    camshaft _intake_cam_0(base_radius: 23.3 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 23.3 * units.mm, lobe_profile: exhaust_lobe_profile)

    _intake_cam_0.add_lobes(ILC, phasing, var)
    _exhaust_cam_0.add_lobes(ELC, phasing, var)

    cylinder_bank _b0(bank_params, angle: angle)

    _b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj0,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl0,
            ignition_wire: _wire1
        )
    _b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj1,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl1,
            ignition_wire: _wire2
        )
    _b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj2,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl2,
            ignition_wire: _wire3
        )
    _b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj3,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl3,
            ignition_wire: _wire4
        )
    _b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj4,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl2,
            ignition_wire: _wire5
        )
    _b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj5,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl3,
            ignition_wire: _wire6
        )

    _b0.set_cylinder_head(
        head(
            intake_camshaft: _intake_cam_0,
            exhaust_camshaft: _exhaust_cam_0
        )
    )

    ignition
        .connect_wire(_wire1, rot_base * var.s1 + phasing)
        .connect_wire(_wire2, rot_base * var.s2 + phasing)
        .connect_wire(_wire3, rot_base * var.s3 + phasing)
        .connect_wire(_wire4, rot_base * var.s4 + phasing)
        .connect_wire(_wire5, rot_base * var.s5 + phasing)
        .connect_wire(_wire6, rot_base * var.s6 + phasing)
}

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node head {
    // port_flow.py v1.7
    // intake port area: 21.8 cm²; saturated lift: 12.83 mm
    // exhaust port area: 15.1 cm²; saturated lift: 10.69 mm
    // cylinder volume: 842.7 cm³ (51.43 CI); engine volume: 35.394 L (2159.86 CI)
    // chamber volume: 140.5 cm³ (8.57 CI); conrod/stroke 2.069; N.A.C.C. 303.45
    // 16 harmonic intake runner length: 21.9 cm; diameter: 4.2 cm
    // primary length: 30.9 cm, area: 15.1 cm², diameter: 4.4 cm
    // collector diameter: 22.9 cm, area: 412.7 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 140.451 * units.cc,
        intake_runner_volume: 308.6 * units.cc,
        intake_runner_cross_section_area: 14.1 * units.cm2,
        exhaust_runner_volume: 118.7 * units.cc,
        exhaust_runner_cross_section_area: 15.1 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "Wright R-2160",
        starter_torque: 1000 * units.Nm,
        starter_speed: 250 * units.rpm,
        redline: redline * units.rpm,
        fuel: a30(),
        throttle_gamma: 2.0,
        jitter: 0.5,
        noise: 0.1,
        hf_gain: 0.005,
        fluid_simulation_steps: 4,
        max_sle_solver_steps: 16,
        simulation_frequency: 2250,
        dyno_min_speed: 500 * units.rpm
    )

    ////////////////////////////////////////
    ///////// change variants here /////////
    ////////////////////////////////////////
    variant_0 variant()

    label crank_moment(disk_moment_of_inertia(mass: crank_mass * units.kg, radius: (stroke / 2.0) * units.mm))
    label flywheel_moment(disk_moment_of_inertia(mass: flywheel_mass * units.kg, radius: (stroke * 3.5) * units.mm))
    label other_moment(disk_moment_of_inertia(mass: 1 * units.kg, radius: 1.0 * units.cm))

    crankshaft c0(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: flywheel_mass * units.kg,
        mass: crank_mass * units.kg,
        friction_torque: 53.1 * units.Nm,
        moment_of_inertia: crank_moment + flywheel_moment + other_moment,
        tdc: 0.0
    )

    bank_builder bank0( // A
        var: variant,
        angle: 0,
        phasing: 0.0 * units.deg,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust,
        rj0, rj1, rj2, rj3, rj4, rj5,
        pl0 + sh0, pl1 + sh0, pl2 + sh0, pl3 + sh0, pl4 + sh0, pl5 + sh0
    )
    bank_builder bank1( // G
        var: variant,
        angle: -vee,
        phasing: vee * 1 + rot360,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust,
        rj0, rj1, rj2, rj3, rj4, rj5,
        pl0 + sh1, pl1 + sh1, pl2 + sh1, pl3 + sh1, pl4 + sh1, pl5 + sh1
    )
    bank_builder bank2( // F
        var: variant,
        angle: -vee * 2,
        phasing: vee * 2,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust,
        rj0, rj1, rj2, rj3, rj4, rj5,
        pl0 + sh2, pl1 + sh2, pl2 + sh2, pl3 + sh2, pl4 + sh2, pl5 + sh2
    )
    bank_builder bank3( // E
        var: variant,
        angle: -vee * 3,
        phasing: vee * 3 + rot360,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust,
        rj0, rj1, rj2, rj3, rj4, rj5,
        pl0 + sh3, pl1 + sh3, pl2 + sh3, pl3 + sh3, pl4 + sh3, pl5 + sh3
    )
    bank_builder bank4( // D
        var: variant,
        angle: -vee * 4,
        phasing: vee * 4,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust,
        rj0, rj1, rj2, rj3, rj4, rj5,
        pl0 + sh4, pl1 + sh4, pl2 + sh4, pl3 + sh4, pl4 + sh4, pl5 + sh4
    )
    bank_builder bank5( // C
        var: variant,
        angle: -vee * 5,
        phasing: vee * 5 + rot360,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust,
        rj0, rj1, rj2, rj3, rj4, rj5,
        pl0 + sh5, pl1 + sh5, pl2 + sh5, pl3 + sh5, pl4 + sh5, pl5 + sh5
    )
    bank_builder bank6( // B
        var: variant,
        angle: -vee * 6,
        phasing: vee * 6,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust,
        rj0, rj1, rj2, rj3, rj4, rj5,
        pl0 + sh6, pl1 + sh6, pl2 + sh6, pl3 + sh6, pl4 + sh6, pl5 + sh6
    )

    ignition_module ignition(timing_curve: timing_curve, rev_limit: 5000 * units.rpm, limiter_duration: 0.001)

    rod_journal rj0(angle: rot_base * variant.s1 + rot90)
    rod_journal rj1(angle: rot_base * variant.s2 + rot90)
    rod_journal rj2(angle: rot_base * variant.s3 + rot90)
    rod_journal rj3(angle: rot_base * variant.s4 + rot90)
    rod_journal rj4(angle: rot_base * variant.s5 + rot90)
    rod_journal rj5(angle: rot_base * variant.s6 + rot90)
    
    c0
        .add_rod_journal(rj0)
        .add_rod_journal(rj1)
        .add_rod_journal(rj2)
        .add_rod_journal(rj3)
        .add_rod_journal(rj4)
        .add_rod_journal(rj5)

    label collector_cross_section_area(412.7)

    label exhaust_pipe_length_0(400.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 100.0) // Litres
    
    label spacing_factor(1.2)
    label flange_density(1.0 * exhaust_delay_coeff)
    label radial_density(3.0 * exhaust_delay_coeff)

    intake intake(
        plenum_volume: 19.9 * units.L,
        plenum_cross_section_area: 296.2 * units.cm2,
        runner_length: 21.9 * units.cm,
        intake_flow_rate: k_carb(2507.1),
        idle_flow_rate: k_carb(0.05),
        idle_throttle_plate_position: 0.994,
        runner_flow_rate: k_carb(119.4),
        velocity_decay: 0.05
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(4178.4),
        collector_cross_section_area: 412.7 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 30.9 * units.cm,
        primary_flow_rate: k_carb(199.0),
        velocity_decay: 1.0,
        volume: exhaust_volume_0 * units.L
    )

    exhaust_system exhaust(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_52.wav", volume: 0.001))

    label sh0 ((1.0 * bore * spacing_factor / radial_density) * units.mm)
    label sh1 ((4.0 * bore * spacing_factor / radial_density) * units.mm)
    label sh2 ((7.0 * bore * spacing_factor / radial_density) * units.mm)
    label sh3 ((10.0 * bore * spacing_factor / radial_density) * units.mm)
    label sh4 ((8.0 * bore * spacing_factor / radial_density) * units.mm)
    label sh5 ((5.0 * bore * spacing_factor / radial_density) * units.mm)
    label sh6 ((2.0 * bore * spacing_factor / radial_density) * units.mm)
    
    label pl0 ((1.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl1 ((2.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl2 ((3.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl3 ((4.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl4 ((5.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl5 ((6.0 * bore * spacing_factor / flange_density) * units.mm)

    engine
        .add_cylinder_bank(bank0.b0)
        .add_cylinder_bank(bank1.b0)
        .add_cylinder_bank(bank2.b0)
        .add_cylinder_bank(bank3.b0)
        .add_cylinder_bank(bank4.b0)
        .add_cylinder_bank(bank5.b0)
        .add_cylinder_bank(bank6.b0)

    engine.add_crankshaft(c0)

    // ignition_timing.py v0.2 a = 7.0, m = 1.15, r = 4250
    function timing_curve(425.0 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 0.0 * units.deg)
        .add_sample(425.0 * units.rpm, 7.0 * units.deg)
        .add_sample(850.0 * units.rpm, 8.0 * units.deg)
        .add_sample(1275.0 * units.rpm, 9.3 * units.deg)
        .add_sample(1700.0 * units.rpm, 10.6 * units.deg)
        .add_sample(2125.0 * units.rpm, 12.2 * units.deg)
        .add_sample(2550.0 * units.rpm, 14.1 * units.deg)
        .add_sample(2975.0 * units.rpm, 16.2 * units.deg)
        .add_sample(3400.0 * units.rpm, 18.6 * units.deg)
        .add_sample(3825.0 * units.rpm, 21.4 * units.deg)
        .add_sample(4250.0 * units.rpm, 24.6 * units.deg)
        .add_sample(4675.0 * units.rpm, -12.5 * units.deg)
        .add_sample(5100.0 * units.rpm, -25.0 * units.deg)

    engine.add_ignition_module(ignition)
}

// Bugatti Type 41
public node veh {
    alias output __out: vehicle;
    vehicle vehicle(
        mass: 3250  * units.kg,
        drag_coefficient: 0.5,
        cross_sectional_area: (1320 * units.mm) * (1050 * units.mm),
        diff_ratio: 3.6,
        tire_radius: (914.4 / 2) * units.mm, // 36 x 6.75
        rolling_resistance: 20
        )
}

// Bugatti Type 41
private node trn {
    alias output __out:
        transmission(
            max_clutch_torque: 12000 * units.Nm,
            max_clutch_flex: 5 * units.deg,
            limit_clutch_flex: true,
            clutch_stiffness: 100 * units.Nm / units.deg,
            clutch_damping: 1.0,
            simulate_flex: true
        )
        .add_gear(7.50)
        .add_gear(1.0)
        .add_gear(0.37);
}

run(
    engine: eng(),
    transmission: trn(),
    vehicle: veh()
)

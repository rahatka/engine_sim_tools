import "engine_sim.mr"
import "../../fuel/30.mr"

units units()
constants constants()

label bore(107.95) // 4 1/4
label stroke(92.075) // 3 5/8
label compression_ratio(7.0)
label con_rod(190.5) // 7 1/2 ?
label compression_height(40.0 * units.mm) // ~
label intake_valve_diameter(60.0) // ?
label exhaust_valve_diameter(50.0) // ?
label intake_valves(1.0)
label exhaust_valves(1.0)

label cyl(42.0)
label row(7.0)
label cyl_base(cyl / row)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label rot_base(720.0 / cyl_base * units.deg)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label vee(rot360 / row)

label redline(4250.0) // RPM

label IVL(12.7) // ?
label EVL(12.7) // ?

// R-2160
label IVO(41.0 * units.deg) //BTDC
label IVC(63.0 * units.deg) //ABDC
label EVO(74.0 * units.deg) //BBDC
label EVC(66.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(900.0) // ? g
label crank_mass(80.0) // ? kg
label flywheel_mass(30.0) // kg

label exhaust_delay_coeff(1.5)


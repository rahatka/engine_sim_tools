import "engine_sim.mr"
import "../../fuel/30.mr"

units units()
constants constants()

label bore(130.175) // 5.125
label stroke(111.125) // 4.375
label compression_ratio(8.7)
label con_rod(171.45) // 6.75

label compression_height(50.4 * units.mm) // ?
label intake_valve_diameter(53.975) // 2.125
label exhaust_valve_diameter(44.45) // 1.750
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(2800.0) // RPM

label cyl(8.0)
label row(2.0)
label cycle(720.0 * units.deg)
label vee(180.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// 
label IVL(11.3792) // .35 * 1.28 rocker
label EVL(11.3792) // .35 * 1.28 rocker
label IVO(-2.0 * units.deg) //BTDC
label IVC(42.0 * units.deg) //ABDC
label EVO(45.5 * units.deg) //BBDC
label EVC(1.5 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(900.0) // g
label piston_mass(800.0) // ? g
label crank_mass(19.5) // kg
label flywheel_mass(10.0) // kg

label exhaust_delay_coeff(1.5)

public node eng_distributor {
    input wires;
    input timing_curve;
    input rev_limit: redline * 1.1 * units.rpm;
    input limiter_duration: 0.0001;
    alias output __out:
        ignition_module(timing_curve: timing_curve, rev_limit: rev_limit, limiter_duration: limiter_duration)
            .connect_wire(wires.wire1, rot * 0)
            .connect_wire(wires.wire7, rot * 1)
            .connect_wire(wires.wire4, rot * 2)
            .connect_wire(wires.wire2, rot * 3)
            .connect_wire(wires.wire5, rot * 4)
            .connect_wire(wires.wire3, rot * 5)
            .connect_wire(wires.wire8, rot * 6)
            .connect_wire(wires.wire6, rot * 7);
}

private node wires {
    output wire1: ignition_wire();
    output wire2: ignition_wire();
    output wire3: ignition_wire();
    output wire4: ignition_wire();
    output wire5: ignition_wire();
    output wire6: ignition_wire();
    output wire7: ignition_wire();
    output wire8: ignition_wire();
}


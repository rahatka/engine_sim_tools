        .add_sample(1353.3 * units.rpm, 19.7 * units.deg)
        .add_sample(1546.7 * units.rpm, 21.0 * units.deg)
        .add_sample(1740.0 * units.rpm, 22.0 * units.deg)
        .add_sample(1933.3 * units.rpm, 22.8 * units.deg)
        .add_sample(2126.7 * units.rpm, 23.5 * units.deg)
        .add_sample(2320.0 * units.rpm, 24.0 * units.deg)
        .add_sample(2513.3 * units.rpm, 24.4 * units.deg)

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node head {
    // port_flow.py v1.7
    // intake port area: 36.6 cm²; saturated lift: 16.64 mm
    // exhaust port area: 31.1 cm²; saturated lift: 15.36 mm
    // cylinder volume: 2981.7 cm³ (181.95 CI); engine volume: 41.744 L (2547.36 CI)
    // chamber volume: 542.1 cm³ (33.08 CI); conrod/stroke 1.923; N.A.C.C. 211.24
    // 16 harmonic intake runner length: 30.0 cm; diameter: 7.0 cm
    // primary length: 44.5 cm, area: 34.2 cm², diameter: 6.6 cm
    // collector diameter: 21.4 cm, area: 359.4 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 542.127 * units.cc,
        intake_runner_volume: 1165.8 * units.cc,
        intake_runner_cross_section_area: 38.9 * units.cm2,
        exhaust_runner_volume: 466.3 * units.cc,
        exhaust_runner_cross_section_area: 34.2 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "BMW 801",
        starter_torque: 400 * units.Nm,
        starter_speed: 100 * units.rpm,
        redline: redline * units.rpm,
        fuel: a30(),
        throttle_gamma: 2.0,
        jitter: 0.7,
        noise: 0.1,
        hf_gain: 0.007,
        simulation_frequency: 8000,
        fluid_simulation_steps: 6,
        max_sle_solver_steps: 64,
        dyno_min_speed: 500 * units.rpm
    )

    wires wires()

    label crank_moment(disk_moment_of_inertia(mass: crank_mass * units.kg, radius: (stroke / 2.0) * units.mm))
    label flywheel_moment(disk_moment_of_inertia(mass: flywheel_mass * units.kg, radius: (stroke * 3.5) * units.mm))
    label other_moment(disk_moment_of_inertia(mass: 1 * units.kg, radius: 1.0 * units.cm))

    crankshaft c0(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: flywheel_mass * units.kg,
        mass: crank_mass * units.kg,
        friction_torque: 62.6 * units.Nm,
        moment_of_inertia: crank_moment + flywheel_moment + other_moment,
        tdc: 0.0
    )

    piston_parameters piston_params(
        mass: 1500 * units.g,
        blowby: k_28inH2O(0.049),
        compression_height: compression_height,
        wrist_pin_position: 0.0,
        displacement: 0.0
    )

    connecting_rod_parameters cr_params(
        mass: con_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.g,
            length: con_rod * units.mm
        ),
        slave_throw: R1 * units.mm,
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    label slave_rod_mass(850)
    connecting_rod_parameters slave_rod_params(
        mass: slave_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: slave_rod_mass * units.g,
            length: L1 * units.mm
        ),
        center_of_mass: 0.0,
        length: L1 * units.mm
    )

    rod_journal rj0(angle: rot90)
    rod_journal sj0_0(angle: -rot * 1)
    rod_journal sj0_1(angle: -rot * 2)
    rod_journal sj0_2(angle: -rot * 3)
    rod_journal sj0_3(angle: -rot * 4)
    rod_journal sj0_4(angle: -rot * 5)
    rod_journal sj0_5(angle: -rot * 6)

    rod_journal rj1(angle: rot180 + rot90)
    rod_journal sj1_0(angle: -rot * 1)
    rod_journal sj1_1(angle: -rot * 2)
    rod_journal sj1_2(angle: -rot * 3)
    rod_journal sj1_3(angle: -rot * 4)
    rod_journal sj1_4(angle: -rot * 5)
    rod_journal sj1_5(angle: -rot * 6)

    connecting_rod mcr0(cr_params)
    connecting_rod mcr1(cr_params)

    mcr0.add_slave_journal(sj0_0)
    mcr0.add_slave_journal(sj0_1)
    mcr0.add_slave_journal(sj0_2)
    mcr0.add_slave_journal(sj0_3)
    mcr0.add_slave_journal(sj0_4)
    mcr0.add_slave_journal(sj0_5)

    mcr1.add_slave_journal(sj1_0)
    mcr1.add_slave_journal(sj1_1)
    mcr1.add_slave_journal(sj1_2)
    mcr1.add_slave_journal(sj1_3)
    mcr1.add_slave_journal(sj1_4)
    mcr1.add_slave_journal(sj1_5)

    c0
        .add_rod_journal(rj0)
        .add_rod_journal(rj1)

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    label collector_cross_section_area(119.8)

    label exhaust_pipe_length_0(110.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 1000.0) // Litres

    intake intake(
        plenum_volume: 19.0 * units.L,
        plenum_cross_section_area: 272.1 * units.cm2,
        runner_length: 30.0 * units.cm,
        intake_flow_rate: k_carb(2269.8),
        idle_flow_rate: k_carb(0.05),
        idle_throttle_plate_position: 0.9969,
        runner_flow_rate: k_carb(364.8),
        velocity_decay: 0.05
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(1339.8),
        collector_cross_section_area: 119.8 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 44.5 * units.cm,
        primary_flow_rate: k_carb(610.1),
        velocity_decay: 0.75,
        volume: exhaust_volume_0 * units.L
    )

    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_45.wav", volume: 0.001))
    exhaust_system exhaust1(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_57.wav", volume: 0.001))
    exhaust_system exhaust2(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_58.wav", volume: 0.001))

    cylinder_bank b0(bank_params, angle: -(0.0 / cyl) * rot360)
    cylinder_bank b1(bank_params, angle: -(2.0 / cyl) * rot360)
    cylinder_bank b2(bank_params, angle: -(4.0 / cyl) * rot360)
    cylinder_bank b3(bank_params, angle: -(6.0 / cyl) * rot360)
    cylinder_bank b4(bank_params, angle: -(8.0 / cyl) * rot360)
    cylinder_bank b5(bank_params, angle: -(10.0 / cyl) * rot360)
    cylinder_bank b6(bank_params, angle: -(12.0 / cyl) * rot360)
    
    cylinder_bank b7(bank_params, angle: -(0.0 / cyl) * rot360 + rot180)
    cylinder_bank b8(bank_params, angle: -(2.0 / cyl) * rot360 + rot180)
    cylinder_bank b9(bank_params, angle: -(4.0 / cyl) * rot360 + rot180)
    cylinder_bank b10(bank_params, angle: -(6.0 / cyl) * rot360 + rot180)

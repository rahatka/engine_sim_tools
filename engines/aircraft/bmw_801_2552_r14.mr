label exhaust_delay_coeff(1.5)

public node eng_distributor {
    input wires;
    input timing_curve;
    input rev_limit: 3500 * units.rpm;
    input limiter_duration: 0.02;
    alias output __out:
        ignition_module(timing_curve: timing_curve, rev_limit: rev_limit, limiter_duration: limiter_duration)
            .connect_wire(wires.wire1, rot * 0)
            .connect_wire(wires.wire3, rot * 2 + 3.1 * units.deg)
            .connect_wire(wires.wire5, rot * 4 - 1.9 * units.deg)
            .connect_wire(wires.wire7, rot * 6 - 0.8 * units.deg)
            .connect_wire(wires.wire2, rot * 8 + 0.8 * units.deg)
            .connect_wire(wires.wire4, rot * 10 + 1.9 * units.deg)
            .connect_wire(wires.wire6, rot * 12 - 3.1 * units.deg)

            .connect_wire(wires.wire8, rot * 0 + rot360)
            .connect_wire(wires.wire10, rot * 2 + rot360 + 3.1 * units.deg)
            .connect_wire(wires.wire12, rot * 4 + rot360 - 1.9 * units.deg)
            .connect_wire(wires.wire14, rot * 6 + rot360 - 0.8 * units.deg)
            .connect_wire(wires.wire9, rot * 8 + rot360 + 0.8 * units.deg)
            .connect_wire(wires.wire11, rot * 10 + rot360 + 1.9 * units.deg)
            .connect_wire(wires.wire13, rot * 12 + rot360 - 3.1 * units.deg);
}


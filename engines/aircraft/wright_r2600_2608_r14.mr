import "engine_sim.mr"
import "_fuel_30.mr"

units units()
constants constants()

label bore(155.575) // 6 1/8
label stroke(160.337) // 6 5/16
label compression_ratio(6.9)
label con_rod(349.25) // ~ 13 3/4 ?
label L1(271.78) // x 10.7
label R1(78.915) // x
label compression_height(56.1975 * units.mm) // x
label intake_valve_diameter(63.5) // 78.7 ; 72 clear
label exhaust_valve_diameter(63.5) // 78.5 ; 69.5 clear
label intake_valves(1.0)
label exhaust_valves(1.0)

label cyl(14.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

label redline(2900.0) // RPM

// Wright R-2600 (@0.070")
label IVL(14.2875) // .5625
label EVL(14.2875) // .5625
label IVO(20.0 * units.deg) //BTDC
label IVC(50.0 * units.deg) //ABDC
label EVO(65.0 * units.deg) //BBDC
label EVC(40.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(2200.0) // ? g
label crank_mass(30.0) // ? kg
label flywheel_mass(20.0) // kg

label exhaust_delay_coeff(1.5)

public node eng_distributor {
    input wires;
    input timing_curve;
    input rev_limit: 3500 * units.rpm;
    input limiter_duration: 0.02;
    alias output __out:
        ignition_module(timing_curve: timing_curve, rev_limit: rev_limit, limiter_duration: limiter_duration)
            .connect_wire(wires.wire1, rot * 0)
            .connect_wire(wires.wire3, rot * 2 + 3.643 * units.deg)
            .connect_wire(wires.wire5, rot * 4 - 2.214 * units.deg)
            .connect_wire(wires.wire7, rot * 6 - 0.871 * units.deg)
            .connect_wire(wires.wire2, rot * 8 + 0.871 * units.deg)
            .connect_wire(wires.wire4, rot * 10 + 2.214 * units.deg)
            .connect_wire(wires.wire6, rot * 12 - 3.643 * units.deg)

            .connect_wire(wires.wire8, rot * 0 + rot360)
            .connect_wire(wires.wire10, rot * 2 + rot360 + 3.643 * units.deg)
            .connect_wire(wires.wire12, rot * 4 + rot360 - 2.214 * units.deg)
            .connect_wire(wires.wire14, rot * 6 + rot360 - 0.871 * units.deg)
            .connect_wire(wires.wire9, rot * 8 + rot360 + 0.871 * units.deg)
            .connect_wire(wires.wire11, rot * 10 + rot360 + 2.214 * units.deg)
            .connect_wire(wires.wire13, rot * 12 + rot360 - 3.643 * units.deg);
}


        )
    b10.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(slave_rod_params),
            rod_journal: sj1_2,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl11,
            ignition_wire: wires.wire11
        )
    b11.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(slave_rod_params),
            rod_journal: sj1_3,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl12,
            ignition_wire: wires.wire12
        )
    b12.add_cylinder(

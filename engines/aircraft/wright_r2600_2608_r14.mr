    cylinder_bank b12(bank_params, angle: -(10.0 / cyl) * rot360 + rot180)
    cylinder_bank b13(bank_params, angle: -(12.0 / cyl) * rot360 + rot180)
    
    label sh(200.0)
    label spacing_factor(3.5)
    label flange_density(1.0 * exhaust_delay_coeff)
    label pl5((4.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl9((3.0 * bore * spacing_factor / flange_density + sh) * units.mm)

import "../../fuel/30.mr"

units units()
constants constants()

label bore(120.0)
label stroke(140.0)
label compression_ratio(5.8) // 5.3 PEI
label con_rod(240.0)
label compression_height(50.0 * units.mm) // ?
label intake_valve_diameter(57.0)
label exhaust_valve_diameter(51.0)
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(2600.0) // RPM

label cyl(4.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// Bengali
label IVL(10.0) // ?
label EVL(10.0) // ?
label IVO(14.0 * units.deg) //BTDC
label IVC(64.0 * units.deg) //ABDC
label EVO(64.0 * units.deg) //BBDC
label EVC(14.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(979.0)
label piston_mass(700.0) // ?
label crank_mass(12.44) // ?
label flywheel_mass(4.0) // kg

label exhaust_delay_coeff(1.5)


    camshaft _intake_cam_7(base_radius: 33.0 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_7(base_radius: 33.0 * units.mm, lobe_profile: exhaust_lobe_profile)

    camshaft _intake_cam_8(base_radius: 33.0 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_8(base_radius: 33.0 * units.mm, lobe_profile: exhaust_lobe_profile)
    
    camshaft _intake_cam_9(base_radius: 33.0 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_9(base_radius: 33.0 * units.mm, lobe_profile: exhaust_lobe_profile)
    
    camshaft _intake_cam_10(base_radius: 33.0 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_10(base_radius: 33.0 * units.mm, lobe_profile: exhaust_lobe_profile)
    
    camshaft _intake_cam_11(base_radius: 33.0 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_11(base_radius: 33.0 * units.mm, lobe_profile: exhaust_lobe_profile)
    
    camshaft _intake_cam_12(base_radius: 33.0 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_12(base_radius: 33.0 * units.mm, lobe_profile: exhaust_lobe_profile)
    
    camshaft _intake_cam_13(base_radius: 33.0 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_13(base_radius: 33.0 * units.mm, lobe_profile: exhaust_lobe_profile)

    label rot(cycle / cyl)

    _intake_cam_0
        .add_lobe(ILC + 0 * rot)
    _exhaust_cam_0
        .add_lobe(ELC + 0 * rot)

    _intake_cam_1
        .add_lobe(ILC + 4 * rot)
    _exhaust_cam_1
        .add_lobe(ELC + 4 * rot)
    
    _intake_cam_2
        .add_lobe(ILC + 1 * rot)
    _exhaust_cam_2
        .add_lobe(ELC + 1 * rot)
    
    _intake_cam_3
        .add_lobe(ILC + 5 * rot)
    _exhaust_cam_3
        .add_lobe(ELC + 5 * rot)
    
    _intake_cam_4
        .add_lobe(ILC + 2 * rot)
    _exhaust_cam_4
        .add_lobe(ELC + 2 * rot)
        
    _intake_cam_5
        .add_lobe(ILC + 6 * rot)
    _exhaust_cam_5
        .add_lobe(ELC + 6 * rot)
        
    _intake_cam_6
        .add_lobe(ILC + 3 * rot)
    _exhaust_cam_6
        .add_lobe(ELC + 3 * rot)
}

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node head {
    // port_flow.py v1.7
    // intake port area: 27.2 cm²; saturated lift: 14.31 mm
    // exhaust port area: 21.9 cm²; saturated lift: 12.81 mm
    // cylinder volume: 2762.4 cm³ (168.57 CI); engine volume: 19.336 L (1179.98 CI)
    // chamber volume: 613.9 cm³ (37.46 CI); conrod/stroke 1.836; N.A.C.C. 92.51
    // 32 harmonic intake runner length: 21.4 cm; diameter: 5.7 cm
    // primary length: 58.2 cm, area: 24.1 cm², diameter: 5.5 cm
    // collector diameter: 13.5 cm, area: 143.5 cm²

    input chamber_volume;
    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: chamber_volume * units.cc,
        intake_runner_volume: 555.1 * units.cc,
        intake_runner_cross_section_area: 25.9 * units.cm2,
        exhaust_runner_volume: 185.0 * units.cc,
        exhaust_runner_cross_section_area: 24.1 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "Gnome-Rhone 7K",
        starter_torque: 400 * units.Nm,
        starter_speed: 100 * units.rpm,
        redline: redline * units.rpm,
        fuel: a30(),
        throttle_gamma: 2.0,
        jitter: 0.7,
        noise: 0.1,
        hf_gain: 0.007,
        simulation_frequency: 8000,
        dyno_min_speed: 500 * units.rpm
    )

    wires wires()

    label crank_moment(disk_moment_of_inertia(mass: crank_mass * units.kg, radius: (stroke / 2.0) * units.mm))
    label flywheel_moment(disk_moment_of_inertia(mass: flywheel_mass * units.kg, radius: (stroke * 3.5) * units.mm))
    label other_moment(disk_moment_of_inertia(mass: 1 * units.kg, radius: 1.0 * units.cm))

    crankshaft c0(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: flywheel_mass * units.kg,
        mass: crank_mass * units.kg,
        friction_torque: 29.0 * units.Nm,
        moment_of_inertia: crank_moment + flywheel_moment + other_moment,
        tdc: 0.0
    )

    piston_parameters piston_params(
        mass: 1500 * units.g,
        blowby: k_28inH2O(0.046),
        compression_height: compression_height
    )

    connecting_rod_parameters cr_params(
        mass: con_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.g,
            length: con_rod * units.mm
        ),
        slave_throw: R1 * units.mm,
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    label slave_rod_mass(850)
    connecting_rod_parameters slave_rod_params(
        mass: slave_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: slave_rod_mass * units.g,
            length: L1 * units.mm
        ),
        center_of_mass: 0.0,
        length: L1 * units.mm
    )

    rod_journal rj0(angle: rot90)
    rod_journal sj0_0(angle: -rot * 0.5)
    rod_journal sj0_1(angle: -rot * 1.0)
    rod_journal sj0_2(angle: -rot * 1.5)
    rod_journal sj0_3(angle: -rot * 2.0)
    rod_journal sj0_4(angle: -rot * 2.5)
    rod_journal sj0_5(angle: -rot * 3.0)

    connecting_rod mcr0(cr_params)

    mcr0.add_slave_journal(sj0_0)
    mcr0.add_slave_journal(sj0_1)
    mcr0.add_slave_journal(sj0_2)
    mcr0.add_slave_journal(sj0_3)
    mcr0.add_slave_journal(sj0_4)
    mcr0.add_slave_journal(sj0_5)

    c0
        .add_rod_journal(rj0)

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    label collector_cross_section_area(143.5)

    label exhaust_pipe_length_0(120.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 1000.0) // Litres

    intake intake(
        plenum_volume: 6.1 * units.L,
        plenum_cross_section_area: 90.6 * units.cm2,
        runner_length: 21.4 * units.cm,
        intake_flow_rate: k_carb(741.2),
        idle_flow_rate: k_carb(0.05),
        idle_throttle_plate_position: 0.9965,
        runner_flow_rate: k_carb(211.8),
        velocity_decay: 0.05
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(1235.4),
        collector_cross_section_area: 143.5 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 58.2 * units.cm,
        primary_flow_rate: k_carb(353.0),
        velocity_decay: 1.0,
        volume: exhaust_volume_0 * units.L
    )

    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_56.wav", volume: 0.0001))
 
    cylinder_bank b0(bank_params, angle: -(0.0 / cyl) * rot360)
    cylinder_bank b1(bank_params, angle: -(1.0 / cyl) * rot360)
    cylinder_bank b2(bank_params, angle: -(2.0 / cyl) * rot360)
    cylinder_bank b3(bank_params, angle: -(3.0 / cyl) * rot360)
    cylinder_bank b4(bank_params, angle: -(4.0 / cyl) * rot360)
    cylinder_bank b5(bank_params, angle: -(5.0 / cyl) * rot360)
    cylinder_bank b6(bank_params, angle: -(6.0 / cyl) * rot360)
    
    label spacing_factor(3.0)
    label flange_density(1.0 * exhaust_delay_coeff)
    label pl0((4.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl1((3.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl2((2.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl3((1.0 * bore * spacing_factor / flange_density) * units.mm)
    
    b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: mcr0,
            rod_journal: rj0,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl1,
            ignition_wire: wires.wire1
        )
    b1.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(slave_rod_params),
            rod_journal: sj0_0,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl2,
            ignition_wire: wires.wire2
        )
    b2.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(slave_rod_params),
            rod_journal: sj0_1,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl3,
            ignition_wire: wires.wire3
        )
    b3.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(slave_rod_params),
            rod_journal: sj0_2,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl3,
            ignition_wire: wires.wire4
        )
    b4.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(slave_rod_params),
            rod_journal: sj0_3,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl2,
            ignition_wire: wires.wire5
        )
    b5.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(slave_rod_params),
            rod_journal: sj0_4,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl1,
            ignition_wire: wires.wire6
        )
    b6.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(slave_rod_params),
            rod_journal: sj0_5,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl0,
            ignition_wire: wires.wire7
        )

    engine
        .add_cylinder_bank(b0)
        .add_cylinder_bank(b1)
        .add_cylinder_bank(b2)
        .add_cylinder_bank(b3)
        .add_cylinder_bank(b4)
        .add_cylinder_bank(b5)
        .add_cylinder_bank(b6)

    engine.add_crankshaft(c0)

    camshaft_builder camshaft()

    b0.set_cylinder_head (
        head(
            chamber_volume: 613.857,
            intake_camshaft: camshaft.intake_cam_0,
            exhaust_camshaft: camshaft.exhaust_cam_0
        )
    )
    b1.set_cylinder_head (
        head(
            chamber_volume: 652.588,
            intake_camshaft: camshaft.intake_cam_1,
            exhaust_camshaft: camshaft.exhaust_cam_1
        )
    )
    b2.set_cylinder_head (
        head(
            chamber_volume: 668.227,
            intake_camshaft: camshaft.intake_cam_2,
            exhaust_camshaft: camshaft.exhaust_cam_2
        )
    )
    b3.set_cylinder_head (
        head(
            chamber_volume: 624.672,
            intake_camshaft: camshaft.intake_cam_3,
            exhaust_camshaft: camshaft.exhaust_cam_3
        )
    )
    b4.set_cylinder_head (
        head(
            chamber_volume: 624.672,
            intake_camshaft: camshaft.intake_cam_4,
            exhaust_camshaft: camshaft.exhaust_cam_4
        )
    )
    b5.set_cylinder_head (
        head(
            chamber_volume: 668.227,
            intake_camshaft: camshaft.intake_cam_5,
            exhaust_camshaft: camshaft.exhaust_cam_5
        )
    )
    b6.set_cylinder_head (
        head(
            chamber_volume: 652.588,
            intake_camshaft: camshaft.intake_cam_6,
            exhaust_camshaft: camshaft.exhaust_cam_6
        )
    )

    function timing_curve(153.3 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 0.0 * units.deg)
        .add_sample(153.3 * units.rpm, 0.0 * units.deg)
        .add_sample(306.7 * units.rpm, 5.2 * units.deg)
        .add_sample(460.0 * units.rpm, 10.0 * units.deg)
        .add_sample(613.3 * units.rpm, 13.9 * units.deg)
        .add_sample(766.7 * units.rpm, 16.9 * units.deg)
        .add_sample(920.0 * units.rpm, 19.3 * units.deg)
        .add_sample(1073.3 * units.rpm, 21.1 * units.deg)
        .add_sample(1226.7 * units.rpm, 22.6 * units.deg)
        .add_sample(1380.0 * units.rpm, 23.8 * units.deg)
        .add_sample(1533.3 * units.rpm, 24.7 * units.deg)
        .add_sample(1686.7 * units.rpm, 25.4 * units.deg)
        .add_sample(1840.0 * units.rpm, 25.9 * units.deg)
        .add_sample(1993.3 * units.rpm, 26.4 * units.deg)
        .add_sample(2146.7 * units.rpm, 26.7 * units.deg)
        .add_sample(2300.0 * units.rpm, 27.0 * units.deg)
        .add_sample(2453.3 * units.rpm, -12.5 * units.deg)
        .add_sample(2606.7 * units.rpm, -25.0 * units.deg)

    engine.add_ignition_module(
        eng_distributor(
            wires: wires,
            timing_curve: timing_curve
        ))
}

// Pz V
public node veh {
    alias output __out: vehicle;

    label car_mass(45000) // 45.0 t panther

    vehicle vehicle(
        mass: car_mass * units.kg,
        drag_coefficient: 0.6,
        cross_sectional_area: (2830 * units.mm) * (2680 * units.mm),
        diff_ratio: 2.5 * 3,
        tire_radius: (821.77 / 2) * units.mm,
        rolling_resistance: 0.06 * car_mass * 9.81
        )
}

// Pz V
private node trn {
    alias output __out:
        transmission(
            max_clutch_torque: 1500 * units.Nm,
            max_clutch_flex: 15 * units.deg,
            limit_clutch_flex: true,
            clutch_stiffness: 50 * units.Nm / units.deg,
            clutch_damping: 1.0,
            simulate_flex: true
        )
        .add_gear(10.8)  // 3.7 @ 2600
        .add_gear(5.26)  // 7.6
        .add_gear(3.33)  // 12.0
        .add_gear(2.13)  // 18.8
        .add_gear(1.48)  // 27.0
        .add_gear(1.06)  // 37.7
        .add_gear(0.85); // 47.3
}

run(
    engine: eng(),
    transmission: trn(),
    vehicle: veh()
)

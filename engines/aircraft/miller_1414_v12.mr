import "engine_sim.mr"
import "_fuel_10.mr"

units units()
constants constants()

label bore(127.0) // 5
label stroke(152.4) // 6
label compression_ratio(4.5) // ?
label con_rod(304.8) // 12
label compression_height(66.0 * units.mm) // ~ 2.6
label intake_valve_diameter(67.4687) // ~ 2 21/32 ; 2 1/4 clear
label exhaust_valve_diameter(67.4687) // ~ 2 21/32 ; 2 1/4 clear
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(2000.0) // RPM

label cyl(12.0)
label row(2.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label vee(60.0 * units.deg)

label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)

// Miller V12 ????
label IVL(12.7) // 1/2
label EVL(12.7) // 1/2
label IVO(5.0 * units.deg) //BTDC
label IVC(40.0 * units.deg) //ABDC
label EVO(40.0 * units.deg) //BBDC
label EVC(5.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(1800.0) // ?
label piston_mass(800.0) // ?
label crank_mass(30.0) // ? kg
label flywheel_mass(10.0) // kg

label exhaust_delay_coeff(1.5)


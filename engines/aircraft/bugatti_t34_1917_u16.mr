import "engine_sim.mr"
import "../../fuel/30.mr"

units units()
constants constants()

label bore(125.0)
label stroke(160.0)
label compression_ratio(5.0) // ?
label con_rod(270.0) // ~
label IVL(13.3) // ~ lobe lift 10 mm + valve rocker 1.3333
label EVL(16.6) // ~ lobe lift 12.5 mm + valve rocker 1.3333
label compression_height(50.0 * units.mm) // ~
label intake_valve_diameter(42.0) // ~
label exhaust_valve_diameter(58.0) // ~
label intake_valves(2.0)
label exhaust_valves(1.0)

label redline(2500.0) // RPM

label cyl(16.0)
label row(2.0)
label cycle(720.0 * units.deg)
label separation(145 * units.mm) // ~
label rot(cycle / cyl)
label rot45(45.0 * units.deg)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// ?
label IVO(13.0 * units.deg) //BTDC
label IVC(47.0 * units.deg) //ABDC
label EVO(43.0 * units.deg) //BBDC
label EVC(17.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label exhaust_delay_coeff(1.5)

public node eng_distributor {
    input wires;
    input timing_curve;
    input rev_limit: 3750 * units.rpm;
    input limiter_duration: 0.00001;
    alias output __out:
        ignition_module(timing_curve: timing_curve, rev_limit: rev_limit, limiter_duration: limiter_duration)
            .connect_wire(wires.wire1, rot * 0)
            .connect_wire(wires.wire16, rot * 1)
            .connect_wire(wires.wire5, rot * 2)
            .connect_wire(wires.wire9, rot * 3)
            .connect_wire(wires.wire2, rot * 4)
            .connect_wire(wires.wire13, rot * 5)
            .connect_wire(wires.wire6, rot * 6)
            .connect_wire(wires.wire10, rot * 7)
            .connect_wire(wires.wire3, rot * 8)
            .connect_wire(wires.wire14, rot * 9)
            .connect_wire(wires.wire7, rot * 10)
            .connect_wire(wires.wire11, rot * 11)
            .connect_wire(wires.wire4, rot * 12)
            .connect_wire(wires.wire15, rot * 13)
            .connect_wire(wires.wire8, rot * 14)
            .connect_wire(wires.wire12, rot * 15);
}


public node camshaft_builder {
    output intake_cam_0: _intake_cam_0;
    output exhaust_cam_0: _exhaust_cam_0;

    output intake_cam_1: _intake_cam_1;
    output exhaust_cam_1: _exhaust_cam_1;


    camshaft _intake_cam_0(base_radius: 23.3 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 29.1 * units.mm, lobe_profile: exhaust_lobe_profile)
    
    camshaft _intake_cam_1(base_radius: 23.3 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_1(base_radius: 29.1 * units.mm, lobe_profile: exhaust_lobe_profile)

    _intake_cam_0
        .add_lobe(ILC + 0 * rot)
        .add_lobe(ILC + 4 * rot)
        .add_lobe(ILC + 8 * rot)
        .add_lobe(ILC + 12 * rot)
        .add_lobe(ILC + 2 * rot)
        .add_lobe(ILC + 6 * rot)
        .add_lobe(ILC + 10 * rot)
        .add_lobe(ILC + 14 * rot)

    _exhaust_cam_0
        .add_lobe(ELC + 0 * rot)
        .add_lobe(ELC + 4 * rot)
        .add_lobe(ELC + 8 * rot)
        .add_lobe(ELC + 12 * rot)
        .add_lobe(ELC + 2 * rot)
        .add_lobe(ELC + 6 * rot)
        .add_lobe(ELC + 10 * rot)
        .add_lobe(ELC + 14 * rot)
        
    _intake_cam_1
        .add_lobe(ILC + 3 * rot)
        .add_lobe(ILC + 7 * rot)
        .add_lobe(ILC + 11 * rot)
        .add_lobe(ILC + 15 * rot)
        .add_lobe(ILC + 5 * rot)
        .add_lobe(ILC + 9 * rot)
        .add_lobe(ILC + 13 * rot)
        .add_lobe(ILC + 1 * rot)

    _exhaust_cam_1
        .add_lobe(ELC + 3 * rot)
        .add_lobe(ELC + 7 * rot)
        .add_lobe(ELC + 11 * rot)
        .add_lobe(ELC + 15 * rot)
        .add_lobe(ELC + 5 * rot)
        .add_lobe(ELC + 9 * rot)
        .add_lobe(ELC + 13 * rot)
        .add_lobe(ELC + 1 * rot)
}

public node head {
    // port_flow.py v1.5
    // intake port area: 21.3 cm²; saturated lift: 8.96 mm
    // exhaust port area: 20.3 cm²; saturated lift: 12.38 mm
    // cylinder volume: 1963.5 cm³ (119.8 CI); engine volume: 31.416 L (1917.1 CI)
    // chamber volume: 490.9 cm³ (30.0 CI)
    // 32 harmonic intake runner length: 19.6 cm; diameter: 5.1 cm
    // primary length: 72.5 cm, area: 22.3 cm², diameter: 5.3 cm
    // collector diameter: 15.1 cm, area: 178.6 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 490.874 * units.cc,
        intake_runner_volume: 395.4 * units.cc,
        intake_runner_cross_section_area: 20.2 * units.cm2,
        exhaust_runner_volume: 146.4 * units.cc,
        exhaust_runner_cross_section_area: 22.3 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "Bugatti Type 34",
        starter_torque: 400 * units.Nm,
        starter_speed: 150 * units.rpm,
        redline: redline * units.rpm,
        fuel: a30(),
        throttle_gamma: 1.5,
        jitter: 0.8,
        noise: 0.1,
        hf_gain: 0.007,
        simulation_frequency: 8000,
        dyno_min_speed: 300 * units.rpm,
        fluid_simulation_steps: 6,
        max_sle_solver_steps: 32
    )

    wires wires()

    crankshaft c0(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: 25.0 * units.kg,
        mass: 50.0 * units.kg,
        friction_torque: 47.1 * units.Nm,
        moment_of_inertia: 2.0,
        position_x: -separation,
        tdc: 0.0
    )
    
    crankshaft c1(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: 25.0 * units.kg,
        mass: 50.0 * units.kg,
        friction_torque: 47.1 * units.Nm,
        moment_of_inertia: 2.0,
        position_x: separation,
        position_y: 0.0,
        tdc: 0.0
    )

    rod_journal rj0_0(angle: rot * 0 + rot90)
    rod_journal rj1_0(angle: rot * 4 + rot90)
    rod_journal rj2_0(angle: rot * 8 + rot90)
    rod_journal rj3_0(angle: rot * 12 + rot90)
    rod_journal rj4_0(angle: rot * 2 + rot90)
    rod_journal rj5_0(angle: rot * 6 + rot90)
    rod_journal rj6_0(angle: rot * 10 + rot90)
    rod_journal rj7_0(angle: rot * 14 + rot90)
    
    rod_journal rj0_1(angle: rot * 3 + rot90)
    rod_journal rj1_1(angle: rot * 7 + rot90)
    rod_journal rj2_1(angle: rot * 11 + rot90)
    rod_journal rj3_1(angle: rot * 15 + rot90)
    rod_journal rj4_1(angle: rot * 5 + rot90)
    rod_journal rj5_1(angle: rot * 9 + rot90)
    rod_journal rj6_1(angle: rot * 13 + rot90)
    rod_journal rj7_1(angle: rot * 1 + rot90)
    
    c0
        .add_rod_journal(rj0_0)
        .add_rod_journal(rj1_0)
        .add_rod_journal(rj2_0)
        .add_rod_journal(rj3_0)
        .add_rod_journal(rj4_0)
        .add_rod_journal(rj5_0)
        .add_rod_journal(rj6_0)
        .add_rod_journal(rj7_0)
        
    c1
        .add_rod_journal(rj0_1)
        .add_rod_journal(rj1_1)
        .add_rod_journal(rj2_1)
        .add_rod_journal(rj3_1)
        .add_rod_journal(rj4_1)
        .add_rod_journal(rj5_1)
        .add_rod_journal(rj6_1)
        .add_rod_journal(rj7_1)

    piston_parameters piston_params(
        mass: 2000 * units.g,
        blowby: k_28inH2O(0.039),
        compression_height: compression_height,
        wrist_pin_position: 0.0,
        displacement: 0.0
    )

    connecting_rod_parameters cr_params(
        mass: 2500.0 * units.g,
        moment_of_inertia: 0.007,
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    label collector_cross_section_area(89.3)

    label exhaust_pipe_length_0(200.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 100.0) // Litres
    
    label exhaust_pipe_length_1(200.0) // cm
    label exhaust_volume_1(collector_cross_section_area * exhaust_pipe_length_1 / 100.0) // Litres
    
    label spacing_factor(1.2)
    label flange_density(1.5 * exhaust_delay_coeff)

    intake intake(
        plenum_volume: 10.8 * units.L,
        plenum_cross_section_area: 161.3 * units.cm2,
        runner_length: 19.6 * units.cm,
        intake_flow_rate: k_carb(1309.0),
        idle_flow_rate: k_carb(0.002),
        idle_throttle_plate_position: 0.997,
        runner_flow_rate: k_carb(163.6),
        velocity_decay: 0.05
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(1090.8),
        collector_cross_section_area: 89.3 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 72.5 * units.cm,
        primary_flow_rate: k_carb(272.7),
        velocity_decay: 0.8,
        volume: exhaust_volume_0 * units.L
    )
    
    exhaust_system_parameters es_params1(
        outlet_flow_rate: k_carb(1090.8),
        collector_cross_section_area: 89.3 * units.cm2,
        length: exhaust_pipe_length_1 * units.cm,
        primary_tube_length: 72.5 * units.cm,
        primary_flow_rate: k_carb(272.7),
        velocity_decay: 0.8,
        volume: exhaust_volume_1 * units.L
    )

    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_56.wav", volume: 0.00012))
    exhaust_system exhaust1(es_params1, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_57.wav", volume: 0.00010))
    
    label pl0 ((4.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl1 ((3.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl2 ((2.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl3 ((1.0 * bore * spacing_factor / flange_density) * units.mm)
 
    cylinder_bank b0(bank_params, angle: 0 * units.deg, position_x: -separation)
    b0
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj0_0,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl0,
            ignition_wire: wires.wire1
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj1_0,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl1,
            ignition_wire: wires.wire2
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj2_0,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl2,
            ignition_wire: wires.wire3
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj3_0,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl3,
            ignition_wire: wires.wire4
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj4_0,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl0,
            ignition_wire: wires.wire5
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj5_0,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl1,
            ignition_wire: wires.wire6
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj6_0,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl2,
            ignition_wire: wires.wire7
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj7_0,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl3,
            ignition_wire: wires.wire8
        )

    cylinder_bank b1(bank_params, angle: 0 * units.deg, position_x: separation)
    b1
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj0_1,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl0,
            ignition_wire: wires.wire9
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj1_1,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl1,
            ignition_wire: wires.wire10
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj2_1,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl2,
            ignition_wire: wires.wire11
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj3_1,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl3,
            ignition_wire: wires.wire12
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj4_1,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl0,
            ignition_wire: wires.wire13
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj5_1,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl1,
            ignition_wire: wires.wire14
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj6_1,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl2,
            ignition_wire: wires.wire15
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj7_1,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl3,
            ignition_wire: wires.wire16
        )

    engine
        .add_cylinder_bank(b0)
        .add_cylinder_bank(b1)

    engine
        .add_crankshaft(c0)
        .add_crankshaft(c1)

    camshaft_builder camshaft()

    b0.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_0,
            exhaust_camshaft: camshaft.exhaust_cam_0
        )
    )
    
    b1.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_1,
            exhaust_camshaft: camshaft.exhaust_cam_1,
            flip_display: true
        )
    )

    function timing_curve(166.7 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 0.0 * units.deg)
        .add_sample(166.7 * units.rpm, 0.0 * units.deg)
        .add_sample(333.3 * units.rpm, 2.2 * units.deg)
        .add_sample(500.0 * units.rpm, 5.0 * units.deg)
        .add_sample(666.7 * units.rpm, 7.8 * units.deg)
        .add_sample(833.3 * units.rpm, 10.6 * units.deg)
        .add_sample(1000.0 * units.rpm, 13.3 * units.deg)
        .add_sample(1166.7 * units.rpm, 16.1 * units.deg)
        .add_sample(1333.3 * units.rpm, 18.9 * units.deg)
        .add_sample(1500.0 * units.rpm, 20.5 * units.deg)
        .add_sample(1666.7 * units.rpm, 21.2 * units.deg)
        .add_sample(1833.3 * units.rpm, 22.0 * units.deg)
        .add_sample(2000.0 * units.rpm, 22.7 * units.deg)
        .add_sample(2166.7 * units.rpm, 23.5 * units.deg)
        .add_sample(2333.3 * units.rpm, 24.2 * units.deg)
        .add_sample(2500.0 * units.rpm, 25.0 * units.deg)
        .add_sample(2666.7 * units.rpm, -15.0 * units.deg)
        .add_sample(2833.3 * units.rpm, -45.0 * units.deg)

    engine.add_ignition_module(
        eng_distributor(
            wires: wires,
            timing_curve: timing_curve
        )
    )
}

// Pz V
public node veh {
    alias output __out: vehicle;

    label car_mass(45000) // 45.0 t panther

    vehicle vehicle(
        mass: car_mass * units.kg,
        drag_coefficient: 0.6,
        cross_sectional_area: (2830 * units.mm) * (2680 * units.mm),
        diff_ratio: 2.5 * 3,
        tire_radius: (821.77 / 2) * units.mm,
        rolling_resistance: 0.06 * car_mass * 9.81
        )
}

// Pz V
private node trn {
    alias output __out:
        transmission(
            max_clutch_torque: 3000 * units.Nm,
            max_clutch_flex: 15 * units.deg,
            limit_clutch_flex: true,
            clutch_stiffness: 50 * units.Nm / units.deg,
            clutch_damping: 1.0,
            simulate_flex: true
        )
        .add_gear(10.8)  // 3.7 @ 2600
        .add_gear(5.26)  // 7.6
        .add_gear(3.33)  // 12.0
        .add_gear(2.13)  // 18.8
        .add_gear(1.48)  // 27.0
        .add_gear(1.06)  // 37.7
        .add_gear(0.85); // 47.3
}

run(
    engine: eng(),
    transmission: trn(),
    vehicle: veh()
)

import "engine_sim.mr"
import "../../fuel/30.mr"

units units()
constants constants()

label bore(137.1981) // 5.4015
label stroke(152.4) // 6.0
label compression_ratio(7.25)
label con_rod(266.7) // 10 1/2 GAA
label compression_height(69.088 * units.mm) // 2.72
label intake_valve_diameter(53.848) // GAA
label exhaust_valve_diameter(48.26) // GAA
label intake_valves(2.0)
label exhaust_valves(2.0)

label redline(2750.0) // RPM

label cyl(12.0)
label row(2.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label vee(60.0 * units.deg)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// Ford GGA/GGB valve timings
label IVL(12.7)
label EVL(12.7)
label IVO(35.0 * units.deg) //BTDC
label IVC(40.0 * units.deg) //ABDC
label EVO(60.0 * units.deg) //BBDC
label EVC(35.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label exhaust_delay_coeff(1.5)

// GGA 1-11-2-10-3-7-6-8-5-9-4-12 :: 1-12-2-11-3-10-6-7-5-8-4-9
public node gga_distributor {
    input wires;
    input timing_curve;
    input rev_limit: 3250 * units.rpm;
    input limiter_duration: 0.0001;
    alias output __out:
        ignition_module(timing_curve: timing_curve, rev_limit: rev_limit, limiter_duration: limiter_duration)
            .connect_wire(wires.wire1, rot * 0)
            .connect_wire(wires.wire12, rot * 1)
            .connect_wire(wires.wire2, rot * 2)
            .connect_wire(wires.wire11, rot * 3)
            .connect_wire(wires.wire3, rot * 4)
            .connect_wire(wires.wire10, rot * 5)
            .connect_wire(wires.wire6, rot * 6)
            .connect_wire(wires.wire7, rot * 7)
            .connect_wire(wires.wire5, rot * 8)
            .connect_wire(wires.wire8, rot * 9)
            .connect_wire(wires.wire4, rot * 10)
            .connect_wire(wires.wire9, rot * 11);
}


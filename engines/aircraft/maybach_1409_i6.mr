import "../../fuel/10.mr"

units units()
constants constants()

label bore(165.0)
label stroke(180.0)
label compression_ratio(5.95)
label con_rod(310.0)
label compression_height(bore / 2 * units.mm) // ~
label intake_valve_diameter(54.0) // 48 clear ; 11 stem
label exhaust_valve_diameter(54.0) // 
label intake_valves(2.0)
label exhaust_valves(2.0)

label redline(1600.0)

label cyl(6.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// Mercedes D.IVa
label IVL(9.45)
label EVL(9.45) // ?
label IVO(8.0 * units.deg) //BTDC
label IVC(35.0 * units.deg) //ABDC
label EVO(33.0 * units.deg) //BBDC
label EVC(7.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(4050.58) // 8.93 lb
label piston_mass(5579.186) // 12.3 lb
label crank_mass(45.313) // 99.9 lb
label flywheel_mass(20.0) // kg

label exhaust_delay_coeff(1.5)


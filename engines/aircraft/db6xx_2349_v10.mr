import "engine_sim.mr"
import "../../fuel/30.mr"

units units()
constants constants()

// There are 2 variants
// 0: Bank angle 72, firing order 1-6-2-7-4-9-5-10-3-8
// 1: Bank angle 144, firing order 1-9-2-10-4-8-5-6-3-7
label variant(0)

label bore(165.0)
label stroke(180.0)
label compression_ratio(6.9)
label con_rod(306.0) // ~ 1.7 of stroke?
label compression_height(65.0 * units.mm) // ?
label intake_valve_diameter(66.5) // ~
label exhaust_valve_diameter(63.0) // ~
label intake_valves(2.0)
label exhaust_valves(2.0)

label cyl(10.0)
label row(2.0)
label cycle(720.0 * units.deg)
label vee((720.0 / cyl) * (variant + 1) * units.deg)
// label vee(60.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

label redline(3250.0)

// Daimler-Benz DB 603 valve timing
label IVL(14.1)
label EVL(14.1)
label IVO(62.0 * units.deg) //BTDC
label IVC(48.0 * units.deg) //ABDC
label EVO(67.0 * units.deg) //BBDC
label EVC(43.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180) 
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label inv((360.0 * variant) * units.deg)

public node distributor_0 {
    input wires;
    input timing_curve;
    input rev_limit: 3500 * units.rpm;
    input limiter_duration: 0.0001;
    alias output __out:
        ignition_module(timing_curve: timing_curve, rev_limit: rev_limit, limiter_duration: limiter_duration)
            .connect_wire(wires.wire1, rot * 0)
            .connect_wire(wires.wire2, rot * 2)
            .connect_wire(wires.wire4, rot * 4)
            .connect_wire(wires.wire5, rot * 6)
            .connect_wire(wires.wire3, rot * 8)
            .connect_wire(wires.wire6, rot * 0 + vee + inv)
            .connect_wire(wires.wire7, rot * 2 + vee + inv)
            .connect_wire(wires.wire9, rot * 4 + vee + inv)
            .connect_wire(wires.wire10, rot * 6 + vee + inv)
            .connect_wire(wires.wire8, rot * 8 + vee + inv);
}

public node distributor_1 {
    input wires;
    input timing_curve;
    input rev_limit: 3500 * units.rpm;
    input limiter_duration: 0.0001;
    alias output __out:
        ignition_module(timing_curve: timing_curve, rev_limit: rev_limit, limiter_duration: limiter_duration)
            .connect_wire(wires.wire1, rot * 0)
            .connect_wire(wires.wire5, rot * 2)
            .connect_wire(wires.wire2, rot * 4)
            .connect_wire(wires.wire3, rot * 6)
            .connect_wire(wires.wire4, rot * 8)
            .connect_wire(wires.wire6, rot * 0 + vee + inv)
            .connect_wire(wires.wire10, rot * 2 + vee + inv)
            .connect_wire(wires.wire7, rot * 4 + vee + inv)
            .connect_wire(wires.wire8, rot * 6 + vee + inv)
            .connect_wire(wires.wire9, rot * 8 + vee + inv);
}

private node wires {
    output wire1: ignition_wire();
    output wire2: ignition_wire();
    output wire3: ignition_wire();
    output wire4: ignition_wire();
    output wire5: ignition_wire();
    output wire6: ignition_wire();
    output wire7: ignition_wire();
    output wire8: ignition_wire();
    output wire9: ignition_wire();
    output wire10: ignition_wire();
}


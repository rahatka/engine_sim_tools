import "engine_sim.mr"
import "../../fuel/30.mr"

units units()
constants constants()

label bore(142.875) // 5 5/8
label stroke(139.7) // 5 1/2
label compression_ratio(5.2)
label con_rod(266.7) // ~ 10 1/2
label L1(196.2) // ?
label R1(73.025) // ~ 2 7/8
label compression_height(50.8 * units.mm) // ?
label intake_valve_diameter(45.2437) // 1 25/32
label exhaust_valve_diameter(45.2437) // 1 25/32
label intake_valves(2.0)
label exhaust_valves(2.0)

label redline(2500.0) // RPM

label cyl(12.0)
label row(1.0)
label cycle(720.0 * units.deg)
label vee(60.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// Curtiss Chieftain
label IVL(12.7) // 1/2
label EVL(12.7) // 1/2
label IVO(18.0 * units.deg) //BTDC
label IVC(60.0 * units.deg) //ABDC
label EVO(80.0 * units.deg) //BBDC
label EVC(5.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(1500.0) // ? g
label crank_mass(25.0) // ? kg
label flywheel_mass(15.0) // kg

// v1 1-10-5-7-4-11-8-3-12-2-9-6
public node distributor_0 {
    input wires;
    input timing_curve;
    input rev_limit: 12000 * units.rpm;
    input limiter_duration: 0.0001;
    alias output __out:
        ignition_module(timing_curve: timing_curve, rev_limit: rev_limit, limiter_duration: limiter_duration)
            .connect_wire(wires.wire1, rot * 0)
            .connect_wire(wires.wire10, rot * 1)
            .connect_wire(wires.wire5, rot * 2)
            .connect_wire(wires.wire7, rot * 3)
            .connect_wire(wires.wire4, rot * 4)
            .connect_wire(wires.wire11, rot * 5)
            .connect_wire(wires.wire8, rot * 6)
            .connect_wire(wires.wire3, rot * 7)
            .connect_wire(wires.wire12, rot * 8)
            .connect_wire(wires.wire2, rot * 9)
            .connect_wire(wires.wire9, rot * 10)
            .connect_wire(wires.wire6, rot * 11);
}


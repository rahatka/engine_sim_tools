private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node head {
    // port_flow.py v1.7
    // intake port area: 26.0 cm²; saturated lift: 9.94 mm
    // exhaust port area: 26.0 cm²; saturated lift: 9.94 mm
    // cylinder volume: 2239.7 cm³ (136.68 CI); engine volume: 26.877 L (1640.13 CI)
    // chamber volume: 533.3 cm³ (32.54 CI); conrod/stroke 1.909; N.A.C.C. 151.88
    // 32 harmonic intake runner length: 18.9 cm; diameter: 5.6 cm
    // primary length: 50.2 cm, area: 28.6 cm², diameter: 6.0 cm
    // collector diameter: 20.9 cm, area: 343.8 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 533.273 * units.cc,
        intake_runner_volume: 469.7 * units.cc,
        intake_runner_cross_section_area: 24.9 * units.cm2,
        exhaust_runner_volume: 174.0 * units.cc,
        exhaust_runner_cross_section_area: 28.6 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "Curtiss Chieftain",
        starter_torque: 300 * units.Nm,
        starter_speed: 200 * units.rpm,
        redline: redline * units.rpm,
        fuel: a30(),
        throttle_gamma: 1.2,
        jitter: 0.8,
        noise: 0.1,
        hf_gain: 0.008,
        simulation_frequency: 8000,
        dyno_min_speed: 500 * units.rpm
    )

    wires wires()

    connecting_rod_parameters cr_params(
        mass: con_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.g,
            length: con_rod * units.mm
        ),
        center_of_mass: 0.0,
        slave_throw: R1 * units.mm,
        length: con_rod * units.mm
    )

    label slave_rod_mass(500.0)
    connecting_rod_parameters slave_rod_params(
        mass: slave_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: slave_rod_mass * units.g,
            length: L1 * units.mm
        ),
        center_of_mass: 0.0,
        length: L1 * units.mm
    )

    rod_journal rj0(angle: rot180)
    rod_journal rj1(angle: 0)

    rod_journal sj0_0(angle: vee)
    rod_journal sj0_1(angle: -vee)

    rod_journal sj0_3(angle: vee)
    rod_journal sj0_4(angle: -vee)

    rod_journal sj1_0(angle: vee)
    rod_journal sj1_1(angle: -vee)

    rod_journal sj1_3(angle: vee)
    rod_journal sj1_4(angle: -vee)

    connecting_rod mcr0_0(cr_params)
    connecting_rod mcr0_1(cr_params)
    connecting_rod mcr1_0(cr_params)
    connecting_rod mcr1_1(cr_params)

    mcr0_0.add_slave_journal(sj0_0)
    mcr0_0.add_slave_journal(sj0_1)

    mcr0_1.add_slave_journal(sj0_3)
    mcr0_1.add_slave_journal(sj0_4)

    mcr1_0.add_slave_journal(sj1_0)
    mcr1_0.add_slave_journal(sj1_1)

    mcr1_1.add_slave_journal(sj1_3)
    mcr1_1.add_slave_journal(sj1_4)

    label crank_moment(disk_moment_of_inertia(mass: crank_mass * units.kg, radius: (stroke / 2.0) * units.mm))
    label flywheel_moment(disk_moment_of_inertia(mass: flywheel_mass * units.kg, radius: (stroke * 3.5) * units.mm))
    label other_moment(disk_moment_of_inertia(mass: 1 * units.kg, radius: 1.0 * units.cm))

    crankshaft c0(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: flywheel_mass * units.kg,
        mass: crank_mass * units.kg,
        friction_torque: 40.3 * units.Nm,
        moment_of_inertia: crank_moment + flywheel_moment + other_moment,
        tdc: 0.0
    )

    c0
        .add_rod_journal(rj0)
        .add_rod_journal(rj1)

    piston_parameters piston_params(
        mass: 310.0 * units.g,
        blowby: k_28inH2O(0.045),
        compression_height: compression_height,
        wrist_pin_position: 0.0,
        displacement: 0.0
    )

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    label collector_cross_section_area(114.6)

    label exhaust_pipe_length_0(50.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 1000.0) // Litres
    
    label spacing_factor(1.1)
    label flange_density(2.0)

    intake intake(
        plenum_volume: 9.7 * units.L,
        plenum_cross_section_area: 149.2 * units.cm2,
        runner_length: 18.9 * units.cm,
        intake_flow_rate: k_carb(1175.9),
        idle_flow_rate: k_carb(0.01),
        idle_throttle_plate_position: 0.997,
        runner_flow_rate: k_carb(205.8),
        velocity_decay: 0.05
    )

    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(669.6),
        collector_cross_section_area: 114.6 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 50.2 * units.cm,
        primary_flow_rate: k_carb(343.2),
        velocity_decay: 0.75,
        volume: exhaust_volume_0 * units.L
    )
    
    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_45.wav", volume: 0.001))
    exhaust_system exhaust1(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_56.wav", volume: 0.001))
    exhaust_system exhaust2(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_58.wav", volume: 0.001))
 
    cylinder_bank b0(bank_params, angle: rot90)
    cylinder_bank b1(bank_params, angle: rot90 - vee * 1)
    cylinder_bank b2(bank_params, angle: rot90 - vee * 2)
    cylinder_bank b3(bank_params, angle: rot90 - vee * 3)
    cylinder_bank b4(bank_params, angle: rot90 - vee * 4)
    cylinder_bank b5(bank_params, angle: rot90 - vee * 5)

    label pl0((1.5 * bore) * units.mm)
    label pl1((1.0 * bore) * units.mm)

    b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: mcr0_0,
            rod_journal: rj0,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl0,
            ignition_wire: wires.wire1
        )
    b1.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(slave_rod_params),
            rod_journal: sj0_1,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl0,
            ignition_wire: wires.wire3
        )
    b2.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(slave_rod_params),
            rod_journal: sj0_3,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl0,
            ignition_wire: wires.wire5
        )
    b3.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: mcr0_1,
            rod_journal: rj0,
            intake: intake,
            exhaust_system: exhaust2,
            primary_length: pl0,
            ignition_wire: wires.wire7
        )
    b4.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(slave_rod_params),
            rod_journal: sj0_4,
            intake: intake,
            exhaust_system: exhaust2,
            primary_length: pl0,
            ignition_wire: wires.wire9
        )
    b5.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(slave_rod_params),
            rod_journal: sj0_0,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl0,
            ignition_wire: wires.wire11
        )

    b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: mcr1_0,
            rod_journal: rj1,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl1,
            ignition_wire: wires.wire2
        )
    b1.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(slave_rod_params),
            rod_journal: sj1_1,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl1,
            ignition_wire: wires.wire4
        )
    b2.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(slave_rod_params),
            rod_journal: sj1_3,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl1,
            ignition_wire: wires.wire6
        )
    b3.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: mcr1_1,
            rod_journal: rj1,
            intake: intake,
            exhaust_system: exhaust2,
            primary_length: pl1,
            ignition_wire: wires.wire8
        )
    b4.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(slave_rod_params),
            rod_journal: sj1_4,
            intake: intake,
            exhaust_system: exhaust2,
            primary_length: pl1,
            ignition_wire: wires.wire10
        )
    b5.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(slave_rod_params),
            rod_journal: sj1_0,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl1,
            ignition_wire: wires.wire12
        )

    engine
        .add_cylinder_bank(b0)
        .add_cylinder_bank(b1)
        .add_cylinder_bank(b2)
        .add_cylinder_bank(b3)
        .add_cylinder_bank(b4)
        .add_cylinder_bank(b5)

    engine.add_crankshaft(c0)

    camshaft_builder_1 camshaft()
    
    engine.add_ignition_module(
        distributor_1(
            wires: wires,
            timing_curve: timing_curve
        )
    )

    b0.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_0,
            exhaust_camshaft: camshaft.exhaust_cam_0
        )
    )
    b1.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_1,
            exhaust_camshaft: camshaft.exhaust_cam_1,
            flip_display: true
        )
    )
    b2.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_2,
            exhaust_camshaft: camshaft.exhaust_cam_2
        )
    )
    b3.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_3,
            exhaust_camshaft: camshaft.exhaust_cam_3,
            flip_display: true
        )
    )
    b4.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_4,
            exhaust_camshaft: camshaft.exhaust_cam_4
        )
    )
    b5.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_5,
            exhaust_camshaft: camshaft.exhaust_cam_5,
            flip_display: true
        )
    )

    function timing_curve(166.7 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 0.0 * units.deg)
        .add_sample(166.7 * units.rpm, 0.0 * units.deg)
        .add_sample(333.3 * units.rpm, 7.5 * units.deg)
        .add_sample(500.0 * units.rpm, 14.0 * units.deg)
        .add_sample(666.7 * units.rpm, 19.0 * units.deg)
        .add_sample(833.3 * units.rpm, 22.8 * units.deg)
        .add_sample(1000.0 * units.rpm, 25.8 * units.deg)
        .add_sample(1166.7 * units.rpm, 28.1 * units.deg)
        .add_sample(1333.3 * units.rpm, 29.9 * units.deg)
        .add_sample(1500.0 * units.rpm, 31.3 * units.deg)
        .add_sample(1666.7 * units.rpm, 32.4 * units.deg)
        .add_sample(1833.3 * units.rpm, 33.2 * units.deg)
        .add_sample(2000.0 * units.rpm, 33.8 * units.deg)
        .add_sample(2166.7 * units.rpm, 34.3 * units.deg)
        .add_sample(2333.3 * units.rpm, 34.7 * units.deg)
        .add_sample(2500.0 * units.rpm, 35.0 * units.deg)
        .add_sample(2666.7 * units.rpm, -12.5 * units.deg)
        .add_sample(2833.3 * units.rpm, -25.0 * units.deg)
}

// Pz V
public node veh {
    alias output __out: vehicle;

    label car_mass(45000) // 45.0 t panther

    vehicle vehicle(
        mass: car_mass * units.kg,
        drag_coefficient: 0.6,
        cross_sectional_area: (2830 * units.mm) * (2680 * units.mm),
        diff_ratio: 3.5 * 3,
        tire_radius: (821.77 / 2) * units.mm,
        rolling_resistance: 0.06 * car_mass * 9.81
        )
}

// Pz V
private node trn {
    alias output __out:
        transmission(
            max_clutch_torque: 2000 * units.Nm,
            max_clutch_flex: 15 * units.deg,
            limit_clutch_flex: true,
            clutch_stiffness: 50 * units.Nm / units.deg,
            clutch_damping: 1.0,
            simulate_flex: true
        )
        .add_gear(10.8)  // 3.7 @ 2600
        .add_gear(5.26)  // 7.6
        .add_gear(3.33)  // 12.0
        .add_gear(2.13)  // 18.8
        .add_gear(1.48)  // 27.0
        .add_gear(1.06)  // 37.7
        .add_gear(0.85); // 47.3
}

run(
    engine: eng(),
    transmission: trn(),
    vehicle: veh()
)

    _intake_cam_3
        .add_lobe(ILC + 3 * rot) // 7
        .add_lobe(ILC + 6 * rot) // 8
    _exhaust_cam_3
        .add_lobe(ELC + 3 * rot) // 7
        .add_lobe(ELC + 6 * rot) // 8
        
    _intake_cam_4
        .add_lobe(ILC + 10 * rot) // 9
        .add_lobe(ILC + 1 * rot) // 10
    _exhaust_cam_4
        .add_lobe(ELC + 10 * rot) // 9

import "engine_sim.mr"
import "../../fuel/10.mr"

units units()
constants constants()

label bore(125.0)
label stroke(180.0)
label compression_ratio(5.0)
label con_rod(267.5) // ~
label compression_height(55.0 * units.mm) // ~
label intake_valve_diameter(41.0) // ~ ; ~ 37.5 clear
label exhaust_valve_diameter(60.0) // ~ ; ~ 55.5 clear
label intake_valves(2.0)
label exhaust_valves(1.0)

label redline(2500.0) // RPM

label cyl(16.0)
label row(2.0)
label cyl_base(cyl / row)
label cycle(720.0 * units.deg)
label separation(145 * units.mm) // ~
label rot(cycle / cyl)
label rot_base(cycle / cyl_base)
label rot45(45.0 * units.deg)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label sb_shift(rot * 5)

// Bugatti U16
label IVL(13.3) // ~ lobe lift 10 mm + valve rocker 1 1/3
label EVL(16.6) // ~ lobe lift 12.5 mm + valve rocker 1 1/3
label IVO(00.0 * units.deg) //BTDC
label IVC(45.0 * units.deg) //ABDC
label EVO(45.0 * units.deg) //BBDC
label EVC(15.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(1587.0) // 3 lb 8 oz
label piston_mass(1000.0) // ?
label crank_mass(43.68) // 96 lb 5 oz
label flywheel_mass(10.0) // kg

label exhaust_delay_coeff(1.5)


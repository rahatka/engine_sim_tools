import "engine_sim.mr"
import "../../fuel/10.mr"

units units()
constants constants()

label bore(145.0)
label stroke(190.0)
label compression_ratio(5.8) // 4.93 ?
label con_rod(330.0)
label compression_height(75.5 * units.mm) // ~
label intake_valve_diameter(57.0) // 57 ? ; 52.0 clear
label exhaust_valve_diameter(57.0) // 57 ? ; 52.0 clear
label intake_valves(2.0)
label exhaust_valves(2.0)

label redline(1600.0)

label cyl(6.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// Benz Bz IV 1-1-24 #1
label IVL(11.8)
label EVL(11.25)
label IVO(10.0 * units.deg) //BTDC
label IVC(55.0 * units.deg) //ABDC
label EVO(60.0 * units.deg) //BBDC
label EVC(20.0 * units.deg) //ATDC

// Benz Bz IV 1-1-24 #2
// label IVO(-4.0 * units.deg) //BTDC
// label IVC(50.0 * units.deg) //ABDC
// label EVO(61.0 * units.deg) //BBDC
// label EVC(22.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(1500.0) // ?
label piston_mass(1000.0) // ?
label crank_mass(50.0) // ?
label flywheel_mass(15.0) // kg

label exhaust_delay_coeff(1.5)


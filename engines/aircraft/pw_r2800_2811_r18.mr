public node bank_builder {
    input shift;
    input ignition;
    input intake;

    input ex0;
    input ex1;
    input ex2;
    input ex3;
    input ex4;
    input ex5;
    input ex6;
    input ex7;
    input ex8;

    input pl0;
    input pl1;
    input pl2;
    input pl3;
    input pl4;
    input pl5;
    input pl6;
    input pl7;
    input pl8;

    output rj0: _rj0;

    output b0: _b0;
    output b1: _b1;
    output b2: _b2;
    output b3: _b3;
    output b4: _b4;
    output b5: _b5;
    output b6: _b6;
    output b7: _b7;
    output b8: _b8;

    ignition_wire _wire1()
    ignition_wire _wire2()
    ignition_wire _wire3()
    ignition_wire _wire4()
    ignition_wire _wire5()
    ignition_wire _wire6()
    ignition_wire _wire7()
    ignition_wire _wire8()
    ignition_wire _wire9()

    connecting_rod_parameters cr_params(
        mass: con_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.g,
            length: con_rod * units.mm
        ),
        slave_throw: R1 * units.mm,
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    label slave_rod_mass(700)
    connecting_rod_parameters slave_rod_params(
        mass: slave_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: slave_rod_mass * units.g,
            length: L1 * units.mm
        ),
        center_of_mass: 0.0,
        length: L1 * units.mm
    )

    rod_journal _rj0(angle: shift / 2.0 + rot90)
    rod_journal sj0(angle: -rot_base * 0.5)
    rod_journal sj1(angle: -rot_base * 1.0)
    rod_journal sj2(angle: -rot_base * 1.5)
    rod_journal sj3(angle: -rot_base * 2.0)
    rod_journal sj4(angle: -rot_base * 2.5)
    rod_journal sj5(angle: -rot_base * 3.0)
    rod_journal sj6(angle: -rot_base * 3.5)
    rod_journal sj7(angle: -rot_base * 4.0)

    connecting_rod mcr0(cr_params)

    mcr0.add_slave_journal(sj0)
    mcr0.add_slave_journal(sj1)
    mcr0.add_slave_journal(sj2)
    mcr0.add_slave_journal(sj3)
    mcr0.add_slave_journal(sj4)
    mcr0.add_slave_journal(sj5)
    mcr0.add_slave_journal(sj6)
    mcr0.add_slave_journal(sj7)

    piston_parameters piston_params(
        mass: 1500 * units.g,
        blowby: k_28inH2O(0.046),
        compression_height: compression_height,
        wrist_pin_position: 0.0,
        displacement: 0.0
    )

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    camshaft _intake_cam_0(base_radius: 35.6 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 35.6 * units.mm, lobe_profile: exhaust_lobe_profile)

    camshaft _intake_cam_1(base_radius: 35.6 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_1(base_radius: 35.6 * units.mm, lobe_profile: exhaust_lobe_profile)
    
    camshaft _intake_cam_2(base_radius: 35.6 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_2(base_radius: 35.6 * units.mm, lobe_profile: exhaust_lobe_profile)
    
    camshaft _intake_cam_3(base_radius: 35.6 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_3(base_radius: 35.6 * units.mm, lobe_profile: exhaust_lobe_profile)
    
    camshaft _intake_cam_4(base_radius: 35.6 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_4(base_radius: 35.6 * units.mm, lobe_profile: exhaust_lobe_profile)
    
    camshaft _intake_cam_5(base_radius: 35.6 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_5(base_radius: 35.6 * units.mm, lobe_profile: exhaust_lobe_profile)
    
    camshaft _intake_cam_6(base_radius: 35.6 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_6(base_radius: 35.6 * units.mm, lobe_profile: exhaust_lobe_profile)

    camshaft _intake_cam_7(base_radius: 35.6 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_7(base_radius: 35.6 * units.mm, lobe_profile: exhaust_lobe_profile)
    
    camshaft _intake_cam_8(base_radius: 35.6 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_8(base_radius: 35.6 * units.mm, lobe_profile: exhaust_lobe_profile)

    _intake_cam_0
        .add_lobe(ILC + 0 * rot_base + shift)
    _exhaust_cam_0
        .add_lobe(ELC + 0 * rot_base + shift)

    _intake_cam_1
        .add_lobe(ILC + 5 * rot_base + shift)
    _exhaust_cam_1
        .add_lobe(ELC + 5 * rot_base + shift)
    
    _intake_cam_2
        .add_lobe(ILC + 1 * rot_base + shift)
    _exhaust_cam_2
        .add_lobe(ELC + 1 * rot_base + shift)
    
    _intake_cam_3
        .add_lobe(ILC + 6 * rot_base + shift)
    _exhaust_cam_3
        .add_lobe(ELC + 6 * rot_base + shift)
    
    _intake_cam_4
        .add_lobe(ILC + 2 * rot_base + shift)
    _exhaust_cam_4
        .add_lobe(ELC + 2 * rot_base + shift)
        
    _intake_cam_5
        .add_lobe(ILC + 7 * rot_base + shift)
    _exhaust_cam_5
        .add_lobe(ELC + 7 * rot_base + shift)
        
    _intake_cam_6
        .add_lobe(ILC + 3 * rot_base + shift)
    _exhaust_cam_6
        .add_lobe(ELC + 3 * rot_base + shift)

    _intake_cam_7
        .add_lobe(ILC + 8 * rot_base + shift)
    _exhaust_cam_7
        .add_lobe(ELC + 8 * rot_base + shift)
        
    _intake_cam_8
        .add_lobe(ILC + 4 * rot_base + shift)
    _exhaust_cam_8
        .add_lobe(ELC + 4 * rot_base + shift)

    cylinder_bank _b0(bank_params, angle: -vee * 0 - shift / 2.0)
    cylinder_bank _b1(bank_params, angle: -vee * 1 - shift / 2.0)
    cylinder_bank _b2(bank_params, angle: -vee * 2 - shift / 2.0)
    cylinder_bank _b3(bank_params, angle: -vee * 3 - shift / 2.0)
    cylinder_bank _b4(bank_params, angle: -vee * 4 - shift / 2.0)
    cylinder_bank _b5(bank_params, angle: -vee * 5 - shift / 2.0)
    cylinder_bank _b6(bank_params, angle: -vee * 6 - shift / 2.0)
    cylinder_bank _b7(bank_params, angle: -vee * 7 - shift / 2.0)
    cylinder_bank _b8(bank_params, angle: -vee * 8 - shift / 2.0)

    _b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: mcr0,
            rod_journal: _rj0,
            intake: intake,
            exhaust_system: ex0,
            primary_length: pl0,
            ignition_wire: _wire1
        )
    _b1.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(slave_rod_params),
            rod_journal: sj0,
            intake: intake,
            exhaust_system: ex1,
            primary_length: pl1,
            ignition_wire: _wire2
        )
    _b2.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(slave_rod_params),
            rod_journal: sj1,
            intake: intake,
            exhaust_system: ex2,
            primary_length: pl2,
            ignition_wire: _wire3
        )
    _b3.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(slave_rod_params),
            rod_journal: sj2,
            intake: intake,
            exhaust_system: ex3,
            primary_length: pl3,
            ignition_wire: _wire4
        )
    _b4.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(slave_rod_params),
            rod_journal: sj3,
            intake: intake,
            exhaust_system: ex4,
            primary_length: pl4,
            ignition_wire: _wire5
        )
    _b5.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(slave_rod_params),
            rod_journal: sj4,
            intake: intake,
            exhaust_system: ex5,
            primary_length: pl5,
            ignition_wire: _wire6
        )
    _b6.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(slave_rod_params),
            rod_journal: sj5,
            intake: intake,
            exhaust_system: ex6,
            primary_length: pl6,
            ignition_wire: _wire7
        )
    _b7.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(slave_rod_params),
            rod_journal: sj6,
            intake: intake,
            exhaust_system: ex7,
            primary_length: pl7,
            ignition_wire: _wire8
        )
    _b8.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(slave_rod_params),
            rod_journal: sj7,
            intake: intake,
            exhaust_system: ex8,
            primary_length: pl8,
            ignition_wire: _wire9
        )

    _b0.set_cylinder_head(
        head(
            chamber_volume: 451.887,
            intake_camshaft: _intake_cam_0,
            exhaust_camshaft: _exhaust_cam_0
        )
    )
    _b1.set_cylinder_head(
        head(
            chamber_volume: 446.817,
            intake_camshaft: _intake_cam_1,
            exhaust_camshaft: _exhaust_cam_1
        )
    )
    _b2.set_cylinder_head(
        head(
            chamber_volume: 474.216,
            intake_camshaft: _intake_cam_2,
            exhaust_camshaft: _exhaust_cam_2
        )
    )
    _b3.set_cylinder_head(
        head(
            chamber_volume: 459.455,
            intake_camshaft: _intake_cam_3,
            exhaust_camshaft: _exhaust_cam_3
        )
    )
    _b4.set_cylinder_head(
        head(
            chamber_volume: 430.068,
            intake_camshaft: _intake_cam_4,
            exhaust_camshaft: _exhaust_cam_4
        )
    )
    _b5.set_cylinder_head(
        head(
            chamber_volume: 430.068,
            intake_camshaft: _intake_cam_5,
            exhaust_camshaft: _exhaust_cam_5
        )
    )
    _b6.set_cylinder_head(
        head(
            chamber_volume: 459.455,
            intake_camshaft: _intake_cam_6,
            exhaust_camshaft: _exhaust_cam_6
        )
    )
    _b7.set_cylinder_head(
        head(
            chamber_volume: 474.216,
            intake_camshaft: _intake_cam_7,
            exhaust_camshaft: _exhaust_cam_7
        )
    )
    _b8.set_cylinder_head(
        head(
            chamber_volume: 446.817,
            intake_camshaft: _intake_cam_8,
            exhaust_camshaft: _exhaust_cam_8
        )
    )

    ignition
        .connect_wire(_wire1, rot_base * 0 - shift)
        .connect_wire(_wire3, rot_base * 1 - shift + 3.1 * units.deg)
        .connect_wire(_wire5, rot_base * 2 - shift + 2.0 * units.deg)
        .connect_wire(_wire7, rot_base * 3 - shift - 4.4 * units.deg)
        .connect_wire(_wire9, rot_base * 4 - shift - 0.6 * units.deg)
        .connect_wire(_wire2, rot_base * 5 - shift + 0.6 * units.deg)
        .connect_wire(_wire4, rot_base * 6 - shift + 4.4 * units.deg)
        .connect_wire(_wire6, rot_base * 7 - shift - 2.0 * units.deg)
        .connect_wire(_wire8, rot_base * 8 - shift - 3.1 * units.deg)
}

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node head {
    // port_flow.py v1.7
    // intake port area: 36.1 cm²; saturated lift: 16.54 mm
    // exhaust port area: 25.6 cm²; saturated lift: 13.92 mm
    // cylinder volume: 2553.2 cm³ (155.80 CI); engine volume: 45.957 L (2804.46 CI)
    // chamber volume: 451.9 cm³ (27.58 CI); conrod/stroke 2.052; N.A.C.C. 238.05
    // 16 harmonic intake runner length: 29.9 cm; diameter: 6.6 cm
    // primary length: 42.5 cm, area: 28.2 cm², diameter: 6.0 cm
    // collector diameter: 22.0 cm, area: 380.2 cm²

    input chamber_volume;
    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: chamber_volume * units.cc,
        intake_runner_volume: 1030.5 * units.cc,
        intake_runner_cross_section_area: 34.5 * units.cm2,
        exhaust_runner_volume: 412.2 * units.cc,
        exhaust_runner_cross_section_area: 28.2 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "P&W R-2800",
        starter_torque: 600 * units.Nm,
        starter_speed: 150 * units.rpm,
        redline: redline * units.rpm,
        fuel: a30(),
        throttle_gamma: 2.0,
        jitter: 0.6,
        noise: 0.1,
        hf_gain: 0.005,
        fluid_simulation_steps: 6,
        max_sle_solver_steps: 32,
        simulation_frequency: 6000,
        dyno_min_speed: 500 * units.rpm
    )

    label crank_moment(disk_moment_of_inertia(mass: crank_mass * units.kg, radius: (stroke / 2.0) * units.mm))
    label flywheel_moment(disk_moment_of_inertia(mass: flywheel_mass * units.kg, radius: (stroke * 3.5) * units.mm))
    label other_moment(disk_moment_of_inertia(mass: 1 * units.kg, radius: 1.0 * units.cm))

    crankshaft c0(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: flywheel_mass * units.kg,
        mass: crank_mass * units.kg,
        friction_torque: 68.9 * units.Nm,
        moment_of_inertia: crank_moment + flywheel_moment + other_moment,
        tdc: 0.0
    )

    bank_builder bank0(
        shift: 0,
        ignition: ignition,
        intake: intake,
        ex0: exhaust0,
        ex1: exhaust0,
        ex2: exhaust0,
        ex3: exhaust0,
        ex4: exhaust0,
        ex5: exhaust1,
        ex6: exhaust1,
        ex7: exhaust1,
        ex8: exhaust1,
        sh0 + pl0,
        sh0 + pl1,
        sh0 + pl2,
        sh0 + pl3,
        sh0 + pl4,
        sh0 + pl3,
        sh0 + pl2,
        sh0 + pl1,
        sh0 + pl0
    )

    bank_builder bank1(
        shift: rot360,
        ignition: ignition,
        intake: intake,
        ex0: exhaust1,
        ex1: exhaust1,
        ex2: exhaust1,
        ex3: exhaust1,
        ex4: exhaust1,
        ex5: exhaust0,
        ex6: exhaust0,
        ex7: exhaust0,
        ex8: exhaust0,
        sh1 + pl4,
        sh1 + pl3,
        sh1 + pl2,
        sh1 + pl1,
        sh1 + pl0,
        sh1 + pl1,
        sh1 + pl2,
        sh1 + pl3,
        sh1 + pl4
    )

    ignition_module ignition(timing_curve: timing_curve, rev_limit: 4000 * units.rpm, limiter_duration: 0.001)
    
    c0
        .add_rod_journal(bank0.rj0)
        .add_rod_journal(bank1.rj0)

    label collector_cross_section_area(190.1)

    intake intake(
        plenum_volume: 21.7 * units.L,
        plenum_cross_section_area: 310.6 * units.cm2,
        runner_length: 29.9 * units.cm,
        intake_flow_rate: k_carb(2585.1),
        idle_flow_rate: k_carb(0.05),
        idle_throttle_plate_position: 0.997,
        runner_flow_rate: k_carb(323.1),
        velocity_decay: 0.05
    )
    
    label exhaust_pipe_length_0(220.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 1000.0) // Litres
    
    label exhaust_pipe_length_1(200.0) // cm
    label exhaust_volume_1(collector_cross_section_area * exhaust_pipe_length_1 / 1000.0) // Litres
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(2288.9),
        collector_cross_section_area: 190.1 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 42.5 * units.cm,
        primary_flow_rate: k_carb(540.4),
        velocity_decay: 0.8,
        volume: exhaust_volume_0 * units.L
    )
    
    exhaust_system_parameters es_params1(
        outlet_flow_rate: k_carb(2288.9),
        collector_cross_section_area: 190.1 * units.cm2,
        length: exhaust_pipe_length_1 * units.cm,
        primary_tube_length: 42.5 * units.cm,
        primary_flow_rate: k_carb(540.4),
        velocity_decay: 0.8,
        volume: exhaust_volume_1 * units.L
    )

    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_56.wav", volume: 0.001))
    exhaust_system exhaust1(es_params1, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_60.wav", volume: 0.001))

    label spacing_factor(4.0)
    label flange_density(1.0 * exhaust_delay_coeff)
    label sh0(100.0 * units.mm)
    label sh1(400.0 * units.mm)

    label pl0((0.5 * bore * spacing_factor / flange_density) * units.mm)
    label pl1((1.5 * bore * spacing_factor / flange_density) * units.mm)
    label pl2((2.5 * bore * spacing_factor / flange_density) * units.mm)
    label pl3((3.5 * bore * spacing_factor / flange_density) * units.mm)
    label pl4((4.5 * bore * spacing_factor / flange_density) * units.mm)

    engine
        .add_cylinder_bank(bank0.b0)
        .add_cylinder_bank(bank0.b1)
        .add_cylinder_bank(bank0.b2)
        .add_cylinder_bank(bank0.b3)
        .add_cylinder_bank(bank0.b4)
        .add_cylinder_bank(bank0.b5)
        .add_cylinder_bank(bank0.b6)
        .add_cylinder_bank(bank0.b7)
        .add_cylinder_bank(bank0.b8)
        
        .add_cylinder_bank(bank1.b0)
        .add_cylinder_bank(bank1.b1)
        .add_cylinder_bank(bank1.b2)
        .add_cylinder_bank(bank1.b3)
        .add_cylinder_bank(bank1.b4)
        .add_cylinder_bank(bank1.b5)
        .add_cylinder_bank(bank1.b6)
        .add_cylinder_bank(bank1.b7)
        .add_cylinder_bank(bank1.b8)

        .add_crankshaft(c0)
        .add_ignition_module(ignition)

    function timing_curve(200.0 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 4.0 * units.deg)
        .add_sample(200.0 * units.rpm, 4.0 * units.deg)
        .add_sample(400.0 * units.rpm, 4.0 * units.deg)
        .add_sample(600.0 * units.rpm, 7.1 * units.deg)
        .add_sample(800.0 * units.rpm, 9.8 * units.deg)
        .add_sample(1000.0 * units.rpm, 12.1 * units.deg)
        .add_sample(1200.0 * units.rpm, 14.2 * units.deg)
        .add_sample(1400.0 * units.rpm, 16.0 * units.deg)
        .add_sample(1600.0 * units.rpm, 17.5 * units.deg)
        .add_sample(1800.0 * units.rpm, 18.9 * units.deg)
        .add_sample(2000.0 * units.rpm, 20.0 * units.deg)
        .add_sample(2200.0 * units.rpm, 21.1 * units.deg)
        .add_sample(2400.0 * units.rpm, 22.0 * units.deg)
        .add_sample(2600.0 * units.rpm, 22.7 * units.deg)
        .add_sample(2800.0 * units.rpm, 23.4 * units.deg)
        .add_sample(3000.0 * units.rpm, 24.0 * units.deg)
        .add_sample(3200.0 * units.rpm, -12.5 * units.deg)
        .add_sample(3400.0 * units.rpm, -25.0 * units.deg)
}

// Pz V
public node veh {
    alias output __out: vehicle;

    label car_mass(45000) // 45.0 t panther

    vehicle vehicle(
        mass: car_mass * units.kg,
        drag_coefficient: 0.6,
        cross_sectional_area: (2830 * units.mm) * (2680 * units.mm),
        diff_ratio: 2.5 * 3,
        tire_radius: (821.77 / 2) * units.mm,
        rolling_resistance: 0.06 * car_mass * 9.81
        )
}

// Pz V
private node trn {
    alias output __out:
        transmission(
            max_clutch_torque: 4000 * units.Nm,
            max_clutch_flex: 12 * units.deg,
            limit_clutch_flex: true,
            clutch_stiffness: 200 * units.Nm / units.deg,
            clutch_damping: 1.0,
            simulate_flex: true
        )
        .add_gear(10.8)  // 3.7 @ 2600
        .add_gear(5.26)  // 7.6
        .add_gear(3.33)  // 12.0
        .add_gear(2.13)  // 18.8
        .add_gear(1.48)  // 27.0
        .add_gear(1.06)  // 37.7
        .add_gear(0.85); // 47.3
}

run(
    engine: eng(),
    transmission: trn(),
    vehicle: veh()
)

import "engine_sim.mr"
import "../../fuel/10.mr"

units units()
constants constants()

label bore(120.0)
label stroke(170.0)
label compression_ratio(5.0)
label con_rod(302.0) // 11.9
label compression_height(59.69 * units.mm) // 2.35
label intake_valve_diameter(61.2) // 2.41
label exhaust_valve_diameter(60.0) // 2.36
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(2000.0) // RPM

label cyl(8.0)
label row(2.0)
label cycle(720.0 * units.deg)
label vee(90.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)

// Lorraine-Dietrich 8B
label IVL(8.71) // .343
label EVL(8.71) // ?
label IVO(-2.0 * units.deg) //BTDC
label IVC(50.0 * units.deg) //ABDC
label EVO(50.0 * units.deg) //BBDC
label EVC(04.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(1700.0) // ? g
label piston_mass(1500.0) // ? g
label crank_mass(25.0) // ? kg
label flywheel_mass(10.0) // kg

label exhaust_delay_coeff(1.5)


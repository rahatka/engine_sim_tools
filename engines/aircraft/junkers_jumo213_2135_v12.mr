import "engine_sim.mr"
import "../../fuel/30.mr"

units units()
constants constants()

label bore(150.0)
label stroke(165.0)
label compression_ratio(6.93) // 6.5 A; 6.93 E
label con_rod(300.0) // ~
label compression_height(57.5 * units.mm) // ~
label intake_valve_diameter(50.0) // Jumo 211 deduced value
label exhaust_valve_diameter(65.0)
label intake_valves(2.0)
label exhaust_valves(1.0)

label cyl(12.0)
label row(2.0)
label cycle(720.0 * units.deg)
label vee(60.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

label redline(3250.0) // RPM

// Jumo 213 valve timings
label IVL(17.272) // Jumo 211
label EVL(16.0)
label IVO(41.0 * units.deg) //BTDC
label IVC(61.0 * units.deg) //ABDC
label EVO(99.0 * units.deg) //BBDC
label EVC(66.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label exhaust_delay_coeff(1.5)

// ver. A 1-10-3-8-5-7-6-9-4-11-2-12 :: 1-12-3-10-5-8-6-7-4-9-2-11
public node eng_distributor_a {
    input wires;
    input timing_curve;
    input rev_limit: 3500 * units.rpm;
    input limiter_duration: 0.0001;
    alias output __out:
        ignition_module(timing_curve: timing_curve, rev_limit: rev_limit, limiter_duration: limiter_duration)
            .connect_wire(wires.wire1, rot * 0)
            .connect_wire(wires.wire12, rot * 1)
            .connect_wire(wires.wire3, rot * 2)
            .connect_wire(wires.wire10, rot * 3)
            .connect_wire(wires.wire5, rot * 4)
            .connect_wire(wires.wire8, rot * 5)
            .connect_wire(wires.wire6, rot * 6)
            .connect_wire(wires.wire7, rot * 7)
            .connect_wire(wires.wire4, rot * 8)
            .connect_wire(wires.wire9, rot * 9)
            .connect_wire(wires.wire2, rot * 10)
            .connect_wire(wires.wire11, rot * 11);
}

// ver. E 1-9-3-11-5-12-6-10-4-8-2-7 :: 1-7-3-9-5-11-6-12-4-10-2-8
public node eng_distributor_e {
    input wires;

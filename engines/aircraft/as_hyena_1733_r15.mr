private node add_lobes {
    input base;
    input shift: 0.0;
    input this;
    alias output __out: this;

    this.add_lobe(base + 0 * rot * row + shift)
    this.add_lobe(base + 2 * rot * row + shift)
    this.add_lobe(base + 1 * rot * row + shift)
}

public node bank_builder {
    input shift;
    input ignition;
    input intake;
    input exhaust;

    input cr0;
    input cr1;
    input cr2;

    input rj0;
    input rj1;
    input rj2;

    input pl0;
    input pl1;
    input pl2;

    output b0: _b0;

    ignition_wire _wire1()
    ignition_wire _wire2()
    ignition_wire _wire3()

    piston_parameters piston_params(
        mass: 2000 * units.g,
        blowby: k_28inH2O(0.043),
        compression_height: compression_height,
        wrist_pin_position: 0.0,
        displacement: 0.0
    )

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    camshaft _intake_cam_0(base_radius: 29.0 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 29.0 * units.mm, lobe_profile: exhaust_lobe_profile)

    _intake_cam_0.add_lobes(ILC, -shift)
    _exhaust_cam_0.add_lobes(ELC, -shift)

    cylinder_bank _b0(bank_params, angle: shift)

    _b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: cr0,
            rod_journal: rj0,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl0,
            ignition_wire: _wire1
        )
    _b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: cr1,
            rod_journal: rj1,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl1,
            ignition_wire: _wire2
        )
    _b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: cr2,
            rod_journal: rj2,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl2,
            ignition_wire: _wire3
        )

    _b0.set_cylinder_head(
        head(
            intake_camshaft: _intake_cam_0,
            exhaust_camshaft: _exhaust_cam_0
        )
    )

    ignition
        .connect_wire(_wire1, rot_base * 0 - shift)
        .connect_wire(_wire3, rot_base * 1 - shift)
        .connect_wire(_wire2, rot_base * 2 - shift)
}

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node head {
    // port_flow.py v1.7
    // intake port area: 25.3 cm²; saturated lift: 13.81 mm
    // exhaust port area: 22.2 cm²; saturated lift: 12.95 mm
    // cylinder volume: 1842.6 cm³ (112.44 CI); engine volume: 27.640 L (1686.67 CI)
    // chamber volume: 329.0 cm³ (20.08 CI); conrod/stroke 1.800; N.A.C.C. 174.55
    // 32 harmonic intake runner length: 16.0 cm; diameter: 5.6 cm
    // primary length: 44.8 cm, area: 22.2 cm², diameter: 5.3 cm
    // collector diameter: 17.2 cm, area: 233.5 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 329.043 * units.cc,
        intake_runner_volume: 397.5 * units.cc,
        intake_runner_cross_section_area: 24.9 * units.cm2,
        exhaust_runner_volume: 137.1 * units.cc,
        exhaust_runner_cross_section_area: 22.2 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "A. S. Hyena",
        starter_torque: 500 * units.Nm,
        starter_speed: 150 * units.rpm,
        redline: redline * units.rpm,
        fuel: a30(),
        throttle_gamma: 2.0,
        jitter: 0.7,
        noise: 0.1,
        hf_gain: 0.007,
        fluid_simulation_steps: 4,
        max_sle_solver_steps: 32,
        simulation_frequency: 7500,
        dyno_min_speed: 500 * units.rpm
    )

    label crank_moment(disk_moment_of_inertia(mass: crank_mass * units.kg, radius: (stroke / 2.0) * units.mm))
    label flywheel_moment(disk_moment_of_inertia(mass: flywheel_mass * units.kg, radius: (stroke * 3.5) * units.mm))
    label other_moment(disk_moment_of_inertia(mass: 1 * units.kg, radius: 1.0 * units.cm))

    crankshaft c0(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: flywheel_mass * units.kg,
        mass: crank_mass * units.kg,
        friction_torque: 41.5 * units.Nm,
        moment_of_inertia: crank_moment + flywheel_moment + other_moment,
        tdc: 0.0
    )

    connecting_rod_parameters cr_params(
        mass: con_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.g,
            length: con_rod * units.mm
        ),
        slave_throw: R1 * units.mm,
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    label slave_rod_mass(680 + 140)
    connecting_rod_parameters slave_rod_params(
        mass: slave_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: slave_rod_mass * units.g,
            length: L1 * units.mm
        ),
        center_of_mass: 0.0,
        length: L1 * units.mm
    )

    label psi0(79.7 * units.deg)
    label psi1(148.7 * units.deg)
    label psi2(211.3 * units.deg)
    label psi3(280.3 * units.deg)

    rod_journal rj0(angle: rot_base * 0 + rot90)
    rod_journal sj0_0(angle: psi0)
    rod_journal sj0_1(angle: psi1)
    rod_journal sj0_2(angle: psi2)
    rod_journal sj0_3(angle: psi3)

    rod_journal rj1(angle: rot_base * 2 + rot90)
    rod_journal sj1_0(angle: psi0)
    rod_journal sj1_1(angle: psi1)
    rod_journal sj1_2(angle: psi2)
    rod_journal sj1_3(angle: psi3)

    rod_journal rj2(angle: rot_base * 1 + rot90)
    rod_journal sj2_0(angle: psi0)
    rod_journal sj2_1(angle: psi1)
    rod_journal sj2_2(angle: psi2)
    rod_journal sj2_3(angle: psi3)

    connecting_rod mcr0(cr_params)
    connecting_rod mcr1(cr_params)
    connecting_rod mcr2(cr_params)

    connecting_rod sr0_0(slave_rod_params)
    connecting_rod sr0_1(slave_rod_params)
    connecting_rod sr0_2(slave_rod_params)
    connecting_rod sr0_3(slave_rod_params)

    connecting_rod sr1_0(slave_rod_params)
    connecting_rod sr1_1(slave_rod_params)
    connecting_rod sr1_2(slave_rod_params)
    connecting_rod sr1_3(slave_rod_params)

    connecting_rod sr2_0(slave_rod_params)
    connecting_rod sr2_1(slave_rod_params)
    connecting_rod sr2_2(slave_rod_params)
    connecting_rod sr2_3(slave_rod_params)

    mcr0.add_slave_journal(sj0_0)
    mcr0.add_slave_journal(sj0_1)
    mcr0.add_slave_journal(sj0_2)
    mcr0.add_slave_journal(sj0_3)

    mcr1.add_slave_journal(sj1_0)
    mcr1.add_slave_journal(sj1_1)
    mcr1.add_slave_journal(sj1_2)
    mcr1.add_slave_journal(sj1_3)

    mcr2.add_slave_journal(sj2_0)
    mcr2.add_slave_journal(sj2_1)
    mcr2.add_slave_journal(sj2_2)
    mcr2.add_slave_journal(sj2_3)

    bank_builder bank0(
        shift: 0.0 * units.deg,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust,
        cr0: mcr0,
        cr1: mcr1,
        cr2: mcr2,
        rj0: rj0,
        rj1: rj1,
        rj2: rj2,
        pl0 + sh0, pl1 + sh0, pl2 + sh0
    )
    bank_builder bank1(
        shift: vee * 1 + rot360,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust,
        cr0: sr0_0,
        cr1: sr1_0,
        cr2: sr2_0,
        rj0: sj0_0,
        rj1: sj1_0,
        rj2: sj2_0,
        pl0 + sh1, pl1 + sh1, pl2 + sh1
    )
    bank_builder bank2(
        shift: vee * 2,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust,
        cr0: sr0_1,
        cr1: sr1_1,
        cr2: sr2_1,
        rj0: sj0_1,
        rj1: sj1_1,
        rj2: sj2_1,
        pl0 + sh2, pl1 + sh2, pl2 + sh2
    )
    bank_builder bank3(
        shift: vee * 3 + rot360,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust,
        cr0: sr0_2,
        cr1: sr1_2,
        cr2: sr2_2,
        rj0: sj0_2,
        rj1: sj1_2,
        rj2: sj2_2,
        pl0 + sh3, pl1 + sh3, pl2 + sh3
    )
    bank_builder bank4(
        shift: vee * 4,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust,
        cr0: sr0_3,
        cr1: sr1_3,
        cr2: sr2_3,
        rj0: sj0_3,
        rj1: sj1_3,
        rj2: sj2_3,
        pl0 + sh4, pl1 + sh4, pl2 + sh4
    )

    ignition_module ignition(timing_curve: timing_curve, rev_limit: 10000 * units.rpm, limiter_duration: 0.001)
    
    c0
        .add_rod_journal(rj0)
        .add_rod_journal(rj1)
        .add_rod_journal(rj2)

    label collector_cross_section_area(233.5)

    label exhaust_pipe_length_0(100.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 100.0) // Litres
    
    label spacing_factor(1.15)
    label flange_density(1.0 * exhaust_delay_coeff)
    label radial_density(0.333 * exhaust_delay_coeff)

    intake intake(
        plenum_volume: 12.2 * units.L,
        plenum_cross_section_area: 186.8 * units.cm2,
        runner_length: 16.0 * units.cm,
        intake_flow_rate: k_carb(1451.1),
        idle_flow_rate: k_carb(0.03),
        idle_throttle_plate_position: 0.994,
        runner_flow_rate: k_carb(203.2),
        velocity_decay: 0.05
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(2478.9),
        collector_cross_section_area: 233.5 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 44.8 * units.cm,
        primary_flow_rate: k_carb(338.8),
        velocity_decay: 0.5,
        volume: exhaust_volume_0 * units.L
    )

    exhaust_system exhaust(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_55.wav", volume: 0.0001))

    label sh0 ((1.0 * bore * spacing_factor / radial_density) * units.mm)
    label sh1 ((2.0 * bore * spacing_factor / radial_density) * units.mm)
    label sh2 ((3.0 * bore * spacing_factor / radial_density) * units.mm)
    label sh3 ((2.2 * bore * spacing_factor / radial_density) * units.mm)
    label sh4 ((1.2 * bore * spacing_factor / radial_density) * units.mm)
    
    label pl0 ((1.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl1 ((2.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl2 ((3.0 * bore * spacing_factor / flange_density) * units.mm)

    engine
        .add_cylinder_bank(bank0.b0)
        .add_cylinder_bank(bank1.b0)
        .add_cylinder_bank(bank2.b0)
        .add_cylinder_bank(bank3.b0)
        .add_cylinder_bank(bank4.b0)

        .add_crankshaft(c0)
        .add_ignition_module(ignition)

    function timing_curve(200.0 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 2.0 * units.deg)
        .add_sample(200.0 * units.rpm, 2.0 * units.deg)
        .add_sample(400.0 * units.rpm, 8.8 * units.deg)
        .add_sample(600.0 * units.rpm, 14.2 * units.deg)
        .add_sample(800.0 * units.rpm, 18.5 * units.deg)
        .add_sample(1000.0 * units.rpm, 21.8 * units.deg)
        .add_sample(1200.0 * units.rpm, 24.4 * units.deg)
        .add_sample(1400.0 * units.rpm, 26.5 * units.deg)
        .add_sample(1600.0 * units.rpm, 28.1 * units.deg)
        .add_sample(1800.0 * units.rpm, 29.3 * units.deg)
        .add_sample(2000.0 * units.rpm, 30.3 * units.deg)
        .add_sample(2200.0 * units.rpm, 31.1 * units.deg)
        .add_sample(2400.0 * units.rpm, 31.7 * units.deg)
        .add_sample(2600.0 * units.rpm, 32.0 * units.deg)
        .add_sample(2800.0 * units.rpm, 32.0 * units.deg)
        .add_sample(3000.0 * units.rpm, 32.0 * units.deg)
        .add_sample(3200.0 * units.rpm, -15.0 * units.deg)
        .add_sample(3400.0 * units.rpm, -45.0 * units.deg)
}

// Pz V
public node veh {
    alias output __out: vehicle;

    label car_mass(45000) // 45.0 t panther

    vehicle vehicle(
        mass: car_mass * units.kg,
        drag_coefficient: 0.6,
        cross_sectional_area: (2830 * units.mm) * (2680 * units.mm),
        diff_ratio: 3.5 * 3,
        tire_radius: (821.77 / 2) * units.mm,
        rolling_resistance: 0.06 * car_mass * 9.81
        )
}

// Pz V
private node trn {
    alias output __out:
        transmission(
            max_clutch_torque: 2000 * units.Nm,
            max_clutch_flex: 15 * units.deg,
            limit_clutch_flex: true,
            clutch_stiffness: 50 * units.Nm / units.deg,
            clutch_damping: 1.0,
            simulate_flex: true
        )
        .add_gear(10.8)  // 3.7 @ 2600
        .add_gear(5.26)  // 7.6
        .add_gear(3.33)  // 12.0
        .add_gear(2.13)  // 18.8
        .add_gear(1.48)  // 27.0
        .add_gear(1.06)  // 37.7
        .add_gear(0.85); // 47.3
}

run(
    engine: eng(),
    transmission: trn(),
    vehicle: veh()
)

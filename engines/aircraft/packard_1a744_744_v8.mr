import "engine_sim.mr"
import "../../fuel/10.mr"

units units()
constants constants()

label bore(120.65) // 4 3/4
label stroke(133.35) // 5 1/4
label compression_ratio(5.02)
label con_rod(228.6) // 9
label compression_height(67.8434 * units.mm) // 2.671
label intake_valve_diameter(57.15) // 2 1/4 ? ; 2 clear
label exhaust_valve_diameter(57.15) // 2 1/4 ? ; 2 clear
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(1800.0) // RPM

label cyl(8.0)
label row(2.0)
label cycle(720.0 * units.deg)
label vee(60.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)

// Packard 1A-744
label IVL(11.1125) // 7/16
label EVL(9.525) // 3/8
label IVO(-10.0 * units.deg) //BTDC
label IVC(45.0 * units.deg) //ABDC
label EVO(48.0 * units.deg) //BBDC
label EVC(8.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(1564.894) // 4.4 and 2.5 = 3.45
label piston_mass(1905.09) // 4.2
label crank_mass(29.4) // 64.8 p
label flywheel_mass(20.0) // kg

label exhaust_delay_coeff(1.5)


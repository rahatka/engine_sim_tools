import "engine_sim.mr"
import "_fuel_10.mr"

units units()
constants constants()

label bore(152.4) // 6
label stroke(190.5) // 7 1/2
label compression_ratio(4.66)
label con_rod(323.85) // 12 3/4
label compression_height(70.0 * units.mm) // ?
label intake_valve_diameter(74.6125) // 2 15/16 ; 2 23/32 port ; 1/2 stem
label exhaust_valve_diameter(55.5625) // 2 3/16 ; 1 31/32 port ; 7/16 stem
label intake_valves(1.0)
label exhaust_valves(2.0)

label cyl(16.0)
label row(2.0)
label cycle(720.0 * units.deg)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label vee(45.0 * units.deg)

label redline(2000.0) // RPM

// Bugatti U16
label IVL(14.2875) // 9/16
label EVL(12.7) // 1/2
label IVO(00.0 * units.deg) //BTDC
label IVC(45.0 * units.deg) //ABDC
label EVO(45.0 * units.deg) //BBDC
label EVC(15.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(2000.0) // ? g
label crank_mass(90.2649) // 199 lb
label flywheel_mass(30.0) // kg

label exhaust_delay_coeff(1.5)


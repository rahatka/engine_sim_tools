import "engine_sim.mr"
import "../../fuel/30.mr"

units units()
constants constants()


label bore(155.5)
label stroke(155.0)
label compression_ratio(6.4)
label con_rod(305.0) // 305.0 главный ; 231.63 прицепной
label L1(231.63)
label R1(75.0) // ?
label compression_height(64.0 * units.mm) // ~
label intake_valve_diameter(78.0)
label exhaust_valve_diameter(78.0)
label intake_valves(1.0)
label exhaust_valves(1.0)

label cyl(7.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

label redline(2500.0) // RPM

// АШ-21
label IVL(17.0)
label EVL(17.0)
label IVO(20.0 * units.deg) //BTDC
label IVC(63.0 * units.deg) //ABDC
label EVO(50.0 * units.deg) //BBDC
label EVC(33.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(1800.0) // ? g
label crank_mass(25.0) // ? kg
label flywheel_mass(15.0) // kg

label exhaust_delay_coeff(1.5)

public node eng_distributor {
    input wires;
    input timing_curve;
    input rev_limit: 3500 * units.rpm;
    input limiter_duration: 0.02;
    alias output __out:
        ignition_module(timing_curve: timing_curve, rev_limit: rev_limit, limiter_duration: limiter_duration)
            .connect_wire(wires.wire1, rot * 0)
            .connect_wire(wires.wire3, rot * 1 + 4.2 * units.deg)
            .connect_wire(wires.wire5, rot * 2 - 2.5 * units.deg)
            .connect_wire(wires.wire7, rot * 3 - 1.1 * units.deg)
            .connect_wire(wires.wire2, rot * 4 + 1.1 * units.deg)
            .connect_wire(wires.wire4, rot * 5 + 2.5 * units.deg)
            .connect_wire(wires.wire6, rot * 6 - 4.2 * units.deg);
}

private node wires {
    output wire1: ignition_wire();
    output wire2: ignition_wire();
    output wire3: ignition_wire();
    output wire4: ignition_wire();
    output wire5: ignition_wire();
    output wire6: ignition_wire();
    output wire7: ignition_wire();
}


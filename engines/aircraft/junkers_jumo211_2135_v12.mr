import "engine_sim.mr"
import "../../fuel/30.mr"

units units()
constants constants()

label bore(150.0)
label stroke(165.0)
label compression_ratio(6.6)
label con_rod(280.0) // 11.01 (280.0)
label compression_height(57.5 * units.mm) // ~
label intake_valve_diameter(50.0) // Jumo 211 deduced value
label exhaust_valve_diameter(65.0)
label intake_valves(2.0)
label exhaust_valves(1.0)

label cyl(12.0)
label row(2.0)
label cyl_base(cyl / row)
label cycle(720.0 * units.deg)
label vee(60.0 * units.deg)
label rot(cycle / cyl)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

label redline(2750.0) // RPM

// Jumo 211 valve timings
label IVL(17.272) // Jumo 211
label EVL(16.0)
label IVO(30.0 * units.deg) //BTDC
label IVC(42.0 * units.deg) //ABDC
label EVO(70.0 * units.deg) //BBDC
label EVC(46.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass((3406.479 + 2621.764) / 2) // 7.51 and 5.78
label piston_mass(1941.375) // 4.28
label crank_mass(74.3891) // 164 lbs
label flywheel_mass(15.0) // kg

label exhaust_delay_coeff(1.5)


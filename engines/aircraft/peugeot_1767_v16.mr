label exhaust_valve_diameter(40.0) // 37 clear
label intake_valves(2.0)
label exhaust_valves(2.0)

label redline(2200.0)

label cyl(16.0)
label row(2.0)
label cyl_base(cyl / row)
label cycle(720.0 * units.deg)
label vee(rot90)
label rot_base(cycle / cyl_base * 2)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// L112 timing
label IVL(9.0) // ?
label EVL(9.0) // ?
label IVO(-11.73 * units.deg) //BTDC
label IVC(46.12 * units.deg) //ABDC
label EVO(52.50 * units.deg) //BBDC
label EVC(7.73 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(1100.0) // ?
label piston_mass(900.0) // ?
label crank_mass(80.0) // ?
label flywheel_mass(15.0) // kg

label exhaust_delay_coeff(1.5)


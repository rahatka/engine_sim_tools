import "engine_sim.mr"
import "../../fuel/10.mr"

units units()
constants constants()

label bore(146.05) // 5 3/4
label stroke(190.5) // 7 1/2
label compression_ratio(5.3)
label con_rod(342.9) // 13 1/2
label L1(292.1) // 11 1/2 IV ; 11.22 VI
label R1(52.0) // 51.2 min ; Pegasus XX: 65.63, 67.33, 66.47, 64.73
label compression_height(51.0 units.mm)
label intake_valve_diameter(56.5) // 51.5 clear ; 1.875 BJ I,II
label exhaust_valve_diameter(49.8) // 44.0 clear ; 1.687 BJ I,II
label intake_valves(2.0)
label exhaust_valves(2.0)

label cyl(9.0)
label cycle(720.0 * units.deg)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label rot(cycle / cyl)

label redline(1850.0) // RPM

// Rhone VI
label IVL(13.0) // .415 BJ I,II
label EVL(13.0) // .4375 BJ I,II
label IVO(13.0 * units.deg) //BTDC
label IVC(51.0 * units.deg) //ABDC
label EVO(65.0 * units.deg) //BBDC
label EVC(31.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(5000.0) // ? g
label slave_rod_mass(600.0) // ?
label crank_mass(25.0) // ? kg
label flywheel_mass(15.0) // kg

label exhaust_delay_coeff(1.5)

public node eng_distributor {
    input wires;
    input timing_curve;
    input rev_limit: 3000 * units.rpm;
    input limiter_duration: 0.0001;
    alias output __out:
        ignition_module(timing_curve: timing_curve, rev_limit: rev_limit, limiter_duration: limiter_duration)
            .connect_wire(wires.wire1, rot * 0)
            .connect_wire(wires.wire3, rot * 1)
            .connect_wire(wires.wire5, rot * 2)
            .connect_wire(wires.wire7, rot * 3)
            .connect_wire(wires.wire9, rot * 4)
            .connect_wire(wires.wire2, rot * 5)
            .connect_wire(wires.wire4, rot * 6)
            .connect_wire(wires.wire6, rot * 7)
            .connect_wire(wires.wire8, rot * 8);
}

private node wires {
    output wire1: ignition_wire();
    output wire2: ignition_wire();
    output wire3: ignition_wire();
    output wire4: ignition_wire();
    output wire5: ignition_wire();
    output wire6: ignition_wire();
    output wire7: ignition_wire();
    output wire8: ignition_wire();
    output wire9: ignition_wire();
}


private node add_lobes {
    input base;
    input shift: 0.0;
    input this;
    alias output __out: this;

    this.add_lobe(base + 0 * rot * row + shift)
    this.add_lobe(base + 0.5 * rot * row + shift)
}

public node bank_builder {
    input shift;
    input ignition;
    input intake;
    input exhaust;
    input cr0;
    input cr1;
    input rj0;
    input rj1;
    input pl0;
    input pl1;

    output b0: _b0;

    ignition_wire _wire1()
    ignition_wire _wire2()

    piston_parameters piston_params(
        mass: 2000 * units.g,
        blowby: k_28inH2O(0.042),
        compression_height: compression_height,
        wrist_pin_position: 0.0,
        displacement: 0.0
    )

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    camshaft _intake_cam_0(base_radius: 29.0 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 29.0 * units.mm, lobe_profile: exhaust_lobe_profile)

    _intake_cam_0.add_lobes(ILC, -shift)
    _exhaust_cam_0.add_lobes(ELC, -shift)

    cylinder_bank _b0(bank_params, angle: shift)

    _b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: cr0,
            rod_journal: rj0,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl0,
            ignition_wire: _wire1
        )
    _b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: cr1,
            rod_journal: rj1,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl1,
            ignition_wire: _wire2
        )

    _b0.set_cylinder_head(
        head(
            intake_camshaft: _intake_cam_0,
            exhaust_camshaft: _exhaust_cam_0
        )
    )

    ignition
        .connect_wire(_wire1, rot_base * 0 - shift)
        .connect_wire(_wire2, rot_base * 0.5 - shift)
}

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node head {
    // port_flow.py v1.7
    // intake port area: 25.3 cm²; saturated lift: 13.82 mm
    // exhaust port area: 22.2 cm²; saturated lift: 12.96 mm
    // cylinder volume: 1816.2 cm³ (110.83 CI); engine volume: 25.426 L (1551.61 CI)
    // chamber volume: 315.9 cm³ (19.27 CI); conrod/stroke 2.000; N.A.C.C. 158.05
    // 32 harmonic intake runner length: 16.5 cm; diameter: 5.6 cm
    // primary length: 44.3 cm, area: 24.5 cm², diameter: 5.6 cm
    // collector diameter: 16.8 cm, area: 222.7 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 315.855 * units.cc,
        intake_runner_volume: 404.1 * units.cc,
        intake_runner_cross_section_area: 24.6 * units.cm2,
        exhaust_runner_volume: 139.3 * units.cc,
        exhaust_runner_cross_section_area: 24.5 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "A. S. Terrier",
        starter_torque: 500 * units.Nm,
        starter_speed: 150 * units.rpm,
        redline: redline * units.rpm,
        fuel: a30(),
        throttle_gamma: 2.0,
        jitter: 0.7,
        noise: 0.1,
        hf_gain: 0.005,
        fluid_simulation_steps: 6,
        max_sle_solver_steps: 32,
        simulation_frequency: 8500,
        dyno_min_speed: 500 * units.rpm
    )

    label crank_moment(disk_moment_of_inertia(mass: crank_mass * units.kg, radius: (stroke / 2.0) * units.mm))
    label flywheel_moment(disk_moment_of_inertia(mass: flywheel_mass * units.kg, radius: (stroke * 3.5) * units.mm))
    label other_moment(disk_moment_of_inertia(mass: 1 * units.kg, radius: 1.0 * units.cm))

    crankshaft c0(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: flywheel_mass * units.kg,
        mass: crank_mass * units.kg,
        friction_torque: 38.1 * units.Nm,
        moment_of_inertia: crank_moment + flywheel_moment + other_moment,
        tdc: 0.0
    )

    bank_builder bank0(
        shift: 0.0 * units.deg,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust,
        cr0: mcr0,
        cr1: mcr1,
        rj0: rj0,
        rj1: rj1,
        pl0 + sh0, pl1 + sh0
    )
    bank_builder bank1(
        shift: vee * 1 + rot360,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust,
        cr0: connecting_rod(slave_rod_params),
        cr1: connecting_rod(slave_rod_params),
        rj0: sj0_0,
        rj1: sj1_0,
        pl0 + sh1, pl1 + sh1
    )
    bank_builder bank2(
        shift: vee * 2,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust,
        cr0: connecting_rod(slave_rod_params),
        cr1: connecting_rod(slave_rod_params),
        rj0: sj0_1,
        rj1: sj1_1,
        pl0 + sh2, pl1 + sh2
    )
    bank_builder bank3(
        shift: vee * 3 + rot360,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust,
        cr0: connecting_rod(slave_rod_params),
        cr1: connecting_rod(slave_rod_params),
        rj0: sj0_2,
        rj1: sj1_2,
        pl0 + sh3, pl1 + sh3
    )
    bank_builder bank4(
        shift: vee * 4,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust,
        cr0: connecting_rod(slave_rod_params),
        cr1: connecting_rod(slave_rod_params),
        rj0: sj0_3,
        rj1: sj1_3,
        pl0 + sh4, pl1 + sh4
    )
    bank_builder bank5(
        shift: vee * 5 + rot360,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust,
        cr0: connecting_rod(slave_rod_params),
        cr1: connecting_rod(slave_rod_params),
        rj0: sj0_4,
        rj1: sj1_4,
        pl0 + sh5, pl1 + sh5
    )
    bank_builder bank6(
        shift: vee * 6,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust,
        cr0: connecting_rod(slave_rod_params),
        cr1: connecting_rod(slave_rod_params),
        rj0: sj0_5,
        rj1: sj1_5,
        pl0 + sh6, pl1 + sh6
    )

    ignition_module ignition(timing_curve: timing_curve, rev_limit: 10000 * units.rpm, limiter_duration: 0.001)

    connecting_rod_parameters cr_params(
        mass: con_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.g,
            length: con_rod * units.mm
        ),
        slave_throw: R1 * units.mm,
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    label slave_rod_mass(1000)
    connecting_rod_parameters slave_rod_params(
        mass: slave_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: slave_rod_mass * units.g,
            length: L1 * units.mm
        ),
        center_of_mass: 0.0,
        length: L1 * units.mm
    )

    rod_journal rj0(angle: rot90)
    rod_journal sj0_0(angle: rot * 1)
    rod_journal sj0_1(angle: rot * 2)
    rod_journal sj0_2(angle: rot * 3)
    rod_journal sj0_3(angle: rot * 4)
    rod_journal sj0_4(angle: rot * 5)
    rod_journal sj0_5(angle: rot * 6)

    rod_journal rj1(angle: rot180 + rot90)
    rod_journal sj1_0(angle: rot * 1)
    rod_journal sj1_1(angle: rot * 2)
    rod_journal sj1_2(angle: rot * 3)
    rod_journal sj1_3(angle: rot * 4)
    rod_journal sj1_4(angle: rot * 5)
    rod_journal sj1_5(angle: rot * 6)

    connecting_rod mcr0(cr_params)
    connecting_rod mcr1(cr_params)

    mcr0.add_slave_journal(sj0_0)
    mcr0.add_slave_journal(sj0_1)
    mcr0.add_slave_journal(sj0_2)
    mcr0.add_slave_journal(sj0_3)
    mcr0.add_slave_journal(sj0_4)
    mcr0.add_slave_journal(sj0_5)

    mcr1.add_slave_journal(sj1_0)
    mcr1.add_slave_journal(sj1_1)
    mcr1.add_slave_journal(sj1_2)
    mcr1.add_slave_journal(sj1_3)
    mcr1.add_slave_journal(sj1_4)
    mcr1.add_slave_journal(sj1_5)

    c0
        .add_rod_journal(rj0)
        .add_rod_journal(rj1)

    label collector_cross_section_area(222.7)

    label exhaust_pipe_length_0(500.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 100.0) // Litres
    
    label spacing_factor(1.15)
    label flange_density(1.0 * exhaust_delay_coeff)
    label radial_density(1.0 * exhaust_delay_coeff)

    intake intake(
        plenum_volume: 10.7 * units.L,
        plenum_cross_section_area: 171.9 * units.cm2,
        runner_length: 16.5 * units.cm,
        intake_flow_rate: k_carb(1271.3),
        idle_flow_rate: k_carb(0.02),
        idle_throttle_plate_position: 0.995,
        runner_flow_rate: k_carb(181.6),
        velocity_decay: 0.05
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(2118.9),
        collector_cross_section_area: 222.7 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 44.3 * units.cm,
        primary_flow_rate: k_carb(302.7),
        velocity_decay: 0.7,
        volume: exhaust_volume_0 * units.L
    )

    exhaust_system exhaust(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_45.wav", volume: 0.00033))

    label sh0 ((1.0 * bore * spacing_factor / radial_density) * units.mm)
    label sh1 ((4.0 * bore * spacing_factor / radial_density) * units.mm)
    label sh2 ((7.0 * bore * spacing_factor / radial_density) * units.mm)
    label sh3 ((10.0 * bore * spacing_factor / radial_density) * units.mm)
    label sh4 ((8.0 * bore * spacing_factor / radial_density) * units.mm)
    label sh5 ((5.0 * bore * spacing_factor / radial_density) * units.mm)
    label sh6 ((2.0 * bore * spacing_factor / radial_density) * units.mm)
    
    label pl0 ((1.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl1 ((2.0 * bore * spacing_factor / flange_density) * units.mm)

    engine
        .add_cylinder_bank(bank0.b0)
        .add_cylinder_bank(bank1.b0)
        .add_cylinder_bank(bank2.b0)
        .add_cylinder_bank(bank3.b0)
        .add_cylinder_bank(bank4.b0)
        .add_cylinder_bank(bank5.b0)
        .add_cylinder_bank(bank6.b0)

        .add_crankshaft(c0)
        .add_ignition_module(ignition)

    function timing_curve(200.0 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 2.0 * units.deg)
        .add_sample(200.0 * units.rpm, 2.0 * units.deg)
        .add_sample(400.0 * units.rpm, 8.8 * units.deg)
        .add_sample(600.0 * units.rpm, 14.2 * units.deg)
        .add_sample(800.0 * units.rpm, 18.5 * units.deg)
        .add_sample(1000.0 * units.rpm, 21.8 * units.deg)
        .add_sample(1200.0 * units.rpm, 24.4 * units.deg)
        .add_sample(1400.0 * units.rpm, 26.5 * units.deg)
        .add_sample(1600.0 * units.rpm, 28.1 * units.deg)
        .add_sample(1800.0 * units.rpm, 29.3 * units.deg)
        .add_sample(2000.0 * units.rpm, 30.3 * units.deg)
        .add_sample(2200.0 * units.rpm, 31.1 * units.deg)
        .add_sample(2400.0 * units.rpm, 31.7 * units.deg)
        .add_sample(2600.0 * units.rpm, 32.0 * units.deg)
        .add_sample(2800.0 * units.rpm, 32.0 * units.deg)
        .add_sample(3000.0 * units.rpm, 32.0 * units.deg)
        .add_sample(3200.0 * units.rpm, -15.0 * units.deg)
        .add_sample(3400.0 * units.rpm, -60.0 * units.deg)
}

// Pz V
public node veh {
    alias output __out: vehicle;

    label car_mass(45000) // 45.0 t panther

    vehicle vehicle(
        mass: car_mass * units.kg,
        drag_coefficient: 0.6,
        cross_sectional_area: (2830 * units.mm) * (2680 * units.mm),
        diff_ratio: 3.5 * 3,
        tire_radius: (821.77 / 2) * units.mm,
        rolling_resistance: 0.06 * car_mass * 9.81
        )
}

// Pz V
private node trn {
    alias output __out:
        transmission(
            max_clutch_torque: 2000 * units.Nm,
            max_clutch_flex: 15 * units.deg,
            limit_clutch_flex: true,
            clutch_stiffness: 50 * units.Nm / units.deg,
            clutch_damping: 1.0,
            simulate_flex: true
        )
        .add_gear(10.8)  // 3.7 @ 2600
        .add_gear(5.26)  // 7.6
        .add_gear(3.33)  // 12.0
        .add_gear(2.13)  // 18.8
        .add_gear(1.48)  // 27.0
        .add_gear(1.06)  // 37.7
        .add_gear(0.85); // 47.3
}

run(
    engine: eng(),
    transmission: trn(),
    vehicle: veh()
)

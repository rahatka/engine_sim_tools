import "engine_sim.mr"
import "_fuel_30.mr"

units units()
constants constants()

label bore(155.575) // 6 1/8
label stroke(161.925) // 6 3/8
label compression_ratio(6.0)
label con_rod(342.9) // 13 1/2
label L1(277.2) // L / 1.237 ; 295 Веселовский
label R1(66.95) // ~ ; 55 Веселовский
label compression_height(61.5 * units.mm)
label intake_valve_diameter(66.675) // 2 5/8 ; 2 1/2 clear
label exhaust_valve_diameter(66.675) // 2 5/8 ; 2 1/2 clear
label intake_valves(1.0)
label exhaust_valves(1.0)

label cyl(9.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label rot(cycle / cyl)

label redline(2500.0) // RPM

// R-1690-A-1
label IVL(17.4625) // 11/16
label EVL(14.2875) // 9/16
label IVO(10.0 * units.deg) //BTDC
label IVC(60.0 * units.deg) //ABDC
label EVO(71.0 * units.deg) //BBDC
label EVC(31.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(1800.0) // ? g
label slave_rod_mass(700)
label crank_mass(30.0) // ? kg
label flywheel_mass(20.0) // kg

label exhaust_delay_coeff(1.5)

public node eng_distributor {
    input wires;
    input timing_curve;
    input rev_limit: 3000 * units.rpm;
    input limiter_duration: 0.0001;
    alias output __out:
        ignition_module(timing_curve: timing_curve, rev_limit: rev_limit, limiter_duration: limiter_duration)
            .connect_wire(wires.wire1, rot * 0)
            .connect_wire(wires.wire3, rot * 1 + 2.2 * units.mm)
            .connect_wire(wires.wire5, rot * 2 + 1.5 * units.mm)
            .connect_wire(wires.wire7, rot * 3 - 3.2 * units.mm)
            .connect_wire(wires.wire9, rot * 4 - 0.4 * units.mm)
            .connect_wire(wires.wire2, rot * 5 + 0.4 * units.mm)
            .connect_wire(wires.wire4, rot * 6 + 3.2 * units.mm)
            .connect_wire(wires.wire6, rot * 7 - 1.5 * units.mm)
            .connect_wire(wires.wire8, rot * 8 - 2.52 * units.mm);
}

private node wires {
    output wire1: ignition_wire();
    output wire2: ignition_wire();
    output wire3: ignition_wire();
    output wire4: ignition_wire();
    output wire5: ignition_wire();
    output wire6: ignition_wire();
    output wire7: ignition_wire();
    output wire8: ignition_wire();
    output wire9: ignition_wire();
}


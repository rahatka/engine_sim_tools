import "engine_sim.mr"
import "../../fuel/10.mr"

units units()
constants constants()

label bore(139.7) // 5 1/2
label stroke(165.1) // 6 1/2
label compression_ratio(4.432)
label con_rod(311.15) // 12 1/4 ; 10.03
label L1(254.762)
label R1(57.7) // ?
label compression_height(58.42 * units.mm) // 1.9 - 2.3
label intake_valve_diameter(73.66) // 2.6 clear
label exhaust_valve_diameter(48.26) // 1.7 clear
label intake_valves(1.0)
label exhaust_valves(2.0)

label cyl(9.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label rot(cycle / cyl)

label redline(1900.0) // RPM

// ABC Dragonfly
label IVL(13.4874) // 0.531 ; .654 AE
label EVL(9.525) // 0.375 ; .413 AE
label IVO(0.0 * units.deg) //BTDC
label IVC(52.0 * units.deg) //ABDC
label EVO(60.0 * units.deg) //BBDC
label EVC(15.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(4531.4) // 13.75 lb main ; 9.52 articulated
label crank_mass(28.8) // 63.6
label flywheel_mass(20.0) // kg

label exhaust_delay_coeff(1.5)

public node eng_distributor {
    input wires;
    input timing_curve;
    input rev_limit: 3000 * units.rpm;
    input limiter_duration: 0.0001;
    alias output __out:
        ignition_module(timing_curve: timing_curve, rev_limit: rev_limit, limiter_duration: limiter_duration)
            .connect_wire(wires.wire1, rot * 0)
            .connect_wire(wires.wire3, rot * 1)
            .connect_wire(wires.wire5, rot * 2)
            .connect_wire(wires.wire7, rot * 3)
            .connect_wire(wires.wire9, rot * 4)
            .connect_wire(wires.wire2, rot * 5)
            .connect_wire(wires.wire4, rot * 6)
            .connect_wire(wires.wire6, rot * 7)
            .connect_wire(wires.wire8, rot * 8);
}

private node wires {
    output wire1: ignition_wire();
    output wire2: ignition_wire();
    output wire3: ignition_wire();
    output wire4: ignition_wire();
    output wire5: ignition_wire();
    output wire6: ignition_wire();
    output wire7: ignition_wire();
    output wire8: ignition_wire();
    output wire9: ignition_wire();
}


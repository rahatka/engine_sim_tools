import "engine_sim.mr"
import "../../fuel/30.mr"

units units()
constants constants()

label bore(127.0) // 5
label stroke(139.7) // 5 1/2
label compression_ratio(5.0)
label con_rod(270.65) // 270.65mm -> 10 21/32 ?
label L1(216.3) // 216.3mm -> 8 33/64 ?
label R1(55.0)
label compression_height(56.8 * units.mm)
label intake_valve_diameter(60.0) // ? 2 1/5 port
label exhaust_valve_diameter(60.0) // ? 2 1/5 port ?
label intake_valves(1.0)
label exhaust_valves(1.0)

label cyl(14.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

label redline(2000.0) // RPM

// Jag III
label IVL(14.25) // ?
label EVL(14.25) // ?
label IVO(1.5 * units.deg) //BTDC
label IVC(41.5 * units.deg) //ABDC
label EVO(51.0 * units.deg) //BBDC
label EVC(11.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(1800.0) // ? g
label crank_mass(50.0) // ? kg
label flywheel_mass(20.0) // kg

label exhaust_delay_coeff(1.5)

public node eng_distributor {
    input wires;
    input timing_curve;
    input rev_limit: 3500 * units.rpm;
    input limiter_duration: 0.02;
    alias output __out:
        ignition_module(timing_curve: timing_curve, rev_limit: rev_limit, limiter_duration: limiter_duration)
            .connect_wire(wires.wire1, rot * 0)
            .connect_wire(wires.wire3, rot * 2 + 3.443 * units.deg)
            .connect_wire(wires.wire5, rot * 4 - 2.114 * units.deg)
            .connect_wire(wires.wire7, rot * 6 - 0.871 * units.deg)
            .connect_wire(wires.wire2, rot * 8 + 0.871 * units.deg)
            .connect_wire(wires.wire4, rot * 10 + 2.114 * units.deg)
            .connect_wire(wires.wire6, rot * 12 - 3.443 * units.deg)

            .connect_wire(wires.wire8, rot * 0 + rot360)
            .connect_wire(wires.wire10, rot * 2 + rot360 + 3.443 * units.deg)
            .connect_wire(wires.wire12, rot * 4 + rot360 - 2.114 * units.deg)
            .connect_wire(wires.wire14, rot * 6 + rot360 - 0.871 * units.deg)
            .connect_wire(wires.wire9, rot * 8 + rot360 + 0.871 * units.deg)
            .connect_wire(wires.wire11, rot * 10 + rot360 + 2.114 * units.deg)
            .connect_wire(wires.wire13, rot * 12 + rot360 - 3.443 * units.deg);
}

private node wires {
    output wire1: ignition_wire();
    output wire2: ignition_wire();
    output wire3: ignition_wire();
    output wire4: ignition_wire();
    output wire5: ignition_wire();
    output wire6: ignition_wire();
    output wire7: ignition_wire();
    
    output wire8: ignition_wire();

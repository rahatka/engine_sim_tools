        .add_sample(1866.7 * units.rpm, 26.6 * units.deg)
        .add_sample(2000.0 * units.rpm, 27.0 * units.deg)
        .add_sample(2133.3 * units.rpm, -15.0 * units.deg)
        .add_sample(2266.7 * units.rpm, -45.0 * units.deg)

    engine.add_ignition_module(
        eng_distributor(
            wires: wires,
            timing_curve: timing_curve

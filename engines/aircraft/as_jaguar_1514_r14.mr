private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node head {
    // port_flow.py v1.5
    // intake port area: 23.3 cm²; saturated lift: 13.30 mm
    // exhaust port area: 23.3 cm²; saturated lift: 13.30 mm
    // cylinder volume: 1769.7 cm³ (108.0 CI); engine volume: 24.775 L (1511.9 CI)
    // chamber volume: 442.4 cm³ (27.0 CI)
    // 32 harmonic intake runner length: 25.9 cm; diameter: 4.3 cm
    // primary length: 90.8 cm, area: 25.7 cm², diameter: 5.7 cm
    // collector diameter: 19.7 cm, area: 305.6 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 442.419 * units.cc,
        intake_runner_volume: 368.6 * units.cc,
        intake_runner_cross_section_area: 14.2 * units.cm2,
        exhaust_runner_volume: 122.9 * units.cc,
        exhaust_runner_cross_section_area: 25.7 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "A.S. Jaguar",
        starter_torque: 500 * units.Nm,
        starter_speed: 300 * units.rpm,
        redline: redline * units.rpm,
        fuel: a30(),
        throttle_gamma: 2.0,
        jitter: 0.7,
        noise: 0.1,
        hf_gain: 0.005,
        simulation_frequency: 8000,
        fluid_simulation_steps: 6,
        max_sle_solver_steps: 64,
        dyno_min_speed: 500 * units.rpm
    )

    wires wires()

    label crank_moment(disk_moment_of_inertia(mass: crank_mass * units.kg, radius: (stroke / 2.0) * units.mm))
    label flywheel_moment(disk_moment_of_inertia(mass: flywheel_mass * units.kg, radius: (stroke * 3.5) * units.mm))
    label other_moment(disk_moment_of_inertia(mass: 1 * units.kg, radius: 1.0 * units.cm))

    crankshaft c0(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: flywheel_mass * units.kg,
        mass: crank_mass * units.kg,
        friction_torque: 37.2 * units.Nm,
        moment_of_inertia: crank_moment + flywheel_moment + other_moment,
        tdc: 0.0
    )
    
    c0
        .add_rod_journal(rj0)
        .add_rod_journal(rj1)

    piston_parameters piston_params(
        mass: 1500 * units.g,
        blowby: k_28inH2O(0.040),
        compression_height: compression_height,
        wrist_pin_position: 0.0,
        displacement: 0.0
    )

    connecting_rod_parameters cr_params(
        mass: con_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.g,
            length: con_rod * units.mm
        ),
        slave_throw: R1 * units.mm,
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    label slave_rod_mass(1000)
    connecting_rod_parameters slave_rod_params(
        mass: slave_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: slave_rod_mass * units.g,
            length: L1 * units.mm
        ),
        center_of_mass: 0.0,
        length: L1 * units.mm
    )

    rod_journal rj0(angle: rot90)
    rod_journal sj0_0(angle: -rot * 1)
    rod_journal sj0_1(angle: -rot * 2)
    rod_journal sj0_2(angle: -rot * 3)
    rod_journal sj0_3(angle: -rot * 4)
    rod_journal sj0_4(angle: -rot * 5)
    rod_journal sj0_5(angle: -rot * 6)

    rod_journal rj1(angle: rot180 + rot90)
    rod_journal sj1_0(angle: -rot * 1)
    rod_journal sj1_1(angle: -rot * 2)
    rod_journal sj1_2(angle: -rot * 3)
    rod_journal sj1_3(angle: -rot * 4)
    rod_journal sj1_4(angle: -rot * 5)
    rod_journal sj1_5(angle: -rot * 6)

    connecting_rod mcr0(cr_params)
    connecting_rod mcr1(cr_params)

    mcr0.add_slave_journal(sj0_0)
    mcr0.add_slave_journal(sj0_1)
    mcr0.add_slave_journal(sj0_2)
    mcr0.add_slave_journal(sj0_3)
    mcr0.add_slave_journal(sj0_4)
    mcr0.add_slave_journal(sj0_5)

    mcr1.add_slave_journal(sj1_0)
    mcr1.add_slave_journal(sj1_1)
    mcr1.add_slave_journal(sj1_2)
    mcr1.add_slave_journal(sj1_3)
    mcr1.add_slave_journal(sj1_4)
    mcr1.add_slave_journal(sj1_5)

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    label collector_cross_section_area(152.8)

    label exhaust_pipe_length_0(50.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 100.0) // Litres

    intake intake(
        plenum_volume: 6.7 * units.L,
        plenum_cross_section_area: 99.5 * units.cm2,
        runner_length: 25.9 * units.cm,
        intake_flow_rate: k_carb(825.8),
        idle_flow_rate: k_carb(0.05),
        idle_throttle_plate_position: 0.996,
        runner_flow_rate: k_carb(118.0),
        velocity_decay: 0.05
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(688.2),
        collector_cross_section_area: 152.8 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 90.8 * units.cm,
        primary_flow_rate: k_carb(196.6),
        velocity_decay: 0.7,
        volume: exhaust_volume_0 * units.L
    )

    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_45.wav", volume: 0.0001))
    exhaust_system exhaust1(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_55.wav", volume: 0.0001))
 
    cylinder_bank b0(bank_params, angle: -(0.0 / cyl) * rot360)
    cylinder_bank b1(bank_params, angle: -(2.0 / cyl) * rot360)
    cylinder_bank b2(bank_params, angle: -(4.0 / cyl) * rot360)
    cylinder_bank b3(bank_params, angle: -(6.0 / cyl) * rot360)
    cylinder_bank b4(bank_params, angle: -(8.0 / cyl) * rot360)
    cylinder_bank b5(bank_params, angle: -(10.0 / cyl) * rot360)
    cylinder_bank b6(bank_params, angle: -(12.0 / cyl) * rot360)
    
    cylinder_bank b7(bank_params, angle: -(0.0 / cyl) * rot360 + rot180)
    cylinder_bank b8(bank_params, angle: -(2.0 / cyl) * rot360 + rot180)
    cylinder_bank b9(bank_params, angle: -(4.0 / cyl) * rot360 + rot180)
    cylinder_bank b10(bank_params, angle: -(6.0 / cyl) * rot360 + rot180)
    cylinder_bank b11(bank_params, angle: -(8.0 / cyl) * rot360 + rot180)
    cylinder_bank b12(bank_params, angle: -(10.0 / cyl) * rot360 + rot180)
    cylinder_bank b13(bank_params, angle: -(12.0 / cyl) * rot360 + rot180)
    
    label sh(250)
    label spacing_factor(5.0)
    label flange_density(1.0 * exhaust_delay_coeff)
    label pl5((4.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl9((3.0 * bore * spacing_factor / flange_density + sh) * units.mm)
    label pl6((2.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl10((1.0 * bore * spacing_factor / flange_density + sh) * units.mm)
    label pl7((2.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl11((3.0 * bore * spacing_factor / flange_density + sh) * units.mm)
    label pl1((4.0 * bore * spacing_factor / flange_density) * units.mm)
    
    label pl12((4.0 * bore * spacing_factor / flange_density + sh) * units.mm)
    label pl2((3.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl13((2.0 * bore * spacing_factor / flange_density + sh) * units.mm)
    label pl3((1.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl14((2.0 * bore * spacing_factor / flange_density + sh) * units.mm)
    label pl4((3.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl8((4.0 * bore * spacing_factor / flange_density + sh) * units.mm)
    
    b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: mcr0,
            rod_journal: rj0,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl1,
            ignition_wire: wires.wire1
        )
    b1.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(slave_rod_params),
            rod_journal: sj0_0,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl2,
            ignition_wire: wires.wire2
        )
    b2.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(slave_rod_params),
            rod_journal: sj0_1,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl3,
            ignition_wire: wires.wire3
        )
    b3.add_cylinder(
            piston: piston(piston_params),

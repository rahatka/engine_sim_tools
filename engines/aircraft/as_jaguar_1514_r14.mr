    b13.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_13,
            exhaust_camshaft: camshaft.exhaust_cam_13
        )
    )

    function timing_curve(133.3 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 2.0 * units.deg)
        .add_sample(133.3 * units.rpm, 2.0 * units.deg)
        .add_sample(266.7 * units.rpm, 4.5 * units.deg)
        .add_sample(400.0 * units.rpm, 8.8 * units.deg)
        .add_sample(533.3 * units.rpm, 12.3 * units.deg)

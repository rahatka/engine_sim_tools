import "engine_sim.mr"
import "../../fuel/10.mr"

units units()
constants constants()

label bore(168.275) // 6 5/8
label stroke(190.5) // 7 1/2
label compression_ratio(6.5)
label con_rod(323.85) // 12 3/4
label compression_height(63.0 * units.mm)
label intake_valve_diameter(57.15) // 2 1/4 ? ; 2 c
label exhaust_valve_diameter(57.15) // 2 1/4 ? ; 2 c
label intake_valves(2.0)
label exhaust_valves(2.0)

label redline(1600.0)

label cyl(6.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// Only lifts
label IVL(11.1125) // 7/16
label EVL(9.525) // 3/8
label IVO(-10.0 * units.deg) //BTDC
label IVC(45.0 * units.deg) //ABDC
label EVO(48.0 * units.deg) //BBDC
label EVC(8.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(2000.0) // ?
label piston_mass(2000.0) // ?
label crank_mass(100.0) // ?
label flywheel_mass(15.0) // kg

label exhaust_delay_coeff(1.5)

// Mc Cook 1-1-24 says 1-6-5-2-3-4
public node variant_0 {
    output s1: 0;
    output s5: 1;
    output s3: 2;
    output s6: 3;
    output s2: 4;
    output s4: 5;
}


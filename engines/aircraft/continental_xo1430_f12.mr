units units()
constants constants()

label bore(139.7) // 5 1/2
label stroke(127.0) // 5
label compression_ratio(6.3)
label con_rod(196.85) // 7 3/4
label compression_height(47.879 * units.mm) // ~ 1.885
label intake_valve_diameter(69.85) // ~ 2 3/4 ; 2.437 c DESIGN REPORT No. 54
label exhaust_valve_diameter(63.5) // ~ 2 1/2 ; 2.125 c DESIGN REPORT No. 54
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(4000.0) // RPM

label cyl(12.0)
label row(2.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label vee(180.0 * units.deg)

label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)

// XO-1430 DESIGN REPORT No. 54
label IVL(14.1732) // .558 ?
label EVL(11.9126) // .469 ?
label IVO(20.0 * units.deg) //BTDC
label IVC(65.0 * units.deg) //ABDC
label EVO(65.0 * units.deg) //BBDC
label EVC(20.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(24947.6 / 12) // 55 lb
label piston_mass(27669.1 / 12) // 61 lb
label crank_mass(42.18) // 93 lb
label flywheel_mass(15.0) // kg

label exhaust_delay_coeff(1.5)


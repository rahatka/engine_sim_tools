import "engine_sim.mr"
import "../../fuel/10.mr"

units units()
constants constants()

label bore(150.0)
label stroke(180.0)
label compression_ratio(6.42)
label con_rod(325.0)
label compression_height(55.0 * units.mm) // 59 Neuman
label intake_valve_diameter(72.0) // 68 clear
label exhaust_valve_diameter(72.0) // 68 clear
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(1750.0)

label cyl(6.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// BMW IIIa
label IVL(12.9)
label EVL(12.5)
label IVO(5.0 * units.deg) //BTDC
label IVC(36.0 * units.deg) //ABDC
label EVO(60.0 * units.deg) //BBDC
label EVC(20.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(16465.4 / cyl) // 36.3 / 6
label piston_mass(13653.13 / cyl) // 30.1 / 6
label crank_mass(38.01) // 83.8 lb
label flywheel_mass(15.0) // kg

label exhaust_delay_coeff(1.5)


public node bank_builder {
    input shift;
    input ignition;
    input intake;
    input exhaust;

    input pl0;
    input pl1;
    input pl2;
    input pl3;
    input pl4;
    input pl5;
    input pl6;
    input pl7;
    input pl8;
    input pl9;
    input pl10;

    output rj0: _rj0;

    output b0: _b0;
    output b1: _b1;
    output b2: _b2;
    output b3: _b3;
    output b4: _b4;
    output b5: _b5;
    output b6: _b6;
    output b7: _b7;
    output b8: _b8;
    output b9: _b9;
    output b10: _b10;

    ignition_wire _wire1()
    ignition_wire _wire2()
    ignition_wire _wire3()
    ignition_wire _wire4()
    ignition_wire _wire5()
    ignition_wire _wire6()
    ignition_wire _wire7()
    ignition_wire _wire8()
    ignition_wire _wire9()
    ignition_wire _wire10()
    ignition_wire _wire11()

    connecting_rod_parameters cr_params(
        mass: con_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.g,
            length: con_rod * units.mm
        ),
        center_of_mass: 0.0,
        slave_throw: R1 * units.mm,
        length: con_rod * units.mm
    )

    label slave_rod_mass(900.0)
    connecting_rod_parameters slave_rod_params(
        mass: slave_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: slave_rod_mass * units.g,
            length: L1 * units.mm
        ),
        center_of_mass: 0.0,
        length: L1 * units.mm
    )

    rod_journal _rj0(angle: shift / 2.0 + rot90)
    rod_journal sj0(angle: -38.5 * units.deg)
    rod_journal sj1(angle: -75.2 * units.deg)
    rod_journal sj2(angle: -108.1 * units.deg)
    rod_journal sj3(angle: -138.9 * units.deg)
    rod_journal sj4(angle: -166.6 * units.deg)
    rod_journal sj5(angle: -193.4 * units.deg)
    rod_journal sj6(angle: -221.1 * units.deg)
    rod_journal sj7(angle: -251.9 * units.deg)
    rod_journal sj8(angle: -284.8 * units.deg)
    rod_journal sj9(angle: -321.5 * units.deg)

    connecting_rod mcr(cr_params)

    mcr.add_slave_journal(sj0)
    mcr.add_slave_journal(sj1)
    mcr.add_slave_journal(sj2)
    mcr.add_slave_journal(sj3)
    mcr.add_slave_journal(sj4)
    mcr.add_slave_journal(sj5)
    mcr.add_slave_journal(sj6)
    mcr.add_slave_journal(sj7)
    mcr.add_slave_journal(sj8)
    mcr.add_slave_journal(sj9)

    piston_parameters piston_params(
        mass: 1500 * units.g,
        blowby: k_28inH2O(0.049),
        compression_height: compression_height,
        wrist_pin_position: 0.0,
        displacement: 0.0
    )

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    camshaft _intake_cam_0(base_radius: 27.8 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 27.8 * units.mm, lobe_profile: exhaust_lobe_profile)

    camshaft _intake_cam_1(base_radius: 27.8 * units.mm, lobe_profile: intake_lobe_profile)

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node head {
    // port_flow.py v1.5
    // intake port area: 16.2 cm²; saturated lift: 7.86 mm
    // exhaust port area: 12.7 cm²; saturated lift: 6.96 mm
    // cylinder volume: 498.9 cm³ (30.4 CI); engine volume: 10.977 L (669.8 CI)
    // chamber volume: 55.4 cm³ (3.4 CI)
    // 16 harmonic intake runner length: 17.3 cm; diameter: 4.0 cm
    // primary length: 29.2 cm, area: 14.0 cm², diameter: 4.2 cm
    // collector diameter: 11.7 cm, area: 107.8 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 55.437 * units.cc,
        intake_runner_volume: 217.3 * units.cc,
        intake_runner_cross_section_area: 12.5 * units.cm2,
        exhaust_runner_volume: 65.8 * units.cc,
        exhaust_runner_cross_section_area: 14.0 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

public node crank {
    input phasing;
    input var;

    output c: _c;
    output rj0: _rj0;
    output rj1: _rj1;
    output rj2: _rj2;
    output rj3: _rj3;
    output rj4: _rj4;
    output rj5: _rj5;
    output rj6: _rj6;
    output rj7: _rj7;
    output rj8: _rj8;
    output rj9: _rj9;
    output rj10: _rj10;

    label crank_moment(disk_moment_of_inertia(mass: crank_mass * units.kg, radius: (stroke / 2.0) * units.mm))
    label flywheel_moment(disk_moment_of_inertia(mass: flywheel_mass * units.kg, radius: (stroke * 3.5) * units.mm))
    label other_moment(disk_moment_of_inertia(mass: 1 * units.kg, radius: 1.0 * units.cm))

    crankshaft _c(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: flywheel_mass * units.kg,
        mass: crank_mass * units.kg,
        friction_torque: 16.5 * units.Nm,
        moment_of_inertia: crank_moment + flywheel_moment + other_moment,
        tdc: 0.0
    )

    rod_journal _rj0(angle: rot_base * var.s1 + rot90 + vee / 2 + vree / 2)
    rod_journal _rj1(angle: rot_base * var.s2 + rot90 + vee / 2 - vree / 2)
    rod_journal _rj2(angle: rot_base * var.s3 + rot90 + vee / 2 + vree / 2)
    rod_journal _rj3(angle: rot_base * var.s4 + rot90 + vee / 2 - vree / 2)
    rod_journal _rj4(angle: rot_base * var.s5 + rot90 + vee / 2 + vree / 2)
    rod_journal _rj5(angle: rot_base * var.s6 + rot90 + vee / 2 - vree / 2)
    rod_journal _rj6(angle: rot_base * var.s7 + rot90 + vee / 2 + vree / 2)
    rod_journal _rj7(angle: rot_base * var.s8 + rot90 + vee / 2 - vree / 2)
    rod_journal _rj8(angle: rot_base * var.s9 + rot90 + vee / 2 + vree / 2)
    rod_journal _rj9(angle: rot_base * var.s10 + rot90 + vee / 2 - vree / 2)
    rod_journal _rj10(angle: rot_base * var.s11 + rot90 + vee / 2 + vree / 2)

    _c
        .add_rod_journal(_rj0)
        .add_rod_journal(_rj1)
        .add_rod_journal(_rj2)
        .add_rod_journal(_rj3)
        .add_rod_journal(_rj4)
        .add_rod_journal(_rj5)
        .add_rod_journal(_rj6)
        .add_rod_journal(_rj7)
        .add_rod_journal(_rj8)
        .add_rod_journal(_rj9)
        .add_rod_journal(_rj10)
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "M120 based W22",
        starter_torque: 200 * units.Nm,
        starter_speed: 500 * units.rpm,
        redline: redline * units.rpm,
        fuel: s90(),
        throttle_gamma: 2.0,
        jitter: 0.8,
        noise: 0.1,
        hf_gain: 0.008,
        simulation_frequency: 5000,
        dyno_min_speed: 1000 * units.rpm,
        fluid_simulation_steps: 6,
        max_sle_solver_steps: 32
    )

    variant_2 variant()

    crank c0(vee + rot90, variant)

    ignition_module ignition(timing_curve: timing_curve, rev_limit: 6500 * units.rpm, limiter_duration: 0.05)

    bank_builder bank0(
        var: variant,
        crankshaft: c0,
        incline: vee / 2,
        phasing: 0,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust0,
        flip_display: true
    )

    bank_builder bank1(
        var: variant,
        crankshaft: c0,
        incline: -vee / 2,
        phasing: vee + rot360,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust1
    )

    label collector_cross_section_area(53.9)

    label exhaust_pipe_length_0(350.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 100.0) // Litres
    
    label exhaust_pipe_length_1(352.0) // cm
    label exhaust_volume_1(collector_cross_section_area * exhaust_pipe_length_1 / 100.0) // Litres

    intake intake(
        plenum_volume: 9.6 * units.L,
        plenum_cross_section_area: 137.8 * units.cm2,
        runner_length: 17.3 * units.cm,
        intake_flow_rate: k_carb(1234.9),
        idle_flow_rate: k_carb(0.03),
        idle_throttle_plate_position: 0.998,
        runner_flow_rate: k_carb(126.3),
        velocity_decay: 0.05
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(1093.4),
        collector_cross_section_area: 53.9 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 29.2 * units.cm,
        primary_flow_rate: k_carb(211.2),
        velocity_decay: 1.0,
        volume: (exhaust_volume_0 + 15.0) * units.L
    )
    
    exhaust_system_parameters es_params1(
        outlet_flow_rate: k_carb(1093.4),
        collector_cross_section_area: 53.9 * units.cm2,
        length: exhaust_pipe_length_1 * units.cm,
        primary_tube_length: 29.2 * units.cm,
        primary_flow_rate: k_carb(211.2),
        velocity_decay: 1.0,
        volume: (exhaust_volume_1 + 15.0) * units.L
    )

    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_39.wav", volume: 0.001))
    exhaust_system exhaust1(es_params1, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_39.wav", volume: 0.001))

    engine
        .add_crankshaft(c0.c)
        .add_cylinder_bank(bank0.b0)
        .add_cylinder_bank(bank0.b1)
        .add_cylinder_bank(bank1.b0)
        .add_cylinder_bank(bank1.b1)
        .add_ignition_module(ignition)

    function timing_curve(400.0 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 5.0 * units.deg)
        .add_sample(400.0 * units.rpm, 5.0 * units.deg)
        .add_sample(800.0 * units.rpm, 6.3 * units.deg)
        .add_sample(1200.0 * units.rpm, 15.0 * units.deg)
        .add_sample(1600.0 * units.rpm, 21.8 * units.deg)
        .add_sample(2000.0 * units.rpm, 27.0 * units.deg)
        .add_sample(2400.0 * units.rpm, 31.1 * units.deg)
        .add_sample(2800.0 * units.rpm, 34.2 * units.deg)
        .add_sample(3200.0 * units.rpm, 36.7 * units.deg)
        .add_sample(3600.0 * units.rpm, 38.5 * units.deg)
        .add_sample(4000.0 * units.rpm, 40.0 * units.deg)
        .add_sample(4400.0 * units.rpm, 40.0 * units.deg)
        .add_sample(4800.0 * units.rpm, 40.0 * units.deg)
        .add_sample(5200.0 * units.rpm, 40.0 * units.deg)
        .add_sample(5600.0 * units.rpm, 40.0 * units.deg)
        .add_sample(6000.0 * units.rpm, 40.0 * units.deg)
        .add_sample(6400.0 * units.rpm, -15.0 * units.deg)
        .add_sample(6800.0 * units.rpm, -45.0 * units.deg)
}

// 2000 CL600
public node veh {
    alias output __out: vehicle;

    label car_mass(2100) // 1955

    vehicle vehicle(
        mass: car_mass * units.kg,
        drag_coefficient: 0.28,
        cross_sectional_area: (1857 * units.mm) * (1398 * units.mm),
        diff_ratio: 2.82,
        tire_radius: (679.0 / 2) * units.mm, // 225/55 R 17 W
        rolling_resistance: 0.012 * car_mass * 9.81
        )
}


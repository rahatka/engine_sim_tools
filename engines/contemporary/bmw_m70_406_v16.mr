private node add_lobes {
    input base;
    input shift: 0.0;
    input var;
    input this;
    alias output __out: this;

    this.add_lobe(base + var.s1 * rot_base + shift)
    this.add_lobe(base + var.s2 * rot_base + shift)
    this.add_lobe(base + var.s3 * rot_base + shift)
    this.add_lobe(base + var.s4 * rot_base + shift)
    this.add_lobe(base + var.s5 * rot_base + shift)
    this.add_lobe(base + var.s6 * rot_base + shift)
    this.add_lobe(base + var.s7 * rot_base + shift)
    this.add_lobe(base + var.s8 * rot_base + shift)
}

public node bank_builder {
    input var;
    input crankshaft;
    input incline;
    input phasing;
    input ignition;
    input intake;
    input exhaust;
    input primaries;
    input sh;
    input flip: false;

    output bank: _b0;

    ignition_wire _wire1()
    ignition_wire _wire2()
    ignition_wire _wire3()
    ignition_wire _wire4()
    ignition_wire _wire5()
    ignition_wire _wire6()
    ignition_wire _wire7()
    ignition_wire _wire8()

    piston_parameters piston_params(
        mass: 550 * units.g,
        blowby: k_28inH2O(0.026),
        compression_height: compression_height,
        wrist_pin_position: 0.0,
        displacement: 0.0
    )

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    connecting_rod_parameters cr_params(
        mass: con_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.g,
            length: con_rod * units.mm
        ),
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    camshaft _intake_cam_0(base_radius: 18.8 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 18.8 * units.mm, lobe_profile: exhaust_lobe_profile)

    _intake_cam_0.add_lobes(ILC, phasing, var)
    _exhaust_cam_0.add_lobes(ELC, phasing, var)

    ignition.connect_wire(_wire1, rot_base * var.s1 + phasing)
    ignition.connect_wire(_wire2, rot_base * var.s2 + phasing)
    ignition.connect_wire(_wire3, rot_base * var.s3 + phasing)
    ignition.connect_wire(_wire4, rot_base * var.s4 + phasing)
    ignition.connect_wire(_wire5, rot_base * var.s5 + phasing)
    ignition.connect_wire(_wire6, rot_base * var.s6 + phasing)
    ignition.connect_wire(_wire7, rot_base * var.s7 + phasing)
    ignition.connect_wire(_wire8, rot_base * var.s8 + phasing)

    cylinder_bank _b0(bank_params, angle: incline)

    _b0
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj0,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: primaries.pl0 + sh,
            ignition_wire: _wire1
        )
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj1,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: primaries.pl1 + sh,
            ignition_wire: _wire2
        )
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj2,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: primaries.pl2 + sh,
            ignition_wire: _wire3
        )
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj3,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: primaries.pl3 + sh,
            ignition_wire: _wire4
        )
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj4,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: primaries.pl4 + sh,
            ignition_wire: _wire5
        )
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj5,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: primaries.pl5 + sh,
            ignition_wire: _wire6
        )
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj6,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: primaries.pl6 + sh,
            ignition_wire: _wire7
        )
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj7,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: primaries.pl7 + sh,
            ignition_wire: _wire8
        )

    _b0.set_cylinder_head(
        head(
            intake_camshaft: _intake_cam_0,
            exhaust_camshaft: _exhaust_cam_0,
            flip_display: flip
        )
    )
}

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node head {
    // port_flow.py v1.5
    // intake port area: 11.1 cm²; saturated lift: 9.25 mm
    // exhaust port area: 7.6 cm²; saturated lift: 7.59 mm
    // cylinder volume: 415.6 cm³ (25.4 CI); engine volume: 6.650 L (405.8 CI)
    // chamber volume: 53.3 cm³ (3.3 CI)
    // 16 harmonic intake runner length: 17.2 cm; diameter: 3.6 cm
    // primary length: 28.6 cm, area: 8.4 cm², diameter: 3.3 cm
    // collector diameter: 7.2 cm, area: 40.2 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 53.286 * units.cc,
        intake_runner_volume: 172.2 * units.cc,
        intake_runner_cross_section_area: 10.0 * units.cm2,
        exhaust_runner_volume: 57.4 * units.cc,
        exhaust_runner_cross_section_area: 8.4 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

public node crank {
    input phasing;
    input var;

    output c: _c;
    output rj0: _rj0;
    output rj1: _rj1;
    output rj2: _rj2;
    output rj3: _rj3;
    output rj4: _rj4;
    output rj5: _rj5;
    output rj6: _rj6;
    output rj7: _rj7;

    label crank_moment(disk_moment_of_inertia(mass: crank_mass * units.kg, radius: (stroke / 2.0) * units.mm))
    label flywheel_moment(disk_moment_of_inertia(mass: flywheel_mass * units.kg, radius: (stroke * 3.5) * units.mm))
    label other_moment(disk_moment_of_inertia(mass: 1 * units.kg, radius: 1.0 * units.cm))

    crankshaft _c(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: flywheel_mass * units.kg,
        mass: crank_mass * units.kg,
        friction_torque: 10.0 * units.Nm,
        moment_of_inertia: crank_moment + flywheel_moment + other_moment,
        tdc: 0.0
    )

    rod_journal _rj0(angle: rot_base * var.s1 + rot90 + vee / 2)
    rod_journal _rj1(angle: rot_base * var.s2 + rot90 + vee / 2)
    rod_journal _rj2(angle: rot_base * var.s3 + rot90 + vee / 2)
    rod_journal _rj3(angle: rot_base * var.s4 + rot90 + vee / 2)
    rod_journal _rj4(angle: rot_base * var.s5 + rot90 + vee / 2)
    rod_journal _rj5(angle: rot_base * var.s6 + rot90 + vee / 2)
    rod_journal _rj6(angle: rot_base * var.s7 + rot90 + vee / 2)
    rod_journal _rj7(angle: rot_base * var.s8 + rot90 + vee / 2)

    _c
        .add_rod_journal(_rj0)
        .add_rod_journal(_rj1)
        .add_rod_journal(_rj2)
        .add_rod_journal(_rj3)
        .add_rod_journal(_rj4)
        .add_rod_journal(_rj5)
        .add_rod_journal(_rj6)
        .add_rod_journal(_rj7)
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: variant.seq,
        starter_torque: 150 * units.Nm,
        starter_speed: 400 * units.rpm,
        redline: redline * units.rpm,
        fuel: s90(),
        throttle_gamma: 1.5,
        jitter: 1.0,
        noise: 0.1,
        hf_gain: 0.01,
        simulation_frequency: 7000,
        dyno_min_speed: 1000 * units.rpm,
        fluid_simulation_steps: 6,
        max_sle_solver_steps: 32
    )

    ////////////////////////////////////////
    ///////// change variants here /////////
    ////////////////////////////////////////
    // 0  1-6-2-5-8-3-7-4 Standard
    // 1  1-6-7-4-8-3-2-5 2D70
    // 2  1-5-7-3-8-4-2-6 5D49
    // 3  1-5-2-6-8-4-7-3 Cadillac V16
    // 4  1-6-2-7-3-8-4-5 Bugatti Type 39
    // 5  1-4-7-6-8-5-2-3 Mercedes-Benz M 196 R ; Baldwin
    // 6  1-2-4-6-8-7-5-3 Mercedes-Benz M 196 S
    // 7  1-5-3-7-4-8-2-6 Bugatti Type 59 ; Duesenberg before 1927 ; Miller
    // 8  1-3-7-4-8-6-2-5 Delage
    // 9  1-4-7-3-8-5-2-6 Hupmobile
    // 10 1-3-2-5-8-6-7-4 Packard before 1927
    // 11 1-5-2-6-3-7-4-8 Bugatti most common
    // 12 1-5-2-3-8-4-7-6 Hupmobile before 1927
    // 13 1-3-5-7-6-8-2-4 2-2-2-2 v1
    // 14 1-8-5-4-6-3-2-7 2-2-2-2 v2
    variant_0 variant()

    em_0 em()

    crank c0(vee / 2 + rot90, variant)

    bank_builder bank0(
        var: variant,
        crankshaft: c0,
        incline: vee / 2,
        phasing: 0,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust0,
        primaries: em,
        sh: 0.0 * units.cm,
        flip: true
    )

    bank_builder bank1(
        var: variant,
        crankshaft: c0,
        incline: -vee / 2,
        phasing: vee,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust1,
        sh: 0.0 * units.cm,
        primaries: em
    )

    ignition_module ignition(timing_curve: timing_curve, rev_limit: redline * 1.05 * units.rpm, limiter_duration: 0.06)

    label collector_cross_section_area(20.1)

    intake intake(
        plenum_volume: 5.4 * units.L,
        plenum_cross_section_area: 80.3 * units.cm2,
        runner_length: 17.2 * units.cm,
        intake_flow_rate: k_carb(698.3),
        idle_flow_rate: k_carb(0.01),
        idle_throttle_plate_position: 0.9975,
        runner_flow_rate: k_carb(91.6),
        velocity_decay: 0.03
    )
    
    label exhaust_pipe_length_0(393.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 1000.0) // Litres
    
    label exhaust_pipe_length_1(400.0) // cm
    label exhaust_volume_1(collector_cross_section_area * exhaust_pipe_length_1 / 1000.0) // Litres
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(596.4),
        collector_cross_section_area: 20.1 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 28.6 * units.cm,
        primary_flow_rate: k_carb(152.8),
        velocity_decay: 1.0,
        volume: (exhaust_volume_0 + 5.0) * units.L
    )

    exhaust_system_parameters es_params1(
        outlet_flow_rate: k_carb(596.4),
        collector_cross_section_area: 20.1 * units.cm2,
        length: exhaust_pipe_length_1 * units.cm,
        primary_tube_length: 28.6 * units.cm,
        primary_flow_rate: k_carb(152.8),
        velocity_decay: 1.0,
        volume: (exhaust_volume_1 + 5.0) * units.L
    )
    
    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_60.wav", volume: 0.00025))
    exhaust_system exhaust1(es_params1, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_60.wav", volume: 0.00025))

    engine
        .add_crankshaft(c0.c)
        .add_cylinder_bank(bank0.bank)
        .add_cylinder_bank(bank1.bank)
        .add_ignition_module(ignition)

    function timing_curve(400.0 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 10.0 * units.deg)
        .add_sample(400.0 * units.rpm, 10.0 * units.deg)
        .add_sample(800.0 * units.rpm, 15.2 * units.deg)
        .add_sample(1200.0 * units.rpm, 21.1 * units.deg)
        .add_sample(1600.0 * units.rpm, 25.9 * units.deg)
        .add_sample(2000.0 * units.rpm, 30.0 * units.deg)
        .add_sample(2400.0 * units.rpm, 33.4 * units.deg)
        .add_sample(2800.0 * units.rpm, 36.2 * units.deg)
        .add_sample(3200.0 * units.rpm, 38.5 * units.deg)
        .add_sample(3600.0 * units.rpm, 40.0 * units.deg)
        .add_sample(4000.0 * units.rpm, 40.0 * units.deg)
        .add_sample(4400.0 * units.rpm, 40.0 * units.deg)
        .add_sample(4800.0 * units.rpm, 40.0 * units.deg)
        .add_sample(5200.0 * units.rpm, 40.0 * units.deg)
        .add_sample(5600.0 * units.rpm, 40.0 * units.deg)
        .add_sample(6000.0 * units.rpm, 40.0 * units.deg)
        .add_sample(6400.0 * units.rpm, -12.5 * units.deg)
        .add_sample(6800.0 * units.rpm, -25.0 * units.deg)
}

label spacing_factor(1.13)
label flange_density(1.0 * exhaust_delay_coeff)
label em_unit(bore * spacing_factor / flange_density)

// 3/4 manifold
private node em_0{
    output pl0: (5.5 * em_unit) * units.mm;
    output pl1: (4.5 * em_unit) * units.mm;
    output pl2: (3.5 * em_unit) * units.mm;
    output pl3: (2.5 * em_unit) * units.mm;
    output pl4: (1.5 * em_unit) * units.mm;
    output pl5: (1.0 * em_unit) * units.mm;
    output pl6: (2.0 * em_unit) * units.mm;
    output pl7: (3.0 * em_unit) * units.mm;
}

// "long end" 4/4 manifold
private node em_1{
    output pl0: (7.0 * em_unit) * units.mm;
    output pl1: (6.0 * em_unit) * units.mm;
    output pl2: (5.0 * em_unit) * units.mm;
    output pl3: (4.0 * em_unit) * units.mm;
    output pl4: (3.0 * em_unit) * units.mm;
    output pl5: (2.0 * em_unit) * units.mm;
    output pl6: (1.2 * em_unit) * units.mm;
    output pl7: (1.0 * em_unit) * units.mm;
}

// two middle manifolds
private node em_2{
    output pl0: (5.0 * em_unit) * units.mm;
    output pl1: (4.0 * em_unit) * units.mm;
    output pl2: (4.0 * em_unit) * units.mm;
    output pl3: (5.0 * em_unit) * units.mm;
    output pl4: (2.0 * em_unit) * units.mm;
    output pl5: (1.0 * em_unit) * units.mm;
    output pl6: (1.0 * em_unit) * units.mm;
    output pl7: (2.0 * em_unit) * units.mm;
}

// ?
private node em_3{
    output pl0: (5.8 * em_unit) * units.mm;
    output pl1: (5.0 * em_unit) * units.mm;
    output pl2: (4.0 * em_unit) * units.mm;
    output pl3: (3.0 * em_unit) * units.mm;
    output pl4: (2.0 * em_unit) * units.mm;
    output pl5: (1.0 * em_unit) * units.mm;
    output pl6: (1.8 * em_unit) * units.mm;
    output pl7: (2.9 * em_unit) * units.mm;
}

// 1988 750iL
public node veh {
    alias output __out: vehicle;

    label car_mass(1960) // 1860 empty

    vehicle vehicle(
        mass: car_mass * units.kg,
        drag_coefficient: 0.34,
        cross_sectional_area: (1845  * units.mm) * (1400 * units.mm),
        diff_ratio: 3.15,
        tire_radius: (653.0 / 2) * units.mm, // 225/60 VR 15
        rolling_resistance: 0.012 * car_mass * 9.81
        )
}

// ZF 4HP24E
private node trn {
    alias output __out:
        transmission(
            max_clutch_torque: 800 * units.Nm,
            max_clutch_flex: 30 * units.deg,
            limit_clutch_flex: true,
            clutch_stiffness: 20 * units.Nm / units.deg,
            clutch_damping: 1.0,
            simulate_flex: true
        )
        .add_gear(2.479)
        .add_gear(1.479)
        .add_gear(1.000)
        .add_gear(0.728);
}

run(
    engine: eng(),
    transmission: trn(),
    vehicle: veh()
)

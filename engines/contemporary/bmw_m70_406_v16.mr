units units()
constants constants()

label bore(84.0)
label stroke(75.0)
label compression_ratio(8.8)
label con_rod(135.0)
label compression_height(34.15 * units.mm)
label intake_valve_diameter(42.0) // 6.94 stem
label exhaust_valve_diameter(35.0) // 6.94 stem
label intake_valves(1.0)
label exhaust_valves(1.0)

label cyl(16.0)
label row(2.0)
label cycle(720.0 * units.deg)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label vee(60.0 * units.deg)

label redline(6000.0) // RPM

// stock M70 246 / 264 110
label IVL(10.5)
label EVL(11.0)
label IVO(17.0 * units.deg) //BTDC
label IVC(49.0 * units.deg) //ABDC
label EVO(66.0 * units.deg) //BBDC
label EVC(18.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(499.0) // g
label crank_mass(30.0) // kg
label flywheel_mass(12.0) // kg

label exhaust_delay_coeff(1.5)


import "engine_sim.mr"
import "../../fuel/90.mr"

units units()
constants constants()

label bore(100.0)
label stroke(76.4)
label compression_ratio(11.3)
label con_rod(127.0)
label IVL(13.2) // valve lift, not lobe lift
label EVL(13.2) // valve lift, not lobe lift
label compression_height(31.7 * units.mm)
label intake_valve_diameter(50.0)
label exhaust_valve_diameter(43.5)
label intake_valves(1.0)
label exhaust_valves(1.0)

label volume(4.200) // L; port_flow.py v0.6
label redline(7000.0) // RPM

label cyl(7.0)
label row(1.0)
label cycle(720.0 * units.deg)
label vree (15.0 * units.deg)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)

// 964 / 993 Street / Sport Cam valve timings
label IVO(30.0 * units.deg) //BTDC
label IVC(78.0 * units.deg) //ABDC
label EVO(74.0 * units.deg) //BBDC
label EVC(26.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(535.0) // g
label piston_mass(440.0) // ?
label crank_mass(20.0) // kg
label flywheel_mass(5.0) // kg

label exhaust_delay_coeff(1.5)

public node variant_0 {
    output s1: 0;
    output s3: 1;
    output s5: 2;
    output s7: 3;
    output s2: 4;
    output s4: 5;
    output s6: 6;
    output seq: "1-3-5-7-2-4-6";
}

public node variant_1 {
    output s1: 0;
    output s3: 1;
    output s5: 2;
    output s7: 3;
    output s6: 4;
    output s4: 5;
    output s2: 6;
    output seq: "1-3-5-7-6-4-2";
}

public node variant_2 {
    output s1: 0;
    output s5: 1;
    output s2: 2;
    output s6: 3;
    output s3: 4;
    output s7: 5;
    output s4: 6;
    output seq: "1-5-2-6-3-7-4";
}

private node add_lobes_0 {
    input base;
    input shift: 0.0;
    input var;
    input this;
    alias output __out: this;

    this.add_lobe(base + var.s1 * rot_base + shift)
    this.add_lobe(base + var.s3 * rot_base + shift)
    this.add_lobe(base + var.s5 * rot_base + shift)
    this.add_lobe(base + var.s7 * rot_base + shift)
}

private node add_lobes_1 {
    input base;
    input shift: 0.0;
    input var;
    input this;
    alias output __out: this;

    this.add_lobe(base + var.s2 * rot_base + shift)
    this.add_lobe(base + var.s4 * rot_base + shift)
    this.add_lobe(base + var.s6 * rot_base + shift)
}


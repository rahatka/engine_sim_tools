public node veh {
    alias output __out: vehicle;
    vehicle vehicle(
        mass: 1600 * units.kg,
        drag_coefficient: 0.32,
        cross_sectional_area: (1925 * units.mm) * (1311 * units.mm),
        diff_ratio: 3.6,
        tire_radius: (780.0 / 2) * units.mm, // XXX/50R22
        rolling_resistance: 20
        )
}

// Chrysler A604
private node trn {
    alias output __out:
        transmission(
            max_clutch_torque: 525 * units.Nm,
            max_clutch_flex: 30 * units.deg,
            limit_clutch_flex: true,
            clutch_stiffness: 10 * units.Nm / units.deg,

// even firing camshaft
public node camshaft_builder_even {
    output intake_cam_0: _intake_cam_0;
    output exhaust_cam_0: _exhaust_cam_0;
    output intake_cam_1: _intake_cam_1;
    output exhaust_cam_1: _exhaust_cam_1;
    output intake_cam_2: _intake_cam_2;
    output exhaust_cam_2: _exhaust_cam_2;
    output intake_cam_3: _intake_cam_3;
    output exhaust_cam_3: _exhaust_cam_3;

    camshaft _intake_cam_0(base_radius: 19.0 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 19.0 * units.mm, lobe_profile: exhaust_lobe_profile)
    camshaft _intake_cam_1(base_radius: 19.0 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_1(base_radius: 19.0 * units.mm, lobe_profile: exhaust_lobe_profile)
    camshaft _intake_cam_2(base_radius: 19.0 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_2(base_radius: 19.0 * units.mm, lobe_profile: exhaust_lobe_profile)
    camshaft _intake_cam_3(base_radius: 19.0 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_3(base_radius: 19.0 * units.mm, lobe_profile: exhaust_lobe_profile)

    _intake_cam_0
        .add_lobe(ILC + 0 * rot) // 1
        .add_lobe(ILC + 6 * rot) // 3
    _exhaust_cam_0
        .add_lobe(ELC + 0 * rot)
        .add_lobe(ELC + 6 * rot)
        
    _intake_cam_1
        .add_lobe(ILC + 2 * rot) // 2
        .add_lobe(ILC + 4 * rot) // 4
    _exhaust_cam_1
        .add_lobe(ELC + 2 * rot)
        .add_lobe(ELC + 4 * rot)

    _intake_cam_2
        .add_lobe(ILC + 1 * rot) // 5
        .add_lobe(ILC + 7 * rot) // 7
    _exhaust_cam_2
        .add_lobe(ELC + 1 * rot)
        .add_lobe(ELC + 7 * rot)
        
    _intake_cam_3
        .add_lobe(ILC + 3 * rot) // 6
        .add_lobe(ILC + 5 * rot) // 8
    _exhaust_cam_3
        .add_lobe(ELC + 3 * rot)
        .add_lobe(ELC + 5 * rot)
}

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node head {
    // port_flow.py v1.5
    // intake port area: 11.5 cm²; saturated lift: 6.59 mm
    // exhaust port area: 8.7 cm²; saturated lift: 5.74 mm
    // cylinder volume: 501.4 cm³ (30.60 CI); engine volume: 4.012 L (244.80 CI)
    // chamber volume: 51.7 cm³ (3.15 CI)
    // 8 harmonic intake runner length: 28.4 cm; diameter: 4.1 cm
    // primary length: 24.3 cm, area: 9.6 cm², diameter: 3.5 cm
    // collector diameter: 7.0 cm, area: 38.3 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 51.696 * units.cc,
        intake_runner_volume: 381.5 * units.cc,
        intake_runner_cross_section_area: 13.4 * units.cm2,
        exhaust_runner_volume: 127.2 * units.cc,
        exhaust_runner_cross_section_area: 9.6 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "VAG 4.0 W8",
        starter_torque: 100 * units.Nm,
        starter_speed: 650 * units.rpm,
        redline: redline * units.rpm,
        fuel: s90(),
        throttle_gamma: 1.5,
        jitter: 0.7,
        noise: 0.1,
        hf_gain: 0.003,
        simulation_frequency: 10000
    )

    wires wires()
    
    connecting_rod_parameters cr_params(
        mass: 750.0 * units.g,
        moment_of_inertia: 0.005,
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    crankshaft c0(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: 6 * units.kg,
        mass: 12 * units.kg,
        friction_torque: 6.0 * units.Nm,
        moment_of_inertia: 0.2,
        position_x: 0.0,
        position_y: 0.0,
        tdc: 0.0
    )

    // even firing crank
    rod_journal rj0_0(angle: rot * 0 + rot90 + vee / 2 + vree / 2)
    rod_journal rj0_1(angle: rot * 0 + rot90 + vee / 2 + vree / 2 + phase_shift)
    rod_journal rj1_0(angle: rot * 6 + rot90 + vee / 2 - vree / 2)
    rod_journal rj1_1(angle: rot * 6 + rot90 + vee / 2 - vree / 2 + phase_shift)
    rod_journal rj2_0(angle: rot * 2 + rot90 + vee / 2 + vree / 2)
    rod_journal rj2_1(angle: rot * 2 + rot90 + vee / 2 + vree / 2 + phase_shift)
    rod_journal rj3_0(angle: rot * 4 + rot90 + vee / 2 - vree / 2)
    rod_journal rj3_1(angle: rot * 4 + rot90 + vee / 2 - vree / 2 + phase_shift)
    
    c0
        .add_rod_journal(rj0_0)
        .add_rod_journal(rj0_1)
        .add_rod_journal(rj1_0)
        .add_rod_journal(rj1_1)
        .add_rod_journal(rj2_0)
        .add_rod_journal(rj2_1)
        .add_rod_journal(rj3_0)
        .add_rod_journal(rj3_1)

    piston_parameters piston_params(
        mass: 750 * units.g,
        blowby: k_28inH2O(0.026),
        compression_height: compression_height,
        wrist_pin_position: 0.0,
        displacement: 0.0
    )

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    label intake_valve_circ(intake_valve_diameter * constants.pi)
    label intake_valve_area(intake_valve_circ * IVL / 100.0) // cm2

    label intake_plenum_area(intake_valve_area * cyl * intake_valves) // cm2
    label collector_cross_section_area(19.1)

    label exhaust_pipe_length_0(250.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 100.0) // Litres
    
    label exhaust_pipe_length_1(252.0) // cm
    label exhaust_volume_1(collector_cross_section_area * exhaust_pipe_length_1 / 100.0) // Litres
    
    label spacing_factor(0.75)
    label flange_density(1.0 * exhaust_delay_coeff)

    intake intake(
        plenum_volume: 3.6 * units.L,
        plenum_cross_section_area: 53.7 * units.cm2,
        intake_flow_rate: k_carb(434.6),
        idle_flow_rate: k_carb(0.03),
        idle_throttle_plate_position: 0.9975,
        runner_flow_rate: k_carb(108.6),
        runner_length: 28.4 * units.cm,
        velocity_decay: 0.05
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(362.2),
        collector_cross_section_area: 19.1 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 24.3 * units.cm,
        primary_flow_rate: k_carb(181.1),
        velocity_decay: 1.0,
        volume: (exhaust_volume_0 + 3.0) * units.L
    )
    
    exhaust_system_parameters es_params1(
        outlet_flow_rate: k_carb(362.2),
        collector_cross_section_area: 19.1 * units.cm2,
        length: exhaust_pipe_length_1 * units.cm,
        primary_tube_length: 24.3 * units.cm,
        primary_flow_rate: k_carb(181.1),
        velocity_decay: 1.0,
        volume: (exhaust_volume_1 + 3.0) * units.L
    )

    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_39.wav", volume: 0.00025))
    exhaust_system exhaust1(es_params1, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_39.wav", volume: 0.00025))
 
    cylinder_bank b0(bank_params, angle: vee / 2 + vree / 2, position_x: -8.60443 * units.mm, position_y: -9.06718 * units.mm)
    cylinder_bank b1(bank_params, angle: vee / 2 - vree / 2, position_x: 10.98521 * units.mm, position_y: 5.96448 * units.mm)
    cylinder_bank b2(bank_params, angle: -vee / 2 + vree / 2, position_x: -10.98521 * units.mm, position_y: 5.96448 * units.mm)
    cylinder_bank b3(bank_params, angle: -vee / 2 - vree / 2, position_x: 8.60443 * units.mm, position_y: -9.06718 * units.mm)
    
    label pl0 ((4.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl1 ((3.0 * bore * spacing_factor / flange_density + bore) * units.mm)
    label pl2 ((2.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl3 ((1.0 * bore * spacing_factor / flange_density + bore) * units.mm)
    
    label pl4 ((4.0 * bore * spacing_factor / flange_density + bore) * units.mm)
    label pl5 ((3.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl6 ((2.0 * bore * spacing_factor / flange_density + bore) * units.mm)
    label pl7 ((1.0 * bore * spacing_factor / flange_density) * units.mm)

    b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj0_0,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl0,
            ignition_wire: wires.wire1
        )
    b1.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj1_0,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl1,
            ignition_wire: wires.wire2
        )
    b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj2_0,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl2,
            ignition_wire: wires.wire3
        )
    b1.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj3_0,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl3,
            ignition_wire: wires.wire4
        )
        
    b2.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj0_1,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl4,
            ignition_wire: wires.wire5
        )
    b3.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj1_1,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl5,
            ignition_wire: wires.wire6
        )
    b2.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj2_1,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl6,
            ignition_wire: wires.wire7
        )
    b3.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj3_1,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl7,
            ignition_wire: wires.wire8
        )

    engine
        .add_cylinder_bank(b0)
        .add_cylinder_bank(b1)
        .add_cylinder_bank(b2)
        .add_cylinder_bank(b3)

    engine.add_crankshaft(c0)

    camshaft_builder_even camshaft()

    b0.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_0,
            exhaust_camshaft: camshaft.exhaust_cam_0,
            flip_display: true
        )
    )
    b1.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_1,
            exhaust_camshaft: camshaft.exhaust_cam_1,
            flip_display: true
        )
    )
    
    b2.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_2,
            exhaust_camshaft: camshaft.exhaust_cam_2
        )
    )
    b3.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_3,
            exhaust_camshaft: camshaft.exhaust_cam_3
        )
    )

    function timing_curve(433.3 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 0.0 * units.deg)
        .add_sample(433.3 * units.rpm, 0.0 * units.deg)
        .add_sample(866.7 * units.rpm, 5.3 * units.deg)
        .add_sample(1300.0 * units.rpm, 11.0 * units.deg)
        .add_sample(1733.3 * units.rpm, 15.4 * units.deg)
        .add_sample(2166.7 * units.rpm, 18.8 * units.deg)
        .add_sample(2600.0 * units.rpm, 21.5 * units.deg)
        .add_sample(3033.3 * units.rpm, 23.6 * units.deg)
        .add_sample(3466.7 * units.rpm, 25.3 * units.deg)
        .add_sample(3900.0 * units.rpm, 26.5 * units.deg)
        .add_sample(4333.3 * units.rpm, 27.5 * units.deg)
        .add_sample(4766.7 * units.rpm, 28.3 * units.deg)
        .add_sample(5200.0 * units.rpm, 28.9 * units.deg)
        .add_sample(5633.3 * units.rpm, 29.4 * units.deg)
        .add_sample(6066.7 * units.rpm, 29.7 * units.deg)
        .add_sample(6500.0 * units.rpm, 30.0 * units.deg)
        .add_sample(6933.3 * units.rpm, -12.5 * units.deg)
        .add_sample(7366.7 * units.rpm, -25.0 * units.deg)

    engine.add_ignition_module(
        eng_distributor_even(
            wires: wires,
            timing_curve: timing_curve
        )
    )
}

 // Passat W8
public node veh {
    alias output __out: vehicle;
    vehicle vehicle(
        mass: 1664 * units.kg,
        drag_coefficient: 0.27,
        cross_sectional_area: (1746 * units.mm) * (1461 * units.mm),
        diff_ratio: 4.11,
        tire_radius: (634.0 / 2) * units.mm, // 225/45R17
        rolling_resistance: 25
        )
}

 // Passat W8
private node trn {
    alias output __out:
        transmission(
            max_clutch_torque: 500 * units.Nm,
            max_clutch_flex: 20 * units.deg,
            limit_clutch_flex: true,
            clutch_stiffness: 15 * units.Nm / units.deg,
            clutch_damping: 0.5,
            simulate_flex: true
        )
        .add_gear(3.50)
        .add_gear(1.89)
        .add_gear(1.23)
        .add_gear(0.97)
        .add_gear(0.81)
        .add_gear(0.68);
}

run(
    engine: eng(),
    transmission: trn(),
    vehicle: veh()
)

import "engine_sim.mr"
import "../../fuel/90.mr"

units units()
constants constants()

label bore(84.5)
label stroke(92.8)
label compression_ratio(12.5)
label con_rod(154.0)
label compression_height(31.7 * units.mm)
label intake_valve_diameter(34.85)
label exhaust_valve_diameter(29.0)
label intake_valves(2.0)
label exhaust_valves(2.0)

label redline(8700.0) // RPM

label cyl(10.0)
label row(2.0)
label cycle(720.0 * units.deg)
label vee(90.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// Schrick 288/278 108LSA
label IVL(11.6) // valve lift, not cam lift
label EVL(12.0) // valve lift, not cam lift
label IVO(36.0 * units.deg) //BTDC
label IVC(72.0 * units.deg) //ABDC
label EVO(67.0 * units.deg) //BBDC
label EVC(31.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(450.0) // ? g
label crank_mass(18.0) // ? kg
label flywheel_mass(5.0) // kg

label exhaust_delay_coeff(1.5)

public node eng_distributor {
    input wires;
    input timing_curve;
    input rev_limit: 9000 * units.rpm;
    input limiter_duration: 0.03;
    alias output __out:
        ignition_module(timing_curve: timing_curve, rev_limit: rev_limit, limiter_duration: limiter_duration)
            .connect_wire(wires.wire1, rot * 0)
            .connect_wire(wires.wire5, rot * 2)
            .connect_wire(wires.wire2, rot * 4)
            .connect_wire(wires.wire3, rot * 6)
            .connect_wire(wires.wire4, rot * 8)
            
            .connect_wire(wires.wire6, rot * 0 + vee)
            .connect_wire(wires.wire10, rot * 2 + vee)
            .connect_wire(wires.wire7, rot * 4 + vee)
            .connect_wire(wires.wire8, rot * 6 + vee)
            .connect_wire(wires.wire9, rot * 8 + vee);
}

private node wires {
    output wire1: ignition_wire();
    output wire2: ignition_wire();
    output wire3: ignition_wire();
    output wire4: ignition_wire();
    output wire5: ignition_wire();
    output wire6: ignition_wire();
    output wire7: ignition_wire();
    output wire8: ignition_wire();
    output wire9: ignition_wire();
    output wire10: ignition_wire();
}


private node add_lobes {
    input base;
    input shift: 0.0;
    input var;
    input this;
    alias output __out: this;

    this.add_lobe(base + var.s1 * rot_base + shift)
    this.add_lobe(base + var.s2 * rot_base + shift)
    this.add_lobe(base + var.s2 * rot_base + second_phase + shift)
    this.add_lobe(base + var.s1 * rot_base + second_phase + shift)
}

public node bank_builder {
    input var;
    input incline;
    input phasing;
    input ignition;
    input intake;
    input exhaust;
    input rj0;
    input rj1;
    input rj2;
    input rj3;
    input flip_display: false;

    output b0: _b0;

    ignition_wire _wire1()
    ignition_wire _wire2()
    ignition_wire _wire3()
    ignition_wire _wire4()

    piston_parameters piston_params(
        mass: piston_mass * units.g,
        blowby: k_28inH2O(0.031),
        compression_height: compression_height,
        wrist_pin_position: 0.0,
        displacement: 0.0
    )

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    connecting_rod_parameters cr_params(
        mass: con_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.g,
            length: con_rod * units.mm
        ),
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    camshaft _intake_cam_0(base_radius: 19.7 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 19.7 * units.mm, lobe_profile: exhaust_lobe_profile)

    _intake_cam_0.add_lobes(ILC, phasing, var)
    _exhaust_cam_0.add_lobes(ELC, phasing, var)

    ignition.connect_wire(_wire1, rot_base * var.s1 + phasing)
    ignition.connect_wire(_wire2, rot_base * var.s2 + phasing)
    ignition.connect_wire(_wire3, rot_base * var.s2 + second_phase + phasing)
    ignition.connect_wire(_wire4, rot_base * var.s1 + second_phase + phasing)

    cylinder_bank _b0(bank_params, angle: incline)

    label spacing_factor(1.1)
    label flange_density(1.2 * exhaust_delay_coeff)

    label pl0 ((2.2 * bore * spacing_factor / flange_density) * units.mm)
    label pl1 ((1.2 * bore * spacing_factor / flange_density) * units.mm)
    label pl2 ((1.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl3 ((2.0 * bore * spacing_factor / flange_density) * units.mm)

    _b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj0,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl0,
            ignition_wire: _wire1
        )
    _b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj1,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl1,
            ignition_wire: _wire2
        )
    _b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj2,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl2,
            ignition_wire: _wire3
        )
    _b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj3,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl3,
            ignition_wire: _wire4
        )
    _b0.set_cylinder_head(
        head(
            intake_camshaft: _intake_cam_0,
            exhaust_camshaft: _exhaust_cam_0,
            flip_display: flip_display
        )
    )
}

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node head {
    // port_flow.py v1.5
    // intake port area: 16.4 cm²; saturated lift: 11.18 mm
    // exhaust port area: 12.4 cm²; saturated lift: 9.73 mm
    // cylinder volume: 600.0 cm³ (36.6 CI); engine volume: 4.800 L (292.9 CI)
    // chamber volume: 58.3 cm³ (3.6 CI)
    // 8 harmonic intake runner length: 25.7 cm; diameter: 5.0 cm
    // primary length: 22.0 cm, area: 13.6 cm², diameter: 4.2 cm
    // collector diameter: 7.9 cm, area: 49.1 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 58.257 * units.cc,
        intake_runner_volume: 502.1 * units.cc,
        intake_runner_cross_section_area: 19.5 * units.cm2,
        exhaust_runner_volume: 200.8 * units.cc,
        exhaust_runner_cross_section_area: 13.6 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

public node crank {
    input phasing;
    input var;

    output c: _c;
    output rj0: _rj0;
    output rj2: _rj2;
    output rj4: _rj4;
    output rj6: _rj6;

    output rj1: _rj1;
    output rj3: _rj3;
    output rj5: _rj5;
    output rj7: _rj7;

    label crank_moment(disk_moment_of_inertia(mass: crank_mass * units.kg, radius: (stroke / 2.0) * units.mm))
    label flywheel_moment(disk_moment_of_inertia(mass: flywheel_mass * units.kg, radius: (stroke * 3.5) * units.mm))
    label other_moment(disk_moment_of_inertia(mass: 1 * units.kg, radius: 1.0 * units.cm))

    crankshaft _c(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: flywheel_mass * units.kg,
        mass: crank_mass * units.kg,
        friction_torque: 7.2 * units.Nm,
        moment_of_inertia: crank_moment + flywheel_moment + other_moment,
        tdc: 0
    )

    rod_journal _rj0(angle: rot_base * var.s1 + vee)
    rod_journal _rj2(angle: rot_base * var.s2 + vee)
    rod_journal _rj4(angle: rot_base * var.s2 + vee + second_phase)
    rod_journal _rj6(angle: rot_base * var.s1 + vee + second_phase)

    rod_journal _rj1(angle: rot_base * var.s1 + vee + rot180)
    rod_journal _rj3(angle: rot_base * var.s2 + vee + rot180)
    rod_journal _rj5(angle: rot_base * var.s2 + vee + second_phase + rot180)
    rod_journal _rj7(angle: rot_base * var.s1 + vee + second_phase + rot180)

    _c
        .add_rod_journal(_rj0)
        .add_rod_journal(_rj1)
        .add_rod_journal(_rj2)
        .add_rod_journal(_rj3)
        .add_rod_journal(_rj4)
        .add_rod_journal(_rj5)
        .add_rod_journal(_rj6)
        .add_rod_journal(_rj7)
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "Porsche 993 Boxer8 (f)",
        starter_torque: 75 * units.Nm,
        starter_speed: 400 * units.rpm,
        redline: redline * units.rpm,
        fuel: s90(),
        throttle_gamma: 1.75,
        jitter: 0.7,
        noise: 0.2,
        hf_gain: 0.002,
        simulation_frequency: 12000
    )

    //////////////////////////////
    //// change variants here ////
    //////////////////////////////
    variant_0 variant()

    crank c0(vee + rot90, variant)

    ignition_module ignition(timing_curve: timing_curve, rev_limit: redline * 1.05 * units.rpm, limiter_duration: 0.05)

    bank_builder bank0(
        var: variant,
        incline: vee / 2,
        phasing: 0,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust0,
        rj0: c0.rj0, rj1: c0.rj2, rj2: c0.rj4, rj3: c0.rj6,
        flip_display: true
    )

    bank_builder bank1(
        var: variant,
        incline: -vee / 2,
        phasing: vee + rot180,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust1,
        rj0: c0.rj1, rj1: c0.rj3, rj2: c0.rj5, rj3: c0.rj7
    )

    label crank_moment(disk_moment_of_inertia(mass: crank_mass * units.kg, radius: (stroke / 2.0) * units.mm))
    label flywheel_moment(disk_moment_of_inertia(mass: flywheel_mass * units.kg, radius: (stroke * 3.5) * units.mm))
    label other_moment(disk_moment_of_inertia(mass: 1 * units.kg, radius: 1.0 * units.cm))

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + (stroke / 2.0)) * units.mm + compression_height
    )

    label collector_cross_section_area(24.6)

    label exhaust_pipe_length_0(107.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 100.0) // Litres
    
    label exhaust_pipe_length_1(100.0) // cm
    label exhaust_volume_1(collector_cross_section_area * exhaust_pipe_length_1 / 100.0) // Litres
    
    label spacing_factor(1.1)
    label flange_density(1.2)
    
    intake intake(
        plenum_volume: 6.3 * units.L,
        plenum_cross_section_area: 78.0 * units.cm2,
        runner_length: 25.7 * units.cm,
        intake_flow_rate: k_carb(750.1),
        idle_flow_rate: k_carb(0.07),
        idle_throttle_plate_position: 0.998,
        runner_flow_rate: k_carb(234.4),
        velocity_decay: 0.05
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(703.2),
        collector_cross_section_area: 24.6 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 22.0 * units.cm,
        primary_flow_rate: k_carb(395.5),
        velocity_decay: 1.0,
        volume: (exhaust_volume_0 + 5) * units.L
    )
    
    exhaust_system_parameters es_params1(
        outlet_flow_rate: k_carb(703.2),
        collector_cross_section_area: 24.6 * units.cm2,
        length: exhaust_pipe_length_1 * units.cm,
        primary_tube_length: 22.0 * units.cm,
        primary_flow_rate: k_carb(395.5),
        velocity_decay: 1.0,
        volume: (exhaust_volume_1 + 5) * units.L
    )

    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_39.wav", volume: 0.0011))
    exhaust_system exhaust1(es_params1, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_39.wav", volume: 0.0010))

    engine
        .add_crankshaft(c0.c)
        .add_cylinder_bank(bank0.b0)
        .add_cylinder_bank(bank1.b0)
        .add_ignition_module(ignition)

    function timing_curve(500.0 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 0.0 * units.deg)
        .add_sample(500.0 * units.rpm, 9.6 * units.deg)
        .add_sample(1000.0 * units.rpm, 19.4 * units.deg)
        .add_sample(1500.0 * units.rpm, 25.6 * units.deg)
        .add_sample(2000.0 * units.rpm, 29.5 * units.deg)
        .add_sample(2500.0 * units.rpm, 31.9 * units.deg)
        .add_sample(3000.0 * units.rpm, 33.5 * units.deg)
        .add_sample(3500.0 * units.rpm, 34.4 * units.deg)
        .add_sample(4000.0 * units.rpm, 35.0 * units.deg)
        .add_sample(4500.0 * units.rpm, 35.0 * units.deg)
        .add_sample(5000.0 * units.rpm, 35.0 * units.deg)
        .add_sample(5500.0 * units.rpm, 35.0 * units.deg)
        .add_sample(6000.0 * units.rpm, 35.0 * units.deg)
        .add_sample(6500.0 * units.rpm, 35.0 * units.deg)
        .add_sample(7000.0 * units.rpm, 35.0 * units.deg)
        .add_sample(7500.0 * units.rpm, 35.0 * units.deg)
        .add_sample(8000.0 * units.rpm, -15.0 * units.deg)
        .add_sample(8500.0 * units.rpm, -45.0 * units.deg)
}

// 993
public node veh {
    alias output __out: vehicle;
    vehicle vehicle(
        mass: 1270  * units.kg,
        drag_coefficient: 0.32,
        cross_sectional_area: (1735  * units.mm) * (1311 * units.mm),
        diff_ratio: 3.444,
        tire_radius: (632.0 / 2) * units.mm, // 225/50ZR16
        rolling_resistance: 20
        )
}

// 993
private node trn {
    alias output __out:
        transmission(
            max_clutch_torque: 600 * units.Nm,
            max_clutch_flex: 10 * units.deg,
            limit_clutch_flex: true,
            clutch_stiffness: 20 * units.Nm / units.deg,
            clutch_damping: 2.0,
            simulate_flex: true
        )
        .add_gear(3.82)
        .add_gear(2.05)
        .add_gear(1.41)
        .add_gear(1.12)
        .add_gear(0.93)
        .add_gear(0.78);
}

run(
    engine: eng(),
    transmission: trn(),
    vehicle: veh()
)

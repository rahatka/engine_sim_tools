import "engine_sim.mr"
import "../../fuel/90.mr"

units units()
constants constants()

label bore(90.22) // 3.552 metric
label stroke(105.79) // 4.165 metric
label compression_ratio(9.8)
label con_rod(169.1) // 6.657
label compression_height(31.01 * units.mm) // 1.221
label intake_valve_diameter(34.0) // 1.338 5.4 3V
label exhaust_valve_diameter(37.5) // 1.476 5.4 3V
label intake_valves(2.0)
label exhaust_valves(1.0)

label cyl(10.0)
label row(2.0)
label cycle(720.0 * units.deg)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label vee(90.0 * units.deg)

label comp(vee - rot)

label redline(5000.0) // RPM

// 176/199@50 with .439 Int/ .436Exh ?
label IVL(11.15)
label EVL(11.07)
label IVO(-26.0 * units.deg) //BTDC
label IVC(22.0 * units.deg) //ABDC
label EVO(33.5 * units.deg) //BBDC
label EVC(-14.5 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(634.0) // g
label crank_mass(25.0) // kg
label flywheel_mass(8.0) // kg

label exhaust_delay_coeff(1.5)


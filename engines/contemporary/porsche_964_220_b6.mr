public node camshaft_builder {
    output intake_cam_0: _intake_cam_0;
    output exhaust_cam_0: _exhaust_cam_0;

    output intake_cam_1: _intake_cam_1;
    output exhaust_cam_1: _exhaust_cam_1;

    camshaft _intake_cam_0(base_radius: 22.0 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 22.0 * units.mm, lobe_profile: exhaust_lobe_profile)
    
    camshaft _intake_cam_1(base_radius: 22.0 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_1(base_radius: 22.0 * units.mm, lobe_profile: exhaust_lobe_profile)
    
    label rot(cycle / cyl)

    _intake_cam_0
        .add_lobe(ILC + 0 * rot)
        .add_lobe(ILC + 2 * rot)
        .add_lobe(ILC + 4 * rot)

    _exhaust_cam_0
        .add_lobe(ELC + 0 * rot)
        .add_lobe(ELC + 2 * rot)
        .add_lobe(ELC + 4 * rot)
        
    _intake_cam_1
        .add_lobe(ILC + 3 * rot)
        .add_lobe(ILC + 5 * rot)
        .add_lobe(ILC + 1 * rot)

    _exhaust_cam_1
        .add_lobe(ELC + 3 * rot)
        .add_lobe(ELC + 5 * rot)
        .add_lobe(ELC + 1 * rot)
}

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node head {
    // port_flow.py v1.5
    // intake port area: 16.0 cm²; saturated lift: 11.04 mm
    // exhaust port area: 12.1 cm²; saturated lift: 9.61 mm
    // cylinder volume: 600.0 cm³ (36.6 CI); engine volume: 3.600 L (219.7 CI)
    // chamber volume: 58.3 cm³ (3.6 CI)
    // 8 harmonic intake runner length: 26.7 cm; diameter: 4.7 cm
    // primary length: 23.4 cm, area: 12.7 cm², diameter: 4.0 cm
    // collector diameter: 5.8 cm, area: 26.7 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 58.257 * units.cc,
        intake_runner_volume: 454.4 * units.cc,
        intake_runner_cross_section_area: 17.0 * units.cm2,
        exhaust_runner_volume: 181.8 * units.cc,
        exhaust_runner_cross_section_area: 12.7 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "Porsche 964",
        starter_torque: 150 *units.Nm,
        starter_speed: 500 * units.rpm,
        redline: 6500 * units.rpm,
        fuel: s90(),
        throttle_gamma: 1.75,
        jitter: 0.7,
        noise: 0.1,
        hf_gain: 0.004,
        simulation_frequency: 10000
    )

    wires wires()

    crankshaft c0(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: 2.0 * units.kg,
        mass: 13.81 * units.kg,
        friction_torque: 5.4 * units.Nm,
        moment_of_inertia: 0.15,
        position_x: 0.0,
        position_y: 0.0,
        tdc: 0.0
    )

    rod_journal rj0(angle: rot * 0 + vee)
    rod_journal rj2(angle: rot * 2 + vee)
    rod_journal rj4(angle: rot * 4 + vee)
    rod_journal rj1(angle: rot * 3)
    rod_journal rj3(angle: rot * 5)
    rod_journal rj5(angle: rot * 1)
    
    c0
        .add_rod_journal(rj0)
        .add_rod_journal(rj1)
        .add_rod_journal(rj2)
        .add_rod_journal(rj3)
        .add_rod_journal(rj4)
        .add_rod_journal(rj5)

    piston_parameters piston_params(
        mass: 440.0 * units.g,
        blowby: k_28inH2O(0.031),
        compression_height: compression_height,
        wrist_pin_position: 0.0,
        displacement: 0.0
    )

    connecting_rod_parameters cr_params(
        mass: 574.0 * units.g,
        moment_of_inertia: 0.002,
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + (stroke / 2.0)) * units.mm + compression_height
    )

    label collector_cross_section_area(26.7)

    label exhaust_pipe_length_0(120.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 100.0) // Litres
    
    label spacing_factor(1.1)
    label flange_density(1.0 * exhaust_delay_coeff)
    

    intake intake(
        plenum_volume: 4.1 * units.L,
        plenum_cross_section_area: 51.0 * units.cm2,
        runner_length: 26.7 * units.cm,
        intake_flow_rate: k_carb(487.5),
        idle_flow_rate: k_carb(0.02),
        idle_throttle_plate_position: 0.9975,
        runner_flow_rate: k_carb(203.1),
        velocity_decay: 0.05
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(914.1),
        collector_cross_section_area: 26.7 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 23.4 * units.cm,
        primary_flow_rate: k_carb(342.8),
        velocity_decay: 1.0,
        volume: (exhaust_volume_0 + 6.0) * units.L
    )

    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_39.wav", volume: 0.001))
 
    cylinder_bank b0(bank_params, angle: vee / 2)
    cylinder_bank b1(bank_params, angle: -vee / 2)
    
    label sh ((60.0 / flange_density) * units.cm)
    label pl0 ((2.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl1 ((3.1 * bore * spacing_factor / flange_density) * units.mm)
    label pl2 ((4.3 * bore * spacing_factor / flange_density) * units.mm)

    b0
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj0,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl0 + sh,
            ignition_wire: wires.wire1
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj2,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl1 + sh,
            ignition_wire: wires.wire2
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj4,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl2 + sh,
            ignition_wire: wires.wire3
        )
        
    b1
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj1,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl0,
            ignition_wire: wires.wire4
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj3,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl1,
            ignition_wire: wires.wire5
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj5,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl2,
            ignition_wire: wires.wire6
        )

    engine
        .add_cylinder_bank(b0)
        .add_cylinder_bank(b1)

    engine.add_crankshaft(c0)

    camshaft_builder camshaft()

    b0.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_0,
            exhaust_camshaft: camshaft.exhaust_cam_0,
            flip_display: true
        )
    )
    b1.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_1,
            exhaust_camshaft: camshaft.exhaust_cam_1
        )
    )

    function timing_curve(433.3 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 0.0 * units.deg)
        .add_sample(433.3 * units.rpm, 7.9 * units.deg)
        .add_sample(866.7 * units.rpm, 17.2 * units.deg)
        .add_sample(1300.0 * units.rpm, 23.5 * units.deg)
        .add_sample(1733.3 * units.rpm, 27.7 * units.deg)
        .add_sample(2166.7 * units.rpm, 30.4 * units.deg)
        .add_sample(2600.0 * units.rpm, 32.3 * units.deg)
        .add_sample(3033.3 * units.rpm, 33.5 * units.deg)
        .add_sample(3466.7 * units.rpm, 34.4 * units.deg)
        .add_sample(3900.0 * units.rpm, 34.9 * units.deg)
        .add_sample(4333.3 * units.rpm, 35.0 * units.deg)
        .add_sample(4766.7 * units.rpm, 35.0 * units.deg)
        .add_sample(5200.0 * units.rpm, 35.0 * units.deg)
        .add_sample(5633.3 * units.rpm, 35.0 * units.deg)
        .add_sample(6066.7 * units.rpm, 35.0 * units.deg)
        .add_sample(6500.0 * units.rpm, 35.0 * units.deg)
        .add_sample(6933.3 * units.rpm, -15.0 * units.deg)
        .add_sample(7366.7 * units.rpm, -45.0 * units.deg)

    engine.add_ignition_module(
        eng_distributor(
            wires: wires,
            timing_curve: timing_curve
        )
    )
}

// 964
public node veh {
    alias output __out: vehicle;
    vehicle vehicle(
        mass: 1450  * units.kg,
        drag_coefficient: 0.32,
        cross_sectional_area: (1650  * units.mm) * (1310 * units.mm),
        diff_ratio: 3.444,
        tire_radius: (632.0 / 2) * units.mm, // 225/50ZR16
        rolling_resistance: 20
        )
}

// 964
private node trn {
    alias output __out:
        transmission(
            max_clutch_torque: 400 * units.Nm,
            max_clutch_flex: 10 * units.deg,
            limit_clutch_flex: true,
            clutch_stiffness: 20 * units.Nm / units.deg,
            clutch_damping: 2.0,
            simulate_flex: true
        )
        .add_gear(3.5)
        .add_gear(2.118)
        .add_gear(1.444)
        .add_gear(1.086)
        .add_gear(0.868);
}

run(
    engine: eng(),
    transmission: trn(),
    vehicle: veh()
)

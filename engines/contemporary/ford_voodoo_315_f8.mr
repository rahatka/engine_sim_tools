    alias output __out: vehicle;
    vehicle vehicle(
        mass: 1680.56 * units.kg,
        drag_coefficient: 0.33,
        cross_sectional_area: (2080.26 * units.mm) * (1379.22 * units.mm),
        diff_ratio: 3.55, // 6-spd 3.55, 10-spd 3.35 

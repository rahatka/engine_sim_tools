label bore(94.0)
label stroke(93.0)
label compression_ratio(12.0)
label compression_height(29.5 * units.mm)
label con_rod(150.7)

label cycle(720.0 * units.deg)
label cyl(8.0)
label row(2.0)
label cyl_base(cyl / row)
label vee(180.0 * units.deg)
label rot(cycle / cyl)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

label redline(8200.0) // RPM

label intake_valve_diameter(37.7)
label exhaust_valve_diameter(32.0)
label intake_valves(2.0)
label exhaust_valves(2.0)

// LM-GT3F-1
label IVL(14.0)
label EVL(14.0)
label IVO(-3.5 * units.deg) //BTDC
label IVC(57.5 * units.deg) //ABDC
label EVO(58.0 * units.deg) //BBDC
label EVC(-3.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(605.0) // g
label piston_mass(380.0) // g
label crank_mass(23.1332) // kg
label flywheel_mass(4.0) // kg

label exhaust_delay_coeff(1.5)

public node distributor_1 {
    input wires;
    input timing_curve;
    input rev_limit: redline * 1.05 * units.rpm;
    input limiter_duration: 0.04;
    alias output __out:
        ignition_module(timing_curve: timing_curve, rev_limit: rev_limit, limiter_duration: limiter_duration)
            .connect_wire(wires.wire1, rot * 0)
            .connect_wire(wires.wire4, rot * 1)
            .connect_wire(wires.wire5, rot * 2)
            .connect_wire(wires.wire3, rot * 3)
            .connect_wire(wires.wire6, rot * 4)
            .connect_wire(wires.wire7, rot * 5)
            .connect_wire(wires.wire2, rot * 6)
            .connect_wire(wires.wire8, rot * 7);
}

private node wires {
    output wire1: ignition_wire();
    output wire2: ignition_wire();
    output wire3: ignition_wire();
    output wire4: ignition_wire();
    output wire5: ignition_wire();
    output wire6: ignition_wire();
    output wire7: ignition_wire();
    output wire8: ignition_wire();
}


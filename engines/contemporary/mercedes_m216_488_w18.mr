import "engine_sim.mr"
import "../../fuel/90.mr"

units units()
constants constants()

label bore(87.7) // ?
label stroke(73.5) // M104.94x
label compression_ratio(10.0)
label con_rod(145.0) // ~
label L1(101.8) // ~
label R1(45.0) // ~
label psi(75.0 * units.deg)
label compression_height(30.0 * units.mm) // ?
label intake_valve_diameter(35.0) // M104.94x
label exhaust_valve_diameter(31.0) // M104.94x
label intake_valves(2.0) // ?
label exhaust_valves(2.0) // ?

label cyl(18.0)
label row(3.0)
label cycle(720.0 * units.deg)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label vee(75.0 * units.deg)

label redline(6000.0) // RPM

// cat cams 4000905 : ­4/37/40/­0 @ 1mm
label IVL(9.55)
label EVL(10.15)
label IVO(4.0 * units.deg) //BTDC
label IVC(37.0 * units.deg) //ABDC
label EVO(40.0 * units.deg) //BBDC
label EVC(0.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(800.0) // g
label slave_rod_mass(200)
label crank_mass(25.0) // kg
label flywheel_mass(8.0) // kg

label exhaust_delay_coeff(1.5)


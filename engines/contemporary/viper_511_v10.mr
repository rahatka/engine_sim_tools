import "../../fuel/90.mr"

units units()
constants constants()

label bore(103.0) // 4.055
label stroke(100.584) // 3.96
label compression_ratio(10.2)
label con_rod(158.5) // 6.24
label compression_height(31.496 * units.mm) // 1.24
label intake_valve_diameter(52.832) // 2.08
label exhaust_valve_diameter(40.894) // 1.61
label intake_valves(1.0)
label exhaust_valves(1.0)

label cyl(10.0)
label row(2.0)
label cycle(720.0 * units.deg)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label vee(90.0 * units.deg)

label redline(6000.0) // RPM

// 2005 Vip
label IVL(14.37386) // 0.5659
label EVL(13.589) // 0.5350
label IVO(36.5 * units.deg) //BTDC
label IVC(88.5 * units.deg) //ABDC
label EVO(86.5 * units.deg) //BBDC
label EVC(23.5 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(649.7) // g
label crank_mass(25.0) // ? kg
label flywheel_mass(7.0) // kg

label exhaust_delay_coeff(1.5)


private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node head {
    // port_flow.py v1.5
    // intake port area: 15.5 cm²; saturated lift: 10.86 mm
    // exhaust port area: 9.3 cm²; saturated lift: 8.40 mm
    // cylinder volume: 716.6 cm³ (43.7 CI); engine volume: 5.733 L (349.8 CI)
    // chamber volume: 76.2 cm³ (4.7 CI)
    // 16 harmonic intake runner length: 16.8 cm; diameter: 4.8 cm
    // primary length: 28.3 cm, area: 11.6 cm², diameter: 3.8 cm
    // collector diameter: 7.7 cm, area: 46.3 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 76.236 * units.cc,
        intake_runner_volume: 305.7 * units.cc,
        intake_runner_cross_section_area: 18.2 * units.cm2,
        exhaust_runner_volume: 101.9 * units.cc,
        exhaust_runner_cross_section_area: 11.6 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

public node crank {
    input phasing;
    input var;

    output c: _c;
    output rj0: _rj0;
    output rj1: _rj1;
    output rj2: _rj2;
    output rj3: _rj3;
    output rj4: _rj4;
    output rj5: _rj5;
    output rj6: _rj6;
    output rj7: _rj7;

    label crank_moment(disk_moment_of_inertia(mass: crank_mass * units.kg, radius: (stroke / 2.0) * units.mm))
    label flywheel_moment(disk_moment_of_inertia(mass: flywheel_mass * units.kg, radius: (stroke * 3.5) * units.mm))
    label other_moment(disk_moment_of_inertia(mass: 1 * units.kg, radius: 1.0 * units.cm))

    crankshaft _c(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: flywheel_mass * units.kg,
        mass: crank_mass * units.kg,
        friction_torque: 8.6 * units.Nm,
        moment_of_inertia: crank_moment + flywheel_moment + other_moment,
        tdc: 0.0
    )

    rod_journal _rj0(angle: rot_base * var.s1 + rot90)
    rod_journal _rj1(angle: rot_base * var.s2 + rot90)
    rod_journal _rj2(angle: rot_base * var.s3 + rot90)
    rod_journal _rj3(angle: rot_base * var.s4 + rot90)
    rod_journal _rj4(angle: rot_base * var.s5 + rot90)
    rod_journal _rj5(angle: rot_base * var.s6 + rot90)
    rod_journal _rj6(angle: rot_base * var.s7 + rot90)
    rod_journal _rj7(angle: rot_base * var.s8 + rot90)

    _c
        .add_rod_journal(_rj0)
        .add_rod_journal(_rj1)
        .add_rod_journal(_rj2)
        .add_rod_journal(_rj3)
        .add_rod_journal(_rj4)
        .add_rod_journal(_rj5)
        .add_rod_journal(_rj6)
        .add_rod_journal(_rj7)
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "Chevrolet LT S8 (f)",
        starter_torque: 100 * units.Nm,
        starter_speed: 300 * units.rpm,
        redline: redline * units.rpm,
        fuel: s90(),
        throttle_gamma: 1.5,
        jitter: es.jitter,
        noise: es.noise,
        hf_gain: es.hf_gain,
        simulation_frequency: 12000,
        dyno_min_speed: 500 * units.rpm
    )

    ////////////////////////////////////////
    ///////// change variants here /////////
    ////////////////////////////////////////
    // 0  1-6-2-5-8-3-7-4 Standard
    // 1  1-6-7-4-8-3-2-5 2D70
    // 2  1-5-7-3-8-4-2-6 5D49
    // 3  1-5-2-6-8-4-7-3 Cadillac 431 V16
    // 4  1-6-2-7-3-8-4-5 Bugatti Type 39
    // 5  1-4-7-6-8-5-2-3 Mercedes-Benz M 196 R
    // 6  1-2-4-6-8-7-5-3 Mercedes-Benz M 196 S
    // 7  1-5-3-7-4-8-2-6 Bugatti Type 59 ; Duesenberg before 1927
    // 8  1-3-7-4-8-6-2-5 Delage
    // 9  1-4-7-3-8-5-2-6 Hupmobile
    // 10 1-3-2-5-8-6-7-4 Packard before 1927
    // 11 1-5-2-6-3-7-4-8 Bugatti most common
    // 12 1-5-2-3-8-4-7-6 Hupmobile before 1927
    // 13 1-3-5-7-6-8-2-4 2-2-2-2 v1
    // 14 1-8-5-4-6-3-2-7 2-2-2-2 v2
    // 15 1-3-2-4-8-6-7-5 mercedes d.iv
    // 16 1-7-3-5-8-2-6-4 Delage racing
    variant_1 variant()
    es1 es()

    crank c0(rot90, variant)

    bank_builder bank0(
        var: variant,
        crankshaft: c0,
        incline: 0,
        phasing: 0,
        ignition: ignition,
        intake: intake,
        exhaust: es,
        flip: true
    )

    ignition_module ignition(timing_curve: timing_curve, rev_limit: redline * 1.2 * units.rpm, limiter_duration: 0.03)

    intake intake(
        plenum_volume: 5.5 * units.L,
        plenum_cross_section_area: 72.9 * units.cm2,
        runner_length: 16.8 * units.cm,
        intake_flow_rate: k_carb(645.0),
        idle_flow_rate: k_carb(0.01),
        idle_throttle_plate_position: 0.997,
        runner_flow_rate: k_carb(181.4),
        velocity_decay: 0.05
    )

/*
{
    "ign_cfg": {
        "es_version": "0.1.14a",
        "resolution": 16,
        "start_rpm": 500,
        "start_deg": 5,
        "end_deg": 37,
        "exp_mode": 2
    }
}
*/
    function timing_curve(400.0 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 5.0 * units.deg)
        .add_sample(400.0 * units.rpm, 5.0 * units.deg)
        .add_sample(800.0 * units.rpm, 8.8 * units.deg)
        .add_sample(1200.0 * units.rpm, 13.3 * units.deg)
        .add_sample(1600.0 * units.rpm, 17.2 * units.deg)
        .add_sample(2000.0 * units.rpm, 20.6 * units.deg)
        .add_sample(2400.0 * units.rpm, 23.5 * units.deg)
        .add_sample(2800.0 * units.rpm, 26.0 * units.deg)
        .add_sample(3200.0 * units.rpm, 28.1 * units.deg)
        .add_sample(3600.0 * units.rpm, 30.0 * units.deg)
        .add_sample(4000.0 * units.rpm, 31.6 * units.deg)
        .add_sample(4400.0 * units.rpm, 33.0 * units.deg)
        .add_sample(4800.0 * units.rpm, 34.3 * units.deg)
        .add_sample(5200.0 * units.rpm, 35.3 * units.deg)
        .add_sample(5600.0 * units.rpm, 36.2 * units.deg)
        .add_sample(6000.0 * units.rpm, 37.0 * units.deg)
        .add_sample(6400.0 * units.rpm, -15.0 * units.deg)
        .add_sample(6800.0 * units.rpm, -60.0 * units.deg)

    engine
        .add_crankshaft(c0.c)
        .add_cylinder_bank(bank0.bank)
        .add_ignition_module(ignition)
}


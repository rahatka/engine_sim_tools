import "engine_sim.mr"
import "../../fuel/90.mr"

units units()
constants constants()

label bore(86.0)
label stroke(86.0)
label compression_ratio(8.3)
label con_rod(164.0)
label compression_height(31.0134 * units.mm)
label intake_valve_diameter(31.0)
label exhaust_valve_diameter(27.0)
label intake_valves(2.0)
label exhaust_valves(2.0)

label redline(6600.0)

label cyl(16.0)
label row(2.0)
label cycle(720.0 * units.deg)
label vee(90.0 * units.deg) // angle between two VR banks
label vree(15.0 * units.deg) // angle between two rows within one VR bank
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// A8L valve timings
label IVL(10.0)
label EVL(10.0)
label IVO(26.0 * units.deg) //BTDC
label IVC(29.0 * units.deg) //ABDC
label EVO(44.0 * units.deg) //BBDC
label EVC(9.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(457.0) // ? g
label crank_mass(32.0) // ? kg
label flywheel_mass(8.0) // kg

label exhaust_delay_coeff(1.5)

public node eng_distributor_even {
    input wires;
    input timing_curve;
    input rev_limit: 7000 * units.rpm;
    input limiter_duration: 0.02;
    alias output __out:
        ignition_module(timing_curve: timing_curve, rev_limit: rev_limit, limiter_duration: limiter_duration)
            .connect_wire(wires.wire1, rot * 0)
            .connect_wire(wires.wire14, rot * 1)
            .connect_wire(wires.wire9, rot * 2)
            .connect_wire(wires.wire4, rot * 3)
            .connect_wire(wires.wire7, rot * 4)
            .connect_wire(wires.wire12, rot * 5)
            .connect_wire(wires.wire15, rot * 6)
            .connect_wire(wires.wire6, rot * 7)
            .connect_wire(wires.wire13, rot * 8)
            .connect_wire(wires.wire8, rot * 9)
            .connect_wire(wires.wire3, rot * 10)
            .connect_wire(wires.wire16, rot * 11)
            .connect_wire(wires.wire11, rot * 12)
            .connect_wire(wires.wire2, rot * 13)
            .connect_wire(wires.wire5, rot * 14)
            .connect_wire(wires.wire10, rot * 15);
}

private node wires {
    output wire1: ignition_wire();
    output wire2: ignition_wire();
    output wire3: ignition_wire();
    output wire4: ignition_wire();
    output wire5: ignition_wire();
    output wire6: ignition_wire();
    output wire7: ignition_wire();
    output wire8: ignition_wire();

public node camshaft_builder_even {
    output intake_cam_0: _intake_cam_0;
    output exhaust_cam_0: _exhaust_cam_0;
    output intake_cam_1: _intake_cam_1;
    output exhaust_cam_1: _exhaust_cam_1;
    output intake_cam_2: _intake_cam_2;
    output exhaust_cam_2: _exhaust_cam_2;
    output intake_cam_3: _intake_cam_3;
    output exhaust_cam_3: _exhaust_cam_3;

    camshaft _intake_cam_0(base_radius: 15.8 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 15.8 * units.mm, lobe_profile: exhaust_lobe_profile)
    camshaft _intake_cam_1(base_radius: 15.8 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_1(base_radius: 15.8 * units.mm, lobe_profile: exhaust_lobe_profile)
    camshaft _intake_cam_2(base_radius: 15.8 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_2(base_radius: 15.8 * units.mm, lobe_profile: exhaust_lobe_profile)
    camshaft _intake_cam_3(base_radius: 15.8 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_3(base_radius: 15.8 * units.mm, lobe_profile: exhaust_lobe_profile)

    _intake_cam_0
        .add_lobe(ILC + 0 * rot)
        .add_lobe(ILC + 10 * rot)
        .add_lobe(ILC + 14 * rot)
        .add_lobe(ILC + 4 * rot)
    _exhaust_cam_0
        .add_lobe(ELC + 0 * rot)
        .add_lobe(ELC + 10 * rot)
        .add_lobe(ELC + 14 * rot)
        .add_lobe(ELC + 4 * rot)

    _intake_cam_1
        .add_lobe(ILC + 13 * rot)
        .add_lobe(ILC + 3 * rot)
        .add_lobe(ILC + 7 * rot)
        .add_lobe(ILC + 9 * rot)
    _exhaust_cam_1
        .add_lobe(ELC + 13 * rot)
        .add_lobe(ELC + 3 * rot)
        .add_lobe(ELC + 7 * rot)
        .add_lobe(ELC + 9 * rot)

    _intake_cam_2
        .add_lobe(ILC + 2 * rot)
        .add_lobe(ILC + 12 * rot)
        .add_lobe(ILC + 8 * rot)
        .add_lobe(ILC + 6 * rot)
    _exhaust_cam_2
        .add_lobe(ELC + 2 * rot)
        .add_lobe(ELC + 12 * rot)
        .add_lobe(ELC + 8 * rot)
        .add_lobe(ELC + 6 * rot)

    _intake_cam_3
        .add_lobe(ILC + 15 * rot)
        .add_lobe(ILC + 5 * rot)
        .add_lobe(ILC + 1 * rot)
        .add_lobe(ILC + 11 * rot)
    _exhaust_cam_3
        .add_lobe(ELC + 15 * rot)
        .add_lobe(ELC + 5 * rot)
        .add_lobe(ELC + 1 * rot)
        .add_lobe(ELC + 11 * rot)
}

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node head {
    // port_flow.py v1.5
    // intake port area: 11.5 cm²; saturated lift: 6.59 mm
    // exhaust port area: 8.7 cm²; saturated lift: 5.74 mm
    // cylinder volume: 499.6 cm³ (30.48 CI); engine volume: 7.993 L (487.76 CI)
    // chamber volume: 68.4 cm³ (4.18 CI)
    // 8 harmonic intake runner length: 28.1 cm; diameter: 4.3 cm
    // primary length: 24.3 cm, area: 9.6 cm², diameter: 3.5 cm
    // collector diameter: 9.9 cm, area: 76.6 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 68.432 * units.cc,
        intake_runner_volume: 407.9 * units.cc,
        intake_runner_cross_section_area: 14.5 * units.cm2,
        exhaust_runner_volume: 136.0 * units.cc,
        exhaust_runner_cross_section_area: 9.6 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "VAG 8.0 W16",
        starter_torque: 150 * units.Nm,
        starter_speed: 650 * units.rpm,
        redline: 6500 * units.rpm,
        fuel: s90(),
        throttle_gamma: 1.5,
        jitter: 0.5,
        noise: 0.1,
        hf_gain: 0.005,
        simulation_frequency: 7000,
        dyno_min_speed: 1000 * units.rpm,
        fluid_simulation_steps: 6,
        max_sle_solver_steps: 32
    )

    wires wires()

    connecting_rod_parameters cr_params(
        mass: con_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.g,
            length: con_rod * units.mm
        ),
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    label crank_moment(disk_moment_of_inertia(mass: crank_mass * units.kg, radius: (stroke / 2.0) * units.mm))
    label flywheel_moment(disk_moment_of_inertia(mass: flywheel_mass * units.kg, radius: (stroke * 3.5) * units.mm))
    label other_moment(disk_moment_of_inertia(mass: 1 * units.kg, radius: 1.0 * units.cm))

    crankshaft c0(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: flywheel_mass * units.kg,
        mass: crank_mass * units.kg,
        friction_torque: 12.0 * units.Nm,
        moment_of_inertia: crank_moment + flywheel_moment + other_moment,
        tdc: 0.0
    )

    rod_journal rj0_0(angle: rot * 0 + rot90 + vee / 2 + vree / 2)
    rod_journal rj1_0(angle: rot * 13 + rot90 + vee / 2 - vree / 2)
    rod_journal rj2_0(angle: rot * 10 + rot90 + vee / 2 + vree / 2)
    rod_journal rj3_0(angle: rot * 3 + rot90 + vee / 2 - vree / 2)
    rod_journal rj4_0(angle: rot * 14 + rot90 + vee / 2 + vree / 2)
    rod_journal rj5_0(angle: rot * 7 + rot90 + vee / 2 - vree / 2)
    rod_journal rj6_0(angle: rot * 4 + rot90 + vee / 2 + vree / 2)
    rod_journal rj7_0(angle: rot * 9 + rot90 + vee / 2 - vree / 2)
    
    c0
        .add_rod_journal(rj0_0)
        .add_rod_journal(rj1_0)
        .add_rod_journal(rj2_0)
        .add_rod_journal(rj3_0)
        .add_rod_journal(rj4_0)
        .add_rod_journal(rj5_0)
        .add_rod_journal(rj6_0)
        .add_rod_journal(rj7_0)

    piston_parameters piston_params(
        mass: 247.252 * units.g,
        blowby: k_28inH2O(0.027),
        compression_height: compression_height,
        wrist_pin_position: 0.0,
        displacement: 0.0
    )

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    label collector_cross_section_area(38.3)

    label exhaust_pipe_length_0(150.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 100.0) // Litres
    
    label exhaust_pipe_length_1(150.0) // cm
    label exhaust_volume_1(collector_cross_section_area * exhaust_pipe_length_1 / 100.0) // Litres
    
    label spacing_factor(0.75)
    label flange_density(1.0 * exhaust_delay_coeff)

    intake intake(
        plenum_volume: 9.2 * units.L,
        plenum_cross_section_area: 116.0 * units.cm2,
        runner_length: 28.1 * units.cm,
        intake_flow_rate: k_carb(1033.1),
        idle_flow_rate: k_carb(0.01),
        idle_throttle_plate_position: 0.998,
        runner_flow_rate: k_carb(151.7),
        velocity_decay: 0.05
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(936.2),
        collector_cross_section_area: 38.3 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 24.3 * units.cm,
        primary_flow_rate: k_carb(254.5),
        velocity_decay: 1.0,
        volume: (exhaust_volume_0 + 3.0) * units.L
    )
    
    exhaust_system_parameters es_params1(
        outlet_flow_rate: k_carb(936.2),
        collector_cross_section_area: 38.3 * units.cm2,
        length: exhaust_pipe_length_1 * units.cm,
        primary_tube_length: 24.3 * units.cm,
        primary_flow_rate: k_carb(254.5),
        velocity_decay: 1.0,
        volume: (exhaust_volume_1 + 3.0) * units.L
    )

    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_39.wav", volume: 0.00025))
    exhaust_system exhaust1(es_params1, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_39.wav", volume: 0.00025))
 
    cylinder_bank b0(bank_params, angle: vee / 2 + vree / 2)
    cylinder_bank b1(bank_params, angle: vee / 2 - vree / 2)
    cylinder_bank b2(bank_params, angle: -vee / 2 + vree / 2)
    cylinder_bank b3(bank_params, angle: -vee / 2 - vree / 2)
    
    label unit(bore / flange_density)
    // Veyron setup
    label sh(740.0)
    label pl0 ((2.0 * unit * spacing_factor + sh) * units.mm)
    label pl1 ((1.0 * unit * spacing_factor + sh + unit) * units.mm)
    label pl2 ((1.0 * unit * spacing_factor + sh) * units.mm)
    label pl3 ((2.0 * unit * spacing_factor + sh + unit) * units.mm)
    label pl4 ((1.0 * unit * spacing_factor) * units.mm)
    label pl5 ((1.2 * unit * spacing_factor + unit) * units.mm)
    label pl6 ((2.2 * unit * spacing_factor) * units.mm)
    label pl7 ((3.2 * unit * spacing_factor + unit) * units.mm)
    
    label pl8 ((2.0 * unit * spacing_factor + sh + unit) * units.mm)
    label pl9 ((1.0 * unit * spacing_factor + sh) * units.mm)
    label pl10 ((1.0 * unit * spacing_factor + sh + unit) * units.mm)
    label pl11 ((2.0 * unit * spacing_factor + sh) * units.mm)
    label pl12 ((1.0 * unit * spacing_factor + unit) * units.mm)
    label pl13 ((1.2 * unit * spacing_factor) * units.mm)
    label pl14 ((2.2 * unit * spacing_factor + unit) * units.mm)
    label pl15 ((3.2 * unit * spacing_factor) * units.mm)

    // straight
    // label pl0 ((8.0 * unit * spacing_factor) * units.mm)
    // label pl1 ((7.0 * unit * spacing_factor + unit) * units.mm)
    // label pl2 ((6.0 * unit * spacing_factor) * units.mm)
    // label pl3 ((5.0 * unit * spacing_factor + unit) * units.mm)
    // label pl4 ((4.0 * unit * spacing_factor) * units.mm)
    // label pl5 ((3.0 * unit * spacing_factor + unit) * units.mm)
    // label pl6 ((2.0 * unit * spacing_factor) * units.mm)
    // label pl7 ((1.0 * unit * spacing_factor + unit) * units.mm)
    
    // label pl8 ((8.0 * unit * spacing_factor + unit) * units.mm)
    // label pl9 ((7.0 * unit * spacing_factor) * units.mm)
    // label pl10 ((6.0 * unit * spacing_factor + unit) * units.mm)
    // label pl11 ((5.0 * unit * spacing_factor) * units.mm)
    // label pl12 ((4.0 * unit * spacing_factor + unit) * units.mm)
    // label pl13 ((3.0 * unit * spacing_factor) * units.mm)
    // label pl14 ((2.0 * unit * spacing_factor + unit) * units.mm)
    // label pl15 ((1.0 * unit * spacing_factor) * units.mm)

    b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj0_0,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl0,
            ignition_wire: wires.wire1
        )
    b1.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj1_0,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl1,
            ignition_wire: wires.wire2
        )
    b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj2_0,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl2,
            ignition_wire: wires.wire3
        )
    b1.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj3_0,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl3,
            ignition_wire: wires.wire4
        )
    b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj4_0,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl4,
            ignition_wire: wires.wire5
        )
    b1.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj5_0,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl5,
            ignition_wire: wires.wire6
        )
    b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj6_0,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl6,
            ignition_wire: wires.wire7
        )
    b1.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj7_0,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl7,
            ignition_wire: wires.wire8
        )
        
    b2.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj0_0,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl8,
            ignition_wire: wires.wire9
        )
    b3.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj1_0,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl9,
            ignition_wire: wires.wire10
        )
    b2.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj2_0,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl10,
            ignition_wire: wires.wire11
        )
    b3.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj3_0,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl11,
            ignition_wire: wires.wire12
        )
    b2.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj4_0,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl12,
            ignition_wire: wires.wire13
        )
    b3.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj5_0,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl13,
            ignition_wire: wires.wire14
        )
    b2.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj6_0,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl14,
            ignition_wire: wires.wire15
        )
    b3.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj7_0,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl15,
            ignition_wire: wires.wire16
        )

    engine
        .add_cylinder_bank(b0)
        .add_cylinder_bank(b1)
        .add_cylinder_bank(b2)
        .add_cylinder_bank(b3)

    engine.add_crankshaft(c0)

    camshaft_builder_even camshaft()

    b0.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_0,
            exhaust_camshaft: camshaft.exhaust_cam_0,
            flip_display: true
        )
    )
    b1.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_1,
            exhaust_camshaft: camshaft.exhaust_cam_1,
            flip_display: true
        )
    )
    
    b2.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_2,
            exhaust_camshaft: camshaft.exhaust_cam_2
        )
    )
    b3.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_3,
            exhaust_camshaft: camshaft.exhaust_cam_3
        )
    )

    // ignition_timing.py v0.3 from = 7.0, to = 40.0, i = 500, r = 6600
    function timing_curve(660.0 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 0.0 * units.deg)
        .add_sample(660.0 * units.rpm, 7.0 * units.deg)
        .add_sample(1320.0 * units.rpm, 14.0 * units.deg)
        .add_sample(1980.0 * units.rpm, 19.8 * units.deg)
        .add_sample(2640.0 * units.rpm, 24.6 * units.deg)
        .add_sample(3300.0 * units.rpm, 28.6 * units.deg)
        .add_sample(3960.0 * units.rpm, 31.8 * units.deg)
        .add_sample(4620.0 * units.rpm, 34.5 * units.deg)
        .add_sample(5280.0 * units.rpm, 36.7 * units.deg)
        .add_sample(5940.0 * units.rpm, 38.5 * units.deg)
        .add_sample(6600.0 * units.rpm, 40.0 * units.deg)
        .add_sample(7260.0 * units.rpm, -12.5 * units.deg)
        .add_sample(7920.0 * units.rpm, -25.0 * units.deg)

    engine.add_ignition_module(
        eng_distributor_even(
            wires: wires,
            timing_curve: timing_curve
        )
    )
}

// Veyron 16.4
public node veh {
    alias output __out: vehicle;
    vehicle vehicle(
        mass: 1888  * units.kg,
        drag_coefficient: 0.355,
        cross_sectional_area: (1998 * units.mm) * (1204 * units.mm),
        diff_ratio: 2.8,
        tire_radius: (711.0 / 2) * units.mm,
        rolling_resistance: 25
        )
}

// Veyron 16.4
private node trn {
    alias output __out:
        transmission(
            max_clutch_torque: 1100 * units.Nm,
            max_clutch_flex: 10 * units.deg,
            limit_clutch_flex: true,
            clutch_stiffness: 20 * units.Nm / units.deg,
            clutch_damping: 2.0,
            simulate_flex: true
        )
        .add_gear(3.18)
        .add_gear(2.26)
        .add_gear(1.68)
        .add_gear(1.29)
        .add_gear(1.06)
        .add_gear(0.88)
        .add_gear(0.80);
}

run(
    engine: eng(),
    transmission: trn(),
    vehicle: veh()
)
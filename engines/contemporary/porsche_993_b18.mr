public node eng {
    alias output __out: engine;

    engine engine(
        name: "Porsche 993 Boxer18 (f)",
        starter_torque: 375 * units.Nm,
        starter_speed: 400 * units.rpm,
        redline: redline * units.rpm,
        fuel: s90(),
        throttle_gamma: 1.75,
        jitter: 0.7,
        noise: 0.1,
        hf_gain: 0.004,
        simulation_frequency: 6500,
        dyno_min_speed: 1000 * units.rpm,
        fluid_simulation_steps: 6,
        max_sle_solver_steps: 64
    )

    wires wires()

    crankshaft c0(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: 2.0 * units.kg,
        mass: 28.0 * units.kg,
        friction_torque: 16.2 * units.Nm, // port_flow.py v0.7
        moment_of_inertia: 0.3,
        position_x: 0.0,
        position_y: 0.0,
        tdc: 0.000001 * units.deg
    )

    // 1-2-4-6-8-9-7-5-3
    // rod_journal rj0(angle: rot_base * 0 + vee)
    // rod_journal rj2(angle: rot_base * 1 + vee)
    // rod_journal rj4(angle: rot_base * 8 + vee)
    // rod_journal rj6(angle: rot_base * 2 + vee)
    // rod_journal rj8(angle: rot_base * 7 + vee)
    // rod_journal rj10(angle: rot_base * 3 + vee)
	// rod_journal rj12(angle: rot_base * 6 + vee)
    // rod_journal rj14(angle: rot_base * 4 + vee)
	// rod_journal rj16(angle: rot_base * 5 + vee)
    // rod_journal rj1(angle: rot_base * 0)
    // rod_journal rj3(angle: rot_base * 1)
    // rod_journal rj5(angle: rot_base * 8)
    // rod_journal rj7(angle: rot_base * 2)
    // rod_journal rj9(angle: rot_base * 7)
	// rod_journal rj11(angle: rot_base * 3)
    // rod_journal rj13(angle: rot_base * 6)
	// rod_journal rj15(angle: rot_base * 4)
    // rod_journal rj17(angle: rot_base * 5)

    // 1-5-6-4-7-3-8-2-9
    rod_journal rj0(angle: rot_base * 0 + vee)
    rod_journal rj2(angle: rot_base * 7 + vee)
    rod_journal rj4(angle: rot_base * 5 + vee)
    rod_journal rj6(angle: rot_base * 3 + vee)
    rod_journal rj8(angle: rot_base * 1 + vee)
    rod_journal rj10(angle: rot_base * 2 + vee)
	rod_journal rj12(angle: rot_base * 4 + vee)
    rod_journal rj14(angle: rot_base * 6 + vee)
	rod_journal rj16(angle: rot_base * 8 + vee)
    rod_journal rj1(angle: rot_base * 0)
    rod_journal rj3(angle: rot_base * 7)
    rod_journal rj5(angle: rot_base * 5)
    rod_journal rj7(angle: rot_base * 3)
    rod_journal rj9(angle: rot_base * 1)
	rod_journal rj11(angle: rot_base * 2)
    rod_journal rj13(angle: rot_base * 4)
	rod_journal rj15(angle: rot_base * 6)
    rod_journal rj17(angle: rot_base * 8)
    
    c0
        .add_rod_journal(rj0)
        .add_rod_journal(rj1)
        .add_rod_journal(rj2)
        .add_rod_journal(rj3)
        .add_rod_journal(rj4)
        .add_rod_journal(rj5)
        .add_rod_journal(rj6)
        .add_rod_journal(rj7)
        .add_rod_journal(rj8)
        .add_rod_journal(rj9)
		.add_rod_journal(rj10)
        .add_rod_journal(rj11)
		.add_rod_journal(rj12)
        .add_rod_journal(rj13)
        .add_rod_journal(rj14)
        .add_rod_journal(rj15)
		.add_rod_journal(rj16)
        .add_rod_journal(rj17)

    piston_parameters piston_params(
        mass: 440.0 * units.g,
        blowby: k_28inH2O(0.05),
        compression_height: compression_height,
        wrist_pin_position: 0.0,
        displacement: 0.0
    )

    connecting_rod_parameters cr_params(
        mass: 535.0 * units.g,
        moment_of_inertia: 0.002,
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + (stroke / 2.0)) * units.mm + compression_height
    )

    label intake_valve_circ(intake_valve_diameter * constants.pi)
    label intake_valve_area(intake_valve_circ * IVL / 100.0) // cm2

    label intake_plenum_area(intake_valve_area * cyl * intake_valves) // cm2
    label collector_cross_section_area(46.5) // port_flow.py v0.7

    label exhaust_pipe_length_0(200.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 100.0) // Litres
    
    label exhaust_pipe_length_1(204.0) // cm
    label exhaust_volume_1(collector_cross_section_area * exhaust_pipe_length_1 / 100.0) // Litres
    
    label spacing_factor(1.1)
    label flange_density(6.0)
    

    intake intake(
        plenum_volume: 13.0 * units.L, // port_flow.py v0.7
        plenum_cross_section_area: 141.0 * units.cm2, // port_flow.py v0.7
        runner_length: 13.9 * units.cm, // port_flow.py v0.7
        intake_flow_rate: k_carb(1608.9), // port_flow.py v0.7
        idle_flow_rate: k_carb(0.02),
        idle_throttle_plate_position: 0.997,
        runner_flow_rate: k_carb(178.8), // port_flow.py v0.7
        velocity_decay: 0.03
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(1340.7), // port_flow.py v0.7
        collector_cross_section_area: 69.8 * units.cm2, // CF 1.5; port_flow.py v0.7
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 49.0 * units.cm, // port_flow.py v0.7
        primary_flow_rate: k_carb(297.9), // port_flow.py v0.7
        velocity_decay: 0.7,
        volume: (exhaust_volume_0 + 4.0) * units.L
    )
    
    exhaust_system_parameters es_params1(
        outlet_flow_rate: k_carb(1340.7), // port_flow.py v0.7
        collector_cross_section_area: 69.8 * units.cm2, // CF 1.5; port_flow.py v0.7
        length: exhaust_pipe_length_1 * units.cm,
        primary_tube_length: 49.0 * units.cm, // port_flow.py v0.7
        primary_flow_rate: k_carb(297.9), // port_flow.py v0.7
        velocity_decay: 0.7,
        volume: (exhaust_volume_1 + 4.0) * units.L
    )

    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_22.wav", volume: 0.001))
    exhaust_system exhaust1(es_params1, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_35.wav", volume: 0.001))
 
    cylinder_bank b0(bank_params, angle: vee / 2)
    cylinder_bank b1(bank_params, angle: -vee / 2)
    
    label pl0 ((6.5 * bore * spacing_factor / flange_density) * units.mm)
    label pl1 ((6.4 * bore * spacing_factor / flange_density) * units.mm)
    label pl2 ((6.3 * bore * spacing_factor / flange_density) * units.mm)
    label pl3 ((6.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl4 ((5.4 * bore * spacing_factor / flange_density) * units.mm)
	label pl5 ((4.7 * bore * spacing_factor / flange_density) * units.mm)
	label pl6 ((3.9 * bore * spacing_factor / flange_density) * units.mm)
    label pl7 ((2.0 * bore * spacing_factor / flange_density) * units.mm)
	label pl8 ((1.0 * bore * spacing_factor / flange_density) * units.mm)

    b0
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj0,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl0,
            ignition_wire: wires.wire1
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj2,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl1,
            ignition_wire: wires.wire2
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj4,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl2,
            ignition_wire: wires.wire3
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj6,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl3,
            ignition_wire: wires.wire4
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj8,

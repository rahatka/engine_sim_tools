import "engine_sim.mr"
import "_fuel_90.mr"

units units()
constants constants()

label bore(100.0)
label stroke(76.4)
label compression_ratio(11.3)
label con_rod(127.0)
label IVL(13.2) // valve lift, not lobe lift
label EVL(13.2) // valve lift, not lobe lift
label compression_height(31.7 * units.mm)
label intake_valve_diameter(50.0)
label exhaust_valve_diameter(43.5)
label intake_valves(1.0)
label exhaust_valves(1.0)

label volume(10.801) // L; port_flow.py v0.7
label redline(6500.0) // RPM

label cyl(18.0)
label row(2.0)
label cyl_per_bank(cyl / row)
label cycle(720.0 * units.deg)
label vee(180.0 * units.deg)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// 964 / 993 Street / Sport Cam valve timings
label IVO(30.0 * units.deg) //BTDC
label IVC(78.0 * units.deg) //ABDC
label EVO(74.0 * units.deg) //BBDC
label EVC(26.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

// 1-2-4-6-8-9-7-5-3
public node distributor_0 {
    input wires;
    input timing_curve;
    input rev_limit: 6750 * units.rpm;
    input limiter_duration: 0.06;
    alias output __out:
        ignition_module(timing_curve: timing_curve, rev_limit: rev_limit, limiter_duration: limiter_duration)
            .connect_wire(wires.wire1, rot_base * 0)
            .connect_wire(wires.wire2, rot_base * 1)
            .connect_wire(wires.wire4, rot_base * 2)
            .connect_wire(wires.wire6, rot_base * 3)
            .connect_wire(wires.wire8, rot_base * 4)
            .connect_wire(wires.wire9, rot_base * 5)
            .connect_wire(wires.wire7, rot_base * 6)
            .connect_wire(wires.wire5, rot_base * 7)
            .connect_wire(wires.wire3, rot_base * 8)

            .connect_wire(wires.wire10, rot_base * 0 + rot360)
            .connect_wire(wires.wire11, rot_base * 1 + rot360)
            .connect_wire(wires.wire13, rot_base * 2 + rot360)
            .connect_wire(wires.wire15, rot_base * 3 + rot360)
            .connect_wire(wires.wire17, rot_base * 4 + rot360)
            .connect_wire(wires.wire18, rot_base * 5 + rot360)
            .connect_wire(wires.wire16, rot_base * 6 + rot360)
            .connect_wire(wires.wire14, rot_base * 7 + rot360)
            .connect_wire(wires.wire12, rot_base * 8 + rot360);
}


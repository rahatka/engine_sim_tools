import "engine_sim.mr"
import "../../fuel/90.mr"

units units()
constants constants()

label bore(100.0)
label stroke(76.4)
label compression_ratio(11.3)
label con_rod(127.0)
label compression_height(31.7 * units.mm)
label intake_valve_diameter(50.0)
label exhaust_valve_diameter(43.5)
label intake_valves(1.0)
label exhaust_valves(1.0)

label volume(6.000) // L; port_flow.py v0.7b
label redline(7500.0) // RPM

label cyl(10.0)
label row(2.0)
label cyl_base(cyl / row)
label cycle(720.0 * units.deg)
label vee(180.0 * units.deg)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label rot_base(2 * (360.0 / cyl_base) * units.deg)

// 1995 993RS M002
label IVL(12.5)
label EVL(11.1)
label IVO(5.0 * units.deg) //BTDC
label IVC(58.0 * units.deg) //ABDC
label EVO(50.0 * units.deg) //BBDC
label EVC(2.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

// 1-2-4-5-3
public node distributor_0 {
    input wires;
    input timing_curve;
    input rev_limit: 7750 * units.rpm;
    input limiter_duration: 0.06;
    alias output __out:
        ignition_module(timing_curve: timing_curve, rev_limit: rev_limit, limiter_duration: limiter_duration)
            .connect_wire(wires.wire1, rot_base * 0)
            .connect_wire(wires.wire2, rot_base * 1)
            .connect_wire(wires.wire4, rot_base * 2)
            .connect_wire(wires.wire5, rot_base * 3)
            .connect_wire(wires.wire3, rot_base * 4)
            .connect_wire(wires.wire6, rot_base * 0 + rot360)
            .connect_wire(wires.wire7, rot_base * 1 + rot360)
            .connect_wire(wires.wire9, rot_base * 2 + rot360)
            .connect_wire(wires.wire10, rot_base * 3 + rot360)
            .connect_wire(wires.wire8, rot_base * 4 + rot360);
}


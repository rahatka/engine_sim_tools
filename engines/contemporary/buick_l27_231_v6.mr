label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label exhaust_delay_coeff(1.5)

public node eng_distributor {
    input wires;
    input timing_curve;
    input rev_limit: 6500 * units.rpm;
    input limiter_duration: 0.08;
    alias output __out:
        ignition_module(timing_curve: timing_curve, rev_limit: rev_limit, limiter_duration: limiter_duration)
            .connect_wire(wires.wire1, rot * 0)
            .connect_wire(wires.wire3, rot * 2)
            .connect_wire(wires.wire2, rot * 4)
            .connect_wire(wires.wire4, rot * 0 + vee)
            .connect_wire(wires.wire6, rot * 2 + vee)
            .connect_wire(wires.wire5, rot * 4 + vee);
}

public node eng_distributor_lego {
    input wires;
    input timing_curve;
    input rev_limit: 6500 * units.rpm;
    input limiter_duration: 0.08;
    alias output __out:
        ignition_module(timing_curve: timing_curve, rev_limit: rev_limit, limiter_duration: limiter_duration)
            .connect_wire(wires.wire1, 0)
            .connect_wire(wires.wire2, rot180)
            .connect_wire(wires.wire3, rot360)
            .connect_wire(wires.wire4, 0 + vee)
            .connect_wire(wires.wire5, rot180 + vee)
            .connect_wire(wires.wire6, rot360 + vee);
}

private node wires {
    output wire1: ignition_wire();
    output wire2: ignition_wire();
    output wire3: ignition_wire();
    output wire4: ignition_wire();
    output wire5: ignition_wire();
    output wire6: ignition_wire();
}


import "engine_sim.mr"
import "../../fuel/90.mr"

units units()
constants constants()

label bore(96.52)
label stroke(86.36)
label compression_ratio(9.4)
label con_rod(151.384)
label compression_height(46.99 * units.mm)
label intake_valve_diameter(43.434)
label exhaust_valve_diameter(43.434)
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(6000.0) // RPM

label cyl(6.0)
label row(2.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label vee(90.0 * units.deg)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// COMP Cams High Energy Cam and Lifter Kits SK69-200-8
label IVL(12.5984)
label EVL(12.5984)
label IVO(19.0 * units.deg) //BTDC
label IVC(59.0 * units.deg) //ABDC
label EVO(59.0 * units.deg) //BBDC
label EVC(19.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label exhaust_delay_coeff(1.5)

public node eng_distributor {
    input wires;
    input timing_curve;
    input rev_limit: 6500 * units.rpm;
    input limiter_duration: 0.08;
    alias output __out:
        ignition_module(timing_curve: timing_curve, rev_limit: rev_limit, limiter_duration: limiter_duration)
            .connect_wire(wires.wire1, rot * 0)
            .connect_wire(wires.wire3, rot * 2)
            .connect_wire(wires.wire2, rot * 4)
            .connect_wire(wires.wire4, rot * 0 + vee)
            .connect_wire(wires.wire6, rot * 2 + vee)
            .connect_wire(wires.wire5, rot * 4 + vee);
}

public node eng_distributor_lego {
    input wires;
    input timing_curve;
    input rev_limit: 6500 * units.rpm;
    input limiter_duration: 0.08;
    alias output __out:
        ignition_module(timing_curve: timing_curve, rev_limit: rev_limit, limiter_duration: limiter_duration)
            .connect_wire(wires.wire1, 0)
            .connect_wire(wires.wire2, rot180)
            .connect_wire(wires.wire3, rot360)
            .connect_wire(wires.wire4, 0 + vee)
            .connect_wire(wires.wire5, rot180 + vee)
            .connect_wire(wires.wire6, rot360 + vee);
}

private node wires {
    output wire1: ignition_wire();
    output wire2: ignition_wire();
    output wire3: ignition_wire();
    output wire4: ignition_wire();
    output wire5: ignition_wire();
    output wire6: ignition_wire();
}


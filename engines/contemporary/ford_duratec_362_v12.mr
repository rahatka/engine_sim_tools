import "engine_sim.mr"
import "../../fuel/90.mr"

units units()
constants constants()

label bore(89.0)
label stroke(79.5)
label compression_ratio(10.3)
label con_rod(138.1)
label compression_height(30.0 * units.mm)
label intake_valve_diameter(35.0)
label exhaust_valve_diameter(30.0)
label intake_valves(2.0)
label exhaust_valves(2.0)

label cyl(12.0)
label row(2.0)
label cycle(720.0 * units.deg)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label vee(60.0 * units.deg)

label redline(6500.0) // RPM

// 2000-3.0L Non Lincoln LS
label IVL(10.5) // 9.8 cam profile
label EVL(11.0) // 9.8 cam profile
label IVO(4.0 * units.deg) //BTDC
label IVC(48.0 * units.deg) //ABDC
label EVO(56.0 * units.deg) //BBDC
label EVC(12.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(535.0) // g
label crank_mass(30.0) // kg
label flywheel_mass(8.0) // kg

label exhaust_delay_coeff(1.5)


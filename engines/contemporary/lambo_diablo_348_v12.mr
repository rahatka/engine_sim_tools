    exhaust_system _ex0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_39.wav", volume: 0.001), audio_volume: 0.002)
    exhaust_system _ex1(es_params1, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_39.wav", volume: 0.001), audio_volume: 0.002)
}

// Diablo 1990
public node veh {
    alias output __out: vehicle;

    label car_mass(1676) // 1576

    vehicle vehicle(
        mass: car_mass * units.kg,
        drag_coefficient: 0.31,
        cross_sectional_area: (2040 * units.mm) * (1100 * units.mm),
        diff_ratio: 2.41 * 1.59,
        tire_radius: (659.0 / 2) * units.mm,
        rolling_resistance: 0.012 * car_mass * 9.81
        )
}

// Diablo 1990
private node trn {
    alias output __out:
        transmission(
            max_clutch_torque: 650 * units.Nm,
            max_clutch_flex: 25 * units.deg,
            limit_clutch_flex: true,
            clutch_stiffness: 20 * units.Nm / units.deg,
            clutch_damping: 1.0,
            simulate_flex: true
        )
        .add_gear(2.31)
        .add_gear(1.52)
        .add_gear(1.12)
        .add_gear(0.88)
        .add_gear(0.68);
}

run(
    engine: eng(),
    transmission: trn(),
    vehicle: veh()
)

public node camshaft_builder {
    output intake_cam_0: _intake_cam_0;
    output exhaust_cam_0: _exhaust_cam_0;

    output intake_cam_1: _intake_cam_1;
    output exhaust_cam_1: _exhaust_cam_1;

    camshaft _intake_cam_0(base_radius: 16.2 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 16.2 * units.mm, lobe_profile: exhaust_lobe_profile)
    
    camshaft _intake_cam_1(base_radius: 16.2 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_1(base_radius: 16.2 * units.mm, lobe_profile: exhaust_lobe_profile)

    _intake_cam_0
        .add_lobe(ILC + 0 * rot)
        .add_lobe(ILC + 3 * rot)
        .add_lobe(ILC + 5 * rot)
        .add_lobe(ILC + 2 * rot)
    _exhaust_cam_0
        .add_lobe(ELC + 0 * rot)
        .add_lobe(ELC + 3 * rot)
        .add_lobe(ELC + 5 * rot)
        .add_lobe(ELC + 2 * rot)
        
    _intake_cam_1
        .add_lobe(ILC + 1 * rot)
        .add_lobe(ILC + 4 * rot)
        .add_lobe(ILC + 6 * rot)
        .add_lobe(ILC + 7 * rot)
    _exhaust_cam_1
        .add_lobe(ELC + 1 * rot)
        .add_lobe(ELC + 4 * rot)
        .add_lobe(ELC + 6 * rot)
        .add_lobe(ELC + 7 * rot)
}

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node head {
    // port_flow.py v1.5
    // intake port area: 13.1 cm²; saturated lift: 7.04 mm
    // exhaust port area: 9.3 cm²; saturated lift: 5.94 mm
    // cylinder volume: 423.9 cm³ (25.9 CI); engine volume: 3.392 L (207.0 CI)
    // chamber volume: 47.1 cm³ (2.9 CI)
    // 16 harmonic intake runner length: 16.9 cm; diameter: 3.8 cm
    // primary length: 27.0 cm, area: 10.2 cm², diameter: 3.6 cm
    // collector diameter: 6.5 cm, area: 32.7 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 47.105 * units.cc,
        intake_runner_volume: 194.9 * units.cc,
        intake_runner_cross_section_area: 11.5 * units.cm2,
        exhaust_runner_volume: 65.0 * units.cc,
        exhaust_runner_cross_section_area: 10.2 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "Ford SHO 3.4 V8",
        starter_torque: 100 * units.Nm,
        starter_speed: 200 * units.rpm,
        redline: redline * units.rpm,
        fuel: s90(),
        throttle_gamma: 1.5,
        jitter: es.jitter,
        noise: es.noise,
        hf_gain: es.hf_gain,
        simulation_frequency: 10000,
        dyno_min_speed: 500 * units.rpm
    )

    wires wires()

    label crank_moment(disk_moment_of_inertia(mass: crank_mass * units.kg, radius: (stroke / 2.0) * units.mm))
    label flywheel_moment(disk_moment_of_inertia(mass: flywheel_mass * units.kg, radius: (stroke * 3.5) * units.mm))
    label other_moment(disk_moment_of_inertia(mass: 1 * units.kg, radius: 1.0 * units.cm))

    crankshaft c0(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: flywheel_mass * units.kg,
        mass: crank_mass * units.kg,
        friction_torque: 5.1 * units.Nm,
        moment_of_inertia: crank_moment + flywheel_moment + other_moment,
        tdc: 0.0
    )

    rod_journal rj0(angle: rot * 0 + rot90 + vee / 2)
    rod_journal rj2(angle: rot * 3 + rot90 + vee / 2)
    rod_journal rj4(angle: rot * 1 + rot90 + vee / 2)
    rod_journal rj6(angle: rot * 6 + rot90 + vee / 2)

    rod_journal rj1(angle: rot * 0 + rot90 + vee / 2 + (rot - vee))
    rod_journal rj3(angle: rot * 3 + rot90 + vee / 2 + (rot - vee))
    rod_journal rj5(angle: rot * 1 + rot90 + vee / 2 + (rot - vee))
    rod_journal rj7(angle: rot * 6 + rot90 + vee / 2 + (rot - vee))
    
    c0
        .add_rod_journal(rj0)
        .add_rod_journal(rj1)
        .add_rod_journal(rj2)
        .add_rod_journal(rj3)
        .add_rod_journal(rj4)
        .add_rod_journal(rj5)
        .add_rod_journal(rj6)
        .add_rod_journal(rj7)

    piston_parameters piston_params(
        mass: piston_mass * units.g,
        blowby: k_28inH2O(0.026),
        compression_height: compression_height,
        wrist_pin_position: 0.0,
        displacement: 0.0
    )

    connecting_rod_parameters cr_params(
        mass: con_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.g,
            length: con_rod * units.mm
        ),
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    intake intake(
        plenum_volume: 3.3 * units.L,
        plenum_cross_section_area: 46.2 * units.cm2,
        runner_length: 16.9 * units.cm,
        intake_flow_rate: k_carb(385.8),
        idle_flow_rate: k_carb(0.02),
        idle_throttle_plate_position: 0.997,
        runner_flow_rate: k_carb(101.3),
        velocity_decay: 0.05
    )
 
    cylinder_bank b0(bank_params, angle: vee / 2)
    cylinder_bank b1(bank_params, angle: -vee / 2)

    es1 es()

    b0
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj0,
            intake: intake,
            exhaust_system: es.es0,
            primary_length: es.pl0,
            ignition_wire: wires.wire1
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj2,
            intake: intake,
            exhaust_system: es.es1,
            primary_length: es.pl1,
            ignition_wire: wires.wire2
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj4,
            intake: intake,
            exhaust_system: es.es2,
            primary_length: es.pl2,
            ignition_wire: wires.wire3
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj6,
            intake: intake,
            exhaust_system: es.es3,
            primary_length: es.pl3,
            ignition_wire: wires.wire4
        )
        
    b1
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj1,
            intake: intake,
            exhaust_system: es.es4,
            primary_length: es.pl4,
            ignition_wire: wires.wire5
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj3,
            intake: intake,
            exhaust_system: es.es5,
            primary_length: es.pl5,
            ignition_wire: wires.wire6
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj5,
            intake: intake,
            exhaust_system: es.es6,
            primary_length: es.pl6,
            ignition_wire: wires.wire7
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj7,
            intake: intake,
            exhaust_system: es.es7,
            primary_length: es.pl7,
            ignition_wire: wires.wire8
        )

    engine
        .add_cylinder_bank(b0)
        .add_cylinder_bank(b1)
        .add_crankshaft(c0)

    camshaft_builder camshaft()
    
    engine.add_ignition_module(
        distributor(
            wires: wires,
            timing_curve: timing_curve
        )
    )

    b0.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_0,
            exhaust_camshaft: camshaft.exhaust_cam_0,
            flip_display: true
        )
    )
    b1.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_1,
            exhaust_camshaft: camshaft.exhaust_cam_1
        )
    )

    // ignition_timing.py v0.3 from = 8.0, to = 30.0, i = 750, r = 6500
    function timing_curve(650.0 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 0.0 * units.deg)
        .add_sample(650.0 * units.rpm, 4.0 * units.deg)
        .add_sample(1300.0 * units.rpm, 8.0 * units.deg)
        .add_sample(1950.0 * units.rpm, 12.6 * units.deg)
        .add_sample(2600.0 * units.rpm, 16.4 * units.deg)
        .add_sample(3250.0 * units.rpm, 19.7 * units.deg)
        .add_sample(3900.0 * units.rpm, 22.5 * units.deg)
        .add_sample(4550.0 * units.rpm, 24.9 * units.deg)
        .add_sample(5200.0 * units.rpm, 26.9 * units.deg)
        .add_sample(5850.0 * units.rpm, 28.6 * units.deg)
        .add_sample(6500.0 * units.rpm, 30.0 * units.deg)
        .add_sample(7150.0 * units.rpm, -12.5 * units.deg)
        .add_sample(7800.0 * units.rpm, -25.0 * units.deg)
}

private node es0{
    output es0: exhaust0;
    output es1: exhaust0;
    output es2: exhaust0;
    output es3: exhaust0;
    output es4: exhaust1;
    output es5: exhaust1;
    output es6: exhaust1;
    output es7: exhaust1;
    output pl0: _pl0;
    output pl1: _pl1;
    output pl2: _pl2;
    output pl3: _pl3;
    output pl4: _pl4;
    output pl5: _pl5;
    output pl6: _pl6;
    output pl7: _pl7;
    output jitter: 0.5;
    output noise: 0.1;
    output hf_gain: 0.004;

    label flange_density(1.0 * exhaust_delay_coeff)
    label collector_cross_section_area(16.4)
    label exhaust_pipe_length_0(425.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 100.0) // Litres
    label exhaust_pipe_length_1(400.0) // cm
    label exhaust_volume_1(collector_cross_section_area * exhaust_pipe_length_1 / 100.0) // Litres
    label spacing_factor(1.1)

    float _pl0 ((1.7 * bore * spacing_factor / flange_density) * units.mm)
    float _pl1 ((1.1 * bore * spacing_factor / flange_density) * units.mm)
    float _pl2 ((2.0 * bore * spacing_factor / flange_density) * units.mm)
    float _pl3 ((3.0 * bore * spacing_factor / flange_density) * units.mm)
    
    float _pl4 ((3.0 * bore * spacing_factor / flange_density) * units.mm)
    float _pl5 ((2.0 * bore * spacing_factor / flange_density) * units.mm)
    float _pl6 ((1.1 * bore * spacing_factor / flange_density) * units.mm)
    float _pl7 ((1.7 * bore * spacing_factor / flange_density) * units.mm)

    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(329.5),
        collector_cross_section_area: 16.4 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 27.0 * units.cm,
        primary_flow_rate: k_carb(168.9),
        velocity_decay: 1.0,
        volume: (exhaust_volume_0 + 2.0) * units.L
    )
    
    exhaust_system_parameters es_params1(
        outlet_flow_rate: k_carb(329.5),
        collector_cross_section_area: 16.4 * units.cm2,
        length: exhaust_pipe_length_1 * units.cm,
        primary_tube_length: 27.0 * units.cm,
        primary_flow_rate: k_carb(168.9),
        velocity_decay: 1.0,
        volume: (exhaust_volume_1 + 2.0) * units.L
    )

    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_56.wav", volume: 0.00025))
    exhaust_system exhaust1(es_params1, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_56.wav", volume: 0.00025))
}

private node es1{
    output es0: exhaust0;
    output es1: exhaust0;
    output es2: exhaust0;
    output es3: exhaust0;
    output es4: exhaust0;
    output es5: exhaust0;
    output es6: exhaust0;
    output es7: exhaust0;
    output pl0: _pl0;
    output pl1: _pl1;
    output pl2: _pl2;
    output pl3: _pl3;
    output pl4: _pl4;
    output pl5: _pl5;
    output pl6: _pl6;
    output pl7: _pl7;
    output jitter: 0.8;
    output noise: 0.1;
    output hf_gain: 0.007;

    label flange_density(1.0 * exhaust_delay_coeff)
    label collector_cross_section_area(32.7)
    label exhaust_pipe_length_0(400.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 100.0) // Litres
    label spacing_factor(1.1)

    label sh((450.0 / exhaust_delay_coeff) * units.mm)

    float _pl0 ((1.8 * bore * spacing_factor / flange_density) * units.mm)
    float _pl1 ((1.0 * bore * spacing_factor / flange_density) * units.mm)
    float _pl2 ((1.0 * bore * spacing_factor / flange_density) * units.mm)
    float _pl3 ((1.8 * bore * spacing_factor / flange_density) * units.mm)
    
    float _pl4 ((1.8 * bore * spacing_factor / flange_density) * units.mm + sh)
    float _pl5 ((1.0 * bore * spacing_factor / flange_density) * units.mm + sh)
    float _pl6 ((1.0 * bore * spacing_factor / flange_density) * units.mm + sh)
    float _pl7 ((1.8 * bore * spacing_factor / flange_density) * units.mm + sh)

    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(659.1),
        collector_cross_section_area: 32.7 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 27.0 * units.cm,
        primary_flow_rate: k_carb(168.9),
        velocity_decay: 1.0,
        volume: (exhaust_volume_0 + 4.0) * units.L
    )

    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_56.wav", volume: 0.001))
}

// 1997 Ford Taurus III SHO
public node veh {
    alias output __out: vehicle;

    label car_mass(1660) // 1560

    vehicle vehicle(
        mass: car_mass * units.kg,
        drag_coefficient: 0.31,
        cross_sectional_area: (1854 * units.mm) * (1400 * units.mm),
        diff_ratio: 3.77,
        tire_radius: (653.9 / 2) * units.mm, // 225/55 R16
        rolling_resistance: 0.012 * car_mass * 9.81
        )
}


import "engine_sim.mr"
import "../../fuel/90.mr"

units units()
constants constants()

label bore(82.4)
label stroke(79.5)
label compression_ratio(10.0)
label con_rod(138.1) // 5.437
label compression_height(30.0 * units.mm) // 1.181
label intake_valve_diameter(32.0) // DNJ Part #IV4199
label exhaust_valve_diameter(27.0) // DNJ Part #EV4199
label intake_valves(2.0)
label exhaust_valves(2.0)

label redline(6500.0) // RPM

label cyl(8.0)
label row(2.0)
label cycle(720.0 * units.deg)
label vee(60.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// stock 176/203
label IVL(10.0) // .394
label EVL(10.41) // .410
label IVO(-21.5 * units.deg) //BTDC
label IVC(14.5 * units.deg) //ABDC
label EVO(33.5 * units.deg) //BBDC
label EVC(-10.5 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(560.0) // ? g
label piston_mass(500.0) // ?
label crank_mass(22.0) // ? kg
label flywheel_mass(5.0) // kg

label exhaust_delay_coeff(1.5)

public node distributor {
    input wires;
    input timing_curve;
    input limiter_duration: 0.09;
    alias output __out:
        ignition_module(timing_curve: timing_curve, rev_limit: redline * 1.05 * units.rpm, limiter_duration: limiter_duration)
            .connect_wire(wires.wire1, rot * 0)
            .connect_wire(wires.wire5, rot * 1)
            .connect_wire(wires.wire4, rot * 2)
            .connect_wire(wires.wire2, rot * 3)
            .connect_wire(wires.wire6, rot * 4)
            .connect_wire(wires.wire3, rot * 5)
            .connect_wire(wires.wire7, rot * 6)
            .connect_wire(wires.wire8, rot * 7);
}

private node wires {
    output wire1: ignition_wire();
    output wire2: ignition_wire();
    output wire3: ignition_wire();
    output wire4: ignition_wire();
    output wire5: ignition_wire();
    output wire6: ignition_wire();
    output wire7: ignition_wire();
    output wire8: ignition_wire();
}


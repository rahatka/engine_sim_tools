private node trn {
    alias output __out:
        transmission(
            max_clutch_torque: 175 * units.Nm,
            max_clutch_flex: 30 * units.deg,
            limit_clutch_flex: true,
            clutch_stiffness: 10 * units.Nm / units.deg,
            clutch_damping: 1.0,
            simulate_flex: true
        )
        .add_gear(3.70)
        .add_gear(2.06)
        .add_gear(1.27)
        .add_gear(0.90);
}

run(
    engine: eng(),
    transmission: trn(),
    vehicle: veh()
)

public node camshaft_builder {
    output intake_cam_0: _intake_cam_0;
    output exhaust_cam_0: _exhaust_cam_0;

    camshaft _intake_cam_0(base_radius: 18.6 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 18.6 * units.mm, lobe_profile: exhaust_lobe_profile)

    _intake_cam_0
        .add_lobe(ILC + 0 * rot)
        .add_lobe(ILC + 1 * rot)
    _exhaust_cam_0
        .add_lobe(ELC + 0 * rot)
        .add_lobe(ELC + 1 * rot)
}

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node head {
    // port_flow.py v1.5
    // intake port area: 7.7 cm²; saturated lift: 7.65 mm
    // exhaust port area: 5.6 cm²; saturated lift: 6.48 mm
    // cylinder volume: 322.1 cm³ (19.66 CI); engine volume: 0.644 L (39.31 CI)
    // chamber volume: 36.2 cm³ (2.21 CI)
    // 16 harmonic intake runner length: 15.3 cm; diameter: 3.2 cm
    // primary length: 28.0 cm, area: 6.1 cm², diameter: 2.8 cm
    // collector diameter: 2.3 cm, area: 4.3 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 36.190 * units.cc,
        intake_runner_volume: 122.9 * units.cc,
        intake_runner_cross_section_area: 8.1 * units.cm2,
        exhaust_runner_volume: 41.0 * units.cc,
        exhaust_runner_cross_section_area: 6.1 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "VAZ-1111",
        starter_torque: 100 * units.Nm,
        starter_speed: 300 * units.rpm,
        redline: redline * units.rpm,
        fuel: s90(),
        throttle_gamma: 1.5,
        jitter: 0.7,
        noise: 0.2,
        hf_gain: 0.002,
        simulation_frequency: 12000 
    )

    wires wires()

    connecting_rod_parameters cr_params(
        mass: con_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.g,
            length: con_rod * units.mm
        ),
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    label crank_moment(disk_moment_of_inertia(mass: crank_mass * units.kg, radius: (stroke / 2.0) * units.mm))
    label flywheel_moment(disk_moment_of_inertia(mass: flywheel_mass * units.kg, radius: (stroke * 2.7) * units.mm))
    label other_moment(disk_moment_of_inertia(mass: 1 * units.kg, radius: 1.0 * units.cm))

    crankshaft c0(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: flywheel_mass * units.kg,
        mass: crank_mass * units.kg,
        friction_torque: 1.0 * units.Nm,
        moment_of_inertia: crank_moment + flywheel_moment + other_moment,
        tdc: 0.0
    )

    rod_journal rj0(angle: rot * 0 + rot90)
    rod_journal rj1(angle: rot * 1 + rot90)
    
    c0
        .add_rod_journal(rj0)
        .add_rod_journal(rj1)

    piston_parameters piston_params(
        mass: piston_mass * units.g,
        blowby: k_28inH2O(0.024),
        compression_height: compression_height,
        wrist_pin_position: 0.0,
        displacement: 0.0
    )

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    label collector_cross_section_area(4.3)

    label exhaust_pipe_length_0(200.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 1000.0) // Litres
    
    label spacing_factor(1.2)
    label flange_density(1.0 * exhaust_delay_coeff)

    intake intake(
        plenum_volume: 0.5 * units.L,
        plenum_cross_section_area: 8.1 * units.cm2,
        runner_length: 15.3 * units.cm,
        intake_flow_rate: k_carb(54.0),
        idle_flow_rate: k_carb(0.001),
        idle_throttle_plate_position: 0.996,
        runner_flow_rate: k_carb(54.0),
        velocity_decay: 0.05
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(84.4),
        collector_cross_section_area: 4.3 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 28.0 * units.cm,
        primary_flow_rate: k_carb(84.4),
        velocity_decay: 1.0,
        volume: exhaust_volume_0 * units.L
    )

    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_39.wav", volume: 0.00025))
 
    cylinder_bank b0(bank_params, angle: 0)

    label pl0((2.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl1((1.0 * bore * spacing_factor / flange_density) * units.mm)

    b0
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj0,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl0,
            ignition_wire: wires.wire1
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj1,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl1,
            ignition_wire: wires.wire2
        )

    engine
        .add_cylinder_bank(b0)

    engine.add_crankshaft(c0)

    camshaft_builder camshaft()

    b0.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_0,
            exhaust_camshaft: camshaft.exhaust_cam_0,
            flip_display: true
        )
    )

    function timing_curve(383.3 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 0.0 * units.deg)
        .add_sample(383.3 * units.rpm, 0.0 * units.deg)
        .add_sample(766.7 * units.rpm, 0.8 * units.deg)
        .add_sample(1150.0 * units.rpm, 2.7 * units.deg)
        .add_sample(1533.3 * units.rpm, 4.5 * units.deg)
        .add_sample(1916.7 * units.rpm, 6.4 * units.deg)
        .add_sample(2300.0 * units.rpm, 8.3 * units.deg)
        .add_sample(2683.3 * units.rpm, 10.1 * units.deg)
        .add_sample(3066.7 * units.rpm, 12.0 * units.deg)
        .add_sample(3450.0 * units.rpm, 13.8 * units.deg)
        .add_sample(3833.3 * units.rpm, 15.7 * units.deg)
        .add_sample(4216.7 * units.rpm, 17.6 * units.deg)
        .add_sample(4600.0 * units.rpm, 19.4 * units.deg)
        .add_sample(4983.3 * units.rpm, 21.3 * units.deg)
        .add_sample(5366.7 * units.rpm, 23.1 * units.deg)
        .add_sample(5750.0 * units.rpm, 25.0 * units.deg)
        .add_sample(6133.3 * units.rpm, -12.5 * units.deg)
        .add_sample(6516.7 * units.rpm, -25.0 * units.deg)

    engine.add_ignition_module(
        eng_distributor(
            wires: wires,
            timing_curve: timing_curve
        )
    )
}

// ВАЗ-1111
public node veh {
    alias output __out: vehicle;

    label car_mass(700) // 645 empty; 975 full

    vehicle vehicle(
        mass: car_mass * units.kg,
        drag_coefficient: 0.45,
        cross_sectional_area: (1420 * units.mm) * (1400 * units.mm),
        diff_ratio: 4.5,
        tire_radius: (520.8 / 2) * units.mm, // 135/80 R12
        rolling_resistance: 0.012 * car_mass * 9.81
        )
}


import "engine_sim.mr"
import "../../fuel/90.mr"

units units()
constants constants()

label bore(76.0)
label stroke(71.0)
label compression_ratio(9.9)
label con_rod(121.0)
label compression_height(35.0 * units.mm) // ?
label intake_valve_diameter(36.0)
label exhaust_valve_diameter(30.5)
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(5750.0) // RPM

label cyl(2.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// ВАЗ-1111
label IVL(9.6)
label EVL(9.0)
label IVO(26.0 * units.deg) //BTDC
label IVC(60.0 * units.deg) //ABDC
label EVO(50.0 * units.deg) //BBDC
label EVC(22.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label piston_mass(300.0) // ? g
label con_rod_mass(400.0) // ? g
label crank_mass(5.0) // ? kg
label flywheel_mass(2.0) // kg

label exhaust_delay_coeff(1.5)

public node eng_distributor {
    input wires;
    input timing_curve;
    input rev_limit: 7000 * units.rpm;
    input limiter_duration: 0.0001;
    alias output __out:
        ignition_module(timing_curve: timing_curve, rev_limit: rev_limit, limiter_duration: limiter_duration)
            .connect_wire(wires.wire1, rot * 0)
            .connect_wire(wires.wire2, rot * 1);
}

private node wires {
    output wire1: ignition_wire();
    output wire2: ignition_wire();
}


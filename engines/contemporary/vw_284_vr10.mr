import "engine_sim.mr"
import "../../fuel/90.mr"

units units()
constants constants()

label bore(81.0)
label stroke(90.2)
label compression_ratio(10.8)
label con_rod(164.0)
label compression_height(31.7 * units.mm) // ?
label intake_valve_diameter(31.0)
label exhaust_valve_diameter(27.0)
label intake_valves(2.0)
label exhaust_valves(2.0)

label redline(7000.0) // RPM

label cyl(10.0)
label row(2.0)
label vee(72.0 * units.deg)
label cycle(720.0 * units.deg)
label vree (15.0 * units.deg)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)

// dbilas 23.156.001U
label IVL(11.4)
label EVL(11.0)
label IVO(22.0 * units.deg) //BTDC
label IVC(66.0 * units.deg) //ABDC
label EVO(63.0 * units.deg) //BBDC
label EVC(19.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(535.0) // g
label piston_mass(440.0) // ?
label crank_mass(20.0) // kg
label flywheel_mass(5.0) // kg

label exhaust_delay_coeff(1.5)

public node variant_0 {
    output s1: 0;
    output s3: 1;
    output s5: 2;
    output s4: 3;
    output s2: 4;
    output seq: "1-3-5-4-2";
}

public node variant_1 {
    output s1: 0;

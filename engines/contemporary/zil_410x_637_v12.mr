public node veh {
    alias output __out: vehicle;

    label car_mass(3100) // 2985 / 3360

    vehicle vehicle(
        mass: car_mass * units.kg,
        drag_coefficient: 0.42,
        cross_sectional_area: (2068 * units.mm) * (1480 * units.mm),
        diff_ratio: 3.54,
        tire_radius: (855.98 / 2) * units.mm, // 9.35 x 15
        rolling_resistance: 0.012 * car_mass * 9.81
        )
}


public node camshaft_builder_0 {
    output intake_cam_0: _intake_cam_0;
    output exhaust_cam_0: _exhaust_cam_0;

    output intake_cam_1: _intake_cam_1;
    output exhaust_cam_1: _exhaust_cam_1;

    camshaft _intake_cam_0(base_radius: 25.3 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 25.3 * units.mm, lobe_profile: exhaust_lobe_profile)
    
    camshaft _intake_cam_1(base_radius: 25.3 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_1(base_radius: 25.3 * units.mm, lobe_profile: exhaust_lobe_profile)

    _intake_cam_0
        .add_lobe(ILC + 0 * rot) // 1
        .add_lobe(ILC + 4 * rot) // 2
        .add_lobe(ILC + 2 * rot) // 3
        .add_lobe(ILC + 11 * rot) // 4
        .add_lobe(ILC + 7 * rot) // 5
        .add_lobe(ILC + 9 * rot) // 6
    _exhaust_cam_0
        .add_lobe(ELC + 0 * rot) // 1
        .add_lobe(ELC + 4 * rot) // 2
        .add_lobe(ELC + 2 * rot) // 3
        .add_lobe(ELC + 11 * rot) // 4
        .add_lobe(ELC + 7 * rot) // 5
        .add_lobe(ELC + 9 * rot) // 6
        
    _intake_cam_1
        .add_lobe(ILC + 1 * rot + (vee - rot)) // 7
        .add_lobe(ILC + 5 * rot + (vee - rot)) // 8
        .add_lobe(ILC + 3 * rot + (vee - rot)) // 9
        .add_lobe(ILC + 6 * rot + (vee - rot)) // 10
        .add_lobe(ILC + 8 * rot + (vee - rot)) // 11
        .add_lobe(ILC + 10 * rot + (vee - rot)) // 12
    _exhaust_cam_1
        .add_lobe(ELC + 1 * rot + (vee - rot)) // 7
        .add_lobe(ELC + 5 * rot + (vee - rot)) // 8
        .add_lobe(ELC + 3 * rot + (vee - rot)) // 9
        .add_lobe(ELC + 6 * rot + (vee - rot)) // 10
        .add_lobe(ELC + 8 * rot + (vee - rot)) // 11
        .add_lobe(ELC + 10 * rot + (vee - rot)) // 12
}


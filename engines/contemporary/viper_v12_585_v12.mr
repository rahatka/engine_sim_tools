import "engine_sim.mr"
import "../../fuel/90.mr"

units units()
constants constants()

label bore(101.6) // 4
label stroke(98.552) // 3.88
label compression_ratio(9.6)
label con_rod(165.1) // 6.5 // custom
label compression_height(40.9448 * units.mm) // ~
label intake_valve_diameter(48.768)
label exhaust_valve_diameter(40.64)
label intake_valves(1.0)
label exhaust_valves(1.0)

label volume(9.593) // L;
label redline(6000.0) // RPM

label cyl(12.0)
label row(2.0)
label cycle(720.0 * units.deg)
label vee(60.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// stock: 212/232 -- .544/.544
label IVL(13.8176)
label EVL(13.8176)
label IVO(0.0 * units.deg) //BTDC
label IVC(32.0 * units.deg) //ABDC
label EVO(50.0 * units.deg) //BBDC
label EVC(2.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(634.0) // g
label piston_mass(700.0) // ?
label crank_mass(45.0) // ? kg
label flywheel_mass(8.0) // kg

label exhaust_delay_coeff(1.5)

public node distributor_0 {
    input wires;
    input timing_curve;
    input rev_limit: 6350 * units.rpm;
    input limiter_duration: 0.08;
    alias output __out:
        ignition_module(timing_curve: timing_curve, rev_limit: rev_limit, limiter_duration: limiter_duration)
            .connect_wire(wires.wire1, rot * 0)
            .connect_wire(wires.wire7, rot * 1)
            .connect_wire(wires.wire3, rot * 2)
            .connect_wire(wires.wire9, rot * 3)
            .connect_wire(wires.wire2, rot * 4)
            .connect_wire(wires.wire8, rot * 5)
            .connect_wire(wires.wire10, rot * 6)
            .connect_wire(wires.wire5, rot * 7)
            .connect_wire(wires.wire11, rot * 8)
            .connect_wire(wires.wire6, rot * 9)
            .connect_wire(wires.wire12, rot * 10)
            .connect_wire(wires.wire4, rot * 11);
}


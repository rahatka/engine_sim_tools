private node add_lobes {
    input base;
    input shift: 0.0;
    input var;
    input this;
    alias output __out: this;

    this.add_lobe(base + var.s1 * rot_base + shift)
    this.add_lobe(base + var.s2 * rot_base + shift)
    this.add_lobe(base + var.s3 * rot_base + shift)
    this.add_lobe(base + var.s4 * rot_base + shift)
    this.add_lobe(base + var.s5 * rot_base + shift)
    this.add_lobe(base + var.s6 * rot_base + shift)
}

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node head {
    // port_flow.py v1.5
    // intake port area: 11.1 cm²; saturated lift: 6.48 mm
    // exhaust port area: 8.6 cm²; saturated lift: 5.71 mm
    // cylinder volume: 347.5 cm³ (21.20 CI); engine volume: 6.255 L (381.69 CI)
    // chamber volume: 38.6 cm³ (2.36 CI)
    // 8 harmonic intake runner length: 26.7 cm; diameter: 3.6 cm
    // primary length: 18.4 cm, area: 9.0 cm², diameter: 3.4 cm
    // collector diameter: 9.1 cm, area: 65.0 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 38.609 * units.cc,
        intake_runner_volume: 268.9 * units.cc,
        intake_runner_cross_section_area: 10.1 * units.cm2,
        exhaust_runner_volume: 89.6 * units.cc,
        exhaust_runner_cross_section_area: 9.0 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

public node bank_builder {
    input var;
    input incline;
    input phasing;
    input crankshaft;
    input ignition;
    input intake;
    input exhaust;
    input sh;
    input flip: false;

    output b0: _b0;

    ignition_wire _wire1()
    ignition_wire _wire2()
    ignition_wire _wire3()
    ignition_wire _wire4()
    ignition_wire _wire5()
    ignition_wire _wire6()

    piston_parameters piston_params(
        mass: 550 * units.g,
        blowby: k_28inH2O(0.024),
        compression_height: compression_height,
        wrist_pin_position: 0.0,
        displacement: 0.0
    )

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    connecting_rod_parameters cr_params(
        mass: con_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.g,
            length: con_rod * units.mm
        ),
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    camshaft _intake_cam_0(base_radius: 16.7 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 16.7 * units.mm, lobe_profile: exhaust_lobe_profile)

    _intake_cam_0.add_lobes(ILC, phasing, var)
    _exhaust_cam_0.add_lobes(ELC, phasing, var)

    ignition.connect_wire(_wire1, rot_base * var.s1 + phasing)
    ignition.connect_wire(_wire2, rot_base * var.s2 + phasing)
    ignition.connect_wire(_wire3, rot_base * var.s3 + phasing)
    ignition.connect_wire(_wire4, rot_base * var.s4 + phasing)
    ignition.connect_wire(_wire5, rot_base * var.s5 + phasing)
    ignition.connect_wire(_wire6, rot_base * var.s6 + phasing)

    cylinder_bank _b0(bank_params, angle: incline)

    label spacing_factor(1.2)
    label flange_density(1.0 * exhaust_delay_coeff)
    label pl0 ((6.0 * bore * spacing_factor / flange_density) * units.mm + sh)
    label pl1 ((5.0 * bore * spacing_factor / flange_density) * units.mm + sh)
    label pl2 ((4.0 * bore * spacing_factor / flange_density) * units.mm + sh)
    label pl3 ((3.0 * bore * spacing_factor / flange_density) * units.mm + sh)
    label pl4 ((2.0 * bore * spacing_factor / flange_density) * units.mm + sh)
    label pl5 ((1.0 * bore * spacing_factor / flange_density) * units.mm + sh)

    _b0
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj0,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl0,
            ignition_wire: _wire1
        )
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj1,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl1,
            ignition_wire: _wire2
        )
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj2,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl2,
            ignition_wire: _wire3
        )
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj3,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl3,
            ignition_wire: _wire4
        )
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj4,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl4,
            ignition_wire: _wire5
        )
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj5,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl5,
            ignition_wire: _wire6
        )

    _b0.set_cylinder_head(
        head(
            intake_camshaft: _intake_cam_0,
            exhaust_camshaft: _exhaust_cam_0,
            flip_display: flip
        )
    )
}

public node crank {
    input var;
    input phasing;

    output c: _c;
    output rj0: _rj0;
    output rj1: _rj1;
    output rj2: _rj2;
    output rj3: _rj3;
    output rj4: _rj4;
    output rj5: _rj5;

    label crank_moment(disk_moment_of_inertia(mass: crank_mass * units.kg, radius: (stroke / 2.0) * units.mm))
    label flywheel_moment(disk_moment_of_inertia(mass: flywheel_mass * units.kg, radius: (stroke * 3.5) * units.mm))
    label other_moment(disk_moment_of_inertia(mass: 1 * units.kg, radius: 1.0 * units.cm))

    crankshaft _c(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: flywheel_mass * units.kg,
        mass: crank_mass * units.kg,
        friction_torque: 9.4 * units.Nm,
        moment_of_inertia: crank_moment + flywheel_moment + other_moment,
        tdc: 0.0
    )

    rod_journal _rj0(angle: rot_base * var.s1 + phasing)
    rod_journal _rj1(angle: rot_base * var.s2 + phasing)
    rod_journal _rj2(angle: rot_base * var.s3 + phasing)
    rod_journal _rj3(angle: rot_base * var.s4 + phasing)
    rod_journal _rj4(angle: rot_base * var.s5 + phasing)
    rod_journal _rj5(angle: rot_base * var.s6 + phasing)

    _c
        .add_rod_journal(_rj0)
        .add_rod_journal(_rj1)
        .add_rod_journal(_rj2)
        .add_rod_journal(_rj3)
        .add_rod_journal(_rj4)
        .add_rod_journal(_rj5)
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: variant.seq,
        starter_torque: 150 * units.Nm,
        starter_speed: 400 * units.rpm,
        redline: redline * units.rpm,
        fuel: s90(),
        throttle_gamma: 1.5,
        jitter: 0.7,
        noise: 0.1,
        hf_gain: 0.002,
        simulation_frequency: 6500,
        dyno_min_speed: 1000 * units.rpm,
        fluid_simulation_steps: 6,
        max_sle_solver_steps: 32
    )

    variant_0 variant()
    crank c0(variant, vee / 2 + rot90)

    label sh0(20.0 * units.cm)
    label sh1(20.0 * units.cm)
    label sh2(0.0 * units.cm)

    bank_builder bank0(
        var: variant,
        incline: vee / 2,
        phasing: 0,
        crankshaft: c0,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust0,
        sh: sh0,
        flip: true
    )

    bank_builder bank1(
        var: variant,
        incline: -vee / 2,
        phasing: vee + rot360,
        crankshaft: c0,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust0,
        sh: sh1
    )

    bank_builder bank2(
        var: variant,
        incline: -vee / 2 - vee,
        phasing: vee * 2,
        crankshaft: c0,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust0,
        sh: sh2
    )

    ignition_module ignition(timing_curve: timing_curve, rev_limit: 7250 * units.rpm, limiter_duration: 0.03)

    label collector_cross_section_area(65.0)

    label exhaust_pipe_length_0(350.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 100.0) // Litres

    intake intake(
        plenum_volume: 6.1 * units.L,
        plenum_cross_section_area: 90.7 * units.cm2,
        runner_length: 26.7 * units.cm,
        intake_flow_rate: k_carb(729.7),
        idle_flow_rate: k_carb(0.04),
        idle_throttle_plate_position: 0.996,
        runner_flow_rate: k_carb(81.1),
        velocity_decay: 0.05
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(1216.2),
        collector_cross_section_area: 65.0 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 18.4 * units.cm,
        primary_flow_rate: k_carb(135.1),
        velocity_decay: 1.0,
        volume: (exhaust_volume_0 + 5.0) * units.L
    )

    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_58.wav", volume: 0.001))

    engine
        .add_cylinder_bank(bank0.b0)
        .add_cylinder_bank(bank1.b0)
        .add_cylinder_bank(bank2.b0)

        .add_crankshaft(c0.c)
        .add_ignition_module(ignition)

    function timing_curve(466.7 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 5.0 * units.deg)
        .add_sample(466.7 * units.rpm, 5.0 * units.deg)
        .add_sample(933.3 * units.rpm, 5.0 * units.deg)
        .add_sample(1400.0 * units.rpm, 19.6 * units.deg)
        .add_sample(1866.7 * units.rpm, 30.3 * units.deg)
        .add_sample(2333.3 * units.rpm, 36.8 * units.deg)
        .add_sample(2800.0 * units.rpm, 40.8 * units.deg)
        .add_sample(3266.7 * units.rpm, 42.0 * units.deg)
        .add_sample(3733.3 * units.rpm, 42.0 * units.deg)
        .add_sample(4200.0 * units.rpm, 42.0 * units.deg)
        .add_sample(4666.7 * units.rpm, 42.0 * units.deg)
        .add_sample(5133.3 * units.rpm, 42.0 * units.deg)
        .add_sample(5600.0 * units.rpm, 42.0 * units.deg)
        .add_sample(6066.7 * units.rpm, 42.0 * units.deg)
        .add_sample(6533.3 * units.rpm, 42.0 * units.deg)
        .add_sample(7000.0 * units.rpm, 42.0 * units.deg)
        .add_sample(7466.7 * units.rpm, -15.0 * units.deg)
        .add_sample(7933.3 * units.rpm, -45.0 * units.deg)
}


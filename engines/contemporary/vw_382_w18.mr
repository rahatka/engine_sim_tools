import "engine_sim.mr"
import "../../fuel/90.mr"

units units()
constants constants()

label bore(76.5)
label stroke(75.6)
label compression_ratio(10.0)
label con_rod(144.0)
label compression_height(31.0134 * units.mm)
label intake_valve_diameter(29.5)
label exhaust_valve_diameter(26.0)
label intake_valves(2.0)
label exhaust_valves(2.0)

label redline(7000.0) // RPM

label cyl(18.0)
label row(3.0)
label cyl_base(cyl / row)
label banks(3.0)
label cycle(720.0 * units.deg)
label vee(60.0 * units.deg)
label rot_base(cycle / cyl_base)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

label comp(rot - vee)

// DBILAS VW 1,4 - 1,6 16V /Performance camshafts
label IVL(10.0)
label EVL(10.0)
label IVO(6.0 * units.deg) //BTDC
label IVC(74.0 * units.deg) //ABDC
label EVO(56.0 * units.deg) //BBDC
label EVC(16.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(457.0) // g
label crank_mass(36.0) // kg
label flywheel_mass(8.0) // kg

label exhaust_delay_coeff(1.5)


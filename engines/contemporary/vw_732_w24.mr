units units()
constants constants()

label bore(86.0)
label stroke(86.0)
label compression_ratio(8.3)
label con_rod(164.0)
label compression_height(31.0134 * units.mm)
label intake_valve_diameter(31.0)
label exhaust_valve_diameter(27.0)
label intake_valves(2.0)
label exhaust_valves(2.0)

label cyl(24.0)
label row(3.0)
label cycle(720.0 * units.deg)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label vee(rot * 2)
label vree(15.0 * units.deg)

label redline(6000.0) // RPM

// A8L valve timings
label IVL(10.0)
label EVL(10.0)
label IVO(26.0 * units.deg) //BTDC
label IVC(29.0 * units.deg) //ABDC
label EVO(44.0 * units.deg) //BBDC
label EVC(9.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(650.0) // ? g
label piston_mass(500.0) // ?
label crank_mass(28.0) // ? kg
label flywheel_mass(15.0) // kg

label exhaust_delay_coeff(1.5)


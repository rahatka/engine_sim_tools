private node add_lobes_0 {
    input base;
    input shift: 0.0;
    input var;
    input this;
    alias output __out: this;

    this.add_lobe(base + var.s1 * rot_base + shift) // 1
    this.add_lobe(base + var.s3 * rot_base + shift) // 3
    this.add_lobe(base + var.s5 * rot_base + shift) // 5
    this.add_lobe(base + var.s7 * rot_base + shift) // 7
}

private node add_lobes_1 {
    input base;
    input shift: 0.0;
    input var;
    input this;
    alias output __out: this;

    this.add_lobe(base + var.s2 * rot_base + shift) // 2
    this.add_lobe(base + var.s4 * rot_base + shift) // 4
    this.add_lobe(base + var.s6 * rot_base + shift) // 6
    this.add_lobe(base + var.s8 * rot_base + shift) // 8
}

public node bank_builder {
    input var;
    input crankshaft;
    input incline;
    input phasing;
    input ignition;
    input intake;
    input exhaust;
    input sh;
    input flip_display: false;

    output b0: _b0;
    output b1: _b1;

    ignition_wire _wire1()
    ignition_wire _wire2()
    ignition_wire _wire3()
    ignition_wire _wire4()
    ignition_wire _wire5()
    ignition_wire _wire6()
    ignition_wire _wire7()
    ignition_wire _wire8()

    piston_parameters piston_params(
        mass: piston_mass * units.g,
        blowby: k_28inH2O(0.027),
        compression_height: compression_height,
        wrist_pin_position: 0.0,
        displacement: 0.0
    )

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    connecting_rod_parameters cr_params(
        mass: con_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.g,
            length: con_rod * units.mm
        ),
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    camshaft _intake_cam_0(base_radius: 16.7 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 16.7 * units.mm, lobe_profile: exhaust_lobe_profile)

    camshaft _intake_cam_1(base_radius: 16.7 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_1(base_radius: 16.7 * units.mm, lobe_profile: exhaust_lobe_profile)

    _intake_cam_0.add_lobes_0(ILC, phasing, var)
    _exhaust_cam_0.add_lobes_0(ELC, phasing, var)

    _intake_cam_1.add_lobes_1(ILC, phasing, var)
    _exhaust_cam_1.add_lobes_1(ELC, phasing, var)

    ignition.connect_wire(_wire1, rot_base * var.s1 + phasing)
    ignition.connect_wire(_wire2, rot_base * var.s2 + phasing)
    ignition.connect_wire(_wire3, rot_base * var.s3 + phasing)
    ignition.connect_wire(_wire4, rot_base * var.s4 + phasing)
    ignition.connect_wire(_wire5, rot_base * var.s5 + phasing)
    ignition.connect_wire(_wire6, rot_base * var.s6 + phasing)
    ignition.connect_wire(_wire7, rot_base * var.s7 + phasing)
    ignition.connect_wire(_wire8, rot_base * var.s8 + phasing)

    cylinder_bank _b0(bank_params, angle: incline + vree / 2)
    cylinder_bank _b1(bank_params, angle: incline - vree / 2)

    label spacing_factor(0.75)
    label flange_density(1.5 * exhaust_delay_coeff)
    label pl0 ((5.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl1 ((4.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl2 ((4.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl3 ((5.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl4 ((2.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl5 ((1.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl6 ((1.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl7 ((2.0 * bore * spacing_factor / flange_density) * units.mm)

    _b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj0,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl0 + sh,
            ignition_wire: _wire1
        )
    _b1.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj1,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl1 + sh,
            ignition_wire: _wire2
        )
    _b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj2,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl2 + sh,
            ignition_wire: _wire3
        )
    _b1.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj3,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl3 + sh,
            ignition_wire: _wire4
        )
    _b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj4,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl4 + sh,
            ignition_wire: _wire5
        )
    _b1.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj5,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl5 + sh,
            ignition_wire: _wire6
        )
    _b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj6,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl6 + sh,
            ignition_wire: _wire7
        )
    _b1.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj7,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl7 + sh,
            ignition_wire: _wire8
        )

    _b0.set_cylinder_head(
        head(
            intake_camshaft: _intake_cam_0,
            exhaust_camshaft: _exhaust_cam_0,
            flip_display: flip_display
        )
    )
    _b1.set_cylinder_head(
        head(
            intake_camshaft: _intake_cam_1,
            exhaust_camshaft: _exhaust_cam_1,
            flip_display: flip_display
        )
    )
}

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node head {
    // port_flow.py v1.5
    // intake port area: 12.2 cm²; saturated lift: 6.81 mm
    // exhaust port area: 9.3 cm²; saturated lift: 5.93 mm
    // cylinder volume: 499.6 cm³ (30.5 CI); engine volume: 11.989 L (731.6 CI)
    // chamber volume: 68.4 cm³ (4.2 CI)
    // 16 harmonic intake runner length: 16.6 cm; diameter: 4.0 cm
    // primary length: 29.1 cm, area: 10.2 cm², diameter: 3.6 cm
    // collector diameter: 9.7 cm, area: 73.5 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 68.432 * units.cc,
        intake_runner_volume: 203.7 * units.cc,
        intake_runner_cross_section_area: 12.3 * units.cm2,
        exhaust_runner_volume: 67.9 * units.cc,
        exhaust_runner_cross_section_area: 10.2 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

public node crank {
    input phasing;
    input var;

    output c: _c;
    output rj0: _rj0;
    output rj1: _rj1;
    output rj2: _rj2;
    output rj3: _rj3;
    output rj4: _rj4;
    output rj5: _rj5;
    output rj6: _rj6;
    output rj7: _rj7;

    label crank_moment(disk_moment_of_inertia(mass: crank_mass * units.kg, radius: (stroke / 2.0) * units.mm))
    label flywheel_moment(disk_moment_of_inertia(mass: flywheel_mass * units.kg, radius: (stroke * 3.5) * units.mm))
    label other_moment(disk_moment_of_inertia(mass: 1 * units.kg, radius: 1.0 * units.cm))

    crankshaft _c(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: flywheel_mass * units.kg,
        mass: crank_mass * units.kg,
        friction_torque: 18.0 * units.Nm,
        moment_of_inertia: crank_moment + flywheel_moment + other_moment,
        tdc: 0
    )

    rod_journal _rj0(angle: rot_base * var.s1 + rot90 + vee + vree / 2)
    rod_journal _rj1(angle: rot_base * var.s2 + rot90 + vee - vree / 2)
    rod_journal _rj2(angle: rot_base * var.s3 + rot90 + vee + vree / 2)
    rod_journal _rj3(angle: rot_base * var.s4 + rot90 + vee - vree / 2)
    rod_journal _rj4(angle: rot_base * var.s5 + rot90 + vee + vree / 2)
    rod_journal _rj5(angle: rot_base * var.s6 + rot90 + vee - vree / 2)
    rod_journal _rj6(angle: rot_base * var.s7 + rot90 + vee + vree / 2)
    rod_journal _rj7(angle: rot_base * var.s8 + rot90 + vee - vree / 2)

    _c
        .add_rod_journal(_rj0)
        .add_rod_journal(_rj1)
        .add_rod_journal(_rj2)
        .add_rod_journal(_rj3)
        .add_rod_journal(_rj4)
        .add_rod_journal(_rj5)
        .add_rod_journal(_rj6)
        .add_rod_journal(_rj7)
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: variant.seq,
        starter_torque: 200 * units.Nm,
        starter_speed: 500 * units.rpm,
        redline: redline * units.rpm,
        fuel: s90(),
        throttle_gamma: 2.0,
        jitter: 0.5,
        noise: 0.1,
        hf_gain: 0.002,
        simulation_frequency: 4000,
        dyno_min_speed: 1000 * units.rpm,
        fluid_simulation_steps: 6,
        max_sle_solver_steps: 32
    )

    ////////////////////////////////////////
    ///////// change variants here /////////
    ////////////////////////////////////////
    // 0  1-6-2-5-8-3-7-4 Standard
    // 1  1-6-7-4-8-3-2-5 2D70
    // 2  1-5-7-3-8-4-2-6 5D49
    // 3  1-5-2-6-8-4-7-3 Cadillac 431 V16
    // 4  1-6-2-7-3-8-4-5 Bugatti Type 39
    // 5  1-4-7-6-8-5-2-3 Mercedes-Benz M 196 R
    // 6  1-2-4-6-8-7-5-3 Mercedes-Benz M 196 S
    // 7  1-5-3-7-4-8-2-6 Bugatti Type 59 ; Duesenberg before 1927
    // 8  1-3-7-4-8-6-2-5 Delage
    // 9  1-4-7-3-8-5-2-6 Hupmobile
    // 10 1-3-2-5-8-6-7-4 Packard before 1927
    // 11 1-5-2-6-3-7-4-8 Bugatti most common
    // 12 1-5-2-3-8-4-7-6 Hupmobile before 1927
    // 13 1-3-5-7-6-8-2-4 2-2-2-2 v1
    // 14 1-8-5-4-6-3-2-7 2-2-2-2 v2
    variant_8 variant()

    crank c0(vee + rot90, variant)

    ignition_module ignition(timing_curve: timing_curve, rev_limit: 6500 * units.rpm, limiter_duration: 0.05)

    bank_builder bank0(
        var: variant,
        crankshaft: c0,
        incline: vee,
        phasing: 0,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust0,
        sh: sh0,
        flip_display: true
    )

    bank_builder bank1(
        var: variant,
        crankshaft: c0,
        incline: 0,
        phasing: vee + rot360,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust0,
        sh: sh1
    )

    bank_builder bank2(
        var: variant,
        crankshaft: c0,
        incline: -vee,
        phasing: vee * 2,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust0,
        sh: sh2
    )

    label collector_cross_section_area(73.5)

    label exhaust_pipe_length_0(300.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 100.0) // Litres

    intake intake(
        plenum_volume: 10.9 * units.L,
        plenum_cross_section_area: 147.2 * units.cm2,
        runner_length: 16.6 * units.cm,
        intake_flow_rate: k_carb(1318.8),
        idle_flow_rate: k_carb(0.03),
        idle_throttle_plate_position: 0.998,
        runner_flow_rate: k_carb(120.9),
        velocity_decay: 0.05
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(2308.0),
        collector_cross_section_area: 73.5 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 29.1 * units.cm,
        primary_flow_rate: k_carb(201.9),
        velocity_decay: 0.8,
        volume: exhaust_volume_0 * units.L
    )

    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_39.wav", volume: 0.001))
    
    label sh0 ((0.0 / exhaust_delay_coeff) * units.cm)
    label sh1 ((50.0 / exhaust_delay_coeff) * units.cm)
    label sh2 ((30.0 / exhaust_delay_coeff) * units.cm)

    engine
        .add_crankshaft(c0.c)
        .add_cylinder_bank(bank0.b0)
        .add_cylinder_bank(bank0.b1)
        .add_cylinder_bank(bank1.b0)
        .add_cylinder_bank(bank1.b1)
        .add_cylinder_bank(bank2.b0)
        .add_cylinder_bank(bank2.b1)
        .add_ignition_module(ignition)

    // ignition_timing.py v0.3 from = 5.0, to = 35.0, i = 500, r = 6000
    function timing_curve(600.0 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 0.0 * units.deg)
        .add_sample(600.0 * units.rpm, 5.0 * units.deg)
        .add_sample(1200.0 * units.rpm, 11.8 * units.deg)
        .add_sample(1800.0 * units.rpm, 17.3 * units.deg)
        .add_sample(2400.0 * units.rpm, 21.7 * units.deg)
        .add_sample(3000.0 * units.rpm, 25.3 * units.deg)
        .add_sample(3600.0 * units.rpm, 28.1 * units.deg)
        .add_sample(4200.0 * units.rpm, 30.4 * units.deg)
        .add_sample(4800.0 * units.rpm, 32.3 * units.deg)
        .add_sample(5400.0 * units.rpm, 33.8 * units.deg)
        .add_sample(6000.0 * units.rpm, 35.0 * units.deg)
        .add_sample(6600.0 * units.rpm, -15.0 * units.deg)
        .add_sample(7200.0 * units.rpm, -60.0 * units.deg)
}


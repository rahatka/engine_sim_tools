private node add_lobes_0 {
    input base;
    input shift: 0.0;
    input var;
    input this;
    alias output __out: this;

    this.add_lobe(base + var.s1 * rot_base + shift)
    this.add_lobe(base + var.s3 * rot_base + shift)
    this.add_lobe(base + var.s5 * rot_base + shift)
    this.add_lobe(base + var.s7 * rot_base + shift)
}

private node add_lobes_1 {
    input base;
    input shift: 0.0;
    input var;
    input this;
    alias output __out: this;

    this.add_lobe(base + var.s2 * rot_base + shift)
    this.add_lobe(base + var.s4 * rot_base + shift)
    this.add_lobe(base + var.s6 * rot_base + shift)
}

public node bank_builder {
    input var;
    input incline;
    input phasing;
    input ignition;
    input intake;
    input exhaust;
    input rj0;
    input rj1;
    input rj2;
    input rj3;
    input rj4;
    input rj5;
    input rj6;
    input flip_display: false;

    output b0: _b0;
    output b1: _b1;

    ignition_wire _wire1()
    ignition_wire _wire2()
    ignition_wire _wire3()
    ignition_wire _wire4()
    ignition_wire _wire5()
    ignition_wire _wire6()
    ignition_wire _wire7()

    piston_parameters piston_params(
        mass: piston_mass * units.g,
        blowby: k_28inH2O(0.031),
        compression_height: compression_height,
        wrist_pin_position: 0.0,
        displacement: 0.0
    )

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    connecting_rod_parameters cr_params(
        mass: con_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.g,
            length: con_rod * units.mm
        ),
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    camshaft _intake_cam_0(base_radius: 20.0 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 20.0 * units.mm, lobe_profile: exhaust_lobe_profile)

    camshaft _intake_cam_1(base_radius: 20.0 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_1(base_radius: 20.0 * units.mm, lobe_profile: exhaust_lobe_profile)

    _intake_cam_0.add_lobes_0(ILC, phasing, var)
    _exhaust_cam_0.add_lobes_0(ELC, phasing, var)

    _intake_cam_1.add_lobes_1(ILC, phasing, var)
    _exhaust_cam_1.add_lobes_1(ELC, phasing, var)

    ignition.connect_wire(_wire1, rot_base * var.s1 + phasing)
    ignition.connect_wire(_wire2, rot_base * var.s2 + phasing)
    ignition.connect_wire(_wire3, rot_base * var.s3 + phasing)
    ignition.connect_wire(_wire4, rot_base * var.s4 + phasing)
    ignition.connect_wire(_wire5, rot_base * var.s5 + phasing)
    ignition.connect_wire(_wire6, rot_base * var.s6 + phasing)
    ignition.connect_wire(_wire7, rot_base * var.s7 + phasing)

    cylinder_bank _b0(bank_params, angle: incline + vree / 2)
    cylinder_bank _b1(bank_params, angle: incline - vree / 2)

    label spacing_factor(1.1)
    label flange_density(3.0)

    // label pl0 ((1.0 * bore * spacing_factor / flange_density) * units.mm)
    // label pl1 ((2.0 * bore * spacing_factor / flange_density) * units.mm)
    // label pl2 ((2.5 * bore * spacing_factor / flange_density) * units.mm)
    // label pl3 ((3.0 * bore * spacing_factor / flange_density) * units.mm)
    // label pl4 ((3.5 * bore * spacing_factor / flange_density) * units.mm)
	// label pl5 ((1.0 * bore * spacing_factor / flange_density) * units.mm)
	// label pl6 ((1.5 * bore * spacing_factor / flange_density) * units.mm)

    label pl0 ((4.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl1 ((2.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl2 ((3.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl3 ((1.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl4 ((3.0 * bore * spacing_factor / flange_density) * units.mm)
	label pl5 ((2.0 * bore * spacing_factor / flange_density) * units.mm)
	label pl6 ((4.0 * bore * spacing_factor / flange_density) * units.mm)

    _b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj0,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl0,
            ignition_wire: _wire1
        )
    _b1.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj1,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl1,
            ignition_wire: _wire2
        )
    _b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj2,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl2,
            ignition_wire: _wire3
        )
    _b1.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj3,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl3,
            ignition_wire: _wire4
        )
    _b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj4,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl4,
            ignition_wire: _wire5
        )
    _b1.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj5,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl5,
            ignition_wire: _wire6
        )
    _b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj6,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl6,
            ignition_wire: _wire7
        )

    _b0.set_cylinder_head(
        head(
            intake_camshaft: _intake_cam_0,
            exhaust_camshaft: _exhaust_cam_0,
            flip_display: flip_display
        )
    )
    _b1.set_cylinder_head(
        head(
            intake_camshaft: _intake_cam_1,
            exhaust_camshaft: _exhaust_cam_1,
            flip_display: flip_display
        )
    )
}

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node head {
    // port_flow.py v1.5
    // intake port area: 19.4 cm²; saturated lift: 8.62 mm
    // exhaust port area: 13.2 cm²; saturated lift: 7.11 mm
    // cylinder volume: 500.1 cm³ (30.5 CI); engine volume: 7.001 L (427.3 CI)
    // chamber volume: 45.5 cm³ (2.8 CI)
    // 8 harmonic intake runner length: 20.0 cm; diameter: 4.8 cm
    // primary length: 16.4 cm, area: 14.5 cm², diameter: 4.3 cm
    // collector diameter: 8.8 cm, area: 61.0 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 45.464 * units.cc,
        intake_runner_volume: 365.1 * units.cc,
        intake_runner_cross_section_area: 18.2 * units.cm2,
        exhaust_runner_volume: 146.0 * units.cc,
        exhaust_runner_cross_section_area: 14.5 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

public node crank {
    input phasing;
    input var;

    output c: _c;
    output rj0: _rj0;
    output rj2: _rj2;
    output rj4: _rj4;
    output rj6: _rj6;
    output rj8: _rj8;
    output rj10: _rj10;
    output rj12: _rj12;

    output rj1: _rj1;
    output rj3: _rj3;
    output rj5: _rj5;
    output rj7: _rj7;
    output rj9: _rj9;
    output rj11: _rj11;
    output rj13: _rj13;

    label crank_moment(disk_moment_of_inertia(mass: crank_mass * units.kg, radius: (stroke / 2.0) * units.mm))
    label flywheel_moment(disk_moment_of_inertia(mass: flywheel_mass * units.kg, radius: (stroke * 3.5) * units.mm))
    label other_moment(disk_moment_of_inertia(mass: 1 * units.kg, radius: 1.0 * units.cm))

    crankshaft _c(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: flywheel_mass * units.kg,
        mass: crank_mass * units.kg,
        friction_torque: 10.5 * units.Nm,
        moment_of_inertia: crank_moment + flywheel_moment + other_moment,
        tdc: 0.0
    )

    rod_journal _rj0(angle: rot_base * var.s1 + rot90 + vee / 2 + vree / 2)
    rod_journal _rj2(angle: rot_base * var.s2 + rot90 + vee / 2 - vree / 2)
    rod_journal _rj4(angle: rot_base * var.s3 + rot90 + vee / 2 + vree / 2)
    rod_journal _rj6(angle: rot_base * var.s4 + rot90 + vee / 2 - vree / 2)
    rod_journal _rj8(angle: rot_base * var.s5 + rot90 + vee / 2 + vree / 2)
    rod_journal _rj10(angle: rot_base * var.s6 + rot90 + vee / 2 - vree / 2)
    rod_journal _rj12(angle: rot_base * var.s7 + rot90 + vee / 2 + vree / 2)

    rod_journal _rj1(angle: rot_base * var.s1 + rot90 + vee / 2 + vree / 2 + rot180)
    rod_journal _rj3(angle: rot_base * var.s2 + rot90 + vee / 2 - vree / 2 + rot180)
    rod_journal _rj5(angle: rot_base * var.s3 + rot90 + vee / 2 + vree / 2 + rot180)
    rod_journal _rj7(angle: rot_base * var.s4 + rot90 + vee / 2 - vree / 2 + rot180)
    rod_journal _rj9(angle: rot_base * var.s5 + rot90 + vee / 2 + vree / 2 + rot180)
    rod_journal _rj11(angle: rot_base * var.s6 + rot90 + vee / 2 - vree / 2 + rot180)
    rod_journal _rj13(angle: rot_base * var.s7 + rot90 + vee / 2 + vree / 2 + rot180)

    _c
        .add_rod_journal(_rj0)
        .add_rod_journal(_rj1)
        .add_rod_journal(_rj2)
        .add_rod_journal(_rj3)
        .add_rod_journal(_rj4)
        .add_rod_journal(_rj5)
        .add_rod_journal(_rj6)
        .add_rod_journal(_rj7)
        .add_rod_journal(_rj8)
        .add_rod_journal(_rj9)
        .add_rod_journal(_rj10)
        .add_rod_journal(_rj11)
        .add_rod_journal(_rj12)
        .add_rod_journal(_rj13)
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "Boxer14 (f)",
        starter_torque: 125 * units.Nm,
        starter_speed: 400 * units.rpm,
        redline: redline * units.rpm,
        fuel: s90(),
        throttle_gamma: 1.75,
        jitter: 0.8,
        noise: 0.1,
        hf_gain: 0.007,
        simulation_frequency: 7000,
        fluid_simulation_steps: 8,
        max_sle_solver_steps: 64,
        dyno_min_speed: 1000 * units.rpm
    )

    //////////////////////////////
    //// change variants here ////
    //////////////////////////////
    variant_2 variant()

    crank c0(vee + rot90, variant)

    ignition_module ignition(timing_curve: timing_curve, rev_limit: 9750 * units.rpm, limiter_duration: 0.05)

    bank_builder bank0(
        var: variant,
        incline: vee / 2,
        phasing: 0,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust0,
        rj0: c0.rj0, rj1: c0.rj2, rj2: c0.rj4, rj3: c0.rj6, rj4: c0.rj8, rj5: c0.rj10, rj6: c0.rj12,
        flip_display: true
    )

    bank_builder bank1(
        var: variant,
        incline: -vee / 2,
        phasing: vee + rot180,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust1,
        rj0: c0.rj1, rj1: c0.rj3, rj2: c0.rj5, rj3: c0.rj7, rj4: c0.rj9, rj5: c0.rj11, rj6: c0.rj13
    )

    label collector_cross_section_area(30.5)

    label exhaust_pipe_length_0(300.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 100.0) // Litres
    
    label exhaust_pipe_length_1(300.0) // cm
    label exhaust_volume_1(collector_cross_section_area * exhaust_pipe_length_1 / 100.0) // Litres

    intake intake(
        plenum_volume: 10.0 * units.L,
        plenum_cross_section_area: 127.6 * units.cm2,
        runner_length: 20.0 * units.cm,
        intake_flow_rate: k_carb(1247.1),
        idle_flow_rate: k_carb(0.01),
        idle_throttle_plate_position: 0.995,
        runner_flow_rate: k_carb(200.4),
        velocity_decay: 0.03
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(1104.2),
        collector_cross_section_area: 30.5 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 16.4 * units.cm,
        primary_flow_rate: k_carb(335.2),
        velocity_decay: 0.5,
        volume: (exhaust_volume_0 + 15.0) * units.L
    )
    
    exhaust_system_parameters es_params1(
        outlet_flow_rate: k_carb(1104.2),
        collector_cross_section_area: 30.5 * units.cm2,
        length: exhaust_pipe_length_1 * units.cm,
        primary_tube_length: 16.4 * units.cm,
        primary_flow_rate: k_carb(335.2),
        velocity_decay: 0.5,
        volume: (exhaust_volume_1 + 15.0) * units.L
    )

    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_42.wav", volume: 0.001))
    exhaust_system exhaust1(es_params1, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_52.wav", volume: 0.001))

    engine
        .add_crankshaft(c0.c)
        .add_cylinder_bank(bank0.b0)
        .add_cylinder_bank(bank0.b1)
        .add_cylinder_bank(bank1.b0)
        .add_cylinder_bank(bank1.b1)
        .add_ignition_module(ignition)

    function timing_curve(633.3 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 5.0 * units.deg)
        .add_sample(633.3 * units.rpm, 5.0 * units.deg)
        .add_sample(1266.7 * units.rpm, 13.9 * units.deg)
        .add_sample(1900.0 * units.rpm, 22.2 * units.deg)
        .add_sample(2533.3 * units.rpm, 28.3 * units.deg)
        .add_sample(3166.7 * units.rpm, 32.7 * units.deg)
        .add_sample(3800.0 * units.rpm, 36.0 * units.deg)
        .add_sample(4433.3 * units.rpm, 38.4 * units.deg)
        .add_sample(5066.7 * units.rpm, 40.0 * units.deg)
        .add_sample(5700.0 * units.rpm, 40.0 * units.deg)
        .add_sample(6333.3 * units.rpm, 40.0 * units.deg)
        .add_sample(6966.7 * units.rpm, 40.0 * units.deg)
        .add_sample(7600.0 * units.rpm, 40.0 * units.deg)
        .add_sample(8233.3 * units.rpm, 40.0 * units.deg)
        .add_sample(8866.7 * units.rpm, 40.0 * units.deg)
        .add_sample(9500.0 * units.rpm, 40.0 * units.deg)
        .add_sample(10133.3 * units.rpm, -15.0 * units.deg)
        .add_sample(10766.7 * units.rpm, -60.0 * units.deg)
}


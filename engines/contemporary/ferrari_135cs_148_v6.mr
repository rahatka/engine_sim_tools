units units()
constants constants()

label bore(92.5)
label stroke(60.0)
label compression_ratio(9.0)
label con_rod(118.0)
label compression_height(30.0 * units.mm) // ?
label intake_valve_diameter(42.4)
label exhaust_valve_diameter(36.7)
label intake_valves(1.0)
label exhaust_valves(1.0)

label cyl(6.0)
label row(2.0)
label cycle(720.0 * units.deg)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label vee(65.0 * units.deg)

label comp(vee - rot)

label redline(7800.0) // RPM

// 246 valve timings
label IVL(9.35)
label EVL(8.55)
label IVO(40.0 * units.deg) //BTDC
label IVC(52.0 * units.deg) //ABDC
label EVO(53.0 * units.deg) //BBDC
label EVC(31.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(499.0) // g
label piston_mass(500.0) // ?
label crank_mass(12.0) // kg
label flywheel_mass(4.0) // kg

label exhaust_delay_coeff(1.5)


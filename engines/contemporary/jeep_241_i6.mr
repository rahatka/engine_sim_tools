public node eng {
    alias output __out: engine;

    engine engine(
        name: "Jeep 4.0",
        starter_torque: 200 * units.Nm,
        starter_speed: 400 * units.rpm,
        redline: redline * units.rpm,
        fuel: s90(),
        throttle_gamma: 1.5,
        jitter: 0.7,
        noise: 0.2,
        hf_gain: 0.005,
        simulation_frequency: 10000 
    )

    wires wires()

    connecting_rod_parameters cr_params(
        mass: con_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.g,
            length: con_rod * units.mm
        ),
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    label crank_moment(disk_moment_of_inertia(mass: crank_mass * units.kg, radius: (stroke / 2.0) * units.mm))
    label flywheel_moment(disk_moment_of_inertia(mass: flywheel_mass * units.kg, radius: (stroke * 3.5) * units.mm))
    label other_moment(disk_moment_of_inertia(mass: 1.0 * units.kg, radius: 1.0 * units.cm))

    crankshaft c0(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: flywheel_mass * units.kg,
        mass: crank_mass * units.kg,
        friction_torque: 5.9 * units.Nm,
        moment_of_inertia: crank_moment + flywheel_moment + other_moment,
        position_x: 0.0,
        position_y: 0.0,
        tdc: 0.0
    )

    rod_journal rj0(angle: rot * 0 + rot90)
    rod_journal rj1(angle: rot * 4 + rot90)
    rod_journal rj2(angle: rot * 2 + rot90)
    rod_journal rj3(angle: rot * 5 + rot90)
    rod_journal rj4(angle: rot * 1 + rot90)
    rod_journal rj5(angle: rot * 3 + rot90)
    
    c0
        .add_rod_journal(rj0)
        .add_rod_journal(rj1)
        .add_rod_journal(rj2)
        .add_rod_journal(rj3)
        .add_rod_journal(rj4)
        .add_rod_journal(rj5)

    piston_parameters piston_params(
        mass: 750.0 * units.g,
        blowby: k_28inH2O(0.031),
        compression_height: compression_height,
        wrist_pin_position: 0.0,
        displacement: 0.0
    )

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    label collector_cross_section_area(18.2)

    label exhaust_pipe_length_0(350.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 1000.0) // Litres
    
    label spacing_factor(1.1)
    label flange_density(1.0 * exhaust_delay_coeff)

    intake intake(
        plenum_volume: 2.7 * units.L,
        plenum_cross_section_area: 40.6 * units.cm2,
        runner_length: 19.1 * units.cm,
        intake_flow_rate: k_carb(346.1),
        idle_flow_rate: k_carb(0.005),
        idle_throttle_plate_position: 0.9975,
        runner_flow_rate: k_carb(115.4),
        velocity_decay: 0.05
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(576.9),
        collector_cross_section_area: 18.2 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 34.5 * units.cm,
        primary_flow_rate: k_carb(192.3),
        velocity_decay: 1.0,
        volume: exhaust_volume_0 * units.L
    )
 
    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_42.wav", volume: 0.001))

    label pl0((2.5 * bore * spacing_factor / flange_density) * units.mm)
    label pl1((1.1 * bore * spacing_factor / flange_density) * units.mm)
    label pl2((1.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl3((1.7 * bore * spacing_factor / flange_density) * units.mm)
    label pl4((2.7 * bore * spacing_factor / flange_density) * units.mm)
    label pl5((4.1 * bore * spacing_factor / flange_density) * units.mm)

    cylinder_bank b0(bank_params, angle: 0)

    b0
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj0,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl0,
            ignition_wire: wires.wire1
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj1,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl1,
            ignition_wire: wires.wire2
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj2,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl2,
            ignition_wire: wires.wire3
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj3,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl3,
            ignition_wire: wires.wire4
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj4,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl4,
            ignition_wire: wires.wire5
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj5,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl5,
            ignition_wire: wires.wire6
        )

    engine
        .add_cylinder_bank(b0)

    engine.add_crankshaft(c0)

    camshaft_builder camshaft()

    b0.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_0,
            exhaust_camshaft: camshaft.exhaust_cam_0
        )
    )

    function timing_curve(350.0 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 5.0 * units.deg)
        .add_sample(350.0 * units.rpm, 5.0 * units.deg)
        .add_sample(700.0 * units.rpm, 5.0 * units.deg)
        .add_sample(1050.0 * units.rpm, 11.7 * units.deg)
        .add_sample(1400.0 * units.rpm, 17.9 * units.deg)
        .add_sample(1750.0 * units.rpm, 22.8 * units.deg)
        .add_sample(2100.0 * units.rpm, 26.5 * units.deg)
        .add_sample(2450.0 * units.rpm, 29.5 * units.deg)
        .add_sample(2800.0 * units.rpm, 31.8 * units.deg)
        .add_sample(3150.0 * units.rpm, 33.6 * units.deg)
        .add_sample(3500.0 * units.rpm, 35.0 * units.deg)
        .add_sample(3850.0 * units.rpm, 35.0 * units.deg)
        .add_sample(4200.0 * units.rpm, 35.0 * units.deg)
        .add_sample(4550.0 * units.rpm, 35.0 * units.deg)
        .add_sample(4900.0 * units.rpm, 35.0 * units.deg)
        .add_sample(5250.0 * units.rpm, 35.0 * units.deg)
        .add_sample(5600.0 * units.rpm, -12.5 * units.deg)
        .add_sample(5950.0 * units.rpm, -25.0 * units.deg)

    engine.add_ignition_module(
        eng_distributor(
            wires: wires,
            timing_curve: timing_curve
        )
    )
}

// Jaguar D-Type
public node veh {
    alias output __out: vehicle;
    vehicle vehicle(
        mass: 950  * units.kg,
        drag_coefficient: 0.45,
        cross_sectional_area: (1660 * units.mm) * (813 * units.mm),
        diff_ratio: 3.54, // or 2.786
        tire_radius: (736.6 / 2) * units.mm, // 6.50 - 16"
        rolling_resistance: 20
        )
}

// Jaguar D-Type
private node trn {
    alias output __out:
        transmission(
            max_clutch_torque: 400 * units.Nm,
            max_clutch_flex: 10 * units.deg,
            limit_clutch_flex: true,
            clutch_stiffness: 20 * units.Nm / units.deg,
            clutch_damping: 2.0,
            simulate_flex: true
        )
        .add_gear(2.14)
        .add_gear(1.64)
        .add_gear(1.27)
        .add_gear(1.00);
}

run(
    engine: eng(),
    transmission: trn(),
    vehicle: veh()
)

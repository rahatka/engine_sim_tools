private node add_lobes {
    input base;
    input shift: 0.0;
    input this;
    alias output __out: this;

    this.add_lobe(base + 0 * rot * row + shift)
    this.add_lobe(base + 4 * rot * row + shift)
    this.add_lobe(base + 2 * rot * row + shift)
    this.add_lobe(base + 5 * rot * row + shift)
    this.add_lobe(base + 1 * rot * row + shift)
    this.add_lobe(base + 3 * rot * row + shift)
}

public node bank_builder {
    input incline;
    input phasing;
    input ignition;
    input intake;
    input exhaust;

    input rj0;
    input rj1;
    input rj2;
    input rj3;
    input rj4;
    input rj5;

    input flip_display: false;

    output b0: _b0;

    ignition_wire _wire1()
    ignition_wire _wire2()
    ignition_wire _wire3()
    ignition_wire _wire4()
    ignition_wire _wire5()
    ignition_wire _wire6()

    piston_parameters piston_params(
        mass: piston_mass * units.g,
        blowby: k_28inH2O(0.026),
        compression_height: compression_height,
        wrist_pin_position: 0.0,
        displacement: 0.0
    )

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    connecting_rod_parameters cr_params(
        mass: con_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.g,
            length: con_rod * units.mm
        ),
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    camshaft _intake_cam_0(base_radius: 15.8 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 15.8 * units.mm, lobe_profile: exhaust_lobe_profile)

    _intake_cam_0.add_lobes(ILC, phasing)
    _exhaust_cam_0.add_lobes(ELC, phasing)

    ignition.connect_wire(_wire1, rot_base * 0 + phasing)
    ignition.connect_wire(_wire5, rot_base * 1 + phasing)
    ignition.connect_wire(_wire3, rot_base * 2 + phasing)
    ignition.connect_wire(_wire6, rot_base * 3 + phasing)
    ignition.connect_wire(_wire2, rot_base * 4 + phasing)
    ignition.connect_wire(_wire4, rot_base * 5 + phasing)

    cylinder_bank _b0(bank_params, angle: incline)

    label spacing_factor(1.1)
    label flange_density(1.1 * exhaust_delay_coeff)
    label sh2 (0.3)
    label pl0 (((2.3 + sh2) * bore * spacing_factor / flange_density) * units.mm)
    label pl1 (((1.4 + sh2) * bore * spacing_factor / flange_density) * units.mm)
    label pl2 (((1.0 + sh2) * bore * spacing_factor / flange_density) * units.mm)
    label pl3 ((1.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl4 ((1.4 * bore * spacing_factor / flange_density) * units.mm)
    label pl5 ((2.3 * bore * spacing_factor / flange_density) * units.mm)

    _b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj0,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl0,
            ignition_wire: _wire1
        )
    _b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj1,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl1,
            ignition_wire: _wire2
        )
    _b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj2,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl2,
            ignition_wire: _wire3
        )
    _b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj3,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl3,
            ignition_wire: _wire4
        )
    _b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj4,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl4,
            ignition_wire: _wire5
        )
    _b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj5,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl5,
            ignition_wire: _wire6
        )

    _b0.set_cylinder_head(
        head(
            intake_camshaft: _intake_cam_0,
            exhaust_camshaft: _exhaust_cam_0,
            flip_display: flip_display
        )
    )
}

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node head {
    // port_flow.py v1.5
    // intake port area: 10.7 cm²; saturated lift: 6.41 mm
    // exhaust port area: 8.6 cm²; saturated lift: 5.73 mm
    // cylinder volume: 411.9 cm³ (25.1 CI); engine volume: 4.943 L (301.6 CI)
    // chamber volume: 50.2 cm³ (3.1 CI)
    // 8 harmonic intake runner length: 28.5 cm; diameter: 4.1 cm
    // primary length: 24.3 cm, area: 9.0 cm², diameter: 3.4 cm
    // collector diameter: 7.4 cm, area: 43.3 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 50.234 * units.cc,
        intake_runner_volume: 370.8 * units.cc,
        intake_runner_cross_section_area: 13.0 * units.cm2,
        exhaust_runner_volume: 109.0 * units.cc,
        exhaust_runner_cross_section_area: 9.0 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "Ferrari F113A",
        starter_torque: 200 * units.Nm,
        starter_speed: 500 * units.rpm,
        redline: redline * units.rpm,
        fuel: s90(),
        throttle_gamma: 2.0,
        jitter: 0.8,
        noise: 0.1,
        hf_gain: 0.008,
        simulation_frequency: 8000,
        dyno_min_speed: 1000 * units.rpm
    )

    ignition_module ignition(timing_curve: timing_curve, rev_limit: 6800 * units.rpm, limiter_duration: 0.075)

    bank_builder bank0(
        incline: vee / 2,
        phasing: 0,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust0,
        rj0: rj0, rj1: rj1, rj2: rj2, rj3: rj3, rj4: rj4, rj5: rj5,
        flip_display: true
    )

    bank_builder bank1(
        incline: -vee / 2,
        phasing: vee,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust1,
        rj0: rj0, rj1: rj1, rj2: rj2, rj3: rj3, rj4: rj4, rj5: rj5
    )

    rod_journal rj0(angle: rot_base * 0 + rot90 + vee / 2)
    rod_journal rj1(angle: rot_base * 4 + rot90 + vee / 2)
    rod_journal rj2(angle: rot_base * 2 + rot90 + vee / 2)
    rod_journal rj3(angle: rot_base * 5 + rot90 + vee / 2)
    rod_journal rj4(angle: rot_base * 1 + rot90 + vee / 2)
    rod_journal rj5(angle: rot_base * 3 + rot90 + vee / 2)

    label crank_moment(disk_moment_of_inertia(mass: crank_mass * units.kg, radius: (stroke / 2.0) * units.mm))
    label flywheel_moment(disk_moment_of_inertia(mass: flywheel_mass * units.kg, radius: (stroke * 3.5) * units.mm))
    label other_moment(disk_moment_of_inertia(mass: 1 * units.kg, radius: 1.0 * units.cm))

    crankshaft c0(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: flywheel_mass * units.kg,
        mass: crank_mass * units.kg,
        friction_torque: 7.4 * units.Nm,
        moment_of_inertia: crank_moment + flywheel_moment + other_moment,
        tdc: 0.0
    )
    
    c0
        .add_rod_journal(rj0)
        .add_rod_journal(rj1)
        .add_rod_journal(rj2)
        .add_rod_journal(rj3)
        .add_rod_journal(rj4)
        .add_rod_journal(rj5)

    label collector_cross_section_area(21.6)

    label exhaust_pipe_length_0(156.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 100.0) // Litres

    label exhaust_pipe_length_1(150.0) // cm
    label exhaust_volume_1(collector_cross_section_area * exhaust_pipe_length_0 / 100.0) // Litres

    intake intake(
        plenum_volume: 5.3 * units.L,
        plenum_cross_section_area: 78.1 * units.cm2,
        runner_length: 28.5 * units.cm,
        intake_flow_rate: k_carb(602.4),
        idle_flow_rate: k_carb(0.01),
        idle_throttle_plate_position: 0.996,
        runner_flow_rate: k_carb(113.0),
        velocity_decay: 0.05
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(533.4),
        collector_cross_section_area: 21.6 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 24.3 * units.cm,
        primary_flow_rate: k_carb(188.9),
        velocity_decay: 0.9,
        volume: (exhaust_volume_0 + 10.0) * units.L
    )

    exhaust_system_parameters es_params1(
        outlet_flow_rate: k_carb(533.4),
        collector_cross_section_area: 21.6 * units.cm2,
        length: exhaust_pipe_length_1 * units.cm,
        primary_tube_length: 24.3 * units.cm,
        primary_flow_rate: k_carb(188.9),
        velocity_decay: 0.9,
        volume: (exhaust_volume_1 + 10.0) * units.L
    )

    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_39.wav", volume: 0.001))
    exhaust_system exhaust1(es_params1, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_39.wav", volume: 0.001))

    engine
        .add_crankshaft(c0)
        .add_cylinder_bank(bank0.b0)
        .add_cylinder_bank(bank1.b0)
        .add_ignition_module(ignition)

    function timing_curve(433.3 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 6.0 * units.deg)
        .add_sample(433.3 * units.rpm, 6.0 * units.deg)
        .add_sample(866.7 * units.rpm, 6.0 * units.deg)
        .add_sample(1300.0 * units.rpm, 15.9 * units.deg)
        .add_sample(1733.3 * units.rpm, 26.0 * units.deg)
        .add_sample(2166.7 * units.rpm, 32.8 * units.deg)
        .add_sample(2600.0 * units.rpm, 37.2 * units.deg)
        .add_sample(3033.3 * units.rpm, 40.0 * units.deg)
        .add_sample(3466.7 * units.rpm, 40.0 * units.deg)
        .add_sample(3900.0 * units.rpm, 40.0 * units.deg)
        .add_sample(4333.3 * units.rpm, 40.0 * units.deg)
        .add_sample(4766.7 * units.rpm, 40.0 * units.deg)
        .add_sample(5200.0 * units.rpm, 40.0 * units.deg)
        .add_sample(5633.3 * units.rpm, 40.0 * units.deg)
        .add_sample(6066.7 * units.rpm, 40.0 * units.deg)
        .add_sample(6500.0 * units.rpm, 40.0 * units.deg)
        .add_sample(6933.3 * units.rpm, -12.5 * units.deg)
        .add_sample(7366.7 * units.rpm, -25.0 * units.deg)
}

// Testarossa
public node veh {
    alias output __out: vehicle;

    label car_mass(1600) // 1506 curb

    vehicle vehicle(
        mass: car_mass * units.kg,
        drag_coefficient: 0.36,
        cross_sectional_area: (1976 * units.mm) * (1130 * units.mm),
        diff_ratio: 3.21,
        tire_radius: (667.0 / 2) * units.mm, // 280/45 VR 415
        rolling_resistance: 0.012 * car_mass * 9.81
        )
}

// Testarossa
private node trn {
    alias output __out:
        transmission(
            max_clutch_torque: 600 * units.Nm,
            max_clutch_flex: 15 * units.deg,
            limit_clutch_flex: true,
            clutch_stiffness: 20 * units.Nm / units.deg,
            clutch_damping: 1.0,
            simulate_flex: true
        )
        .add_gear(3.139)
        .add_gear(2.104)
        .add_gear(1.526)
        .add_gear(1.167)
        .add_gear(0.875);
}

run(
    engine: eng(),
    transmission: trn(),
    vehicle: veh()
)

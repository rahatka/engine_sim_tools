import "engine_sim.mr"
import "../../fuel/90.mr"

units units()
constants constants()

label bore(83.0)
label stroke(81.0)
label compression_ratio(8.0)
label con_rod(141.0) // ?
label compression_height(33.3 * units.mm)
label intake_valve_diameter(33.5)
label exhaust_valve_diameter(30.0)
label intake_valves(2.0)
label exhaust_valves(2.0)

label cyl(8.0)
label row(2.0)
label cycle(720.0 * units.deg)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label vee(rot90)

label redline(7500.0) // RPM

// 57+51
label IVL(9.0)
label EVL(8.15)
label IVO(6.5 * units.deg) //BTDC
label IVC(58.5 * units.deg) //ABDC
label EVO(39.0 * units.deg) //BBDC
label EVC(11.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(535.0) // g
label crank_mass(22.0) // kg
label flywheel_mass(6.0) // kg

label exhaust_delay_coeff(1.5)


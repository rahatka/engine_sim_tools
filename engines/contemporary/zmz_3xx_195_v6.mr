private node add_lobes {
    input base;
    input shift: 0.0;
    input this;
    alias output __out: this;

    this.add_lobe(base + 0 * rot * row + shift)
    this.add_lobe(base + 1 * rot * row + shift)
    this.add_lobe(base + 2 * rot * row + shift)
}

public node camshaft_builder {
    output intake_cam_0: _intake_cam_0;
    output exhaust_cam_0: _exhaust_cam_0;

    output intake_cam_1: _intake_cam_1;
    output exhaust_cam_1: _exhaust_cam_1;

    camshaft _intake_cam_0(base_radius: 16.1 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 16.1 * units.mm, lobe_profile: exhaust_lobe_profile)
    
    camshaft _intake_cam_1(base_radius: 16.1 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_1(base_radius: 16.1 * units.mm, lobe_profile: exhaust_lobe_profile)

    _intake_cam_0.add_lobes(ILC)
    _exhaust_cam_0.add_lobes(ELC)

    _intake_cam_1.add_lobes(ILC, vee)
    _exhaust_cam_1.add_lobes(ELC, vee)
}

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node head {
    // port_flow.py v1.5
    // intake port area: 16.7 cm²; saturated lift: 7.93 mm
    // exhaust port area: 12.1 cm²; saturated lift: 6.75 mm
    // cylinder volume: 578.3 cm³ (35.3 CI); engine volume: 3.470 L (211.8 CI)
    // chamber volume: 68.0 cm³ (4.2 CI)
    // 16 harmonic intake runner length: 16.4 cm; diameter: 4.3 cm
    // primary length: 29.1 cm, area: 13.3 cm², diameter: 4.1 cm
    // collector diameter: 5.5 cm, area: 24.0 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 68.040 * units.cc,
        intake_runner_volume: 233.5 * units.cc,
        intake_runner_cross_section_area: 14.2 * units.cm2,
        exhaust_runner_volume: 77.8 * units.cc,
        exhaust_runner_cross_section_area: 13.3 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "ZMZ-3xx V6 (f)",
        starter_torque: 100 * units.Nm,
        starter_speed: 300 * units.rpm,
        redline: redline * units.rpm,
        fuel: s90(),
        throttle_gamma: 1.5,
        jitter: 0.7,
        noise: 0.2,
        hf_gain: 0.005,
        simulation_frequency: 12000 
    )

    wires wires()

    connecting_rod_parameters cr_params(
        mass: con_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.g,
            length: con_rod * units.mm
        ),
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    label crank_moment(disk_moment_of_inertia(mass: crank_mass * units.kg, radius: (stroke / 2.0) * units.mm))
    label flywheel_moment(disk_moment_of_inertia(mass: flywheel_mass * units.kg, radius: (stroke * 2.7) * units.mm))
    label other_moment(disk_moment_of_inertia(mass: 1 * units.kg, radius: 1.0 * units.cm))

    crankshaft c0(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: flywheel_mass * units.kg,
        mass: crank_mass * units.kg,
        friction_torque: 5.2 * units.Nm,
        moment_of_inertia: crank_moment + flywheel_moment + other_moment,
        position_x: 0.0,
        position_y: 0.0,
        tdc: 0.0
    )

    rod_journal rj0(angle: rot * 0 + vee / 2 + rot90)
    rod_journal rj1(angle: rot * 2 + vee / 2 + rot90)
    rod_journal rj2(angle: rot * 4 + vee / 2 + rot90)
    
    c0
        .add_rod_journal(rj0)
        .add_rod_journal(rj1)
        .add_rod_journal(rj2)

    piston_parameters piston_params(
        mass: piston_mass * units.g,
        blowby: k_28inH2O(0.029),
        compression_height: compression_height,
        wrist_pin_position: 0.0,
        displacement: 0.0
    )

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    label collector_cross_section_area(24.0)

    label exhaust_pipe_length_0(250.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 100.0) // Litres

    intake intake(
        plenum_volume: 3.0 * units.L,
        plenum_cross_section_area: 42.6 * units.cm2,
        runner_length: 16.4 * units.cm,
        intake_flow_rate: k_carb(394.7),
        idle_flow_rate: k_carb(0.03),
        idle_throttle_plate_position: 0.9965,
        runner_flow_rate: k_carb(138.2),
        velocity_decay: 0.05
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(674.3),
        collector_cross_section_area: 24.0 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 29.1 * units.cm,
        primary_flow_rate: k_carb(230.4),
        velocity_decay: 0.7,
        volume: (exhaust_volume_0 + 10.0) * units.L
    )

    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_56.wav", volume: 0.001))
 
    cylinder_bank b0(bank_params, angle: vee / 2)
    cylinder_bank b1(bank_params, angle: -vee / 2)

    em1 em()

    b0
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj0,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: em.pl0,
            ignition_wire: wires.wire1
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj1,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: em.pl1,
            ignition_wire: wires.wire2
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj2,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: em.pl2,
            ignition_wire: wires.wire3
        )
        
    b1
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj0,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: em.pl3,
            ignition_wire: wires.wire4
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj1,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: em.pl4,
            ignition_wire: wires.wire5
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj2,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: em.pl5,
            ignition_wire: wires.wire6
        )

    engine
        .add_cylinder_bank(b0)
        .add_cylinder_bank(b1)

    engine.add_crankshaft(c0)

    camshaft_builder camshaft()

    b0.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_0,
            exhaust_camshaft: camshaft.exhaust_cam_0,
            flip_display: true
        )
    )
    b1.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_1,
            exhaust_camshaft: camshaft.exhaust_cam_1
        )
    )

    function timing_curve(433.3 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 7.0 * units.deg)
        .add_sample(433.3 * units.rpm, 7.0 * units.deg)
        .add_sample(866.7 * units.rpm, 15.0 * units.deg)
        .add_sample(1300.0 * units.rpm, 22.3 * units.deg)
        .add_sample(1733.3 * units.rpm, 27.7 * units.deg)
        .add_sample(2166.7 * units.rpm, 31.8 * units.deg)
        .add_sample(2600.0 * units.rpm, 34.9 * units.deg)
        .add_sample(3033.3 * units.rpm, 37.0 * units.deg)
        .add_sample(3466.7 * units.rpm, 37.0 * units.deg)
        .add_sample(3900.0 * units.rpm, 37.0 * units.deg)
        .add_sample(4333.3 * units.rpm, 37.0 * units.deg)
        .add_sample(4766.7 * units.rpm, 37.0 * units.deg)
        .add_sample(5200.0 * units.rpm, 37.0 * units.deg)
        .add_sample(5633.3 * units.rpm, 37.0 * units.deg)
        .add_sample(6066.7 * units.rpm, 37.0 * units.deg)
        .add_sample(6500.0 * units.rpm, 37.0 * units.deg)
        .add_sample(6933.3 * units.rpm, -15.0 * units.deg)
        .add_sample(7366.7 * units.rpm, -45.0 * units.deg)

    engine.add_ignition_module(
        eng_distributor(
            wires: wires,
            timing_curve: timing_curve
        )
    )
}

label spacing_factor(1.1)
label flange_density(1.2 * exhaust_delay_coeff)
label em_unit(bore * spacing_factor / flange_density)

private node em0{
    output pl0: (4.0 * em_unit) * units.mm;
    output pl1: (3.0 * em_unit) * units.mm;
    output pl2: (2.0 * em_unit) * units.mm;
    output pl3: (1.0 * em_unit) * units.mm;
    output pl4: (2.0 * em_unit) * units.mm;
    output pl5: (3.0 * em_unit) * units.mm;
}

private node em1{
    output pl0: (3.0 * em_unit) * units.mm;
    output pl1: (4.0 * em_unit) * units.mm;
    output pl2: (5.0 * em_unit) * units.mm;
    output pl3: (3.0 * em_unit) * units.mm;
    output pl4: (2.0 * em_unit) * units.mm;
    output pl5: (1.0 * em_unit) * units.mm;
}

private node em2{
    output pl0: (2.8 * em_unit) * units.mm;
    output pl1: (2.0 * em_unit) * units.mm;
    output pl2: (2.8 * em_unit) * units.mm;
    output pl3: (1.8 * em_unit) * units.mm;
    output pl4: (1.0 * em_unit) * units.mm;
    output pl5: (1.8 * em_unit) * units.mm;
}

// ГАЗ-3111
public node veh {
    alias output __out: vehicle;

    label car_mass(1700) // 1550 empty; 2030 full

    vehicle vehicle(
        mass: car_mass * units.kg,
        drag_coefficient: 0.38, // ?
        cross_sectional_area: (1850 * units.mm) * (1450 * units.mm),
        diff_ratio: 3.9,
        tire_radius: (647.0 / 2) * units.mm, // 205/65 R15
        rolling_resistance: 0.012 * car_mass * 9.81
        )
}

// ГАЗ-3111
private node trn {
    alias output __out:
        transmission(
            max_clutch_torque: 400 * units.Nm,
            max_clutch_flex: 10 * units.deg,
            limit_clutch_flex: true,
            clutch_stiffness: 10 * units.Nm / units.deg,
            clutch_damping: 1.0,
            simulate_flex: true
        )
        .add_gear(4.05)
        .add_gear(2.34)
        .add_gear(1.40)
        .add_gear(1.00)
        .add_gear(0.85);
}

run(
    engine: eng(),
    transmission: trn(),
    vehicle: veh()
)

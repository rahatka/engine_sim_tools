public node camshaft_builder {
    output intake_cam_0: _intake_cam_0;
    output exhaust_cam_0: _exhaust_cam_0;

    camshaft _intake_cam_0(base_radius: 14.8 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 14.8 * units.mm, lobe_profile: exhaust_lobe_profile)

    _intake_cam_0
        .add_lobe(ILC + 0 * rot)
        .add_lobe(ILC + 4 * rot)
        .add_lobe(ILC + 2 * rot)
        .add_lobe(ILC + 5 * rot)
        .add_lobe(ILC + 1 * rot)
        .add_lobe(ILC + 3 * rot)
    _exhaust_cam_0
        .add_lobe(ELC + 0 * rot)
        .add_lobe(ELC + 4 * rot)
        .add_lobe(ELC + 2 * rot)
        .add_lobe(ELC + 5 * rot)
        .add_lobe(ELC + 1 * rot)
        .add_lobe(ELC + 3 * rot)
}

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node head {
    // port_flow.py v1.5
    // intake port area: 16.2 cm²; saturated lift: 7.87 mm
    // exhaust port area: 12.2 cm²; saturated lift: 6.85 mm
    // cylinder volume: 428.1 cm³ (26.12 CI); engine volume: 2.569 L (156.75 CI)
    // chamber volume: 57.1 cm³ (3.48 CI)
    // 8 harmonic intake runner length: 22.1 cm; diameter: 4.6 cm
    // primary length: 18.4 cm, area: 13.5 cm², diameter: 4.1 cm
    // collector diameter: 5.6 cm, area: 24.2 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 57.081 * units.cc,
        intake_runner_volume: 360.7 * units.cc,
        intake_runner_cross_section_area: 16.3 * units.cm2,
        exhaust_runner_volume: 133.6 * units.cc,
        exhaust_runner_cross_section_area: 13.5 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "Nissan RB26DETT NA",
        starter_torque: 300 * units.Nm,
        starter_speed: 150 * units.rpm,
        redline: redline * units.rpm,
        fuel: s90(),
        throttle_gamma: 1.5,
        jitter: 0.5,
        noise: 0.1,
        hf_gain: 0.001,
        simulation_frequency: 12000 
    )

    wires wires()

    connecting_rod_parameters cr_params(
        mass: con_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.g,
            length: con_rod * units.mm
        ),
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    label crank_moment(
         disk_moment_of_inertia(mass: crank_mass * units.kg, radius: (stroke / 2.0) * units.mm)
    )
    label flywheel_moment(
        disk_moment_of_inertia(mass: flywheel_mass * units.kg, radius: (stroke * 3.0) * units.mm)
    )
    label other_moment(
        disk_moment_of_inertia(mass: 1.0 * units.kg, radius: 1.0 * units.cm)
    )

    crankshaft c0(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: flywheel_mass * units.kg,
        mass: crank_mass * units.kg,
        friction_torque: 3.9 * units.Nm,
        moment_of_inertia: crank_moment + flywheel_moment + other_moment,
        position_x: 0.0,
        position_y: 0.0,
        tdc: 0.0 * units.deg
    )

    rod_journal rj0(angle: rot * 0 + rot90)
    rod_journal rj1(angle: rot * 4 + rot90)
    rod_journal rj2(angle: rot * 2 + rot90)
    rod_journal rj3(angle: rot * 5 + rot90)
    rod_journal rj4(angle: rot * 1 + rot90)
    rod_journal rj5(angle: rot * 3 + rot90)
    
    c0
        .add_rod_journal(rj0)
        .add_rod_journal(rj1)
        .add_rod_journal(rj2)
        .add_rod_journal(rj3)
        .add_rod_journal(rj4)
        .add_rod_journal(rj5)

    piston_parameters piston_params(
        mass: 368.0 * units.g,
        blowby: k_28inH2O(0.027),
        compression_height: compression_height,
        wrist_pin_position: 0.0,
        displacement: 0.0
    )

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    label collector_cross_section_area(24.2)

    label exhaust_pipe_length_0(400.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 100.0) // Litres

    intake intake(
        plenum_volume: 3.9 * units.L,
        plenum_cross_section_area: 48.9 * units.cm2,
        intake_flow_rate: k_carb(428.1),
        idle_flow_rate: k_carb(0.01),
        idle_throttle_plate_position: 0.997,
        runner_flow_rate: k_carb(178.4),
        runner_length: 22.1 * units.cm,
        velocity_decay: 0.05
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(802.7),
        collector_cross_section_area: 24.2 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 18.4 * units.cm,
        primary_flow_rate: k_carb(301.0),
        velocity_decay: 1.0,
        volume: exhaust_volume_0 * units.L
    )
 
    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_39.wav", volume: 0.001))

    label spacing_factor(1.3)
    label flange_density(1.0 * exhaust_delay_coeff)

    // RB26 ST
    label pl0 ((2.6 * bore * spacing_factor / flange_density) * units.mm)
    label pl1 ((1.8 * bore * spacing_factor / flange_density) * units.mm)
    label pl2 ((1.1 * bore * spacing_factor / flange_density) * units.mm)
    label pl3 ((1.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl4 ((1.5 * bore * spacing_factor / flange_density) * units.mm)
    label pl5 ((2.2 * bore * spacing_factor / flange_density) * units.mm)

    // label pl0 ((3.2 * bore * spacing_factor / flange_density) * units.mm)
    // label pl1 ((2.2 * bore * spacing_factor / flange_density) * units.mm)
    // label pl2 ((1.2 * bore * spacing_factor / flange_density) * units.mm)
    // label pl3 ((3.0 * bore * spacing_factor / flange_density) * units.mm)
    // label pl4 ((2.0 * bore * spacing_factor / flange_density) * units.mm)
    // label pl5 ((1.0 * bore * spacing_factor / flange_density) * units.mm)

    cylinder_bank b0(bank_params, angle: 0)

    b0
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj0,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl0,
            ignition_wire: wires.wire1
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj1,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl1,
            ignition_wire: wires.wire2
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj2,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl2,
            ignition_wire: wires.wire3
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj3,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl3,
            ignition_wire: wires.wire4
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj4,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl4,
            ignition_wire: wires.wire5
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj5,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl5,
            ignition_wire: wires.wire6
        )

    engine
        .add_cylinder_bank(b0)

    engine.add_crankshaft(c0)

    camshaft_builder camshaft()

    b0.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_0,
            exhaust_camshaft: camshaft.exhaust_cam_0
        )
    )

    function timing_curve(533.3 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 5.0 * units.deg)
        .add_sample(533.3 * units.rpm, 5.6 * units.deg)
        .add_sample(1066.7 * units.rpm, 14.2 * units.deg)
        .add_sample(1600.0 * units.rpm, 20.9 * units.deg)
        .add_sample(2133.3 * units.rpm, 26.2 * units.deg)
        .add_sample(2666.7 * units.rpm, 30.3 * units.deg)
        .add_sample(3200.0 * units.rpm, 33.5 * units.deg)
        .add_sample(3733.3 * units.rpm, 36.0 * units.deg)
        .add_sample(4266.7 * units.rpm, 38.0 * units.deg)
        .add_sample(4800.0 * units.rpm, 39.5 * units.deg)
        .add_sample(5333.3 * units.rpm, 40.0 * units.deg)
        .add_sample(5866.7 * units.rpm, 40.0 * units.deg)
        .add_sample(6400.0 * units.rpm, 40.0 * units.deg)
        .add_sample(6933.3 * units.rpm, 40.0 * units.deg)
        .add_sample(7466.7 * units.rpm, 40.0 * units.deg)
        .add_sample(8000.0 * units.rpm, 40.0 * units.deg)
        .add_sample(8533.3 * units.rpm, -15.0 * units.deg)
        .add_sample(9066.7 * units.rpm, -60.0 * units.deg)

    engine.add_ignition_module(
        eng_distributor(
            wires: wires,
            timing_curve: timing_curve
        )
    )
}

// R34
public node veh {
    alias output __out: vehicle;
    vehicle vehicle(
        mass: 1800 * units.kg,
        drag_coefficient: 0.36,
        cross_sectional_area: (1785 * units.mm) * (1360 * units.mm),
        diff_ratio: 3.545,
        tire_radius: (649 / 2) * units.mm, // 275/35ZR18
        rolling_resistance: 20
        )
}

// R34
private node trn {
    alias output __out:
        transmission(
            max_clutch_torque: 400 * units.Nm,
            max_clutch_flex: 10 * units.deg,
            limit_clutch_flex: true,
            clutch_stiffness: 20 * units.Nm / units.deg,
            clutch_damping: 2.0,
            simulate_flex: true
        )
        .add_gear(3.827)
        .add_gear(2.360)
        .add_gear(1.685)
        .add_gear(1.312)
        .add_gear(1.000)
        .add_gear(0.793);
}

run(
    engine: eng(),
    transmission: trn(),
    vehicle: veh()
)

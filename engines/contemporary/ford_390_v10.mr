label compression_height(30.0 * units.mm) // ?
label intake_valve_diameter(37.0) // ? 1.457 4.6 4V
label exhaust_valve_diameter(30.0) // ? 1.181 4.6 4V
label intake_valves(2.0)
label exhaust_valves(2.0)

label cyl(10.0)
label row(2.0)
label cycle(720.0 * units.deg)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label vee(90.0 * units.deg)

// label comp(vee - rot)
label comp(0)

label redline(7500.0) // RPM

// 240/250 114 LSA 4.6 4V spec
label IVL(10.0) // 0.394
label EVL(10.0) // 0.394

private node add_lobes_0 {
    input base;
    input shift: 0.0;
    input var;
    input this;
    alias output __out: this;

    this.add_lobe(base + var.s1 * rot_base_0 + shift)
    this.add_lobe(base + var.s2 * rot_base_0 + shift)
    this.add_lobe(base + var.s3 * rot_base_0 + shift)
}

private node add_lobes_1 {
    input base;
    input shift: 0.0;
    input var;
    input this;
    alias output __out: this;

    this.add_lobe(base + var.s1 * rot_base_1 + shift)
    this.add_lobe(base + var.s2 * rot_base_1 + shift)
}

public node bank_builder {
    input var0;
    input var1;
    input incline;
    input phasing;
    input ignition;
    input intake;
    input exhaust;
    input sh;
    input rj0;
    input rj1;
    input rj2;
    input rj3;
    input rj4;
    input flip_display: false;

    output b0: _b0;
    output b1: _b1;

    ignition_wire _wire1()
    ignition_wire _wire2()
    ignition_wire _wire3()
    ignition_wire _wire4()
    ignition_wire _wire5()

    piston_parameters piston_params(
        mass: piston_mass * units.g,
        blowby: k_28inH2O(0.031),
        compression_height: compression_height,
        wrist_pin_position: 0.0,
        displacement: 0.0
    )

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    connecting_rod_parameters cr_params(
        mass: con_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.g,
            length: con_rod * units.mm
        ),
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    camshaft _intake_cam_0(base_radius: 20.0 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 20.0 * units.mm, lobe_profile: exhaust_lobe_profile)

    camshaft _intake_cam_1(base_radius: 20.0 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_1(base_radius: 20.0 * units.mm, lobe_profile: exhaust_lobe_profile)

    _intake_cam_0.add_lobes_0(ILC, phasing, var0)
    _exhaust_cam_0.add_lobes_0(ELC, phasing, var0)

    _intake_cam_1.add_lobes_1(ILC, phasing + phase, var1)
    _exhaust_cam_1.add_lobes_1(ELC, phasing + phase, var1)

    ignition.connect_wire(_wire1, rot_base_0 * var0.s1 + phasing)
    ignition.connect_wire(_wire2, rot_base_1 * var1.s1 + phasing + phase)
    ignition.connect_wire(_wire3, rot_base_0 * var0.s2 + phasing)
    ignition.connect_wire(_wire4, rot_base_1 * var1.s2 + phasing + phase)
    ignition.connect_wire(_wire5, rot_base_0 * var0.s3 + phasing)

    cylinder_bank _b0(bank_params, angle: incline + vree / 2)
    cylinder_bank _b1(bank_params, angle: incline - vree / 2)

    label spacing_factor(0.75)
    label flange_density(3.0)

    label pl0 ((1.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl2 ((1.1 * bore * spacing_factor / flange_density) * units.mm)
    label pl4 ((1.2 * bore * spacing_factor / flange_density) * units.mm)

    label pl1 ((1.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl3 ((1.1 * bore * spacing_factor / flange_density) * units.mm)

    _b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj0,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl0 + sh,
            ignition_wire: _wire1
        )
    _b1.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj1,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl1 + sh,
            ignition_wire: _wire2
        )
    _b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj2,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl2 + sh,
            ignition_wire: _wire3
        )
    _b1.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj3,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl3 + sh,
            ignition_wire: _wire4
        )
    _b0.add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj4,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl4 + sh,
            ignition_wire: _wire5
        )

    _b0.set_cylinder_head(
        head(
            intake_camshaft: _intake_cam_0,
            exhaust_camshaft: _exhaust_cam_0,
            flip_display: flip_display
        )
    )
    _b1.set_cylinder_head(
        head(
            intake_camshaft: _intake_cam_1,
            exhaust_camshaft: _exhaust_cam_1,
            flip_display: flip_display
        )
    )
}

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node head {
    // port_flow.py v1.5
    // intake port area: 19.0 cm²; saturated lift: 8.50 mm
    // exhaust port area: 12.9 cm²; saturated lift: 7.01 mm
    // cylinder volume: 500.1 cm³ (30.5 CI); engine volume: 5.001 L (305.2 CI)
    // chamber volume: 45.5 cm³ (2.8 CI)
    // 8 harmonic intake runner length: 18.9 cm; diameter: 5.1 cm
    // primary length: 15.4 cm, area: 14.2 cm², diameter: 4.3 cm
    // collector diameter: 7.4 cm, area: 42.7 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 45.464 * units.cc,
        intake_runner_volume: 380.5 * units.cc,
        intake_runner_cross_section_area: 20.1 * units.cm2,
        exhaust_runner_volume: 126.8 * units.cc,
        exhaust_runner_cross_section_area: 14.2 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

public node crank {
    input phasing;
    input var0;
    input var1;

    output c: _c;
    output rj0: _rj0;
    output rj2: _rj2;
    output rj4: _rj4;
    output rj6: _rj6;
    output rj8: _rj8;

    output rj1: _rj1;
    output rj3: _rj3;
    output rj5: _rj5;
    output rj7: _rj7;
    output rj9: _rj9;

    label crank_moment(disk_moment_of_inertia(mass: crank_mass * units.kg, radius: (stroke / 2.0) * units.mm))
    label flywheel_moment(disk_moment_of_inertia(mass: flywheel_mass * units.kg, radius: (stroke * 3.5) * units.mm))
    label other_moment(disk_moment_of_inertia(mass: 1 * units.kg, radius: 1.0 * units.cm))

    crankshaft _c(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: flywheel_mass * units.kg,
        mass: crank_mass * units.kg,
        friction_torque: 7.5 * units.Nm,
        moment_of_inertia: crank_moment + flywheel_moment + other_moment,
        tdc: 0.0
    )

    rod_journal _rj0(angle: rot_base_0 * var0.s1 + rot90 + vee / 2 + vree / 2)
    rod_journal _rj2(angle: rot_base_1 * var1.s1 + rot90 + vee / 2 - vree / 2 + phase)
    rod_journal _rj4(angle: rot_base_0 * var0.s2 + rot90 + vee / 2 + vree / 2)
    rod_journal _rj6(angle: rot_base_1 * var1.s2 + rot90 + vee / 2 - vree / 2 + phase)
    rod_journal _rj8(angle: rot_base_0 * var0.s3 + rot90 + vee / 2 + vree / 2)

    rod_journal _rj1(angle: rot_base_0 * var0.s1 + rot90 + vee / 2 + vree / 2 + rot180)
    rod_journal _rj3(angle: rot_base_1 * var1.s1 + rot90 + vee / 2 - vree / 2 + rot180 + phase)
    rod_journal _rj5(angle: rot_base_0 * var0.s2 + rot90 + vee / 2 + vree / 2 + rot180)
    rod_journal _rj7(angle: rot_base_1 * var1.s2 + rot90 + vee / 2 - vree / 2 + rot180 + phase)
    rod_journal _rj9(angle: rot_base_0 * var0.s3 + rot90 + vee / 2 + vree / 2 + rot180)

    _c
        .add_rod_journal(_rj0)
        .add_rod_journal(_rj1)
        .add_rod_journal(_rj2)
        .add_rod_journal(_rj3)
        .add_rod_journal(_rj4)
        .add_rod_journal(_rj5)
        .add_rod_journal(_rj6)
        .add_rod_journal(_rj7)
        .add_rod_journal(_rj8)
        .add_rod_journal(_rj9)
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "3:2",
        starter_torque: 125 * units.Nm,
        starter_speed: 400 * units.rpm,
        redline: redline * units.rpm,
        fuel: s90(),
        throttle_gamma: 1.75,
        jitter: 0.5,
        noise: 0.1,
        hf_gain: 0.003,
        simulation_frequency: 7000,
        fluid_simulation_steps: 8,
        max_sle_solver_steps: 64,
        dyno_min_speed: 1000 * units.rpm
    )

    //////////////////////////////
    //// change variants here ////
    //////////////////////////////
    variant_0 variant0()
    variant_1 variant1()

    crank c0(vee + rot90, variant0, variant1)

    ignition_module ignition(timing_curve: timing_curve, rev_limit: 9750 * units.rpm, limiter_duration: 0.05)

    bank_builder bank0(
        var0: variant0,
        var1: variant1,
        incline: vee / 2,
        phasing: 0,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust0,
        sh: 0 * units.cm,
        rj0: c0.rj0, rj1: c0.rj2, rj2: c0.rj4, rj3: c0.rj6, rj4: c0.rj8,
        flip_display: true
    )

    bank_builder bank1(
        var0: variant0,
        var1: variant1,
        incline: -vee / 2,
        phasing: vee + rot180,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust0,
        sh: 1.2 * units.cm,
        rj0: c0.rj1, rj1: c0.rj3, rj2: c0.rj5, rj3: c0.rj7, rj4: c0.rj9
    )

    label collector_cross_section_area(42.7)

    label exhaust_pipe_length_0(300.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 100.0) // Litres

    intake intake(
        plenum_volume: 8.4 * units.L,
        plenum_cross_section_area: 100.5 * units.cm2,
        runner_length: 18.9 * units.cm,
        intake_flow_rate: k_carb(989.8),
        idle_flow_rate: k_carb(0.015),
        idle_throttle_plate_position: 0.998,
        runner_flow_rate: k_carb(247.4),
        velocity_decay: 0.03
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(1855.8),
        collector_cross_section_area: 42.7 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 15.4 * units.cm,
        primary_flow_rate: k_carb(417.6),
        velocity_decay: 1.0,
        volume: (exhaust_volume_0 + 5.0) * units.L
    )

    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_40.wav", volume: 0.001))

    engine
        .add_crankshaft(c0.c)
        .add_cylinder_bank(bank0.b0)
        .add_cylinder_bank(bank0.b1)
        .add_cylinder_bank(bank1.b0)
        .add_cylinder_bank(bank1.b1)
        .add_ignition_module(ignition)

    function timing_curve(633.3 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 5.0 * units.deg)
        .add_sample(633.3 * units.rpm, 5.0 * units.deg)
        .add_sample(1266.7 * units.rpm, 14.7 * units.deg)
        .add_sample(1900.0 * units.rpm, 29.3 * units.deg)
        .add_sample(2533.3 * units.rpm, 36.9 * units.deg)
        .add_sample(3166.7 * units.rpm, 40.0 * units.deg)
        .add_sample(3800.0 * units.rpm, 40.0 * units.deg)
        .add_sample(4433.3 * units.rpm, 40.0 * units.deg)
        .add_sample(5066.7 * units.rpm, 40.0 * units.deg)
        .add_sample(5700.0 * units.rpm, 40.0 * units.deg)
        .add_sample(6333.3 * units.rpm, 40.0 * units.deg)
        .add_sample(6966.7 * units.rpm, 40.0 * units.deg)
        .add_sample(7600.0 * units.rpm, 40.0 * units.deg)
        .add_sample(8233.3 * units.rpm, 40.0 * units.deg)
        .add_sample(8866.7 * units.rpm, 40.0 * units.deg)
        .add_sample(9500.0 * units.rpm, 40.0 * units.deg)
        .add_sample(10133.3 * units.rpm, -12.5 * units.deg)
        .add_sample(10766.7 * units.rpm, -25.0 * units.deg)
}


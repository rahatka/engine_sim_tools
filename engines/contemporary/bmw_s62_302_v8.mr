label compression_ratio(11.0)
label con_rod(141.5)
label compression_height(31.62 * units.mm)
label intake_valve_diameter(35.0)
label exhaust_valve_diameter(30.5)
label intake_valves(2.0)
label exhaust_valves(2.0)

label redline(7000.0) // RPM

label cyl(8.0)
label row(2.0)
label cycle(720.0 * units.deg)

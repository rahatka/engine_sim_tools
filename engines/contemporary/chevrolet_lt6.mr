import "engine_sim.mr"
import "_fuel_30.mr"

units units()
constants constants()

label bore(104.2416)
label stroke(80.01)
label compression_ratio(12.5)
label con_rod(145.0)
label compression_height(31.0134 * units.mm)
label intake_valve_diameter(42.0116)
label exhaust_valve_diameter(35.0012)
label intake_valves(2.0)
label exhaust_valves(2.0)

label volume(5.463) // L; port_flow.py v0.6
label redline(8700.0) // RPM

label cyl(8.0)
label row(2.0)
label cycle(720.0 * units.deg)
label vee(90.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// COMP Cams Xtreme Energy Camshafts 07-305-8
label IVL(13.9954)
label EVL(13.3096)
label IVO(24.0 * units.deg) //BTDC
label IVC(72.0 * units.deg) //ABDC
label EVO(79.0 * units.deg) //BBDC
label EVC(31.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(364.0) // g
label crank_mass(21.3188) // kg
label flywheel_mass(6.0) // kg

public node distributor {
    input wires;
    input timing_curve;
    input rev_limit: 8700 * units.rpm;
    input limiter_duration: 0.09;
    alias output __out:
        ignition_module(timing_curve: timing_curve, rev_limit: rev_limit, limiter_duration: limiter_duration)
            .connect_wire(wires.wire1, rot * 0)
            .connect_wire(wires.wire2, rot * 2)
            .connect_wire(wires.wire4, rot * 4)
            .connect_wire(wires.wire3, rot * 6)
            .connect_wire(wires.wire5, rot * 0 + vee)
            .connect_wire(wires.wire6, rot * 2 + vee)
            .connect_wire(wires.wire8, rot * 4 + vee)
            .connect_wire(wires.wire7, rot * 6 + vee);
}

private node wires {
    output wire1: ignition_wire();
    output wire2: ignition_wire();
    output wire3: ignition_wire();
    output wire4: ignition_wire();
    output wire5: ignition_wire();
    output wire6: ignition_wire();
    output wire7: ignition_wire();
    output wire8: ignition_wire();
}


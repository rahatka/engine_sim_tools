label bore(90.2)
label stroke(90.0)
label compression_ratio(9.9)
label con_rod(150.7)
label compression_height(31.0 * units.mm)
label intake_valve_diameter(37.0)
label exhaust_valve_diameter(30.0)
label intake_valves(2.0)
label exhaust_valves(2.0)

label redline(6500.0) // RPM

label cyl(8.0)
label row(2.0)
label cycle(720.0 * units.deg)
label vee(90.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// TFS-K51802001 @ 50 thou
label IVL(13.97)
label EVL(13.97)
label IVO(5.0 * units.deg) //BTDC
label IVC(43.0 * units.deg) //ABDC
label EVO(50.0 * units.deg) //BBDC
label EVC(0.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label exhaust_delay_coeff(1.5)

// AMC, Most V8s
// Buick exc Nailhead
// Chevy exc LS
// Pontiac Most V8
// Chrysler Small Block
// Chrysler Big Block & HEMI
// Modern HEMI
// Olds
// Ford 289 302 FE 429 460
// front 2-4-6-8
//       1-3-5-7
// 1-8-4-3-6-5-7-2
// 1-5-4-2-6-3-7-8
public node distributor_0 {
    input wires;
    input timing_curve;
    input rev_limit: 6750 * units.rpm;
    input limiter_duration: 0.0001;
    alias output __out:
        ignition_module(timing_curve: timing_curve, rev_limit: rev_limit, limiter_duration: limiter_duration)
            .connect_wire(wires.wire1, rot * 0)
            .connect_wire(wires.wire5, rot * 1 + (vee - rot90))
            .connect_wire(wires.wire4, rot * 2)
            .connect_wire(wires.wire2, rot * 3)
            .connect_wire(wires.wire6, rot * 4 + (vee - rot90))
            .connect_wire(wires.wire3, rot * 5)
            .connect_wire(wires.wire7, rot * 6 + (vee - rot90))
            .connect_wire(wires.wire8, rot * 7 + (vee - rot90));
}


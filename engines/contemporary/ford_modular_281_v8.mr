            .connect_wire(wires.wire1, rot * 0)
            .connect_wire(wires.wire5, rot * 1 + (vee - rot90))
            .connect_wire(wires.wire4, rot * 2)
            .connect_wire(wires.wire8, rot * 3 + (vee - rot90))
            .connect_wire(wires.wire6, rot * 4 + (vee - rot90))
            .connect_wire(wires.wire3, rot * 5)
            .connect_wire(wires.wire7, rot * 6 + (vee - rot90))
            .connect_wire(wires.wire2, rot * 7);
}


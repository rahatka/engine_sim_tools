public node camshaft_builder {
    output intake_cam_0: _intake_cam_0;
    output exhaust_cam_0: _exhaust_cam_0;

    camshaft _intake_cam_0(base_radius: 16.1 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 16.1 * units.mm, lobe_profile: exhaust_lobe_profile)

    _intake_cam_0
        .add_lobe(ILC + 0 * rot)
        .add_lobe(ILC + 3 * rot)
        .add_lobe(ILC + 1 * rot)
        .add_lobe(ILC + 2 * rot)
    _exhaust_cam_0
        .add_lobe(ELC + 0 * rot)
        .add_lobe(ELC + 3 * rot)
        .add_lobe(ELC + 1 * rot)
        .add_lobe(ELC + 2 * rot)
}

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node head {
    // port_flow.py v1.5
    // intake port area: 16.7 cm²; saturated lift: 7.93 mm
    // exhaust port area: 12.1 cm²; saturated lift: 6.75 mm
    // cylinder volume: 571.7 cm³ (34.9 CI); engine volume: 2.287 L (139.5 CI)
    // chamber volume: 68.9 cm³ (4.2 CI)
    // 16 harmonic intake runner length: 17.4 cm; diameter: 4.1 cm
    // primary length: 30.9 cm, area: 12.1 cm², diameter: 3.9 cm
    // collector diameter: 5.0 cm, area: 19.4 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 68.879 * units.cc,
        intake_runner_volume: 230.7 * units.cc,
        intake_runner_cross_section_area: 13.3 * units.cm2,
        exhaust_runner_volume: 76.9 * units.cc,
        exhaust_runner_cross_section_area: 12.1 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "ZMZ-406",
        starter_torque: 100 * units.Nm,
        starter_speed: 300 * units.rpm,
        redline: redline * units.rpm,
        fuel: s90(),
        throttle_gamma: 1.5,
        jitter: 0.7,
        noise: 0.2,
        hf_gain: 0.002,
        simulation_frequency: 12000 
    )

    wires wires()

    connecting_rod_parameters cr_params(
        mass: con_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.g,
            length: con_rod * units.mm
        ),
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    label crank_moment(disk_moment_of_inertia(mass: crank_mass * units.kg, radius: (stroke / 2.0) * units.mm))
    label flywheel_moment(disk_moment_of_inertia(mass: flywheel_mass * units.kg, radius: (stroke * 2.7) * units.mm))
    label other_moment(disk_moment_of_inertia(mass: 1 * units.kg, radius: 1.0 * units.cm))

    crankshaft c0(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: flywheel_mass * units.kg,
        mass: crank_mass * units.kg,
        friction_torque: 3.4 * units.Nm,
        moment_of_inertia: crank_moment + flywheel_moment + other_moment,
        tdc: 0.0
    )

    rod_journal rj0(angle: rot * 0 + rot90)
    rod_journal rj1(angle: rot * 3 + rot90)
    rod_journal rj2(angle: rot * 1 + rot90)
    rod_journal rj3(angle: rot * 2 + rot90)
    
    c0
        .add_rod_journal(rj0)
        .add_rod_journal(rj1)
        .add_rod_journal(rj2)
        .add_rod_journal(rj3)

    piston_parameters piston_params(
        mass: piston_mass * units.g,
        blowby: k_28inH2O(0.029),
        compression_height: compression_height,
        wrist_pin_position: 0.0,
        displacement: 0.0
    )

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    label collector_cross_section_area(19.4)

    label exhaust_pipe_length_0(300.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 100.0) // Litres
    
    label spacing_factor(1.1)
    label flange_density(1.0 * exhaust_delay_coeff)
    

    intake intake(
        plenum_volume: 1.9 * units.L,
        plenum_cross_section_area: 26.5 * units.cm2,
        runner_length: 17.4 * units.cm,
        intake_flow_rate: k_carb(220.1),
        idle_flow_rate: k_carb(0.02),
        idle_throttle_plate_position: 0.996,
        runner_flow_rate: k_carb(115.6),
        velocity_decay: 0.05
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(376.0),
        collector_cross_section_area: 19.4 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 30.9 * units.cm,
        primary_flow_rate: k_carb(192.7),
        velocity_decay: 1.0,
        volume: exhaust_volume_0 * units.L
    )

    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_40.wav", volume: 0.00025))
 
    cylinder_bank b0(bank_params, angle: 0)

    // ЗМЗ-406
    label pl0((2.7 * bore * spacing_factor / flange_density) * units.mm)
    label pl1((1.7 * bore * spacing_factor / flange_density) * units.mm)
    label pl2((1.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl3((1.9 * bore * spacing_factor / flange_density) * units.mm)

    b0
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj0,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl0,
            ignition_wire: wires.wire1
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj1,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl1,
            ignition_wire: wires.wire2
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj2,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl2,
            ignition_wire: wires.wire3
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj3,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl3,
            ignition_wire: wires.wire4
        )

    engine
        .add_cylinder_bank(b0)

    engine.add_crankshaft(c0)

    camshaft_builder camshaft()

    b0.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_0,
            exhaust_camshaft: camshaft.exhaust_cam_0,
            flip_display: false
        )
    )

    function timing_curve(366.7 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 7.0 * units.deg)
        .add_sample(366.7 * units.rpm, 7.0 * units.deg)
        .add_sample(733.3 * units.rpm, 12.3 * units.deg)
        .add_sample(1100.0 * units.rpm, 19.2 * units.deg)
        .add_sample(1466.7 * units.rpm, 24.6 * units.deg)
        .add_sample(1833.3 * units.rpm, 28.8 * units.deg)
        .add_sample(2200.0 * units.rpm, 32.1 * units.deg)
        .add_sample(2566.7 * units.rpm, 34.7 * units.deg)
        .add_sample(2933.3 * units.rpm, 36.7 * units.deg)
        .add_sample(3300.0 * units.rpm, 37.0 * units.deg)
        .add_sample(3666.7 * units.rpm, 37.0 * units.deg)
        .add_sample(4033.3 * units.rpm, 37.0 * units.deg)
        .add_sample(4400.0 * units.rpm, 37.0 * units.deg)
        .add_sample(4766.7 * units.rpm, 37.0 * units.deg)
        .add_sample(5133.3 * units.rpm, 37.0 * units.deg)
        .add_sample(5500.0 * units.rpm, 37.0 * units.deg)
        .add_sample(5866.7 * units.rpm, -12.5 * units.deg)
        .add_sample(6233.3 * units.rpm, -25.0 * units.deg)

    engine.add_ignition_module(
        eng_distributor(
            wires: wires,
            timing_curve: timing_curve
        )
    )
}

// ГАЗ-3110
public node veh {
    alias output __out: vehicle;

    label car_mass(1800) // 1550 empty; 1950 full

    vehicle vehicle(
        mass: car_mass * units.kg,
        drag_coefficient: 0.461,
        cross_sectional_area: (1810 * units.mm) * (1422 * units.mm),
        diff_ratio: 3.9,
        tire_radius: (635.0 / 2) * units.mm, // 195/65R15
        rolling_resistance: 0.012 * car_mass * 9.81
        )
}


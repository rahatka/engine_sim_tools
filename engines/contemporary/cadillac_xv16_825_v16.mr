// 1-3-7-2-6-5-4-8 + 1-5-4-3-7-2-6-8
public node camshaft_builder {
    output intake_cam_0: _intake_cam_0;
    output exhaust_cam_0: _exhaust_cam_0;

    output intake_cam_1: _intake_cam_1;
    output exhaust_cam_1: _exhaust_cam_1;

    camshaft _intake_cam_0(base_radius: 23.7 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 23.7 * units.mm, lobe_profile: exhaust_lobe_profile)
    
    camshaft _intake_cam_1(base_radius: 23.7 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_1(base_radius: 23.7 * units.mm, lobe_profile: exhaust_lobe_profile)

    _intake_cam_0
        .add_lobe(ILC + 0 * rot_base)
        .add_lobe(ILC + 3 * rot_base)
        .add_lobe(ILC + 1 * rot_base)
        .add_lobe(ILC + 6 * rot_base)
        .add_lobe(ILC + 0 * rot_base + sb_shift)
        .add_lobe(ILC + 5 * rot_base + sb_shift)
        .add_lobe(ILC + 3 * rot_base + sb_shift)
        .add_lobe(ILC + 2 * rot_base + sb_shift)
    _exhaust_cam_0
        .add_lobe(ELC + 0 * rot_base)
        .add_lobe(ELC + 3 * rot_base)
        .add_lobe(ELC + 1 * rot_base)
        .add_lobe(ELC + 6 * rot_base)
        .add_lobe(ELC + 0 * rot_base + sb_shift)
        .add_lobe(ELC + 5 * rot_base + sb_shift)
        .add_lobe(ELC + 3 * rot_base + sb_shift)
        .add_lobe(ELC + 2 * rot_base + sb_shift)
        
    _intake_cam_1
        .add_lobe(ILC + 5 * rot_base)
        .add_lobe(ILC + 4 * rot_base)
        .add_lobe(ILC + 2 * rot_base)
        .add_lobe(ILC + 7 * rot_base)
        .add_lobe(ILC + 1 * rot_base + sb_shift)
        .add_lobe(ILC + 6 * rot_base + sb_shift)
        .add_lobe(ILC + 4 * rot_base + sb_shift)
        .add_lobe(ILC + 7 * rot_base + sb_shift)
    _exhaust_cam_1
        .add_lobe(ELC + 5 * rot_base)
        .add_lobe(ELC + 4 * rot_base)
        .add_lobe(ELC + 2 * rot_base)
        .add_lobe(ELC + 7 * rot_base)
        .add_lobe(ELC + 1 * rot_base + sb_shift)
        .add_lobe(ELC + 6 * rot_base + sb_shift)
        .add_lobe(ELC + 4 * rot_base + sb_shift)
        .add_lobe(ELC + 7 * rot_base + sb_shift)
}

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node head {
    // port_flow.py v1.5
    // intake port area: 19.9 cm²; saturated lift: 12.32 mm
    // exhaust port area: 10.7 cm²; saturated lift: 9.04 mm
    // cylinder volume: 845.3 cm³ (51.6 CI); engine volume: 13.525 L (825.4 CI)
    // chamber volume: 88.1 cm³ (5.4 CI)
    // 16 harmonic intake runner length: 15.5 cm; diameter: 5.4 cm
    // primary length: 24.8 cm, area: 11.8 cm², diameter: 3.9 cm
    // collector diameter: 9.8 cm, area: 75.6 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 88.055 * units.cc,
        intake_runner_volume: 351.1 * units.cc,
        intake_runner_cross_section_area: 22.6 * units.cm2,
        exhaust_runner_volume: 117.0 * units.cc,
        exhaust_runner_cross_section_area: 11.8 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "Cadillac XV16",
        starter_torque: 300 * units.Nm,
        starter_speed: 400 * units.rpm,
        redline: redline * units.rpm,
        fuel: s90(),
        throttle_gamma: 1.5,
        jitter: 0.5,
        noise: 0.1,
        hf_gain: 0.001,
        simulation_frequency: 7000,
        dyno_min_speed: 1000 * units.rpm,
        fluid_simulation_steps: 6,
        max_sle_solver_steps: 32
    )

    wires wires()

    label crank_moment(disk_moment_of_inertia(mass: crank_mass * units.kg, radius: (stroke / 2.0) * units.mm))
    label flywheel_moment(disk_moment_of_inertia(mass: flywheel_mass * units.kg, radius: (stroke * 3.0) * units.mm))
    label other_moment(disk_moment_of_inertia(mass: 1 * units.kg, radius: 1.0 * units.cm))

    crankshaft c0(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: flywheel_mass * units.kg,
        mass: crank_mass * units.kg,
        friction_torque: 20.3 * units.Nm,
        moment_of_inertia: crank_moment + flywheel_moment + other_moment,
        tdc: 0.0
    )

    rod_journal rj0(angle: rot_base * 0 + rot90 + vee / 2)
    rod_journal rj1(angle: rot_base * 3 + rot90 + vee / 2)
    rod_journal rj2(angle: rot_base * 1 + rot90 + vee / 2)
    rod_journal rj3(angle: rot_base * 6 + rot90 + vee / 2)
    rod_journal rj4(angle: rot_base * 0 + rot90 + vee / 2 + sb_shift)
    rod_journal rj5(angle: rot_base * 5 + rot90 + vee / 2 + sb_shift)
    rod_journal rj6(angle: rot_base * 3 + rot90 + vee / 2 + sb_shift)
    rod_journal rj7(angle: rot_base * 2 + rot90 + vee / 2 + sb_shift)
    
    c0
        .add_rod_journal(rj0)
        .add_rod_journal(rj1)
        .add_rod_journal(rj2)
        .add_rod_journal(rj3)
        .add_rod_journal(rj4)
        .add_rod_journal(rj5)
        .add_rod_journal(rj6)
        .add_rod_journal(rj7)

    piston_parameters piston_params(
        mass: 482.0 * units.g,
        blowby: k_28inH2O(0.033),
        compression_height: compression_height,
        wrist_pin_position: 0.0,
        displacement: 0.0
    )

    connecting_rod_parameters cr_params(
        mass: con_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.g,
            length: con_rod * units.mm
        ),
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    label collector_cross_section_area(37.8)

    label exhaust_pipe_length_0(365.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 100.0) // Litres
    
    label exhaust_pipe_length_1(360.0) // cm
    label exhaust_volume_1(collector_cross_section_area * exhaust_pipe_length_1 / 100.0) // Litres
    
    label spacing_factor(1.1)
    label flange_density(1.0 * exhaust_delay_coeff)

    intake intake(
        plenum_volume: 12.8 * units.L,
        plenum_cross_section_area: 181.1 * units.cm2,
        runner_length: 15.5 * units.cm,
        intake_flow_rate: k_carb(1538.5),
        idle_flow_rate: k_carb(0.04),
        idle_throttle_plate_position: 0.9977,
        runner_flow_rate: k_carb(201.9),
        velocity_decay: 0.05
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(1314.1),
        collector_cross_section_area: 37.8 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 24.8 * units.cm,
        primary_flow_rate: k_carb(336.7),
        velocity_decay: 1.0,
        volume: (exhaust_volume_0 + 10.0) * units.L
    )
    
    exhaust_system_parameters es_params1(
        outlet_flow_rate: k_carb(1314.1),
        collector_cross_section_area: 37.8 * units.cm2,
        length: exhaust_pipe_length_1 * units.cm,
        primary_tube_length: 24.8 * units.cm,
        primary_flow_rate: k_carb(336.7),
        velocity_decay: 1.0,
        volume: (exhaust_volume_1 + 10.0) * units.L
    )

    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_39.wav", volume: 0.0011))
    exhaust_system exhaust1(es_params1, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_39.wav", volume: 0.0010))
 
    cylinder_bank b0(bank_params, angle: vee / 2)
    cylinder_bank b1(bank_params, angle: -vee / 2)

    label pl0((7.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl1((6.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl2((5.5 * bore * spacing_factor / flange_density) * units.mm)
    label pl3((4.5 * bore * spacing_factor / flange_density) * units.mm)
    label pl4((3.5 * bore * spacing_factor / flange_density) * units.mm)
    label pl5((2.5 * bore * spacing_factor / flange_density) * units.mm)
    label pl6((1.2 * bore * spacing_factor / flange_density) * units.mm)
    label pl7((1.0 * bore * spacing_factor / flange_density) * units.mm)

    // label pl0((5.0 * bore * spacing_factor / flange_density) * units.mm)
    // label pl1((4.0 * bore * spacing_factor / flange_density) * units.mm)
    // label pl2((4.0 * bore * spacing_factor / flange_density) * units.mm)
    // label pl3((5.0 * bore * spacing_factor / flange_density) * units.mm)
    // label pl4((2.0 * bore * spacing_factor / flange_density) * units.mm)
    // label pl5((1.0 * bore * spacing_factor / flange_density) * units.mm)
    // label pl6((1.0 * bore * spacing_factor / flange_density) * units.mm)
    // label pl7((2.0 * bore * spacing_factor / flange_density) * units.mm)

    b0
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj0,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl0,
            ignition_wire: wires.wire1
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj1,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl1,
            ignition_wire: wires.wire2
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj2,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl2,
            ignition_wire: wires.wire3
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj3,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl3,
            ignition_wire: wires.wire4
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj4,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl4,
            ignition_wire: wires.wire9
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj5,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl5,
            ignition_wire: wires.wire10
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj6,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl6,
            ignition_wire: wires.wire11
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj7,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl7,
            ignition_wire: wires.wire12
        )
        
    b1
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj0,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl0,
            ignition_wire: wires.wire5
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj1,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl1,
            ignition_wire: wires.wire6
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj2,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl2,
            ignition_wire: wires.wire7
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj3,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl3,
            ignition_wire: wires.wire8
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj4,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl4,
            ignition_wire: wires.wire13
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj5,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl5,
            ignition_wire: wires.wire14
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj6,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl6,
            ignition_wire: wires.wire15
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj7,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl7,
            ignition_wire: wires.wire16
        )

    engine
        .add_cylinder_bank(b0)
        .add_cylinder_bank(b1)

    engine.add_crankshaft(c0)

    camshaft_builder camshaft()
    
    engine.add_ignition_module(
        distributor(
            wires: wires,
            timing_curve: timing_curve
        )
    )

    b0.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_0,
            exhaust_camshaft: camshaft.exhaust_cam_0,
            flip_display: true
        )
    )
    b1.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_1,
            exhaust_camshaft: camshaft.exhaust_cam_1
        )
    )

    function timing_curve(433.3 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 6.0 * units.deg)
        .add_sample(433.3 * units.rpm, 6.0 * units.deg)
        .add_sample(866.7 * units.rpm, 6.0 * units.deg)
        .add_sample(1300.0 * units.rpm, 14.1 * units.deg)
        .add_sample(1733.3 * units.rpm, 23.1 * units.deg)
        .add_sample(2166.7 * units.rpm, 29.5 * units.deg)
        .add_sample(2600.0 * units.rpm, 34.1 * units.deg)
        .add_sample(3033.3 * units.rpm, 37.4 * units.deg)
        .add_sample(3466.7 * units.rpm, 39.8 * units.deg)
        .add_sample(3900.0 * units.rpm, 40.0 * units.deg)
        .add_sample(4333.3 * units.rpm, 40.0 * units.deg)
        .add_sample(4766.7 * units.rpm, 40.0 * units.deg)
        .add_sample(5200.0 * units.rpm, 40.0 * units.deg)
        .add_sample(5633.3 * units.rpm, 40.0 * units.deg)
        .add_sample(6066.7 * units.rpm, 40.0 * units.deg)
        .add_sample(6500.0 * units.rpm, 40.0 * units.deg)
        .add_sample(6933.3 * units.rpm, -15.0 * units.deg)
        .add_sample(7366.7 * units.rpm, -60.0 * units.deg)
}

// Cadillac Sixteen
public node veh {
    alias output __out: vehicle;
    vehicle vehicle(
        mass: 2270 * units.kg,
        drag_coefficient: 0.35,
        cross_sectional_area: (2029.0 * units.mm) * (1392.0 * units.mm),
        diff_ratio: 3.42,
        tire_radius: (821.6 / 2) * units.mm, // P265/40R24
        rolling_resistance: 20
        )
}

// 4L85-E
private node trn {
    alias output __out:
        transmission(
            max_clutch_torque: 1600 * units.Nm,
            max_clutch_flex: 30 * units.deg,
            limit_clutch_flex: true,
            clutch_stiffness: 10 * units.Nm / units.deg,
            clutch_damping: 1.0,
            simulate_flex: true
        )
        .add_gear(2.482)
        .add_gear(1.482)
        .add_gear(1.000)
        .add_gear(0.750);
}

public node main {
    set_engine(eng())
    set_transmission(trn())
    set_vehicle(veh())
}

main()
// cadillac_xv16
// Created by oror 2023

import "engine_sim.mr"
import "../../fuel/90.mr"

units units()
constants constants()

label sb_pos(7) // change this value from 0 to 7 to rotate the second block

label bore(104.775) // 4.25
label stroke(98.044) // 3.86
label compression_ratio(10.6)
label con_rod(154.1018)
label compression_height(29.845 * units.mm)
label intake_valve_diameter(55.88)
label exhaust_valve_diameter(41.021)
label intake_valves(1.0)
label exhaust_valves(1.0)

label volume(13.525) // L; port_flow.py v0.8a
label redline(6500.0) // RPM

label cyl(16.0)
label row(2.0)
label cyl_base(cyl / row)
label cycle(720.0 * units.deg)
label vee(90.0 * units.deg)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label sb_shift((45.0 + (90.0 * sb_pos)) * units.deg)

// Chevrolet LS7 Camshaft 211/230 121LSA
label IVL(15.0114)
label EVL(14.9606)
label IVO(5.0 * units.deg) //BTDC
label IVC(42.0 * units.deg) //ABDC
label EVO(63.0 * units.deg) //BBDC
label EVC(17.0 * units.deg) //ATDC
label at_lift(1.0) // mm

// KAT-7522 Katech LS7 Track Attack Camshaft
// label IVL(17.018)
// label EVL(17.018)
// label IVO(11.5 * units.deg) //BTDC
// label IVC(55.5 * units.deg) //ABDC
// label EVO(62.5 * units.deg) //BBDC
// label EVC(18.5 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(464.0) // g
label crank_mass(28.0) // kg
label flywheel_mass(18.0) // kg

label exhaust_delay_coeff(1.5)

// GM LS
// 1-3-7-2-6-5-4-8 + 1-5-4-3-7-2-6-8
public node distributor {
    input wires;
    input timing_curve;
    input rev_limit: 6750 * units.rpm;
    input limiter_duration: 0.03;
    alias output __out:
        ignition_module(timing_curve: timing_curve, rev_limit: rev_limit, limiter_duration: limiter_duration)
            .connect_wire(wires.wire1, rot_base * 0)
            .connect_wire(wires.wire3, rot_base * 1)
            .connect_wire(wires.wire7, rot_base * 2)
            .connect_wire(wires.wire2, rot_base * 3)
            .connect_wire(wires.wire6, rot_base * 4)
            .connect_wire(wires.wire5, rot_base * 5)
            .connect_wire(wires.wire4, rot_base * 6)
            .connect_wire(wires.wire8, rot_base * 7)
            
            .connect_wire(wires.wire9, rot_base * 0 + sb_shift)
            .connect_wire(wires.wire13, rot_base * 1 + sb_shift)
            .connect_wire(wires.wire12, rot_base * 2 + sb_shift)
            .connect_wire(wires.wire11, rot_base * 3 + sb_shift)
            .connect_wire(wires.wire15, rot_base * 4 + sb_shift)
            .connect_wire(wires.wire10, rot_base * 5 + sb_shift)
            .connect_wire(wires.wire14, rot_base * 6 + sb_shift)
            .connect_wire(wires.wire16, rot_base * 7 + sb_shift);
}

private node wires {
    output wire1: ignition_wire();
    output wire2: ignition_wire();
    output wire3: ignition_wire();
    output wire4: ignition_wire();
    output wire5: ignition_wire();
    output wire6: ignition_wire();
    output wire7: ignition_wire();
    output wire8: ignition_wire();
    output wire9: ignition_wire();
    output wire10: ignition_wire();
    output wire11: ignition_wire();
    output wire12: ignition_wire();
    output wire13: ignition_wire();
    output wire14: ignition_wire();
    output wire15: ignition_wire();
    output wire16: ignition_wire();
}


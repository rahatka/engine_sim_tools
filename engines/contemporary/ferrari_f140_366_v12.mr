label stroke(75.2)
label compression_ratio(11.2)
label con_rod(134.0)
label compression_height(30.0 * units.mm) // ~
label intake_valve_diameter(35.0) // ?
label exhaust_valve_diameter(30.0) // ?
label intake_valves(2.0)
label exhaust_valves(2.0)

label redline(8200.0) // RPM

label cyl(12.0)
label cyl_base(cyl / row)
label row(2.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label rot_base(cycle / cyl_base)
label vee(65.0 * units.deg)

// F50 only timing
label IVL(7.5) // ?
label EVL(8.0) // ?
label IVO(18.0 * units.deg) //BTDC
label IVC(54.0 * units.deg) //ABDC
label EVO(61.0 * units.deg) //BBDC
label EVC(21.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(400.0) // ? g
label piston_mass(300.0) // ?
label crank_mass(15.0) // ?
label flywheel_mass(5.0) // kg

label exhaust_delay_coeff(1.5)


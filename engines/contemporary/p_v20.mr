import "engine_sim.mr"
import "../../fuel/90.mr"

units units()
constants constants()

label bore(98.21)
label stroke(66.0)
label compression_ratio(12.0)
label con_rod(132.6)
label compression_height(32.0 * units.mm)
label intake_valve_diameter(38.2)
label exhaust_valve_diameter(32.5)
label intake_valves(2.0)
label exhaust_valves(2.0)

label redline(9500.0) // RPM

label cyl(20.0)
label row(2.0)
label cyl_base(cyl / row)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label vee(rot * 3)

label IVL(12.0)
label EVL(12.0)
label IVO(25.0 * units.deg) //BTDC
label IVC(65.0 * units.deg) //ABDC
label EVO(71.0 * units.deg) //BBDC
label EVC(31.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(538.0) // g
label crank_mass(25.0) // kg
label piston_mass(500.0) // g
label flywheel_mass(6.0) // kg

label exhaust_delay_coeff(1.5)

// basic inline-5 firing orders
public node variant_0 {
    output s1: 0;
    output s3: 1;
    output s5: 2;
    output s4: 3;
    output s2: 4;
}

public node variant_1 {
    output s1: 0;
    output s4: 1;
    output s3: 2;
    output s2: 3;
    output s5: 4;
}

public node variant_2 {
    output s1: 0;
    output s2: 1;
    output s3: 2;
    output s4: 3;
    output s5: 4;
}

public node variant_3 {
    output s1: 0;
    output s3: 1;
    output s4: 2;
    output s2: 3;
    output s5: 4;
}

public node variant_4 {
    output s1: 0;
    output s5: 1;
    output s3: 2;
    output s4: 3;
    output s2: 4;
}


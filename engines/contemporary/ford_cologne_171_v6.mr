import "engine_sim.mr"
import "../../fuel/90.mr"

units units()
constants constants()

label bore(93.03)
label stroke(68.58)
label compression_ratio(9.2)
label con_rod(130.556) // 5.14
label compression_height(39.09 * units.mm)
label intake_valve_diameter(42.1)
label exhaust_valve_diameter(38.1)
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(6000.0) // RPM

label cyl(6.0)
label row(2.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label vee(60.0 * units.deg)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// KENT CAMS STANDARD OEM CAMSHAFT for FORD TAUNUS/COLOGNE 2.8 V6
label IVL(9.4996)
label EVL(9.4488)
label IVO(24.0 * units.deg) //BTDC
label IVC(72.0 * units.deg) //ABDC
label EVO(73.0 * units.deg) //BBDC
label EVC(25.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(350.0) // g
label crank_mass(15.0) // kg

label exhaust_delay_coeff(1.5)

public node eng_distributor {
    input wires;
    input timing_curve;
    input rev_limit: 6500 * units.rpm;
    input limiter_duration: 0.08;
    alias output __out:
        ignition_module(timing_curve: timing_curve, rev_limit: rev_limit, limiter_duration: limiter_duration)
            .connect_wire(wires.wire1, rot * 0)
            .connect_wire(wires.wire4, rot * 1)
            .connect_wire(wires.wire2, rot * 2)
            .connect_wire(wires.wire5, rot * 3)
            .connect_wire(wires.wire3, rot * 4)
            .connect_wire(wires.wire6, rot * 5);
}

private node wires {
    output wire1: ignition_wire();
    output wire2: ignition_wire();
    output wire3: ignition_wire();
    output wire4: ignition_wire();
    output wire5: ignition_wire();
    output wire6: ignition_wire();
}


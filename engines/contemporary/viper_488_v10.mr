import "engine_sim.mr"
import "../../fuel/90.mr"

units units()
constants constants()

label bore(101.6) // 4
label stroke(98.552) // 3.88
label compression_ratio(9.6)
label con_rod(155.5242) // 6.123
label intake_valve_diameter(48.768)
label exhaust_valve_diameter(40.64)
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(6000.0) // RPM

label cyl(10.0)
label row(2.0)
label cycle(720.0 * units.deg)
label vee(90.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// stock: 212/232 -- .544/.544
label IVL(13.8176)
label EVL(13.8176)
label IVO(0.0 * units.deg) //BTDC
label IVC(32.0 * units.deg) //ABDC
label EVO(50.0 * units.deg) //BBDC
label EVC(2.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label exhaust_delay_coeff(1.5)

public node eng_distributor {
    input wires;
    input timing_curve;
    input limiter_duration: 0.08;
    alias output __out:
        ignition_module(timing_curve: timing_curve, rev_limit: redline * 1.05 * units.rpm, limiter_duration: limiter_duration)
            .connect_wire(wires.wire1, rot * 0)
            .connect_wire(wires.wire6, rot * 0 + vee)
            .connect_wire(wires.wire4, rot * 2)
            .connect_wire(wires.wire9, rot * 2 + vee)
            .connect_wire(wires.wire3, rot * 4)
            .connect_wire(wires.wire8, rot * 4 + vee)
            .connect_wire(wires.wire2, rot * 6)
            .connect_wire(wires.wire7, rot * 6 + vee)
            .connect_wire(wires.wire5, rot * 8)
            .connect_wire(wires.wire10, rot * 8 + vee);
}

private node wires {
    output wire1: ignition_wire();
    output wire2: ignition_wire();
    output wire3: ignition_wire();
    output wire4: ignition_wire();
    output wire5: ignition_wire();
    output wire6: ignition_wire();
    output wire7: ignition_wire();
    output wire8: ignition_wire();
    output wire9: ignition_wire();
    output wire10: ignition_wire();
}


import "engine_sim.mr"
import "_fuel_90.mr"

units units()
constants constants()

label bore(88.0)
label stroke(82.0)
label compression_ratio(11.0)
label con_rod(144.0)
label compression_height(30.0 * units.mm) // ~
label intake_valve_diameter(37.8) // Supertech FEIVC-F136
label exhaust_valve_diameter(31.0) // Supertech FEEVC-F136
label intake_valves(2.0)
label exhaust_valves(2.0)

label redline(8000.0)

label cyl(6.0)
label row(2.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label vee(90.0 * units.deg)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

label IVL(10.0) // ?
label EVL(10.0) // ?
label IVO(20.0 * units.deg) //BTDC
label IVC(64.0 * units.deg) //ABDC
label EVO(64.0 * units.deg) //BBDC
label EVC(20.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(538.0) // g 458
label crank_mass(15.0) // ? kg
label flywheel_mass(3.0) // kg

label exhaust_delay_coeff(1.5)

public node eng_distributor {
    input wires;
    input timing_curve;
    input rev_limit: 8250 * units.rpm;
    input limiter_duration: 0.08;
    alias output __out:
        ignition_module(timing_curve: timing_curve, rev_limit: rev_limit, limiter_duration: limiter_duration)
            .connect_wire(wires.wire1, rot * 0)
            .connect_wire(wires.wire2, rot * 2)
            .connect_wire(wires.wire3, rot * 4)
            .connect_wire(wires.wire4, rot * 0 + vee)
            .connect_wire(wires.wire5, rot * 2 + vee)
            .connect_wire(wires.wire6, rot * 4 + vee);
}

private node wires {
    output wire1: ignition_wire();
    output wire2: ignition_wire();
    output wire3: ignition_wire();
    output wire4: ignition_wire();
    output wire5: ignition_wire();
    output wire6: ignition_wire();
}


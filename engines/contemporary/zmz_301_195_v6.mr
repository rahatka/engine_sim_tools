label bore(92.0)
label stroke(80.0)
label compression_ratio(9.5)
label con_rod(158.0)
label compression_height(38.0 * units.mm)
label intake_valve_diameter(37.0)
label exhaust_valve_diameter(31.5)
label intake_valves(2.0)
label exhaust_valves(2.0)

label redline(5800.0) // RPM

label cyl(6.0)
label row(2.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label vee(90.0 * units.deg)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// ЗМЗ-406 сер. 301
label IVL(9.2)
label EVL(9.2)
label IVO(18.0 * units.deg) //BTDC
label IVC(42.0 * units.deg) //ABDC
label EVO(44.0 * units.deg) //BBDC
label EVC(16.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label piston_mass(500.0) // ? g
label con_rod_mass(900.0) // g
label crank_mass(18.0) // ? kg
label flywheel_mass(5.0) // kg

label exhaust_delay_coeff(1.5)

public node eng_distributor {
    input wires;
    input timing_curve;
    input rev_limit: 6300 * units.rpm;
    input limiter_duration: 0.08;
    alias output __out:
        ignition_module(timing_curve: timing_curve, rev_limit: rev_limit, limiter_duration: limiter_duration)
            .connect_wire(wires.wire1, rot * 0)
            .connect_wire(wires.wire4, rot * 0 + vee)
            .connect_wire(wires.wire2, rot * 2)
            .connect_wire(wires.wire5, rot * 2 + vee)
            .connect_wire(wires.wire3, rot * 4)
            .connect_wire(wires.wire6, rot * 4 + vee);
}

private node wires {
    output wire1: ignition_wire();
    output wire2: ignition_wire();
    output wire3: ignition_wire();
    output wire4: ignition_wire();
    output wire5: ignition_wire();
    output wire6: ignition_wire();
}


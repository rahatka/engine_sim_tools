public node eng {
    alias output __out: engine;

    engine engine(
        name: "McLaren M838T NA",
        starter_torque: 75 * units.Nm,
        starter_speed: 750 * units.rpm,
        redline: redline * units.rpm,
        fuel: fuel(
            max_turbulence_effect: 3.0,
            burning_efficiency_randomness: 0.1,
            max_burning_efficiency: 0.99
        ),
        throttle_gamma: 1.5,
        jitter: 0.8,
        noise: 0.3,
        hf_gain: 0.005,
        simulation_frequency: 12000
    )

    wires wires()
    
    connecting_rod_parameters master_rod_params(
        mass: 350.0 * units.g,
        moment_of_inertia: 0.001,
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    crankshaft c0(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: 4 * units.kg,
        mass: 9 * units.kg,
        friction_torque: 5.7 * units.Nm, // port_flow.py v0.7
        moment_of_inertia: 0.1,
        position_x: 0.0,
        position_y: 0.0,
        tdc: 0.0
    )

    rod_journal rj0(angle: rot * 0 + rot90 + vee / 2)
    rod_journal rj1(angle: rot * 6 + rot90 + vee / 2)
    rod_journal rj2(angle: rot * 2 + rot90 + vee / 2)
    rod_journal rj3(angle: rot * 4 + rot90 + vee / 2)
    
    c0
        .add_rod_journal(rj0)
        .add_rod_journal(rj1)
        .add_rod_journal(rj2)
        .add_rod_journal(rj3)

    piston_parameters piston_params(
        mass: 400 * units.g,
        blowby: k_28inH2O(0.05),
        compression_height: compression_height,
        wrist_pin_position: 0.0,
        displacement: 0.0
    )

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    label collector_cross_section_area(21.4) // port_flow.py v0.7

    label exhaust_pipe_length_0(150.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 100.0) // Litres
    
    label exhaust_pipe_length_1(153.0) // cm
    label exhaust_volume_1(collector_cross_section_area * exhaust_pipe_length_1 / 100.0) // Litres
    
    label spacing_factor(1.135)
    label flange_density(1.3 * exhaust_delay_coeff)
    

    intake intake(
        plenum_volume: 4.9 * units.L, // port_flow.py v0.7
        plenum_cross_section_area: 64.9 * units.cm2, // port_flow.py v0.7
        runner_length: 19.8 * units.cm, // port_flow.py v0.7
        intake_flow_rate: k_carb(605.4), // port_flow.py v0.7
        idle_flow_rate: k_carb(0.03),
        idle_throttle_plate_position: 0.9975,
        runner_flow_rate: k_carb(151.4), // port_flow.py v0.7
        velocity_decay: 0.05
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(504.5), // port_flow.py v0.7
        collector_cross_section_area: 32.1 * units.cm2, // CF 1.5; port_flow.py v0.7
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 33.2 * units.cm, // port_flow.py v0.7
        primary_flow_rate: k_carb(252.3), // port_flow.py v0.7
        velocity_decay: 1.0,
        volume: (exhaust_volume_0 + 2.0) * units.L
    )
    
    exhaust_system_parameters es_params1(
        outlet_flow_rate: k_carb(504.5), // port_flow.py v0.7
        collector_cross_section_area: 32.1 * units.cm2, // CF 1.5; port_flow.py v0.7
        length: exhaust_pipe_length_1 * units.cm,
        primary_tube_length: 33.2 * units.cm, // port_flow.py v0.7
        primary_flow_rate: k_carb(252.3), // port_flow.py v0.7
        velocity_decay: 1.0,
        volume: (exhaust_volume_1 + 2.0) * units.L
    )

    exhaust_system exhaust0(es_params0, impulse_response: ir_lib.default_0, audio_volume: 5.0)
    exhaust_system exhaust1(es_params1, impulse_response: ir_lib.default_0, audio_volume: 5.0)
 
    cylinder_bank b0(bank_params, angle: vee / 2)
    cylinder_bank b1(bank_params, angle: -vee / 2)
    
    label pl0 ((3.1 * bore * spacing_factor / flange_density) * units.mm)
    label pl1 ((2.1 * bore * spacing_factor / flange_density) * units.mm)
    label pl2 ((1.1 * bore * spacing_factor / flange_density) * units.mm)
    label pl3 ((2.0 * bore * spacing_factor / flange_density) * units.mm)

    b0
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(master_rod_params),
            rod_journal: rj0,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl0,
            ignition_wire: wires.wire1
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(master_rod_params),
            rod_journal: rj1,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl1,
            ignition_wire: wires.wire2
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(master_rod_params),
            rod_journal: rj2,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl2,
            ignition_wire: wires.wire3
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(master_rod_params),
            rod_journal: rj3,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl3,
            ignition_wire: wires.wire4
        )
        
    b1
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(master_rod_params),
            rod_journal: rj0,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl0,
            ignition_wire: wires.wire5
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(master_rod_params),
            rod_journal: rj1,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl1,
            ignition_wire: wires.wire6
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(master_rod_params),
            rod_journal: rj2,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl2,
            ignition_wire: wires.wire7
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(master_rod_params),
            rod_journal: rj3,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl3,
            ignition_wire: wires.wire8
        )

    engine
        .add_cylinder_bank(b0)
        .add_cylinder_bank(b1)

    engine.add_crankshaft(c0)

    camshaft_builder camshaft()

    b0.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_0,
            exhaust_camshaft: camshaft.exhaust_cam_0,
            flip_display: true
        )
    )
    b1.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_1,
            exhaust_camshaft: camshaft.exhaust_cam_1
        )
    )

    // ignition_timing.py v0.3 from = 5.0, to = 37.0, i = 500, r = 8500
    function timing_curve(850.0 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 0.0 * units.deg)
        .add_sample(850.0 * units.rpm, 5.0 * units.deg)
        .add_sample(1700.0 * units.rpm, 12.4 * units.deg)
        .add_sample(2550.0 * units.rpm, 18.3 * units.deg)
        .add_sample(3400.0 * units.rpm, 23.0 * units.deg)
        .add_sample(4250.0 * units.rpm, 26.8 * units.deg)
        .add_sample(5100.0 * units.rpm, 29.8 * units.deg)
        .add_sample(5950.0 * units.rpm, 32.3 * units.deg)
        .add_sample(6800.0 * units.rpm, 34.2 * units.deg)
        .add_sample(7650.0 * units.rpm, 35.8 * units.deg)
        .add_sample(8500.0 * units.rpm, 37.0 * units.deg)
        .add_sample(9350.0 * units.rpm, -15.0 * units.deg)
        .add_sample(10200.0 * units.rpm, -60.0 * units.deg)

    engine.add_ignition_module(
        eng_distributor(
            wires: wires,
            timing_curve: timing_curve
        ))
}


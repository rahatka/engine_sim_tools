import "engine_sim.mr"
import "../../fuel/90.mr"

units units()
constants constants()

label bore(90.0)
label stroke(78.5)
label compression_ratio(11.0)
label con_rod(151.45)
label compression_height(39.624 * units.mm) // 1.56
label intake_valve_diameter(41.275)
label exhaust_valve_diameter(34.46)
label intake_valves(1.0)
label exhaust_valves(1.0)

label cyl(12.0)
label row(2.0)
label cycle(720.0 * units.deg)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label vee(60.0 * units.deg)

label redline(6500.0) // RPM

// jag v12
label IVL(9.525) // 0.375
label EVL(9.525) // 0.375
label IVO(17.0 * units.deg) //BTDC
label IVC(59.0 * units.deg) //ABDC
label EVO(59.0 * units.deg) //BBDC
label EVC(17.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(650.0)
label crank_mass(30.0) // ?
label flywheel_mass(7.0) // kg

label exhaust_delay_coeff(1.5)


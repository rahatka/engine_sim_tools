public node camshaft_builder {
    output intake_cam_0: _intake_cam_0;
    output exhaust_cam_0: _exhaust_cam_0;

    output intake_cam_1: _intake_cam_1;
    output exhaust_cam_1: _exhaust_cam_1;

    camshaft _intake_cam_0(base_radius: 22.0 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 22.0 * units.mm, lobe_profile: exhaust_lobe_profile)
    
    camshaft _intake_cam_1(base_radius: 22.0 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_1(base_radius: 22.0 * units.mm, lobe_profile: exhaust_lobe_profile)

    _intake_cam_0
        .add_lobe(ILC + 0 * rot_base)
        .add_lobe(ILC + 4 * rot_base + sb_shift)
        .add_lobe(ILC + 2 * rot_base)
        .add_lobe(ILC + 2 * rot_base + sb_shift)
        .add_lobe(ILC + 4 * rot_base)
        .add_lobe(ILC + 0 * rot_base + sb_shift)

    _exhaust_cam_0
        .add_lobe(ELC + 0 * rot_base)
        .add_lobe(ELC + 4 * rot_base + sb_shift)
        .add_lobe(ELC + 2 * rot_base)
        .add_lobe(ELC + 2 * rot_base + sb_shift)
        .add_lobe(ELC + 4 * rot_base)
        .add_lobe(ELC + 0 * rot_base + sb_shift)
        
    _intake_cam_1
        .add_lobe(ILC + 3 * rot_base)
        .add_lobe(ILC + 1 * rot_base + sb_shift)
        .add_lobe(ILC + 5 * rot_base)
        .add_lobe(ILC + 5 * rot_base + sb_shift)
        .add_lobe(ILC + 1 * rot_base)
        .add_lobe(ILC + 3 * rot_base + sb_shift)

    _exhaust_cam_1
        .add_lobe(ELC + 3 * rot_base)
        .add_lobe(ELC + 1 * rot_base + sb_shift)
        .add_lobe(ELC + 5 * rot_base)
        .add_lobe(ELC + 5 * rot_base + sb_shift)
        .add_lobe(ELC + 1 * rot_base)
        .add_lobe(ELC + 3 * rot_base + sb_shift)
}

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node head {
    // port_flow.py v1.5
    // intake port area: 16.4 cm²; saturated lift: 11.18 mm
    // exhaust port area: 12.4 cm²; saturated lift: 9.73 mm
    // cylinder volume: 600.0 cm³ (36.6 CI); engine volume: 7.201 L (439.4 CI)
    // chamber volume: 58.3 cm³ (3.6 CI)
    // 8 harmonic intake runner length: 23.3 cm; diameter: 5.0 cm
    // primary length: 20.1 cm, area: 13.6 cm², diameter: 4.2 cm
    // collector diameter: 9.7 cm, area: 73.7 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 58.257 * units.cc,
        intake_runner_volume: 455.1 * units.cc,
        intake_runner_cross_section_area: 19.5 * units.cm2,
        exhaust_runner_volume: 182.0 * units.cc,
        exhaust_runner_cross_section_area: 13.6 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "Porsche 993 Boxer12 (f)",
        starter_torque: 115 * units.Nm,
        starter_speed: 400 * units.rpm,
        redline: redline * units.rpm,
        fuel: s90(),
        throttle_gamma: 1.75,
        jitter: 0.8,
        noise: 0.2,
        hf_gain: 0.003,
        simulation_frequency: 8000
    )

    wires wires()

    crankshaft c0(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: 2.0 * units.kg,
        mass: 24.0 * units.kg,
        friction_torque: 10.8 * units.Nm,
        moment_of_inertia: 0.3,
        position_x: 0.0,
        position_y: 0.0,
        tdc: 0.0
    )

    rod_journal rj0(angle: rot_base * 0 + vee)
    rod_journal rj0_1(angle: rot_base * 4 + vee + sb_shift)
    rod_journal rj2(angle: rot_base * 2 + vee)
    rod_journal rj2_1(angle: rot_base * 2 + vee + sb_shift)
    rod_journal rj4(angle: rot_base * 4 + vee)
    rod_journal rj4_1(angle: rot_base * 0 + vee + sb_shift)
    rod_journal rj1(angle: rot_base * 3)
    rod_journal rj1_1(angle: rot_base * 1 + sb_shift)
    rod_journal rj3(angle: rot_base * 5)
    rod_journal rj3_1(angle: rot_base * 5 + sb_shift)
    rod_journal rj5(angle: rot_base * 1)
    rod_journal rj5_1(angle: rot_base * 3 + sb_shift)
    
    c0
        .add_rod_journal(rj0)
        .add_rod_journal(rj1)
        .add_rod_journal(rj0_1)
        .add_rod_journal(rj1_1)
        .add_rod_journal(rj2)
        .add_rod_journal(rj3)
        .add_rod_journal(rj2_1)
        .add_rod_journal(rj3_1)
        .add_rod_journal(rj4)
        .add_rod_journal(rj5)
        .add_rod_journal(rj4_1)
        .add_rod_journal(rj5_1)

    piston_parameters piston_params(
        mass: 440.0 * units.g,
        blowby: k_28inH2O(0.031),
        compression_height: compression_height,
        wrist_pin_position: 0.0,
        displacement: 0.0
    )

    connecting_rod_parameters cr_params(
        mass: 535.0 * units.g,
        moment_of_inertia: 0.0015,
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + (stroke / 2.0)) * units.mm + compression_height
    )

    label collector_cross_section_area(36.8)

    label exhaust_pipe_length_0(152.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 100.0) // Litres
    
    label exhaust_pipe_length_1(150.0) // cm
    label exhaust_volume_1(collector_cross_section_area * exhaust_pipe_length_1 / 100.0) // Litres

    intake intake(
        plenum_volume: 9.5 * units.L,
        plenum_cross_section_area: 117.0 * units.cm2,
        runner_length: 23.3 * units.cm,
        intake_flow_rate: k_carb(1125.1),
        idle_flow_rate: k_carb(0.02),
        idle_throttle_plate_position: 0.9975,
        runner_flow_rate: k_carb(234.4),
        velocity_decay: 0.05
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(1054.8),
        collector_cross_section_area: 36.8 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 20.1 * units.cm,
        primary_flow_rate: k_carb(395.5),
        velocity_decay: 0.88,
        volume: (exhaust_volume_0 + 3.0) * units.L
    )
    
    exhaust_system_parameters es_params1(
        outlet_flow_rate: k_carb(1054.8),
        collector_cross_section_area: 36.8 * units.cm2,
        length: exhaust_pipe_length_1 * units.cm,
        primary_tube_length: 20.1 * units.cm,
        primary_flow_rate: k_carb(395.5),
        velocity_decay: 0.88,
        volume: (exhaust_volume_1 + 3.0) * units.L
    )

    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_39.wav", volume: 0.001))
    exhaust_system exhaust1(es_params1, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_39.wav", volume: 0.001))

    cylinder_bank b0(bank_params, angle: vee / 2)
    cylinder_bank b1(bank_params, angle: -vee / 2)
    
    // label pl0 ((4.0 * bore * spacing_factor / flange_density) * units.mm)
    // label pl1 ((3.0 * bore * spacing_factor / flange_density) * units.mm)
    // label pl2 ((3.0 * bore * spacing_factor / flange_density) * units.mm)
    // label pl3 ((2.0 * bore * spacing_factor / flange_density) * units.mm)
    // label pl4 ((1.0 * bore * spacing_factor / flange_density) * units.mm)
    // label pl5 ((2.0 * bore * spacing_factor / flange_density) * units.mm)

    label spacing_factor(1.1)
    label flange_density(4.0)

    label pl0 ((3.2 * bore * spacing_factor / flange_density) * units.mm)
    label pl1 ((3.5 * bore * spacing_factor / flange_density) * units.mm)
    label pl2 ((3.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl3 ((1.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl4 ((1.5 * bore * spacing_factor / flange_density) * units.mm)
    label pl5 ((1.2 * bore * spacing_factor / flange_density) * units.mm)

    b0
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj0,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl0,
            ignition_wire: wires.wire1
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj0_1,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl1,
            ignition_wire: wires.wire7
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj2,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl2,
            ignition_wire: wires.wire2
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj2_1,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl3,
            ignition_wire: wires.wire8
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj4,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl4,
            ignition_wire: wires.wire3
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj4_1,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl5,
            ignition_wire: wires.wire9
        )
        
    b1
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj1,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl0,
            ignition_wire: wires.wire4
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj1_1,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl1,
            ignition_wire: wires.wire10
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj3,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl2,
            ignition_wire: wires.wire5
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj3_1,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl3,
            ignition_wire: wires.wire11
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj5,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl4,
            ignition_wire: wires.wire6
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj5_1,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl5,
            ignition_wire: wires.wire12
        )

    engine
        .add_cylinder_bank(b0)
        .add_cylinder_bank(b1)

    engine.add_crankshaft(c0)

    camshaft_builder camshaft()

    b0.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_0,
            exhaust_camshaft: camshaft.exhaust_cam_0,
            flip_display: true
        )
    )
    b1.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_1,
            exhaust_camshaft: camshaft.exhaust_cam_1
        )
    )

    function timing_curve(500.0 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 0.0 * units.deg)
        .add_sample(500.0 * units.rpm, 9.6 * units.deg)
        .add_sample(1000.0 * units.rpm, 19.4 * units.deg)
        .add_sample(1500.0 * units.rpm, 25.6 * units.deg)
        .add_sample(2000.0 * units.rpm, 29.5 * units.deg)
        .add_sample(2500.0 * units.rpm, 31.9 * units.deg)
        .add_sample(3000.0 * units.rpm, 33.5 * units.deg)
        .add_sample(3500.0 * units.rpm, 34.4 * units.deg)
        .add_sample(4000.0 * units.rpm, 35.0 * units.deg)
        .add_sample(4500.0 * units.rpm, 35.0 * units.deg)
        .add_sample(5000.0 * units.rpm, 35.0 * units.deg)
        .add_sample(5500.0 * units.rpm, 35.0 * units.deg)
        .add_sample(6000.0 * units.rpm, 35.0 * units.deg)
        .add_sample(6500.0 * units.rpm, 35.0 * units.deg)
        .add_sample(7000.0 * units.rpm, 35.0 * units.deg)
        .add_sample(7500.0 * units.rpm, 35.0 * units.deg)
        .add_sample(8000.0 * units.rpm, -15.0 * units.deg)
        .add_sample(8500.0 * units.rpm, -45.0 * units.deg)

    engine.add_ignition_module(
        eng_distributor(
            wires: wires,
            timing_curve: timing_curve
        )
    )
}

// 993
public node veh {
    alias output __out: vehicle;
    vehicle vehicle(
        mass: 1270  * units.kg,
        drag_coefficient: 0.32,
        cross_sectional_area: (1735  * units.mm) * (1311 * units.mm),
        diff_ratio: 3.2,
        tire_radius: (632.0 / 2) * units.mm, // 225/50ZR16
        rolling_resistance: 20
        )
}

// 993
private node trn {
    alias output __out:
        transmission(
            max_clutch_torque: 1300 * units.Nm,
            max_clutch_flex: 30 * units.deg,
            limit_clutch_flex: true,
            clutch_stiffness: 20 * units.Nm / units.deg,
            clutch_damping: 1.0,
            simulate_flex: true
        )
        .add_gear(3.82)
        .add_gear(2.05)
        .add_gear(1.41)
        .add_gear(1.12)
        .add_gear(0.93)
        .add_gear(0.78);
}

run(
    engine: eng(),
    transmission: trn(),
    vehicle: veh()
)

private node add_lobes {
    input base;
    input shift: 0.0;
    input this;
    alias output __out: this;

    this.add_lobe(base + 0 * rot * row + shift)
    this.add_lobe(base + 3 * rot * row + shift)
    this.add_lobe(base + 2 * rot * row + shift)
    this.add_lobe(base + 1 * rot * row + shift)
}

public node bank_builder {
    input angle;
    input phasing;
    input crank;
    input ignition;
    input intake;
    input exhaust;

    input flip: false;

    output bank: _b0;

    label spacing_factor(1.1)
    label flange_density(1.0 * exhaust_delay_coeff)

    label pl0 ((5.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl1 ((3.1 * bore * spacing_factor / flange_density) * units.mm)
    label pl2 ((2.9 * bore * spacing_factor / flange_density) * units.mm)
    label pl3 ((1.0 * bore * spacing_factor / flange_density) * units.mm)

    ignition_wire _wire1()
    ignition_wire _wire2()
    ignition_wire _wire3()
    ignition_wire _wire4()

    piston_parameters piston_params(
        mass: piston_mass * units.g,
        blowby: k_28inH2O(0.030),
        compression_height: compression_height,
        wrist_pin_position: 0.0,
        displacement: 0.0
    )

    connecting_rod_parameters cr_params(
        mass: con_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.g,
            length: con_rod * units.mm
        ),
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    camshaft _intake_cam_0(base_radius: 23.3 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 23.3 * units.mm, lobe_profile: exhaust_lobe_profile)

    _intake_cam_0.add_lobes(ILC, phasing)
    _exhaust_cam_0.add_lobes(ELC, phasing)

    ignition.connect_wire(_wire1, rot_base * 0 + phasing)
    ignition.connect_wire(_wire4, rot_base * 1 + phasing)
    ignition.connect_wire(_wire3, rot_base * 2 + phasing)
    ignition.connect_wire(_wire2, rot_base * 3 + phasing)

    cylinder_bank _b0(bank_params, angle: angle)

    _b0
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crank.rj0,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl0,
            ignition_wire: _wire1
        )
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crank.rj1,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl1,
            ignition_wire: _wire2
        )
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crank.rj2,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl2,
            ignition_wire: _wire3
        )
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crank.rj3,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl3,
            ignition_wire: _wire4
        )
    .set_cylinder_head(
        head(
            intake_camshaft: _intake_cam_0,
            exhaust_camshaft: _exhaust_cam_0,
            flip_display: flip
        )
    )
}

public node crank {
    input phasing;

    output c: _c;
    output rj0: _rj0;
    output rj1: _rj1;
    output rj2: _rj2;
    output rj3: _rj3;

    label crank_moment(disk_moment_of_inertia(mass: crank_mass * units.kg, radius: (stroke / 2.0) * units.mm))
    label flywheel_moment(disk_moment_of_inertia(mass: flywheel_mass * units.kg, radius: (stroke * 3.5) * units.mm))
    label other_moment(disk_moment_of_inertia(mass: 1 * units.kg, radius: 1.0 * units.cm))

    crankshaft _c(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: flywheel_mass * units.kg,
        mass: crank_mass * units.kg,
        friction_torque: 7.7 * units.Nm,
        moment_of_inertia: crank_moment + flywheel_moment + other_moment,
        tdc: 0.0
    )

    rod_journal _rj0(angle: rot_base * 0 + phasing)
    rod_journal _rj1(angle: rot_base * 3 + phasing)
    rod_journal _rj2(angle: rot_base * 2 + phasing)
    rod_journal _rj3(angle: rot_base * 1 + phasing)

    _c
        .add_rod_journal(_rj0)
        .add_rod_journal(_rj1)
        .add_rod_journal(_rj2)
        .add_rod_journal(_rj3)
}

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node head {
    // port_flow.py v1.5
    // intake port area: 18.1 cm²; saturated lift: 8.30 mm
    // exhaust port area: 13.1 cm²; saturated lift: 7.04 mm
    // cylinder volume: 645.4 cm³ (39.4 CI); engine volume: 5.163 L (315.1 CI)
    // chamber volume: 58.7 cm³ (3.6 CI)
    // 8 harmonic intake runner length: 24.5 cm; diameter: 5.2 cm
    // primary length: 19.8 cm, area: 14.4 cm², diameter: 4.3 cm
    // collector diameter: 7.6 cm, area: 45.9 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 58.673 * units.cc,
        intake_runner_volume: 529.3 * units.cc,
        intake_runner_cross_section_area: 21.6 * units.cm2,
        exhaust_runner_volume: 176.4 * units.cc,
        exhaust_runner_cross_section_area: 14.4 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "Ford Voodoo V8",
        starter_torque: 100 * units.Nm,
        starter_speed: 750 * units.rpm,
        redline: redline * units.rpm,
        fuel: s90(),
        throttle_gamma: 1.5,
        jitter: 0.9,
        noise: 0.01,
        hf_gain: 0.008,
        simulation_frequency: 12000
    )

    ignition_module ignition(timing_curve: timing_curve, rev_limit: redline * 1.05 * units.rpm, limiter_duration: 0.07)

    crank c0(vee / 2 + rot90)

    bank_builder bank0(
        angle: vee / 2,
        phasing: 0,
        crank: c0,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust0,
        flip: true
    )

    bank_builder bank1(
        angle: -vee / 2,
        phasing: vee,
        crank: c0,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust1
    )

    label collector_cross_section_area(23.0)

    label exhaust_pipe_length_0(350.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 1000.0) // Litres
    
    label exhaust_pipe_length_1(350.0) // cm
    label exhaust_volume_1(collector_cross_section_area * exhaust_pipe_length_1 / 1000.0) // Litres

    intake intake(
        plenum_volume: 6.5 * units.L,
        plenum_cross_section_area: 86.4 * units.cm2,
        runner_length: 24.5 * units.cm,
        intake_flow_rate: k_carb(793.8),
        idle_flow_rate: k_carb(0.003),
        idle_throttle_plate_position: 0.998,
        runner_flow_rate: k_carb(223.3),
        velocity_decay: 0.05
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(702.9),
        collector_cross_section_area: 23.0 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 19.8 * units.cm,
        primary_flow_rate: k_carb(373.4),
        velocity_decay: 0.88,
        volume: (exhaust_volume_0 + 5.0) * units.L
    )
    
    exhaust_system_parameters es_params1(
        outlet_flow_rate: k_carb(702.9),
        collector_cross_section_area: 23.0 * units.cm2,
        length: exhaust_pipe_length_1 * units.cm,
        primary_tube_length: 19.8 * units.cm,
        primary_flow_rate: k_carb(373.4),
        velocity_decay: 0.88,
        volume: (exhaust_volume_1 + 5.0) * units.L
    )

    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_52.wav", volume: 0.0012))
    exhaust_system exhaust1(es_params1, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_52.wav", volume: 0.0010))

    engine
        .add_crankshaft(c0.c)
        .add_cylinder_bank(bank0.bank)
        .add_cylinder_bank(bank1.bank)
        .add_ignition_module(ignition)

    function timing_curve(546.7 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 5.0 * units.deg)
        .add_sample(546.7 * units.rpm, 5.0 * units.deg)
        .add_sample(1093.3 * units.rpm, 7.5 * units.deg)
        .add_sample(1640.0 * units.rpm, 19.3 * units.deg)
        .add_sample(2186.7 * units.rpm, 27.4 * units.deg)
        .add_sample(2733.3 * units.rpm, 33.0 * units.deg)
        .add_sample(3280.0 * units.rpm, 36.8 * units.deg)
        .add_sample(3826.7 * units.rpm, 39.4 * units.deg)
        .add_sample(4373.3 * units.rpm, 40.0 * units.deg)
        .add_sample(4920.0 * units.rpm, 40.0 * units.deg)
        .add_sample(5466.7 * units.rpm, 40.0 * units.deg)
        .add_sample(6013.3 * units.rpm, 40.0 * units.deg)
        .add_sample(6560.0 * units.rpm, 40.0 * units.deg)
        .add_sample(7106.7 * units.rpm, 40.0 * units.deg)
        .add_sample(7653.3 * units.rpm, 40.0 * units.deg)
        .add_sample(8200.0 * units.rpm, 40.0 * units.deg)
        .add_sample(8746.7 * units.rpm, -12.5 * units.deg)
        .add_sample(9293.3 * units.rpm, -25.0 * units.deg)
}

// 458 Italia
public node veh {
    alias output __out: vehicle;

    label car_mass(1565 + 100)

    vehicle vehicle(
        mass: car_mass * units.kg,
        drag_coefficient: 0.35,
        cross_sectional_area: (1937 * units.mm) * (1213 * units.mm),
        diff_ratio: 5.14,
        tire_radius: (690.88 / 2) * units.mm, // P305/30YR20
        rolling_resistance: 0.012 * car_mass * 9.81
        )
}

// 458 Italia
private node trn {
    alias output __out:
        transmission(
            max_clutch_torque: 750 * units.Nm,
            max_clutch_flex: 15 * units.deg,
            limit_clutch_flex: true,
            clutch_stiffness: 50 * units.Nm / units.deg,
            clutch_damping: 1.0,
            simulate_flex: true
        )
        .add_gear(3.08)
        .add_gear(2.18)
        .add_gear(1.63)
        .add_gear(1.29)
        .add_gear(1.03)
        .add_gear(0.84)
        .add_gear(0.69);
}

run(
    engine: eng(),
    transmission: trn(),
    vehicle: veh()
)

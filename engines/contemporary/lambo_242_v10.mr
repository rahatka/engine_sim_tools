import "engine_sim.mr"
import "../../fuel/90.mr"

units units()
constants constants()

label bore(82.0)
label stroke(75.0)
label compression_ratio(10.5)
label con_rod(131.25) // ?
label compression_height(30.0 * units.mm) // ?
label intake_valve_diameter(31.5) // ?
label exhaust_valve_diameter(27.0) // ?
label intake_valves(2.0)
label exhaust_valves(2.0)

label cyl(10.0)
label row(2.0)
label cycle(720.0 * units.deg)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label vee(90.0 * units.deg)

label comp(vee - rot)
// label comp(0)

label redline(7500.0) // RPM

// Diablo 1990 valve timings
label IVL(9.5) // ?
label EVL(8.5) // ?
label IVO(27.0 * units.deg) //BTDC
label IVC(73.0 * units.deg) //ABDC
label EVO(70.0 * units.deg) //BBDC
label EVC(10.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(499.0) // g
label crank_mass(15.0) // kg
label flywheel_mass(8.0) // kg

label exhaust_delay_coeff(1.5)


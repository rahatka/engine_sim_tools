private node add_lobes {
    input base;
    input shift: 0.0;
    input this;
    alias output __out: this;

    this.add_lobe(base + 0 * rot_base * row + shift)
    this.add_lobe(base + 1 * rot_base * row + shift)
    this.add_lobe(base + 2 * rot_base * row + shift)
    this.add_lobe(base + 2 * rot_base * row + shift + sb_shift)
    this.add_lobe(base + 1 * rot_base * row + shift + sb_shift)
    this.add_lobe(base + 0 * rot_base * row + shift + sb_shift)
}

public node camshaft_builder {
    output intake_cam_0: _intake_cam_0;
    output exhaust_cam_0: _exhaust_cam_0;

    output intake_cam_1: _intake_cam_1;
    output exhaust_cam_1: _exhaust_cam_1;

    camshaft _intake_cam_0(base_radius: 16.1 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 16.1 * units.mm, lobe_profile: exhaust_lobe_profile)
    
    camshaft _intake_cam_1(base_radius: 16.1 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_1(base_radius: 16.1 * units.mm, lobe_profile: exhaust_lobe_profile)

    _intake_cam_0.add_lobes(ILC)
    _exhaust_cam_0.add_lobes(ELC)

    _intake_cam_1.add_lobes(ILC, vee)
    _exhaust_cam_1.add_lobes(ELC, vee)
}

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node head {
    // port_flow.py v1.5
    // intake port area: 16.7 cm²; saturated lift: 7.93 mm
    // exhaust port area: 12.1 cm²; saturated lift: 6.75 mm
    // cylinder volume: 578.3 cm³ (35.3 CI); engine volume: 6.940 L (423.5 CI)
    // chamber volume: 68.0 cm³ (4.2 CI)
    // 16 harmonic intake runner length: 16.4 cm; diameter: 4.3 cm
    // primary length: 29.1 cm, area: 12.7 cm², diameter: 4.0 cm
    // collector diameter: 9.3 cm, area: 68.6 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 68.040 * units.cc,
        intake_runner_volume: 233.5 * units.cc,
        intake_runner_cross_section_area: 14.2 * units.cm2,
        exhaust_runner_volume: 77.8 * units.cc,
        exhaust_runner_cross_section_area: 12.7 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "ZMZ-3xx V12 (f)",
        starter_torque: 200 *units.Nm,
        starter_speed: 300 * units.rpm,
        redline: redline * units.rpm,
        fuel: s90(),
        throttle_gamma: 1.5,
        jitter: 0.6,
        noise: 0.1,
        hf_gain: 0.006,
        simulation_frequency: 8000 
    )

    wires wires()

    connecting_rod_parameters cr_params(
        mass: con_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.g,
            length: con_rod * units.mm
        ),
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    label crank_moment(disk_moment_of_inertia(mass: crank_mass * units.kg, radius: (stroke / 2.0) * units.mm))
    label flywheel_moment(disk_moment_of_inertia(mass: flywheel_mass * units.kg, radius: (stroke * 2.7) * units.mm))
    label other_moment(disk_moment_of_inertia(mass: 1 * units.kg, radius: 1.0 * units.cm))

    crankshaft c0(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: flywheel_mass * units.kg,
        mass: crank_mass * units.kg,
        friction_torque: 10.4 * units.Nm,
        moment_of_inertia: crank_moment + flywheel_moment + other_moment,
        tdc: 0.0
    )

    rod_journal rj0(angle: rot_base * 0 + vee / 2 + rot90)
    rod_journal rj1(angle: rot_base * 2 + vee / 2 + rot90)
    rod_journal rj2(angle: rot_base * 4 + vee / 2 + rot90)
    rod_journal rj3(angle: rot_base * 4 + vee / 2 + rot90 + sb_shift)
    rod_journal rj4(angle: rot_base * 2 + vee / 2 + rot90 + sb_shift)
    rod_journal rj5(angle: rot_base * 0 + vee / 2 + rot90 + sb_shift)
    
    c0
        .add_rod_journal(rj0)
        .add_rod_journal(rj1)
        .add_rod_journal(rj2)
        .add_rod_journal(rj3)
        .add_rod_journal(rj4)
        .add_rod_journal(rj5)

    piston_parameters piston_params(
        mass: piston_mass * units.g,
        blowby: k_28inH2O(0.029),
        compression_height: compression_height,
        wrist_pin_position: 0.0,
        displacement: 0.0
    )

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    label collector_cross_section_area(34.3)

    label exhaust_pipe_length_0(350.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 100.0) // Litres
    
    label exhaust_pipe_length_1(355.0) // cm
    label exhaust_volume_1(collector_cross_section_area * exhaust_pipe_length_1 / 100.0) // Litres

    intake intake(
        plenum_volume: 6.0 * units.L,
        plenum_cross_section_area: 85.2 * units.cm2,
        runner_length: 16.4 * units.cm,
        intake_flow_rate: k_carb(728.7),
        idle_flow_rate: k_carb(0.005),
        idle_throttle_plate_position: 0.9977,
        runner_flow_rate: k_carb(127.5),
        velocity_decay: 0.05
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(622.4),
        collector_cross_section_area: 34.3 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 29.1 * units.cm,
        primary_flow_rate: k_carb(212.7),
        velocity_decay: 0.75,
        volume: (exhaust_volume_0 + 10.0) * units.L
    )
    
    exhaust_system_parameters es_params1(
        outlet_flow_rate: k_carb(622.4),
        collector_cross_section_area: 34.3 * units.cm2,
        length: exhaust_pipe_length_1 * units.cm,
        primary_tube_length: 29.1 * units.cm,
        primary_flow_rate: k_carb(212.7),
        velocity_decay: 0.75,
        volume: (exhaust_volume_1 + 10.0) * units.L
    )

    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_42.wav", volume: 0.001))
    exhaust_system exhaust1(es_params1, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_42.wav", volume: 0.001))
 
    cylinder_bank b0(bank_params, angle: vee / 2)
    cylinder_bank b1(bank_params, angle: -vee / 2)

    em0 em()

    b0
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj0,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: em.pl0,
            ignition_wire: wires.wire1
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj1,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: em.pl1,
            ignition_wire: wires.wire2
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj2,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: em.pl2,
            ignition_wire: wires.wire3
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj3,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: em.pl3,
            ignition_wire: wires.wire4
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj4,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: em.pl4,
            ignition_wire: wires.wire5
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj5,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: em.pl5,
            ignition_wire: wires.wire6
        )
        
    b1
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj0,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: em.pl0,
            ignition_wire: wires.wire7
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj1,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: em.pl1,
            ignition_wire: wires.wire8
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj2,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: em.pl2,
            ignition_wire: wires.wire9
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj3,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: em.pl3,
            ignition_wire: wires.wire10
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj4,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: em.pl4,
            ignition_wire: wires.wire11
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj5,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: em.pl5,
            ignition_wire: wires.wire12
        )

    engine
        .add_cylinder_bank(b0)
        .add_cylinder_bank(b1)

    engine.add_crankshaft(c0)

    camshaft_builder camshaft()

    b0.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_0,
            exhaust_camshaft: camshaft.exhaust_cam_0,
            flip_display: true
        )
    )
    b1.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_1,
            exhaust_camshaft: camshaft.exhaust_cam_1
        )
    )

    function timing_curve(400.0 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 7.0 * units.deg)
        .add_sample(400.0 * units.rpm, 7.0 * units.deg)
        .add_sample(800.0 * units.rpm, 13.7 * units.deg)
        .add_sample(1200.0 * units.rpm, 20.8 * units.deg)
        .add_sample(1600.0 * units.rpm, 26.2 * units.deg)
        .add_sample(2000.0 * units.rpm, 30.4 * units.deg)
        .add_sample(2400.0 * units.rpm, 33.6 * units.deg)
        .add_sample(2800.0 * units.rpm, 36.0 * units.deg)
        .add_sample(3200.0 * units.rpm, 37.0 * units.deg)
        .add_sample(3600.0 * units.rpm, 37.0 * units.deg)
        .add_sample(4000.0 * units.rpm, 37.0 * units.deg)
        .add_sample(4400.0 * units.rpm, 37.0 * units.deg)
        .add_sample(4800.0 * units.rpm, 37.0 * units.deg)
        .add_sample(5200.0 * units.rpm, 37.0 * units.deg)
        .add_sample(5600.0 * units.rpm, 37.0 * units.deg)
        .add_sample(6000.0 * units.rpm, 37.0 * units.deg)
        .add_sample(6400.0 * units.rpm, -15.0 * units.deg)
        .add_sample(6800.0 * units.rpm, -45.0 * units.deg)

    engine.add_ignition_module(
        eng_distributor(
            wires: wires,
            timing_curve: timing_curve
        )
    )
}

label spacing_factor(1.1)
label flange_density(1.8 * exhaust_delay_coeff)
label em_unit(bore * spacing_factor / flange_density)

private node em0{
    output pl0: (3.2 * em_unit) * units.mm;
    output pl1: (2.2 * em_unit) * units.mm;
    output pl2: (1.2 * em_unit) * units.mm;
    output pl3: (1.0 * em_unit) * units.mm;
    output pl4: (2.0 * em_unit) * units.mm;
    output pl5: (3.0 * em_unit) * units.mm;
}

private node em1{
    output pl0: (6.0 * em_unit) * units.mm;
    output pl1: (5.0 * em_unit) * units.mm;
    output pl2: (4.0 * em_unit) * units.mm;
    output pl3: (3.0 * em_unit) * units.mm;
    output pl4: (2.0 * em_unit) * units.mm;
    output pl5: (1.0 * em_unit) * units.mm;
}

private node em2{
    output pl0: (6.5 * em_unit) * units.mm;
    output pl1: (6.0 * em_unit) * units.mm;
    output pl2: (5.0 * em_unit) * units.mm;
    output pl3: (2.5 * em_unit) * units.mm;
    output pl4: (1.0 * em_unit) * units.mm;
    output pl5: (2.0 * em_unit) * units.mm;
}

// ГАЗ-3111
public node veh {
    alias output __out: vehicle;

    label car_mass(1700) // 1550 empty; 2030 full

    vehicle vehicle(
        mass: car_mass * units.kg,
        drag_coefficient: 0.38, // ?
        cross_sectional_area: (1850 * units.mm) * (1450 * units.mm),
        diff_ratio: 3.3, // 3.9
        tire_radius: (647.0 / 2) * units.mm, // 205/65 R15
        rolling_resistance: 0.012 * car_mass * 9.81
        )
}

// ГАЗ-3111
private node trn {
    alias output __out:
        transmission(
            max_clutch_torque: 800 * units.Nm,
            max_clutch_flex: 10 * units.deg,
            limit_clutch_flex: true,
            clutch_stiffness: 10 * units.Nm / units.deg,
            clutch_damping: 1.0,
            simulate_flex: true
        )
        .add_gear(4.05)
        .add_gear(2.34)
        .add_gear(1.40)
        .add_gear(1.00)
        .add_gear(0.85)
        .add_gear(0.72);
}

run(
    engine: eng(),
    transmission: trn(),
    vehicle: veh()
)

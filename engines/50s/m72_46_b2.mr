import "engine_sim.mr"
import "../../fuel/50.mr"

units units()
constants constants()

label bore(78.0)
label stroke(78.0)
label compression_ratio(5.5)
label con_rod(150.0)
label compression_height(41.0 * units.mm)
label intake_valve_diameter(38.0)
label exhaust_valve_diameter(38.0)
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(5000.0) // RPM

label cyl(2.0)
label row(2.0)
label cycle(720.0 * units.deg)
label vee(180.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// M-72
label IVL(6.3)
label EVL(6.3)
label IVO(76.0 * units.deg) //BTDC
label IVC(92.0 * units.deg) //ABDC
label EVO(116.0 * units.deg) //BBDC
label EVC(52.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(342.0) // g
label crank_mass(5.0) // ? kg
label flywheel_mass(1.5) // kg

public node eng_distributor {
    input wires;
    input timing_curve;
    input rev_limit: 6000 * units.rpm;
    input limiter_duration: 0.01;
    alias output __out:
        ignition_module(timing_curve: timing_curve, rev_limit: rev_limit, limiter_duration: limiter_duration)
            .connect_wire(wires.wire1, rot * 0)
            .connect_wire(wires.wire2, rot * 1);
}

private node wires {
    output wire1: ignition_wire();
    output wire2: ignition_wire();
}


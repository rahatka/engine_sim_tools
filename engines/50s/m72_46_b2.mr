public node camshaft_builder {
    output intake_cam_0: _intake_cam_0;
    output exhaust_cam_0: _exhaust_cam_0;

    output intake_cam_1: _intake_cam_1;
    output exhaust_cam_1: _exhaust_cam_1;

    camshaft _intake_cam_0(base_radius: 15.2 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 15.2 * units.mm, lobe_profile: exhaust_lobe_profile)
    
    camshaft _intake_cam_1(base_radius: 15.2 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_1(base_radius: 15.2 * units.mm, lobe_profile: exhaust_lobe_profile)

    _intake_cam_0
        .add_lobe(ILC + 0 * rot)
    _exhaust_cam_0
        .add_lobe(ELC + 0 * rot)
        
    _intake_cam_1
        .add_lobe(ILC + 1 * rot)
    _exhaust_cam_1
        .add_lobe(ELC + 1 * rot)
}

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node head {
    // port_flow.py v1.7
    // intake port area: 8.9 cm²; saturated lift: 8.22 mm
    // exhaust port area: 8.9 cm²; saturated lift: 8.22 mm
    // cylinder volume: 372.7 cm³ (22.74 CI); engine volume: 0.745 L (45.49 CI)
    // chamber volume: 82.8 cm³ (5.05 CI); conrod/stroke 1.923; N.A.C.C. 7.54
    // 8 harmonic intake runner length: 29.5 cm; diameter: 3.2 cm
    // primary length: 20.3 cm, area: 9.8 cm², diameter: 3.5 cm
    // collector diameter: 2.7 cm, area: 5.9 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 82.825 * units.cc,
        intake_runner_volume: 233.0 * units.cc,
        intake_runner_cross_section_area: 7.9 * units.cm2,
        exhaust_runner_volume: 77.7 * units.cc,
        exhaust_runner_cross_section_area: 9.8 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "M-72",
        starter_torque: 30 * units.Nm,
        starter_speed: 100 * units.rpm,
        redline: redline * units.rpm,
        fuel: s50(),
        throttle_gamma: 1.5,
        jitter: 1.0,
        noise: 1.0,
        simulation_frequency: 10000
    )

    wires wires()

    connecting_rod_parameters cr_params(
        mass: con_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.g,
            length: con_rod * units.mm
        ),
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    label crank_moment(disk_moment_of_inertia(mass: crank_mass * units.kg, radius: (stroke / 2.0) * units.mm))
    label flywheel_moment(disk_moment_of_inertia(mass: flywheel_mass * units.kg, radius: (stroke * 3.0) * units.mm))
    label other_moment(disk_moment_of_inertia(mass: 1 * units.kg, radius: 1.0 * units.cm))

    crankshaft c0(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: flywheel_mass * units.kg,
        mass: crank_mass * units.kg,
        friction_torque: 1.1 * units.Nm,
        moment_of_inertia: crank_moment + flywheel_moment + other_moment,
        position_x: 0.0,
        position_y: 0.0,
        tdc: 0.0
    )

    rod_journal rj0(angle: rot * 0 + rot90 + vee / 2)
    rod_journal rj1(angle: rot * 1 + rot180 + rot90 + vee / 2)
    
    c0
        .add_rod_journal(rj0)
        .add_rod_journal(rj1)

    piston_parameters piston_params(
        mass: 350.0 * units.g,
        blowby: k_28inH2O(0.025),
        compression_height: compression_height,
        wrist_pin_position: 0.0,
        displacement: 0.0
    )

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    label collector_cross_section_area(3.0)

    label exhaust_pipe_length_0(105.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 100.0) // Litres
    
    label exhaust_pipe_length_1(100.0) // cm
    label exhaust_volume_1(collector_cross_section_area * exhaust_pipe_length_1 / 100.0) // Litres

    intake intake(
        plenum_volume: 0.6 * units.L,
        plenum_cross_section_area: 7.9 * units.cm2,
        runner_length: 29.5 * units.cm,
        intake_flow_rate: k_carb(66.8),
        idle_flow_rate: k_carb(0.01),
        idle_throttle_plate_position: 0.994,
        runner_flow_rate: k_carb(71.8),
        velocity_decay: 0.05
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(57.7),
        collector_cross_section_area: 3.0 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 20.3 * units.cm,
        primary_flow_rate: k_carb(119.8),
        velocity_decay: 1.0,
        volume: (exhaust_volume_0 + 2.0) * units.L
    )

    exhaust_system_parameters es_params1(
        outlet_flow_rate: k_carb(57.7),
        collector_cross_section_area: 3.0 * units.cm2,
        length: exhaust_pipe_length_1 * units.cm,
        primary_tube_length: 20.3 * units.cm,
        primary_flow_rate: k_carb(119.8),
        velocity_decay: 1.0,
        volume: (exhaust_volume_1 + 2.0) * units.L
    )

    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_42.wav", volume: 0.00025))
    exhaust_system exhaust1(es_params1, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_42.wav", volume: 0.00025))

    cylinder_bank b0(bank_params, angle: vee / 2)
    cylinder_bank b1(bank_params, angle: -vee / 2)
    
    label pl0 (30 * units.mm)

    b0
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj0,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl0,
            ignition_wire: wires.wire1
        )
        
    b1
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj1,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl0,
            ignition_wire: wires.wire2
        )

    engine
        .add_cylinder_bank(b0)
        .add_cylinder_bank(b1)

    engine.add_crankshaft(c0)

    camshaft_builder camshaft()

    b0.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_0,
            exhaust_camshaft: camshaft.exhaust_cam_0,
            flip_display: true
        )
    )
    b1.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_1,
            exhaust_camshaft: camshaft.exhaust_cam_1
        )
    )

    function timing_curve(333.3 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 2.0 * units.deg)
        .add_sample(333.3 * units.rpm, 2.0 * units.deg)
        .add_sample(666.7 * units.rpm, 2.2 * units.deg)
        .add_sample(1000.0 * units.rpm, 2.7 * units.deg)
        .add_sample(1333.3 * units.rpm, 3.2 * units.deg)
        .add_sample(1666.7 * units.rpm, 3.9 * units.deg)
        .add_sample(2000.0 * units.rpm, 4.8 * units.deg)
        .add_sample(2333.3 * units.rpm, 5.8 * units.deg)
        .add_sample(2666.7 * units.rpm, 7.0 * units.deg)
        .add_sample(3000.0 * units.rpm, 8.5 * units.deg)
        .add_sample(3333.3 * units.rpm, 10.3 * units.deg)
        .add_sample(3666.7 * units.rpm, 12.5 * units.deg)
        .add_sample(4000.0 * units.rpm, 15.1 * units.deg)
        .add_sample(4333.3 * units.rpm, 18.4 * units.deg)
        .add_sample(4666.7 * units.rpm, 22.3 * units.deg)
        .add_sample(5000.0 * units.rpm, 27.0 * units.deg)
        .add_sample(5333.3 * units.rpm, -15.0 * units.deg)
        .add_sample(5666.7 * units.rpm, -45.0 * units.deg)

    engine.add_ignition_module(
        eng_distributor(
            wires: wires,
            timing_curve: timing_curve
        )
    )
}

// M-72
public node veh {
    alias output __out: vehicle;
    vehicle vehicle(
        mass: (225 + 77) * units.kg, // 350 sidecar
        drag_coefficient: 0.55,
        cross_sectional_area: (815 * units.mm) * (960 * units.mm),
        diff_ratio: 4.62,
        tire_radius: (673.1 / 2) * units.mm, // 3,75-19
        rolling_resistance: 20
        )
}

// M-72
private node trn {
    alias output __out:
        transmission(
            max_clutch_torque: 50 * units.Nm,
            max_clutch_flex: 3 * units.deg,
            limit_clutch_flex: true,
            clutch_stiffness: 50 * units.Nm / units.deg,
            clutch_damping: 2.0,
            simulate_flex: true
        )
        .add_gear(3.60)
        .add_gear(2.28)
        .add_gear(1.70)
        .add_gear(1.30);
}

run(
    engine: eng(),
    transmission: trn(),
    vehicle: veh()
)

public node camshaft_builder {
    output intake_cam_0: _intake_cam_0;
    output exhaust_cam_0: _exhaust_cam_0;

    camshaft _intake_cam_0(base_radius: 16.1 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 16.1 * units.mm, lobe_profile: exhaust_lobe_profile)

    _intake_cam_0
        .add_lobe(ILC + 0 * rot)
        .add_lobe(ILC + 4 * rot)
        .add_lobe(ILC + 2 * rot)
        .add_lobe(ILC + 5 * rot)
        .add_lobe(ILC + 1 * rot)
        .add_lobe(ILC + 3 * rot)
    _exhaust_cam_0
        .add_lobe(ELC + 0 * rot)
        .add_lobe(ELC + 4 * rot)
        .add_lobe(ELC + 2 * rot)
        .add_lobe(ELC + 5 * rot)
        .add_lobe(ELC + 1 * rot)
        .add_lobe(ELC + 3 * rot)
}

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node head {
    // port_flow.py v1.7
    // intake port area: 10.4 cm²; saturated lift: 8.85 mm
    // exhaust port area: 8.7 cm²; saturated lift: 8.09 mm
    // cylinder volume: 580.9 cm³ (35.45 CI); engine volume: 3.485 L (212.70 CI)
    // chamber volume: 111.7 cm³ (6.82 CI); conrod/stroke 1.836; N.A.C.C. 25.01
    // 32 harmonic intake runner length: 16.1 cm; diameter: 3.2 cm
    // primary length: 46.0 cm, area: 9.6 cm², diameter: 3.5 cm
    // collector diameter: 6.0 cm, area: 28.7 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 111.714 * units.cc,
        intake_runner_volume: 126.4 * units.cc,
        intake_runner_cross_section_area: 7.9 * units.cm2,
        exhaust_runner_volume: 43.6 * units.cc,
        exhaust_runner_cross_section_area: 9.6 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "GAZ-51",
        starter_torque: 250 * units.Nm,
        starter_speed: 300 * units.rpm,
        redline: redline * units.rpm,
        fuel: s30(),
        throttle_gamma: 1.5,
        jitter: 0.8,
        noise: 0.1,
        hf_gain: 0.008,
        simulation_frequency: 12000,
        dyno_min_speed: 500 * units.rpm
    )

    wires wires()

    connecting_rod_parameters cr_params(
        mass: con_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.g,
            length: con_rod * units.mm
        ),
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    label crank_moment(disk_moment_of_inertia(mass: crank_mass * units.kg, radius: (stroke / 2.0) * units.mm))
    label flywheel_moment(disk_moment_of_inertia(mass: flywheel_mass * units.kg, radius: (stroke * 3.0) * units.mm))
    label other_moment(disk_moment_of_inertia(mass: 1 * units.kg, radius: 1.0 * units.cm))

    crankshaft c0(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: flywheel_mass * units.kg,
        mass: crank_mass * units.kg,
        friction_torque: 5.2 * units.Nm,
        moment_of_inertia: crank_moment + flywheel_moment + other_moment,
        position_x: -3.0 * units.mm,
        position_y: 0.0,
        tdc: 0.0
    )

    label tdc_comp(-0.669 * units.deg) // 90 - math.acos(x_shift / (stroke / 2 + con_rod))

    rod_journal rj0(angle: rot * 0 + rot90 + tdc_comp)
    rod_journal rj1(angle: rot * 4 + rot90 + tdc_comp)
    rod_journal rj2(angle: rot * 2 + rot90 + tdc_comp)
    rod_journal rj3(angle: rot * 5 + rot90 + tdc_comp)
    rod_journal rj4(angle: rot * 1 + rot90 + tdc_comp)
    rod_journal rj5(angle: rot * 3 + rot90 + tdc_comp)
    
    c0
        .add_rod_journal(rj0)
        .add_rod_journal(rj1)
        .add_rod_journal(rj2)
        .add_rod_journal(rj3)
        .add_rod_journal(rj4)
        .add_rod_journal(rj5)

    piston_parameters piston_params(
        mass: 450.0 * units.g, // GAZ-12
        blowby: k_28inH2O(0.026),
        compression_height: compression_height,
        wrist_pin_position: 0.0,
        displacement: 0.0
    )

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    label collector_cross_section_area(28.7)

    label exhaust_pipe_length_0(350.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 1000.0) // Litres
    
    label spacing_factor(1.1)
    label flange_density(1.0 * exhaust_delay_coeff)

    intake intake(
        plenum_volume: 1.4 * units.L,
        plenum_cross_section_area: 23.6 * units.cm2,
        runner_length: 16.1 * units.cm,
        intake_flow_rate: k_carb(188.1),
        idle_flow_rate: k_carb(0.02),
        idle_throttle_plate_position: 0.994,
        runner_flow_rate: k_carb(62.7),
        velocity_decay: 0.05
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(301.7),
        collector_cross_section_area: 28.7 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 46.0 * units.cm,
        primary_flow_rate: k_carb(100.6),
        velocity_decay: 1.0,
        volume: (exhaust_volume_0 + 10.0) * units.L
    )

    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_52.wav", volume: 0.00025))
 
    cylinder_bank b0(bank_params, angle: 0)
    
    // GAZ-51
    label pl0((5.71 * bore * spacing_factor / flange_density) * units.mm)
    label pl1((3.66 * bore * spacing_factor / flange_density) * units.mm)
    label pl2((2.56 * bore * spacing_factor / flange_density) * units.mm)
    label pl3((2.21 * bore * spacing_factor / flange_density) * units.mm)
    label pl4((1.57 * bore * spacing_factor / flange_density) * units.mm)
    label pl5((2.43 * bore * spacing_factor / flange_density) * units.mm)

    b0
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj0,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl0,
            ignition_wire: wires.wire1
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj1,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl1,
            ignition_wire: wires.wire2
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj2,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl2,
            ignition_wire: wires.wire3
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj3,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl3,
            ignition_wire: wires.wire4
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj4,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl4,
            ignition_wire: wires.wire5
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj5,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl5,
            ignition_wire: wires.wire6
        )

    engine
        .add_cylinder_bank(b0)

    engine.add_crankshaft(c0)

    camshaft_builder camshaft()

    b0.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_0,
            exhaust_camshaft: camshaft.exhaust_cam_0,
            flip_display: true
        )
    )

    function timing_curve(233.3 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 2.0 * units.deg)
        .add_sample(233.3 * units.rpm, 2.0 * units.deg)
        .add_sample(466.7 * units.rpm, 2.0 * units.deg)
        .add_sample(700.0 * units.rpm, 5.2 * units.deg)
        .add_sample(933.3 * units.rpm, 8.4 * units.deg)
        .add_sample(1166.7 * units.rpm, 11.1 * units.deg)
        .add_sample(1400.0 * units.rpm, 13.3 * units.deg)
        .add_sample(1633.3 * units.rpm, 15.1 * units.deg)
        .add_sample(1866.7 * units.rpm, 16.6 * units.deg)
        .add_sample(2100.0 * units.rpm, 17.9 * units.deg)
        .add_sample(2333.3 * units.rpm, 18.9 * units.deg)
        .add_sample(2566.7 * units.rpm, 19.8 * units.deg)
        .add_sample(2800.0 * units.rpm, 20.5 * units.deg)
        .add_sample(3033.3 * units.rpm, 21.1 * units.deg)
        .add_sample(3266.7 * units.rpm, 21.6 * units.deg)
        .add_sample(3500.0 * units.rpm, 22.0 * units.deg)
        .add_sample(3733.3 * units.rpm, -12.5 * units.deg)
        .add_sample(3966.7 * units.rpm, -25.0 * units.deg)

    engine.add_ignition_module(
        eng_distributor(
            wires: wires,
            timing_curve: timing_curve
        )
    )
}

// ПАЗ-651
public node veh {
    alias output __out: vehicle;

    label car_mass(3750 + 2500) // 3750 empty + 3185 max load

    vehicle vehicle(
        mass: car_mass * units.kg,
        drag_coefficient: 0.53,
        cross_sectional_area: (2400 * units.mm) * (2870 * units.mm),
        diff_ratio: 6.67,
        tire_radius: (927.1 / 2) * units.mm, // 8.25x20
        rolling_resistance: 0.012 * car_mass * 9.81
        )
}

// ПАЗ-651
private node trn {
    alias output __out:
        transmission(
            max_clutch_torque: 300 * units.Nm,
            max_clutch_flex: 30 * units.deg,
            limit_clutch_flex: true,
            clutch_stiffness: 20 * units.Nm / units.deg,
            clutch_damping: 1.0,
            simulate_flex: true
        )
        .add_gear(6.40)
        .add_gear(3.09)
        .add_gear(1.69)
        .add_gear(1.0);
}

run(
    engine: eng(),
    transmission: trn(),
    vehicle: veh()
)

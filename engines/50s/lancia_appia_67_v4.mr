public node camshaft_builder {
    output intake_cam_0: _intake_cam_0;
    output exhaust_cam_0: _exhaust_cam_0;

    output intake_cam_1: _intake_cam_1;
    output exhaust_cam_1: _exhaust_cam_1;

    camshaft _intake_cam_0(base_radius: 10.0 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 10.0 * units.mm, lobe_profile: exhaust_lobe_profile)
    
    camshaft _intake_cam_1(base_radius: 10.0 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_1(base_radius: 10.0 * units.mm, lobe_profile: exhaust_lobe_profile)

    _intake_cam_0.add_lobe(ILC + 0 * rot)
    _intake_cam_0.add_lobe(ILC + 1 * rot)
    _exhaust_cam_0.add_lobe(ELC + 0 * rot)
    _exhaust_cam_0.add_lobe(ELC + 1 * rot)

    _intake_cam_1.add_lobe(ILC + 3 * rot)
    _intake_cam_1.add_lobe(ILC + 2 * rot)
    _exhaust_cam_1.add_lobe(ELC + 3 * rot)
    _exhaust_cam_1.add_lobe(ELC + 2 * rot)
}

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node head {
    // port_flow.py v1.5
    // intake port area: 5.2 cm²; saturated lift: 6.24 mm
    // exhaust port area: 4.3 cm²; saturated lift: 5.72 mm
    // cylinder volume: 272.4 cm³ (16.6 CI); engine volume: 1.090 L (66.5 CI)
    // chamber volume: 40.1 cm³ (2.4 CI)
    // 16 harmonic intake runner length: 18.5 cm; diameter: 2.7 cm
    // primary length: 32.7 cm, area: 4.8 cm², diameter: 2.5 cm
    // collector diameter: 2.7 cm, area: 5.7 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 40.055 * units.cc,
        intake_runner_volume: 108.1 * units.cc,
        intake_runner_cross_section_area: 5.8 * units.cm2,
        exhaust_runner_volume: 36.0 * units.cc,
        exhaust_runner_cross_section_area: 4.8 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "Lancia Appia",
        starter_torque: 50 *units.Nm,
        starter_speed: 300 * units.rpm,
        redline: redline * units.rpm,
        fuel: s50(),
        throttle_gamma: 1.5,
        jitter: 0.7,
        noise: 0.3,
        hf_gain: 0.007,
        simulation_frequency: 10000
    )

    wires wires()
    
    connecting_rod_parameters cr_params(
        mass: con_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.g,
            length: con_rod * units.mm
        ),
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    label crank_moment(disk_moment_of_inertia(mass: crank_mass * units.kg, radius: (stroke / 2.0) * units.mm))
    label flywheel_moment(disk_moment_of_inertia(mass: flywheel_mass * units.kg, radius: (stroke * 3.5) * units.mm))
    label other_moment(disk_moment_of_inertia(mass: 1 * units.kg, radius: 1.0 * units.cm))

    crankshaft c0(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: 5.0 * units.kg,
        mass: crank_mass * units.kg,
        friction_torque: 1.6 * units.Nm,
        moment_of_inertia: crank_moment + flywheel_moment + other_moment,
        position_x: 0.0,
        position_y: 0.0,
        tdc: 0.0
    )

    rod_journal rj0(angle: rot * 0 + rot90 + vee / 2)
    rod_journal rj1(angle: rot * 3 + rot90 - vee / 2)
    rod_journal rj2(angle: rot * 1 + rot90 + vee / 2)
    rod_journal rj3(angle: rot * 2 + rot90 - vee / 2)
    
    c0
        .add_rod_journal(rj0)
        .add_rod_journal(rj1)
        .add_rod_journal(rj2)
        .add_rod_journal(rj3)

    piston_parameters piston_params(
        mass: 400 * units.g,
        blowby: k_28inH2O(0.021),
        compression_height: compression_height,
        wrist_pin_position: 0.0,
        displacement: 0.0
    )

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    label collector_cross_section_area(5.7)

    label exhaust_pipe_length_0(300.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 100.0) // Litres
    
    label spacing_factor(1.5)
    label flange_density(1.0 * exhaust_delay_coeff)

    intake intake(
        plenum_volume: 0.8 * units.L,
        plenum_cross_section_area: 11.7 * units.cm2,
        runner_length: 18.5 * units.cm,
        intake_flow_rate: k_carb(90.8),
        idle_flow_rate: k_carb(0.01),
        idle_throttle_plate_position: 0.9955,
        runner_flow_rate: k_carb(45.4),
        velocity_decay: 0.05
    )

    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(151.3),
        collector_cross_section_area: 5.7 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 32.7 * units.cm,
        primary_flow_rate: k_carb(75.7),
        velocity_decay: 1.0,
        volume: (exhaust_volume_0 + 3.0) * units.L
    )
    
    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_58.wav", volume: 0.00025))
 
    cylinder_bank b0(bank_params, angle: vee / 2)
    cylinder_bank b1(bank_params, angle: -vee / 2)
    
    label pl0 ((2.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl1 ((1.0 * bore * spacing_factor / flange_density) * units.mm)
    label sh (bore * units.mm)

    b0
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj0,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl0 + sh,
            ignition_wire: wires.wire1
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj2,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl1 + sh,
            ignition_wire: wires.wire3
        )
        
    b1
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj1,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl0,
            ignition_wire: wires.wire2
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj3,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl1,
            ignition_wire: wires.wire4
        )

    engine
        .add_cylinder_bank(b0)
        .add_cylinder_bank(b1)

    engine.add_crankshaft(c0)

    camshaft_builder camshaft()

    b0.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_0,
            exhaust_camshaft: camshaft.exhaust_cam_0
        )
    )
    b1.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_1,
            exhaust_camshaft: camshaft.exhaust_cam_1
        )
    )

    function timing_curve(333.3 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 5.0 * units.deg)
        .add_sample(333.3 * units.rpm, 5.0 * units.deg)
        .add_sample(666.7 * units.rpm, 5.0 * units.deg)
        .add_sample(1000.0 * units.rpm, 6.4 * units.deg)
        .add_sample(1333.3 * units.rpm, 8.7 * units.deg)
        .add_sample(1666.7 * units.rpm, 11.0 * units.deg)
        .add_sample(2000.0 * units.rpm, 13.2 * units.deg)
        .add_sample(2333.3 * units.rpm, 15.5 * units.deg)
        .add_sample(2666.7 * units.rpm, 17.8 * units.deg)
        .add_sample(3000.0 * units.rpm, 20.1 * units.deg)
        .add_sample(3333.3 * units.rpm, 22.4 * units.deg)
        .add_sample(3666.7 * units.rpm, 24.7 * units.deg)
        .add_sample(4000.0 * units.rpm, 27.0 * units.deg)
        .add_sample(4333.3 * units.rpm, 27.0 * units.deg)
        .add_sample(4666.7 * units.rpm, 27.0 * units.deg)
        .add_sample(5000.0 * units.rpm, 27.0 * units.deg)
        .add_sample(5333.3 * units.rpm, -15.0 * units.deg)
        .add_sample(5666.7 * units.rpm, -60.0 * units.deg)

    engine.add_ignition_module(
        eng_distributor(
            wires: wires,
            timing_curve: timing_curve
        )
    )
}

// Scorpio
public node veh {
    alias output __out: vehicle;
    vehicle vehicle(
        mass: 1340 * units.kg,
        drag_coefficient: 0.32,
        cross_sectional_area: (1760 * units.mm) * (1390 * units.mm),
        diff_ratio: 3.36,
        tire_radius: (616.0 / 2) * units.mm, // 185/70 R14
        rolling_resistance: 20
        )
}

// Scorpio
private node trn {
    alias output __out:
        transmission(
            max_clutch_torque: 250 * units.Nm,
            max_clutch_flex: 30 * units.deg,
            limit_clutch_flex: true,
            clutch_stiffness: 10 * units.Nm / units.deg,
            clutch_damping: 1.0,
            simulate_flex: true
        )
        .add_gear(2.47)
        .add_gear(1.47)
        .add_gear(1.0)
        .add_gear(0.75);
}

run(
    engine: eng(),
    transmission: trn(),
    vehicle: veh()
)

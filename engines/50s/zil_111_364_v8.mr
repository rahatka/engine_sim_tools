    label spacing_factor(1.1)

    float _pl0 ((3.6 * bore * spacing_factor / flange_density) * units.mm)
    float _pl1 ((2.1 * bore * spacing_factor / flange_density) * units.mm)
    float _pl2 ((2.0 * bore * spacing_factor / flange_density) * units.mm)

import "engine_sim.mr"
import "../../fuel/50.mr"

units units()
constants constants()

label bore(96.8375) // 3 13/16
label stroke(114.3) // 4 1/2
label compression_ratio(7.5)
label con_rod(206.375) // 8 1/8
label compression_height(50.0 * units.mm) // 1.969
label intake_valve_diameter(46.5074) // 1.831 ; .3417 stem
label exhaust_valve_diameter(39.5224) // 1.556 ; .3407 stem
label intake_valves(1.0)
label exhaust_valves(1.0)

label cyl(6.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)

label redline(4000.0) // RPM

// 1951 Hudson
label IVL(8.7884) // .346
label EVL(8.7884) // .346
label IVO(33.7 * units.deg) //BTDC
label IVC(80.1 * units.deg) //ABDC
label EVO(73.9 * units.deg) //BBDC
label EVC(28.4 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label piston_mass(526.2) // 18.56 oz
label con_rod_mass(935.5) // 2 lb 1 oz
label crank_mass(39.69) // 87.5 lb
label flywheel_mass(5.0) // kg

label exhaust_delay_coeff(1.5)


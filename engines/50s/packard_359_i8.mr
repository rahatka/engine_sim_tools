    float _pl2 ((2.75 * em_unit) * units.mm)
    float _pl3 ((0.50 * em_unit) * units.mm)
    float _pl4 ((0.25 * em_unit) * units.mm)
    float _pl5 ((1.25 * em_unit) * units.mm)

    float _pl6 ((2.75 * em_unit) * units.mm)
    float _pl7 ((4.25 * em_unit) * units.mm)

    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(310.2),

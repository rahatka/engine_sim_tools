// Packard 5406
public node veh {
    alias output __out: vehicle;

    label car_mass(2100) // 1900 empty

    vehicle vehicle(
        mass: car_mass * units.kg,
        drag_coefficient: 0.48,
        cross_sectional_area: (1978 * units.mm) * (1597 * units.mm), // 77 7/8 x 62 7/8
        diff_ratio: 3.54, // 4.55 overdrive 3.54 ultramatic
        tire_radius: (751.0 / 2) * units.mm, // 8.0 x 15
        rolling_resistance: 0.012 * car_mass * 9.81,
        max_brake_force: 15000 * units.Nm
        )
}

// Packard 5406
private node trn {
    alias output __out:
        transmission(
            max_clutch_torque: 550 * units.Nm,
            max_clutch_flex: 20 * units.deg,
            limit_clutch_flex: true,
            clutch_stiffness: 20 * units.Nm / units.deg,
            clutch_damping: 1.0,
            simulate_flex: true
        )
        .add_gear(2.43)
        .add_gear(1.53)
        .add_gear(1.00);
}

run(
    engine: eng(),
    transmission: trn(),
    vehicle: veh()
)

            exhaust_system: es.es7,
            primary_length: es.pl7,
            ignition_wire: wires.wire8
        )

    camshaft_builder camshaft()

    b0.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_0,
            exhaust_camshaft: camshaft.exhaust_cam_0,
            flip_display: true
        )
    )

    function timing_curve(273.3 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 6.0 * units.deg)
        .add_sample(273.3 * units.rpm, 6.0 * units.deg)
        .add_sample(546.7 * units.rpm, 6.0 * units.deg)
        .add_sample(820.0 * units.rpm, 7.4 * units.deg)
        .add_sample(1093.3 * units.rpm, 9.0 * units.deg)

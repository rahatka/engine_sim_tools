import "engine_sim.mr"
import "../../fuel/30.mr"

units units()
constants constants()

label bore(101.6)
label stroke(114.3)
label compression_ratio(6.0) // 6.2 ZIL-158 / 164
label con_rod(217.0)
label compression_height(56.0 * units.mm) // ZIS-120
label intake_valve_diameter(48.5) // ZIS-120 / ZIL-164 150-В-1007010-Б
label exhaust_valve_diameter(44.0) // ZIS-120 / ZIL-164 150-В-1007015-Б
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(3000.0) // RPM

label cyl(12.0)
label row(2.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label vee(rot180)

label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)

// ZIL-120 valve timings
label IVL(10.42) // ZIS-120
label EVL(10.42) // ZIS-120
label IVO(20.0 * units.deg) //BTDC
label IVC(69.0 * units.deg) //ABDC
label EVO(67.0 * units.deg) //BBDC
label EVC(22.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label piston_mass(1300.0) // ?
label con_rod_mass(1485.0) // g
label crank_mass(28.0) // ? kg
label flywheel_mass(15.0) // kg

label exhaust_delay_coeff(1.5)


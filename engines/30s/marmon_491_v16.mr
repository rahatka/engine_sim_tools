import "engine_sim.mr"
import "_fuel_30.mr"

units units()
constants constants()

label vee(45.0 * units.deg)
label bore(79.375) // 3 1/8
label stroke(101.6) // 4
label compression_ratio(6.0)
label con_rod(203.2) // 8
label compression_height(50.8 * units.mm)  // ~
label intake_valve_diameter(39.6875) // 1 9/16
label exhaust_valve_diameter(34.925) // 1 3/8
label intake_valves(1.0)
label exhaust_valves(1.0)

label cyl(16.0)
label row(2.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

label volume(8.044) // L;
label redline(3750.0) // RPM

// Marmon V16
label IVL(8.33437) // 21/64
label EVL(8.33437) // 21/64
label IVO(6.0 * units.deg) //BTDC
label IVC(40.0 * units.deg) //ABDC
label EVO(40.0 * units.deg) //BBDC
label EVC(6.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(800.0) // g
label crank_mass(50.0) // kg
label flywheel_mass(12.0) // kg

label exhaust_delay_coeff(1.5)

public node eng_distributor {
    input wires;
    input timing_curve;
    input rev_limit: 4750 * units.rpm;
    input limiter_duration: 0.0001;
    alias output __out:
        ignition_module(timing_curve: timing_curve, rev_limit: rev_limit, limiter_duration: limiter_duration)
            .connect_wire(wires.wire1, rot * 0)
            .connect_wire(wires.wire6, rot * 2)
            .connect_wire(wires.wire2, rot * 4)
            .connect_wire(wires.wire5, rot * 6)
            .connect_wire(wires.wire8, rot * 8)
            .connect_wire(wires.wire3, rot * 10)
            .connect_wire(wires.wire7, rot * 12)
            .connect_wire(wires.wire4, rot * 14)
            .connect_wire(wires.wire9, rot * 0 + vee + rot360)
            .connect_wire(wires.wire14, rot * 2 + vee + rot360)
            .connect_wire(wires.wire10, rot * 4 + vee + rot360)
            .connect_wire(wires.wire13, rot * 6 + vee + rot360)
            .connect_wire(wires.wire16, rot * 8 + vee + rot360)
            .connect_wire(wires.wire11, rot * 10 + vee + rot360)
            .connect_wire(wires.wire15, rot * 12 + vee + rot360)
            .connect_wire(wires.wire12, rot * 14 + vee + rot360);
}


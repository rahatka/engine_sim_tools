import "_fuel_30.mr"

units units()
constants constants()

label bore(111.125) // 4 3/8
label stroke(146.05) // 5 3/4
label compression_ratio(5.5)
label con_rod(319.087) // 12 9/16
label compression_height(146.05 / 2 * units.mm) // 5.75 len
label intake_valve_diameter(55.372) // 2.18 ; 2.04 clear ; .437 stem
label exhaust_valve_diameter(48.006) // 1.89 ; 1.75 clear ; .437 stem
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(2500.0) // RPM

label cyl(6.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// 1939 Mack
label IVL(12.7) // 1/2
label EVL(12.7) // 1/2
label IVO(10.0 * units.deg) //BTDC
label IVC(45.0 * units.deg) //ABDC
label EVO(45.0 * units.deg) //BBDC
label EVC(10.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(2849.13) // 100.5
label piston_mass(2353.0) // 83
label crank_mass(30.0) // ?
label flywheel_mass(5.0) // kg

label exhaust_delay_coeff(1.5)


import "engine_sim.mr"
import "../../fuel/30.mr"

units units()
constants constants()

label bore(76.2) // 3
label stroke(101.6) // 4
label compression_ratio(5.5)
label con_rod(228.6) // 9 ?
label compression_height(bore / 2 * units.mm) // ?
label intake_valve_diameter(42.8625) // 1 11/16 ?
label exhaust_valve_diameter(38.1) // 1 1/2 ?
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(3250.0) // RPM

label cyl(12.0)
label cyl_base(cyl / row)
label row(2.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label rot_base(cycle / cyl_base)
label vee(60.0 * units.deg)

// 1932 Franklin Series 12 Airman
label IVL(7.9375) // .120 ; maybe 5/16 ?
label EVL(7.9375) // .120 ; maybe 5/16 ?
label IVO(0.0 * units.deg) //BTDC
label IVC(57.0 * units.deg) //ABDC
label EVO(43.5 * units.deg) //BBDC
label EVC(25.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(1000.0) // ? g
label piston_mass(600.0) // ?
label crank_mass(30.0) // ? kg
label flywheel_mass(10.0) // kg

label exhaust_delay_coeff(1.5)


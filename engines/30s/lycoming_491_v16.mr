import "engine_sim.mr"
import "../../fuel/30.mr"

units units()
constants constants()

label bore(79.375) // 3 1/8
label stroke(101.6) // 4
label compression_ratio(5.5)
label con_rod(239.712) // 9 7/16 ?
label compression_height(60.0 * units.mm) // ?
label intake_valve_diameter(42.0687) // 1 21/32
label exhaust_valve_diameter(38.8937) // 1 17/32
label intake_valves(1.0)
label exhaust_valves(1.0)

label cyl(16.0)
label row(2.0)
label cycle(720.0 * units.deg)
label vee(45.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)

label redline(3750.0) // RPM

// Lyc BB
label IVL(8.73125) // 11/32
label EVL(8.73125) // 11/32
label IVO(0.0 * units.deg) //BTDC
label IVC(45.0 * units.deg) //ABDC
label EVO(50.0 * units.deg) //BBDC
label EVC(10.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(1061.406) // ?
label piston_mass(481.942) // 17 oz
label crank_mass(46.946) // ?
label flywheel_mass(8.0) // kg

label exhaust_delay_coeff(1.5)


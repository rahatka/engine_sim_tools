public node veh {
    alias output __out: vehicle;
    vehicle vehicle(
        mass: 1370 * units.kg,
        drag_coefficient: 0.497,
        cross_sectional_area: (1770 * units.mm) * (1780 * units.mm),
        diff_ratio: 4.11,
        tire_radius: (774.7 / 2) * units.mm,
        rolling_resistance: 20
        )
}

// ГАЗ М-1
private node trn {
    alias output __out:
        transmission(
            max_clutch_torque: 200 * units.Nm,
            max_clutch_flex: 20 * units.deg,
            limit_clutch_flex: true,
            clutch_stiffness: 20 * units.Nm / units.deg,
            clutch_damping: 2.0,
            simulate_flex: true
        )
        .add_gear(2.82)
        .add_gear(1.64)
        .add_gear(1.0);
}

run(
    engine: eng(),
    transmission: trn(),
    vehicle: veh()
)


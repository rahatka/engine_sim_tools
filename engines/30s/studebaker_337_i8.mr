        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj7,
            intake: intake,
            exhaust_system: es.es7,
            primary_length: es.pl7,
            ignition_wire: wires.wire8
        )

    camshaft_builder camshaft()

    b0.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_0,
            exhaust_camshaft: camshaft.exhaust_cam_0,
            flip_display: true
        )
    )

    function timing_curve(233.3 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 5.0 * units.deg)
        .add_sample(233.3 * units.rpm, 5.0 * units.deg)
        .add_sample(466.7 * units.rpm, 5.0 * units.deg)
        .add_sample(700.0 * units.rpm, 6.2 * units.deg)
        .add_sample(933.3 * units.rpm, 8.8 * units.deg)
        .add_sample(1166.7 * units.rpm, 11.1 * units.deg)
        .add_sample(1400.0 * units.rpm, 13.1 * units.deg)
        .add_sample(1633.3 * units.rpm, 14.8 * units.deg)
        .add_sample(1866.7 * units.rpm, 16.3 * units.deg)
        .add_sample(2100.0 * units.rpm, 17.6 * units.deg)
        .add_sample(2333.3 * units.rpm, 18.8 * units.deg)
        .add_sample(2566.7 * units.rpm, 19.8 * units.deg)
        .add_sample(2800.0 * units.rpm, 20.7 * units.deg)
        .add_sample(3033.3 * units.rpm, 21.5 * units.deg)
        .add_sample(3266.7 * units.rpm, 22.0 * units.deg)
        .add_sample(3500.0 * units.rpm, 22.0 * units.deg)
        .add_sample(3733.3 * units.rpm, -12.5 * units.deg)
        .add_sample(3966.7 * units.rpm, -25.0 * units.deg)

    engine
        .add_cylinder_bank(b0)
        .add_crankshaft(c0)
        .add_ignition_module(
            eng_distributor(
                wires: wires,
                timing_curve: timing_curve
            )
        )
}

label spacing_factor(1.2)
label flange_density(1.0 * exhaust_delay_coeff)
label em_unit(bore * spacing_factor / flange_density)

// exhaust port layout
// |   ||   ||   ||   |
//  O O  O O  O O  O O

private node es0{
    output es0: exhaust0; output es1: exhaust0; output es2: exhaust0; output es3: exhaust0;
    output es4: exhaust0; output es5: exhaust0; output es6: exhaust0; output es7: exhaust0;
    output pl0: _pl0; output pl1: _pl1; output pl2: _pl2; output pl3: _pl3;
    output pl4: _pl4; output pl5: _pl5; output pl6: _pl6; output pl7: _pl7;
    output jitter: 0.7;
    output noise: 0.05;
    output hf_gain: 0.004;

    label collector_cross_section_area(25.4)
    label exhaust_pipe_length_0(450.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 100.0) // Litres

    float _pl0 ((8.0 * em_unit) * units.mm)
    float _pl1 ((6.4 * em_unit) * units.mm)
    float _pl2 ((6.1 * em_unit) * units.mm)
    float _pl3 ((4.7 * em_unit) * units.mm)
    float _pl4 ((4.3 * em_unit) * units.mm)
    float _pl5 ((2.7 * em_unit) * units.mm)
    float _pl6 ((2.6 * em_unit) * units.mm)
    float _pl7 ((1.3 * em_unit) * units.mm)

    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(536.5),
        collector_cross_section_area: 25.4 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 41.0 * units.cm,
        primary_flow_rate: k_carb(134.1),
        velocity_decay: 1.0,
        volume: (exhaust_volume_0 + 20.0) * units.L
    )

    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_52.wav", volume: 0.00025))
}

// LaSalle 350
public node veh {
    alias output __out: vehicle;

    label car_mass(1910) // 1710 empty

    vehicle vehicle(
        mass: car_mass * units.kg,
        drag_coefficient: 0.42,
        cross_sectional_area: (1866.9 * units.mm) * (1700 * units.mm), // height ?
        diff_ratio: 4.78,
        tire_radius: (786.0 / 2) * units.mm, // 7.0 x 16
        rolling_resistance: 0.012 * car_mass * 9.81
        )
}

// LaSalle 350
private node trn {
    alias output __out:
        transmission(
            max_clutch_torque: 500 * units.Nm,
            max_clutch_flex: 20 * units.deg,
            limit_clutch_flex: true,
            clutch_stiffness: 20 * units.Nm / units.deg,
            clutch_damping: 1.0,
            simulate_flex: true
        )
        .add_gear(2.68)
        .add_gear(1.70)
        .add_gear(1.00);
}

run(
    engine: eng(),
    transmission: trn(),
    vehicle: veh()
)

units units()
constants constants()

label bore(79.4) // 3 1/8
label stroke(117.5) // 4 5/8
label compression_ratio(5.5) // 5.2 solex
label con_rod(247.66) // 9 3/4
label compression_height(bore / 2 * units.mm) // ?
label intake_valve_diameter(42.0)
label exhaust_valve_diameter(39.0)
label intake_valves(1.0)
label exhaust_valves(1.0)

label cyl(6.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)

label redline(3400.0) // RPM

// Opel 3.5
label IVL(9.0) // ?
label EVL(9.0) // ?
label IVO(5.0 * units.deg) //BTDC
label IVC(45.0 * units.deg) //ABDC
label EVO(45.0 * units.deg) //BBDC
label EVC(18.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label piston_mass(400.0) // ?
label con_rod_mass(800.0) // ? g
label crank_mass(15.0) // ? kg
label flywheel_mass(3.0) // kg

label exhaust_delay_coeff(1.5)


public node rods {
    input crank;
    output cr0_0: mcr0; output cr1_0: mcr1; output cr2_0: mcr2; output cr3_0: mcr3; output cr4_0: mcr4; output cr5_0: mcr5;
    output rj0_0: crank.rj0; output rj1_0: crank.rj1; output rj2_0: crank.rj2; output rj3_0: crank.rj3; output rj4_0: crank.rj4; output rj5_0: crank.rj5;
    output rj0_1: sj0_0; output rj1_1: sj1_0; output rj2_1: sj2_0; output rj3_1: sj3_0; output rj4_1: sj4_0; output rj5_1: sj5_0;
    output cr0_1: sr0_0; output cr1_1: sr1_0; output cr2_1: sr2_0; output cr3_1: sr3_0; output cr4_1: sr4_0; output cr5_1: sr5_0;

    connecting_rod_parameters master_rod_params(
        mass: con_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.g,
            length: con_rod * units.mm
        ),
        slave_throw: R1 * units.mm,
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    connecting_rod_parameters slave_rod_params(
        mass: slave_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: slave_rod_mass * units.g,
            length: L1 * units.mm
        ),
        center_of_mass: 0.0,
        length: L1 * units.mm
    )

    connecting_rod mcr0(master_rod_params)
    connecting_rod mcr1(master_rod_params)
    connecting_rod mcr2(master_rod_params)
    connecting_rod mcr3(master_rod_params)
    connecting_rod mcr4(master_rod_params)
    connecting_rod mcr5(master_rod_params)

    connecting_rod sr0_0(slave_rod_params)
    connecting_rod sr1_0(slave_rod_params)
    connecting_rod sr2_0(slave_rod_params)
    connecting_rod sr3_0(slave_rod_params)
    connecting_rod sr4_0(slave_rod_params)
    connecting_rod sr5_0(slave_rod_params)

    rod_journal sj0_0(angle: -psi)
    rod_journal sj1_0(angle: -psi)
    rod_journal sj2_0(angle: -psi)
    rod_journal sj3_0(angle: -psi)
    rod_journal sj4_0(angle: -psi)
    rod_journal sj5_0(angle: -psi)

    mcr0.add_slave_journal(sj0_0)
    mcr1.add_slave_journal(sj1_0)
    mcr2.add_slave_journal(sj2_0)
    mcr3.add_slave_journal(sj3_0)
    mcr4.add_slave_journal(sj4_0)
    mcr5.add_slave_journal(sj5_0)
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "Tatra T80",
        starter_torque: 100 * units.Nm,
        starter_speed: 300 * units.rpm,
        redline: redline * units.rpm,
        fuel: s30(),
        throttle_gamma: 1.5,
        jitter: 0.7,
        noise: 0.1,
        hf_gain: 0.007,
        simulation_frequency: 8000,
        dyno_min_speed: 500 * units.rpm
    )

    ////////////////////////////////////////
    ///////// change variants here /////////
    ////////////////////////////////////////
    variant_0 variant()

    es0 es()

    crank c0(vee / 2 + rot90, variant)
    rods r(c0)

    ignition_module ignition(timing_curve: timing_curve, rev_limit: redline * 1.5 * units.rpm, limiter_duration: 0.05)

    bank_builder bank0(
        var: variant,
        rods: r0(r),
        incline: vee / 2,
        phasing: 0,
        ignition: ignition,
        intake: intake,
        exhaust: es.es0,
        pl0: es.pl0, pl1: es.pl1, pl2: es.pl2, pl3: es.pl3, pl4: es.pl4, pl5: es.pl5,
        flip: true
    )

    bank_builder bank1(
        var: variant,
        rods: r1(r),
        incline: -vee / 2,
        phasing: vee + rot360,
        ignition: ignition,
        intake: intake,
        exhaust: es.es1,
        pl0: es.pl6, pl1: es.pl7, pl2: es.pl8, pl3: es.pl9, pl4: es.pl10, pl5: es.pl11
    )

    intake intake(
        plenum_volume: 2.7 * units.L,
        plenum_cross_section_area: 40.1 * units.cm2,
        runner_length: 15.5 * units.cm,
        intake_flow_rate: k_carb(359.4),
        idle_flow_rate: k_carb(0.02),
        idle_throttle_plate_position: 0.995,
        runner_flow_rate: k_carb(59.9),
        velocity_decay: 0.05
    )

    function timing_curve(240.0 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 2.0 * units.deg)
        .add_sample(240.0 * units.rpm, 2.0 * units.deg)
        .add_sample(480.0 * units.rpm, 2.0 * units.deg)
        .add_sample(720.0 * units.rpm, 5.1 * units.deg)
        .add_sample(960.0 * units.rpm, 10.4 * units.deg)
        .add_sample(1200.0 * units.rpm, 14.6 * units.deg)
        .add_sample(1440.0 * units.rpm, 18.0 * units.deg)
        .add_sample(1680.0 * units.rpm, 20.8 * units.deg)
        .add_sample(1920.0 * units.rpm, 23.0 * units.deg)
        .add_sample(2160.0 * units.rpm, 24.8 * units.deg)
        .add_sample(2400.0 * units.rpm, 26.2 * units.deg)
        .add_sample(2640.0 * units.rpm, 27.4 * units.deg)
        .add_sample(2880.0 * units.rpm, 28.3 * units.deg)
        .add_sample(3120.0 * units.rpm, 29.0 * units.deg)
        .add_sample(3360.0 * units.rpm, 29.6 * units.deg)
        .add_sample(3600.0 * units.rpm, 30.1 * units.deg)
        .add_sample(3840.0 * units.rpm, -12.5 * units.deg)
        .add_sample(4080.0 * units.rpm, -25.0 * units.deg)

    engine
        .add_crankshaft(c0.c)
        .add_cylinder_bank(bank0.bank)
        .add_cylinder_bank(bank1.bank)
        .add_ignition_module(ignition)
}

// std1
private node es0{
    output es0: exhaust0; output es1: exhaust1;
    output pl0: _pl0; output pl1: _pl1; output pl2: _pl2; output pl3: _pl3; output pl4: _pl4; output pl5: _pl5;
    output pl6: _pl6; output pl7: _pl7; output pl8: _pl8; output pl9: _pl9; output pl10: _pl10; output pl11: _pl11;

    label collector_cross_section_area(15.2)

    label exhaust_pipe_length_0(400.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 100.0) // Litres
    
    label exhaust_pipe_length_1(400.0) // cm
    label exhaust_volume_1(collector_cross_section_area * exhaust_pipe_length_1 / 100.0) // Litres

    label spacing_factor(1.2)
    label flange_density(1.0 * exhaust_delay_coeff)
    float _pl0 ((3.6 * bore * spacing_factor / flange_density) * units.mm)
    float _pl1 ((3.4 * bore * spacing_factor / flange_density) * units.mm)
    float _pl2 ((1.5 * bore * spacing_factor / flange_density) * units.mm)
    float _pl3 ((1.5 * bore * spacing_factor / flange_density) * units.mm)
    float _pl4 ((3.4 * bore * spacing_factor / flange_density) * units.mm)
    float _pl5 ((3.6 * bore * spacing_factor / flange_density) * units.mm)

    float _pl6  (_pl0)
    float _pl7  (_pl1)
    float _pl8  (_pl2)
    float _pl9  (_pl3)
    float _pl10 (_pl4)
    float _pl11 (_pl5)

    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(299.5),
        collector_cross_section_area: 15.2 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 43.8 * units.cm,
        primary_flow_rate: k_carb(99.8),
        velocity_decay: 1.0,
        volume: (exhaust_volume_0 + 4.0) * units.L
    )

    exhaust_system_parameters es_params1(
        outlet_flow_rate: k_carb(299.5),
        collector_cross_section_area: 15.2 * units.cm2,
        length: exhaust_pipe_length_1 * units.cm,
        primary_tube_length: 43.8 * units.cm,
        primary_flow_rate: k_carb(99.8),
        velocity_decay: 1.0,
        volume: (exhaust_volume_1 + 4.0) * units.L
    )

    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_56.wav", volume: 0.00025))
    exhaust_system exhaust1(es_params1, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_57.wav", volume: 0.00025))

}


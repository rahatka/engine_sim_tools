import "engine_sim.mr"
import "../../fuel/30.mr"

units units()
constants constants()

label bore(100.0)
label stroke(120.0)
label compression_ratio(5.0)
label con_rod(250.0)
label compression_height(63.0 * units.mm) // ~
label intake_valve_diameter(47.0) // ?
label exhaust_valve_diameter(47.0) // ?
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(3250.0) // RPM

label cyl(12.0)
label cyl_base(cyl / row)
label row(2.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label rot_base(cycle / cyl_base)
label vee(60.0 * units.deg)

// Hispano-Suiza J12
label IVL(10.0) // ?
label EVL(10.0) // ?
label IVO(7.0 * units.deg) //BTDC
label IVC(45.0 * units.deg) //ABDC
label EVO(60.0 * units.deg) //BBDC
label EVC(5.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(1200.0) // ? g
label piston_mass(700.0) // ?
label crank_mass(31.75) // 70 lbs
label flywheel_mass(10.0) // kg

label exhaust_delay_coeff(1.5)


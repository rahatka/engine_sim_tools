public node camshaft_builder {
    output intake_cam_0: _intake_cam_0;
    output exhaust_cam_0: _exhaust_cam_0;

    output intake_cam_1: _intake_cam_1;
    output exhaust_cam_1: _exhaust_cam_1;

    camshaft _intake_cam_0(base_radius: 19.7 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 19.7 * units.mm, lobe_profile: exhaust_lobe_profile)
    
    camshaft _intake_cam_1(base_radius: 19.7 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_1(base_radius: 19.7 * units.mm, lobe_profile: exhaust_lobe_profile)

    _intake_cam_0
        .add_lobe(ILC + 0 * rot)
        .add_lobe(ILC + 4 * rot)
        .add_lobe(ILC + 10 * rot)
        .add_lobe(ILC + 14 * rot)
        .add_lobe(ILC + 6 * rot)
        .add_lobe(ILC + 2 * rot)
        .add_lobe(ILC + 12 * rot)
        .add_lobe(ILC + 8 * rot)
    _exhaust_cam_0
        .add_lobe(ELC + 0 * rot)
        .add_lobe(ELC + 4 * rot)
        .add_lobe(ELC + 10 * rot)
        .add_lobe(ELC + 14 * rot)
        .add_lobe(ELC + 6 * rot)
        .add_lobe(ELC + 2 * rot)
        .add_lobe(ELC + 12 * rot)
        .add_lobe(ELC + 8 * rot)
        
    _intake_cam_1
        .add_lobe(ILC + 0 * rot + vee + inv)
        .add_lobe(ILC + 4 * rot + vee + inv)
        .add_lobe(ILC + 10 * rot + vee + inv)
        .add_lobe(ILC + 14 * rot + vee + inv)
        .add_lobe(ILC + 6 * rot + vee + inv)
        .add_lobe(ILC + 2 * rot + vee + inv)
        .add_lobe(ILC + 12 * rot + vee + inv)
        .add_lobe(ILC + 8 * rot + vee + inv)
    _exhaust_cam_1
        .add_lobe(ELC + 0 * rot + vee + inv)
        .add_lobe(ELC + 4 * rot + vee + inv)
        .add_lobe(ELC + 10 * rot + vee + inv)
        .add_lobe(ELC + 14 * rot + vee + inv)
        .add_lobe(ELC + 6 * rot + vee + inv)
        .add_lobe(ELC + 2 * rot + vee + inv)
        .add_lobe(ELC + 12 * rot + vee + inv)
        .add_lobe(ELC + 8 * rot + vee + inv)
}

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node head {
    // port_flow.py v1.5
    // intake port area: 12.3 cm²; saturated lift: 9.62 mm
    // exhaust port area: 9.7 cm²; saturated lift: 8.56 mm
    // cylinder volume: 720.7 cm³ (44.0 CI); engine volume: 11.531 L (703.6 CI)
    // chamber volume: 160.1 cm³ (9.8 CI)
    // 16 harmonic intake runner length: 25.9 cm; diameter: 3.6 cm
    // primary length: 56.9 cm, area: 10.7 cm², diameter: 3.7 cm
    // collector diameter: 8.1 cm, area: 51.4 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 160.147 * units.cc,
        intake_runner_volume: 267.1 * units.cc,
        intake_runner_cross_section_area: 10.3 * units.cm2,
        exhaust_runner_volume: 106.8 * units.cc,
        exhaust_runner_cross_section_area: 10.7 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "ZiS-101 V16 (f)",
        starter_torque: 150 * units.Nm,
        starter_speed: 100 * units.rpm,
        redline: redline * units.rpm,
        fuel: s30(),
        throttle_gamma: 1.5,
        jitter: 0.7,
        noise: 0.05,
        hf_gain: 0.007,
        simulation_frequency: 7000,
        fluid_simulation_steps: 6,
        max_sle_solver_steps: 32,
        dyno_min_speed: 500 * units.rpm
    )

    wires wires()

    connecting_rod_parameters cr_params(
        mass: con_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.g,
            length: con_rod * units.mm
        ),
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    label crank_moment(disk_moment_of_inertia(mass: crank_mass * units.kg, radius: (stroke / 2.0) * units.mm))
    label flywheel_moment(disk_moment_of_inertia(mass: flywheel_mass * units.kg, radius: (stroke * 3.5) * units.mm))
    label other_moment(disk_moment_of_inertia(mass: 1 * units.kg, radius: 1.0 * units.cm))

    crankshaft c0(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: flywheel_mass * units.kg,
        mass: crank_mass * units.kg,
        friction_torque: 17.3 * units.Nm,
        moment_of_inertia: crank_moment + flywheel_moment + other_moment,
        tdc: 0.0
    )

    rod_journal rj0(angle: rot * 0 + rot90 + vee / 2)
    rod_journal rj1(angle: rot * 4 + rot90 + vee / 2)
    rod_journal rj2(angle: rot * 10 + rot90 + vee / 2)
    rod_journal rj3(angle: rot * 14 + rot90 + vee / 2)
    rod_journal rj4(angle: rot * 6 + rot90 + vee / 2)
    rod_journal rj5(angle: rot * 2 + rot90 + vee / 2)
    rod_journal rj6(angle: rot * 12 + rot90 + vee / 2)
    rod_journal rj7(angle: rot * 8 + rot90 + vee / 2)
    
    c0
        .add_rod_journal(rj0)
        .add_rod_journal(rj1)
        .add_rod_journal(rj2)
        .add_rod_journal(rj3)
        .add_rod_journal(rj4)
        .add_rod_journal(rj5)
        .add_rod_journal(rj6)
        .add_rod_journal(rj7)

    piston_parameters piston_params(
        mass: 900.0 * units.g,
        blowby: k_28inH2O(0.027),
        compression_height: compression_height,
        wrist_pin_position: 0.0,
        displacement: 0.0
    )

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    label collector_cross_section_area(25.7)

    label exhaust_pipe_length_0(405.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 100.0) // Litres
    
    label exhaust_pipe_length_1(400.0) // cm
    label exhaust_volume_1(collector_cross_section_area * exhaust_pipe_length_1 / 100.0) // Litres

    intake intake(
        plenum_volume: 5.5 * units.L,
        plenum_cross_section_area: 82.4 * units.cm2,
        runner_length: 25.9 * units.cm,
        intake_flow_rate: k_carb(672.6),
        idle_flow_rate: k_carb(0.05),
        idle_throttle_plate_position: 0.996,
        runner_flow_rate: k_carb(84.1),
        velocity_decay: 0.05
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(560.5),
        collector_cross_section_area: 25.7 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 56.9 * units.cm,
        primary_flow_rate: k_carb(140.1),
        velocity_decay: 1.0,
        volume: exhaust_volume_0 * units.L
    )
    
    exhaust_system_parameters es_params1(
        outlet_flow_rate: k_carb(560.5),
        collector_cross_section_area: 25.7 * units.cm2,
        length: exhaust_pipe_length_1 * units.cm,
        primary_tube_length: 56.9 * units.cm,
        primary_flow_rate: k_carb(140.1),
        velocity_decay: 1.0,
        volume: exhaust_volume_1 * units.L
    )

    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_56.wav", volume: 0.00025))
    exhaust_system exhaust1(es_params1, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_57.wav", volume: 0.00025))
 
    cylinder_bank b0(bank_params, angle: vee / 2)
    cylinder_bank b1(bank_params, angle: -vee / 2)

    em5 em()

    b0
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj0,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: em.pl0,
            ignition_wire: wires.wire1
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj1,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: em.pl1,
            ignition_wire: wires.wire2
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj2,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: em.pl2,
            ignition_wire: wires.wire3
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj3,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: em.pl3,
            ignition_wire: wires.wire4
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj4,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: em.pl4,
            ignition_wire: wires.wire5
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj5,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: em.pl5,
            ignition_wire: wires.wire6
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj6,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: em.pl6,
            ignition_wire: wires.wire7
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj7,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: em.pl7,
            ignition_wire: wires.wire8
        )
        
    b1
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj0,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: em.pl0,
            ignition_wire: wires.wire9
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj1,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: em.pl1,
            ignition_wire: wires.wire10
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj2,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: em.pl2,
            ignition_wire: wires.wire11
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj3,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: em.pl3,
            ignition_wire: wires.wire12
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj4,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: em.pl4,
            ignition_wire: wires.wire13
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj5,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: em.pl5,
            ignition_wire: wires.wire14
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj6,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: em.pl6,
            ignition_wire: wires.wire15
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj7,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: em.pl7,
            ignition_wire: wires.wire16
        )

    camshaft_builder camshaft()

    b0.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_0,
            exhaust_camshaft: camshaft.exhaust_cam_0,
            flip_display: true
        )
    )
    b1.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_1,
            exhaust_camshaft: camshaft.exhaust_cam_1
        )
    )

    function timing_curve(233.3 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 0.0 * units.deg)
        .add_sample(233.3 * units.rpm, 2.2 * units.deg)
        .add_sample(466.7 * units.rpm, 9.1 * units.deg)
        .add_sample(700.0 * units.rpm, 14.0 * units.deg)
        .add_sample(933.3 * units.rpm, 17.5 * units.deg)
        .add_sample(1166.7 * units.rpm, 19.9 * units.deg)
        .add_sample(1400.0 * units.rpm, 21.7 * units.deg)
        .add_sample(1633.3 * units.rpm, 22.9 * units.deg)
        .add_sample(1866.7 * units.rpm, 23.8 * units.deg)
        .add_sample(2100.0 * units.rpm, 24.4 * units.deg)
        .add_sample(2333.3 * units.rpm, 24.9 * units.deg)
        .add_sample(2566.7 * units.rpm, 25.0 * units.deg)
        .add_sample(2800.0 * units.rpm, 25.0 * units.deg)
        .add_sample(3033.3 * units.rpm, 25.0 * units.deg)
        .add_sample(3266.7 * units.rpm, 25.0 * units.deg)
        .add_sample(3500.0 * units.rpm, 25.0 * units.deg)
        .add_sample(3733.3 * units.rpm, -15.0 * units.deg)
        .add_sample(3966.7 * units.rpm, -45.0 * units.deg)

    engine
        .add_cylinder_bank(b0)
        .add_cylinder_bank(b1)

        .add_crankshaft(c0)
        .add_ignition_module(
            eng_distributor(
                wires: wires,
                timing_curve: timing_curve
            )
        )
}

label spacing_factor(1.2)
label flange_density(1.0 * exhaust_delay_coeff)
label em_unit(bore * spacing_factor / flange_density)

// exhaust port layout
// |   ||   ||   ||   |
//  O O  O O  O O  O O

// stock 1/2 manifold
private node em0{
    output pl0: (4.25 * em_unit) * units.mm;
    output pl1: (2.84 * em_unit) * units.mm;
    output pl2: (2.42 * em_unit) * units.mm;
    output pl3: (1.00 * em_unit) * units.mm;
    output pl4: (1.00 * em_unit) * units.mm;
    output pl5: (2.42 * em_unit) * units.mm;
    output pl6: (2.84 * em_unit) * units.mm;
    output pl7: (4.25 * em_unit) * units.mm;
}

// 3/4 manifold
private node em1{
    output pl0: (6.01 * em_unit) * units.mm;
    output pl1: (4.60 * em_unit) * units.mm;
    output pl2: (4.18 * em_unit) * units.mm;
    output pl3: (2.76 * em_unit) * units.mm;
    output pl4: (2.33 * em_unit) * units.mm;
    output pl5: (1.71 * em_unit) * units.mm;
    output pl6: (1.71 * em_unit) * units.mm;
    output pl7: (2.33 * em_unit) * units.mm;
}

// "long end" 4/4 manifold
private node em2{
    output pl0: (7.92 * em_unit) * units.mm;
    output pl1: (6.51 * em_unit) * units.mm;
    output pl2: (6.09 * em_unit) * units.mm;
    output pl3: (4.67 * em_unit) * units.mm;
    output pl4: (4.25 * em_unit) * units.mm;
    output pl5: (2.83 * em_unit) * units.mm;
    output pl6: (2.41 * em_unit) * units.mm;
    output pl7: (1.00 * em_unit) * units.mm;
}

// two pipes manifold
private node em3{
    output pl0: (5.0 * em_unit) * units.mm;
    output pl1: (5.5 * em_unit) * units.mm;
    output pl2: (5.5 * em_unit) * units.mm;
    output pl3: (5.0 * em_unit) * units.mm;
    output pl4: (1.0 * em_unit) * units.mm;
    output pl5: (1.5 * em_unit) * units.mm;
    output pl6: (1.5 * em_unit) * units.mm;
    output pl7: (1.0 * em_unit) * units.mm;
}

// four pipes manifold
private node em4{
    output pl0: (6.0 * em_unit) * units.mm;
    output pl1: (6.0 * em_unit) * units.mm;
    output pl2: (4.5 * em_unit) * units.mm;
    output pl3: (4.5 * em_unit) * units.mm;
    output pl4: (3.0 * em_unit) * units.mm;
    output pl5: (3.0 * em_unit) * units.mm;
    output pl6: (1.0 * em_unit) * units.mm;
    output pl7: (1.0 * em_unit) * units.mm;
}

// slight middle offset
private node em5{
    output pl0: (4.65 * em_unit) * units.mm;
    output pl1: (3.24 * em_unit) * units.mm;
    output pl2: (2.82 * em_unit) * units.mm;
    output pl3: (1.40 * em_unit) * units.mm;
    output pl4: (0.60 * em_unit) * units.mm;
    output pl5: (2.02 * em_unit) * units.mm;
    output pl6: (2.44 * em_unit) * units.mm;
    output pl7: (3.85 * em_unit) * units.mm;
}

// ЗИС-101
public node veh {
    alias output __out: vehicle;

    label car_mass(2800) // 2500 empty; 3000 full ?

    vehicle vehicle(
        mass: car_mass * units.kg,
        drag_coefficient: 0.42,
        cross_sectional_area: (1892 * units.mm) * (1856 * units.mm),
        diff_ratio: 3.3, // custom
        tire_radius: (868.68 / 2) * units.mm, // 7.50 x 17
        rolling_resistance: 0.012 * car_mass * 9.81
        )
}

// ЗИС-101
private node trn {
    alias output __out:
        transmission(
            max_clutch_torque: 1000 * units.Nm,
            max_clutch_flex: 20 * units.deg,
            limit_clutch_flex: true,
            clutch_stiffness: 20 * units.Nm / units.deg,
            clutch_damping: 1.0,
            simulate_flex: true
        )
        .add_gear(2.89)
        .add_gear(1.75)
        .add_gear(1.0);
}

run(
    engine: eng(),
    transmission: trn(),
    vehicle: veh()
)

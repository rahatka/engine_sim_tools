import "engine_sim.mr"
import "../../fuel/30.mr"

units units()
constants constants()

// 0: mirrored firing
// 1: inverted firing
label variant(1)

label vee(45.0 * units.deg)
label bore(85.0)
label stroke(127.0)
label compression_ratio(5.5)
label con_rod(280.0)
label compression_height(50.0 * units.mm)  // ~
label intake_valve_diameter(45.0)
label exhaust_valve_diameter(40.0)
label intake_valves(1.0)
label exhaust_valves(1.0)

label cyl(16.0)
label row(2.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

label redline(3500.0) // RPM

label inv((360.0 * variant) * units.deg)

// ЗИС-101 роликовый толкатель
// label IVL(8.75) // 1938 Аймат, Айзенберг, Автомобиль ЗИС-101
// label EVL(8.75) // 1938 Аймат, Айзенберг, Автомобиль ЗИС-101
// label IVO(16.30 * units.deg) //BTDC
// label IVC(58.30 * units.deg) //ABDC
// label EVO(56.30 * units.deg) //BBDC
// label EVC(31.30 * units.deg) //ATDC

// ЗИС-101 плоский толкатель
label IVL(8.75) // 1938 Аймат, Айзенберг, Автомобиль ЗИС-101
label EVL(8.75) // 1938 Аймат, Айзенберг, Автомобиль ЗИС-101
label IVO(24.15 * units.deg) //BTDC
label IVC(74.15 * units.deg) //ABDC
label EVO(11.15 * units.deg) //BBDC
label EVC(39.15 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(1000.0) // ? g
label crank_mass(52.8233) // ? kg
label flywheel_mass(12.0) // kg

label exhaust_delay_coeff(1.5)

public node eng_distributor {
    input wires;
    input timing_curve;
    input rev_limit: 4500 * units.rpm;
    input limiter_duration: 0.0001;
    alias output __out:
        ignition_module(timing_curve: timing_curve, rev_limit: rev_limit, limiter_duration: limiter_duration)
            .connect_wire(wires.wire1, rot * 0)
            .connect_wire(wires.wire6, rot * 2)
            .connect_wire(wires.wire2, rot * 4)
            .connect_wire(wires.wire5, rot * 6)
            .connect_wire(wires.wire8, rot * 8)
            .connect_wire(wires.wire3, rot * 10)
            .connect_wire(wires.wire7, rot * 12)
            .connect_wire(wires.wire4, rot * 14)
            .connect_wire(wires.wire9, rot * 0 + vee + inv)
            .connect_wire(wires.wire14, rot * 2 + vee + inv)
            .connect_wire(wires.wire10, rot * 4 + vee + inv)
            .connect_wire(wires.wire13, rot * 6 + vee + inv)
            .connect_wire(wires.wire16, rot * 8 + vee + inv)
            .connect_wire(wires.wire11, rot * 10 + vee + inv)
            .connect_wire(wires.wire15, rot * 12 + vee + inv)
            .connect_wire(wires.wire12, rot * 14 + vee + inv);
}

private node wires {
    output wire1: ignition_wire();
    output wire2: ignition_wire();
    output wire3: ignition_wire();
    output wire4: ignition_wire();
    output wire5: ignition_wire();
    output wire6: ignition_wire();
    output wire7: ignition_wire();
    output wire8: ignition_wire();
    output wire9: ignition_wire();
    output wire10: ignition_wire();
    output wire11: ignition_wire();
    output wire12: ignition_wire();
    output wire13: ignition_wire();
    output wire14: ignition_wire();
    output wire15: ignition_wire();
    output wire16: ignition_wire();
}


import "_fuel_30.mr"

units units()
constants constants()

label bore(85.725) // 3 3/8
label stroke(127.0) // 5
label compression_ratio(4.9)
label con_rod(276.225) // 10 7/8
label compression_height(58.7375 * units.mm) // 2 5/16
label intake_valve_diameter(43.6626) // 1.719 or 1 23/32
label exhaust_valve_diameter(42.0624) // 1.656 or 1 21/32
label intake_valves(1.0)
label exhaust_valves(1.0)

label cyl(6.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)

label redline(3400.0) // RPM

// 1930 chrysler 70 77
label IVL(7.9375) // 5/16
label EVL(7.9375) // 5/16
label IVO(-6.0 * units.deg) //BTDC
label IVC(46.0 * units.deg) //ABDC
label EVO(42.0 * units.deg) //BBDC
label EVC(8.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label piston_mass(500.0) // ?
label con_rod_mass(800.0) // ?
label crank_mass(18.0) // ? kg
label flywheel_mass(3.0) // kg

label exhaust_delay_coeff(1.5)


            ignition_wire: wires.wire7
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj7,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: em.pl7,
            ignition_wire: wires.wire8
        )

    engine
        .add_cylinder_bank(b0)

    engine.add_crankshaft(c0)

    camshaft_builder camshaft()

    b0.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_0,
            exhaust_camshaft: camshaft.exhaust_cam_0,
            flip_display: true
        )
    )

    function timing_curve(253.3 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 7.0 * units.deg)
        .add_sample(253.3 * units.rpm, 7.1 * units.deg)
        .add_sample(506.7 * units.rpm, 7.9 * units.deg)
        .add_sample(760.0 * units.rpm, 8.7 * units.deg)
        .add_sample(1013.3 * units.rpm, 9.7 * units.deg)
        .add_sample(1266.7 * units.rpm, 10.7 * units.deg)
        .add_sample(1520.0 * units.rpm, 11.8 * units.deg)
        .add_sample(1773.3 * units.rpm, 13.0 * units.deg)
        .add_sample(2026.7 * units.rpm, 14.4 * units.deg)
        .add_sample(2280.0 * units.rpm, 15.9 * units.deg)
        .add_sample(2533.3 * units.rpm, 17.6 * units.deg)
        .add_sample(2786.7 * units.rpm, 19.4 * units.deg)
        .add_sample(3040.0 * units.rpm, 21.5 * units.deg)
        .add_sample(3293.3 * units.rpm, 23.7 * units.deg)
        .add_sample(3546.7 * units.rpm, 26.2 * units.deg)
        .add_sample(3800.0 * units.rpm, 29.0 * units.deg)
        .add_sample(4053.3 * units.rpm, -15.0 * units.deg)
        .add_sample(4306.7 * units.rpm, -60.0 * units.deg)

    engine.add_ignition_module(
        eng_distributor(
            wires: wires,
            timing_curve: timing_curve
        )
    )
}

// ЗИС-110
public node veh {
    alias output __out: vehicle;
    vehicle vehicle(
        mass: 2450  * units.kg,
        drag_coefficient: 0.42,
        cross_sectional_area: (1960 * units.mm) * (1730 * units.mm),
        diff_ratio: 4.36,
        tire_radius: (685.0 / 2) * units.mm,
        rolling_resistance: 20
        )
}

label spacing_factor(1.25)
label flange_density(1.0 * exhaust_delay_coeff)
label em_unit(bore * spacing_factor / flange_density)

// exhaust port layout
// |   ||   ||   ||   |
//  O O  O O  O O  O O

// stock 1/2 manifold
private node em0{
    output pl0: (4.25 * em_unit) * units.mm;

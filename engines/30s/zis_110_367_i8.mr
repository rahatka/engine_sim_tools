    output pl2: (2.42 * em_unit) * units.mm;
    output pl3: (1.00 * em_unit) * units.mm;
    output pl4: (1.00 * em_unit) * units.mm;
    output pl5: (2.42 * em_unit) * units.mm;
    output pl6: (2.84 * em_unit) * units.mm;
    output pl7: (4.25 * em_unit) * units.mm;
}

// 3/4 manifold
private node em1{
    output pl0: (6.01 * em_unit) * units.mm;
    output pl1: (4.60 * em_unit) * units.mm;
    output pl2: (4.18 * em_unit) * units.mm;
    output pl3: (2.76 * em_unit) * units.mm;
    output pl4: (2.33 * em_unit) * units.mm;
    output pl5: (1.71 * em_unit) * units.mm;
    output pl6: (1.71 * em_unit) * units.mm;
    output pl7: (2.33 * em_unit) * units.mm;
}

// "long end" 4/4 manifold
private node em2{
    output pl0: (7.92 * em_unit) * units.mm;
    output pl1: (6.51 * em_unit) * units.mm;
    output pl2: (6.09 * em_unit) * units.mm;
    output pl3: (4.67 * em_unit) * units.mm;
    output pl4: (4.25 * em_unit) * units.mm;
    output pl5: (2.83 * em_unit) * units.mm;
    output pl6: (2.41 * em_unit) * units.mm;
    output pl7: (1.00 * em_unit) * units.mm;
}

// two pipes manifold
private node em3{
    output pl0: (5.0 * em_unit) * units.mm;
    output pl1: (5.5 * em_unit) * units.mm;
    output pl2: (5.5 * em_unit) * units.mm;
    output pl3: (5.0 * em_unit) * units.mm;

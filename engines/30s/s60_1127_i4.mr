import "../../fuel/10.mr"

units units()
constants constants()

label bore(165.0) // 6 1/2
label stroke(216.0) // 8 1/2
label compression_ratio(3.96)
label con_rod(406.0) // 16
label compression_height(108.0 * units.mm)
label intake_valve_diameter(81.0) // 66.5
label exhaust_valve_diameter(76.0) // 66.0
label intake_valves(1.0)
label exhaust_valves(1.0)

label cyl(4.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)

label redline(750.0) // RPM

// С-60
label IVL(16.7) // 21/32 ?
label EVL(16.7) // 21/32 ?
label IVO(-10.0 * units.deg) //BTDC
label IVC(42.0 * units.deg) //ABDC
label EVO(35.0 * units.deg) //BBDC
label EVC(10.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label piston_mass(2500.0) // ?
label con_rod_mass(3500.0) // ?
label crank_mass(40.0) // ? kg
label flywheel_mass(15.0) // kg

label exhaust_delay_coeff(1.5)


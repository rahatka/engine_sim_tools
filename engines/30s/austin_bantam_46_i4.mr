import "engine_sim.mr"
import "../../fuel/30.mr"

units units()
constants constants()

label bore(55.88) // 2.2
label stroke(76.2) // 3
label compression_ratio(5.0)
label con_rod(152.4) // ~ 6
label compression_height(23.4 * units.mm) // ~
label intake_valve_diameter(26.1937) // 1 1/32 ; 9/32 stem
label exhaust_valve_diameter(26.1937) // 1 1/32 ; 9/32 stem
label intake_valves(1.0)
label exhaust_valves(1.0)

label cyl(4.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)

label redline(3500.0) // RPM

// bantam
label IVL(6.35) // 5/32 ; 1/4
label EVL(6.35) // 5/32 ; 1/4
label IVO(00.0 * units.deg) //BTDC
label IVC(40.0 * units.deg) //ABDC
label EVO(45.0 * units.deg) //BBDC
label EVC(15.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label piston_mass(250.0) // ?
label con_rod_mass(300.0) // ? g
label crank_mass(8.0) // ? kg
label flywheel_mass(2.0) // kg

label exhaust_delay_coeff(1.5)

public node variant_0 {
    output s1: 0;
    output s3: 1;
    output s4: 2;
    output s2: 3;
}


import "../../fuel/10.mr"

units units()
constants constants()

label bore(95.0)
label stroke(127.0)
label compression_ratio(4.1)
label con_rod(255.0)
label compression_height(55.0 * units.mm)
label intake_valve_diameter(45.0)
label exhaust_valve_diameter(45.0)
label intake_valves(1.0)
label exhaust_valves(1.0)

label cyl(4.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)

label redline(1500.0) // RPM

// У-1, У-2
label IVL(6.92) // 6.92 lobe, 1.0 mult
label EVL(7.72) // 7.72 lobe, 1.0 mult
label IVO(-7.5 * units.deg) //BTDC
label IVC(37.5 * units.deg) //ABDC
label EVO(52.5 * units.deg) //BBDC
label EVC(7.5 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label piston_mass(1300.0)
label con_rod_mass(1225.0)
label crank_mass(28.15)
label flywheel_mass(6.0) // kg

label exhaust_delay_coeff(1.5)


import "engine_sim.mr"
import "../../fuel/30.mr"

units units()
constants constants()

label bore(79.375) // 3 1/8
label stroke(107.95) // 4 1/4
label compression_ratio(5.3)
label con_rod(225.425) // 8 7/8
label compression_height(50.8 * units.mm) // 2
label intake_valve_diameter(35.7188) // 1 13/32
label exhaust_valve_diameter(35.7188) // 1 13/32
label intake_valves(1.0)
label exhaust_valves(1.0)

label cyl(8.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

label redline(3600.0) // RPM

// 1931 cd
label IVL(7.9375) // .3125
label EVL(7.9375) // .3125
label IVO(04.0 * units.deg) //BTDC
label IVC(36.0 * units.deg) //ABDC
label EVO(52.0 * units.deg) //BBDC
label EVC(02.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(907.185) // 32
label piston_mass(609.515) // ?
label crank_mass(30.0) // ?
label flywheel_mass(7.0) // kg

label exhaust_delay_coeff(1.5)


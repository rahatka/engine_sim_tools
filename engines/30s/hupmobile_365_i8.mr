import "engine_sim.mr"
import "_fuel_30.mr"

units units()
constants constants()

label bore(88.9) // 3 1/2
label stroke(120.65) // 4 3/4
label compression_ratio(5.2)
label con_rod(241.3) // 9 1/2
label compression_height(50.8 * units.mm) // ?
label intake_valve_diameter(44.323) // 1.745
label exhaust_valve_diameter(40.386) // 1.590
label intake_valves(1.0)
label exhaust_valves(1.0)

label cyl(8.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

label redline(3750.0) // RPM

// Hupp 365
label IVL(7.239) // .285
label EVL(7.239) // .285
label IVO(-1.0 * units.deg) //BTDC
label IVC(51.0 * units.deg) //ABDC
label EVO(47.0 * units.deg) //BBDC
label EVC(3.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(750.0) // g ?
label piston_mass(650.0) // g ?
label crank_mass(45.0) // kg ?
label flywheel_mass(5.0) // kg

label exhaust_delay_coeff(1.5)


import "engine_sim.mr"
import "../../fuel/racing.mr"

units units()
constants constants()

label bore(66.675) // 2 5/8
label stroke(88.9) // 3 1/2
label compression_ratio(11.0)
label con_rod(177.8) // 7
label compression_height(53.975 / 2 * units.mm) // 2 1/8 len
label intake_valve_diameter(32.5437) // 1 9/32 ?
label exhaust_valve_diameter(32.5437) // 1 9/32 ?
label intake_valves(1.0)
label exhaust_valves(1.0)

label cyl(16.0)
label row(2.0)
label cycle(720.0 * units.deg)
label vee(45.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)

label redline(6500.0) // RPM

// Miller 120
label IVL(7.9375) // 5/16 ?
label EVL(7.9375) // 5/16 ?
label IVO(0.0 * units.deg) //BTDC
label IVC(40.0 * units.deg) //ABDC
label EVO(45.0 * units.deg) //BBDC
label EVC(7.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(700.0) // ?
label piston_mass(700.0) // ?
label crank_mass(25.0) // kg
label flywheel_mass(5.0) // kg

label exhaust_delay_coeff(1.5)


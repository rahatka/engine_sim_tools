import "engine_sim.mr"
import "../../fuel/30.mr"

units units()
constants constants()

label bore(95.25) // 3 3/4
label stroke(129.381) // 5 3/32
label compression_ratio(6.0)
label con_rod(279.4) // 11
label compression_height(50.8 * units.mm) // 2
label intake_valve_diameter(47.625) // 1 7/8
label exhaust_valve_diameter(44.45) // 1 3/4
label intake_valves(1.0)
label exhaust_valves(1.0)

label cyl(8.0)
label row(1.0)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label vee(20.0 * units.deg)

label redline(3100.0) // RPM

label IVL(10.3187) // 13/32
label EVL(9.525) // 3/8
label IVO(10.5 * units.deg) //BTDC
label IVC(59.5 * units.deg) //ABDC
label EVO(55.5 * units.deg) //BBDC
label EVC(14.5 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(1200.0) // ~
label piston_mass(600.0) // ?
label crank_mass(28.0) // kg
label flywheel_mass(7.0) // kg

label exhaust_delay_coeff(1.5)


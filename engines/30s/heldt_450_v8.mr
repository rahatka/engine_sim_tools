private node add_lobes_0 {
    input base;
    input shift: 0.0;
    input var;
    input this;
    alias output __out: this;

    this.add_lobe(base + var.s1 + shift)
    this.add_lobe(base + var.s3 + shift)
    this.add_lobe(base + var.s6 + shift)
    this.add_lobe(base + var.s8 + shift)
}

private node add_lobes_1 {
    input base;
    input shift: 0.0;
    input var;
    input this;
    alias output __out: this;

    this.add_lobe(base + var.s2 + shift)
    this.add_lobe(base + var.s4 + shift)
    this.add_lobe(base + var.s5 + shift)
    this.add_lobe(base + var.s7 + shift)
}

public node bank_builder {
    input var;
    input crankshaft;
    input incline;
    input phasing;
    input ignition;
    input intake;
    input exhaust;

    output bank0: _b0;
    output bank1: _b1;

    ignition_wire _wire1()
    ignition_wire _wire2()
    ignition_wire _wire3()
    ignition_wire _wire4()
    ignition_wire _wire5()
    ignition_wire _wire6()
    ignition_wire _wire7()
    ignition_wire _wire8()

    piston_parameters piston_params(
        mass: piston_mass * units.g,
        blowby: k_28inH2O(0.030),
        compression_height: compression_height,
        wrist_pin_position: 0.0,
        displacement: 0.0
    )

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    connecting_rod_parameters cr_params(
        mass: con_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.g,
            length: con_rod * units.mm
        ),
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    camshaft _intake_cam_0(base_radius: 19.8 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 19.8 * units.mm, lobe_profile: exhaust_lobe_profile)

    camshaft _intake_cam_1(base_radius: 19.8 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_1(base_radius: 19.8 * units.mm, lobe_profile: exhaust_lobe_profile)

    _intake_cam_0.add_lobes_0(ILC, phasing, var)
    _exhaust_cam_0.add_lobes_0(ELC, phasing, var)

    _intake_cam_1.add_lobes_1(ILC, phasing, var)
    _exhaust_cam_1.add_lobes_1(ELC, phasing, var)

    ignition.connect_wire(_wire1, var.s1 + phasing)
    ignition.connect_wire(_wire2, var.s2 + phasing + vee)
    ignition.connect_wire(_wire3, var.s3 + phasing)
    ignition.connect_wire(_wire4, var.s4 + phasing + vee)
    ignition.connect_wire(_wire5, var.s5 + phasing + vee)
    ignition.connect_wire(_wire6, var.s6 + phasing)
    ignition.connect_wire(_wire7, var.s7 + phasing + vee)
    ignition.connect_wire(_wire8, var.s8 + phasing)

    cylinder_bank _b0(bank_params, angle: vee / 2)
    cylinder_bank _b1(bank_params, angle: -vee / 2)

    _b0
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj0,
            intake: intake,
            exhaust_system: exhaust.es0,
            primary_length: exhaust.pl0,
            ignition_wire: _wire1
        )
    _b1
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj1,
            intake: intake,
            exhaust_system: exhaust.es1,
            primary_length: exhaust.pl1,
            ignition_wire: _wire2
        )
    _b0
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj2,
            intake: intake,
            exhaust_system: exhaust.es2,
            primary_length: exhaust.pl2,
            ignition_wire: _wire3
        )
    _b1
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj3,
            intake: intake,
            exhaust_system: exhaust.es3,
            primary_length: exhaust.pl3,
            ignition_wire: _wire4
        )
    _b1
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj4,
            intake: intake,
            exhaust_system: exhaust.es4,
            primary_length: exhaust.pl4,
            ignition_wire: _wire5
        )
    _b0
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj5,
            intake: intake,
            exhaust_system: exhaust.es5,
            primary_length: exhaust.pl5,
            ignition_wire: _wire6
        )
    _b1
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj6,
            intake: intake,
            exhaust_system: exhaust.es6,
            primary_length: exhaust.pl6,
            ignition_wire: _wire7
        )
    _b0
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj7,
            intake: intake,
            exhaust_system: exhaust.es7,
            primary_length: exhaust.pl7,
            ignition_wire: _wire8
        )

    _b0.set_cylinder_head(
        head(
            intake_camshaft: _intake_cam_0,
            exhaust_camshaft: _exhaust_cam_0,
            flip_display: true
        )
    )
    _b1.set_cylinder_head(
        head(
            intake_camshaft: _intake_cam_1,
            exhaust_camshaft: _exhaust_cam_1,
            flip_display: false
        )
    )
}

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node head {
    // port_flow.py v1.5
    // intake port area: 13.7 cm²; saturated lift: 10.14 mm
    // exhaust port area: 11.9 cm²; saturated lift: 9.47 mm
    // cylinder volume: 921.9 cm³ (56.3 CI); engine volume: 7.375 L (450.1 CI)
    // chamber volume: 184.4 cm³ (11.3 CI)
    // 16 harmonic intake runner length: 29.4 cm; diameter: 4.0 cm
    // primary length: 52.9 cm, area: 13.1 cm², diameter: 4.1 cm
    // collector diameter: 6.8 cm, area: 36.6 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 184.383 * units.cc,
        intake_runner_volume: 363.6 * units.cc,
        intake_runner_cross_section_area: 12.3 * units.cm2,
        exhaust_runner_volume: 121.2 * units.cc,
        exhaust_runner_cross_section_area: 13.1 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

public node crank {
    input phasing;
    input var;

    output c: _c;
    output rj0: _rj0;
    output rj1: _rj1;
    output rj2: _rj2;
    output rj3: _rj3;
    output rj4: _rj4;
    output rj5: _rj5;
    output rj6: _rj6;
    output rj7: _rj7;

    label crank_moment(disk_moment_of_inertia(mass: crank_mass * units.kg, radius: (stroke / 2.0) * units.mm))
    label flywheel_moment(disk_moment_of_inertia(mass: flywheel_mass * units.kg, radius: (stroke * 3.5) * units.mm))
    label other_moment(disk_moment_of_inertia(mass: 1 * units.kg, radius: 1.0 * units.cm))

    crankshaft _c(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: flywheel_mass * units.kg,
        mass: crank_mass * units.kg,
        friction_torque: 11.1 * units.Nm,
        moment_of_inertia: crank_moment + flywheel_moment + other_moment,
        tdc: 0.0
    )

    rod_journal _rj0(angle: var.s1 + rot90 + vee / 2)
    rod_journal _rj1(angle: var.s2 + rot90 + vee / 2)
    rod_journal _rj2(angle: var.s3 + rot90 + vee / 2)
    rod_journal _rj3(angle: var.s4 + rot90 + vee / 2)
    rod_journal _rj4(angle: var.s5 + rot90 + vee / 2)
    rod_journal _rj5(angle: var.s6 + rot90 + vee / 2)
    rod_journal _rj6(angle: var.s7 + rot90 + vee / 2)
    rod_journal _rj7(angle: var.s8 + rot90 + vee / 2)

    _c
        .add_rod_journal(_rj0)
        .add_rod_journal(_rj1)
        .add_rod_journal(_rj2)
        .add_rod_journal(_rj3)
        .add_rod_journal(_rj4)
        .add_rod_journal(_rj5)
        .add_rod_journal(_rj6)
        .add_rod_journal(_rj7)
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "Heldt Experiment",
        starter_torque: 150 *units.Nm,
        starter_speed: 100 * units.rpm,
        redline: redline * units.rpm,
        fuel: s30(),
        throttle_gamma: 1.5,
        jitter: es.jitter,
        noise: es.noise,
        hf_gain: es.hf_gain,
        simulation_frequency: 12000,
        dyno_min_speed: 500 * units.rpm
    )

    variant_0 variant()

    crank c0(rot90, variant)

    bank_builder bank(
        var: variant,
        crankshaft: c0,
        incline: 0,
        phasing: 0,
        ignition: ignition,
        intake: intake,
        exhaust: es
    )

    ignition_module ignition(timing_curve: timing_curve, rev_limit: redline * 1.5 * units.rpm, limiter_duration: 0.03)

    intake intake(
        plenum_volume: 3.3 * units.L,
        plenum_cross_section_area: 49.4 * units.cm2,
        runner_length: 29.4 * units.cm,
        intake_flow_rate: k_carb(381.1),
        idle_flow_rate: k_carb(0.01),
        idle_throttle_plate_position: 0.995,
        runner_flow_rate: k_carb(95.3),
        velocity_decay: 0.05
    )

    es0 es()

    function timing_curve(206.7 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 2.0 * units.deg)
        .add_sample(206.7 * units.rpm, 2.0 * units.deg)
        .add_sample(413.3 * units.rpm, 2.0 * units.deg)
        .add_sample(620.0 * units.rpm, 5.1 * units.deg)
        .add_sample(826.7 * units.rpm, 9.5 * units.deg)
        .add_sample(1033.3 * units.rpm, 13.2 * units.deg)
        .add_sample(1240.0 * units.rpm, 16.1 * units.deg)
        .add_sample(1446.7 * units.rpm, 18.5 * units.deg)
        .add_sample(1653.3 * units.rpm, 20.5 * units.deg)
        .add_sample(1860.0 * units.rpm, 22.1 * units.deg)
        .add_sample(2066.7 * units.rpm, 23.4 * units.deg)
        .add_sample(2273.3 * units.rpm, 24.4 * units.deg)
        .add_sample(2480.0 * units.rpm, 25.3 * units.deg)
        .add_sample(2686.7 * units.rpm, 26.0 * units.deg)
        .add_sample(2893.3 * units.rpm, 26.5 * units.deg)
        .add_sample(3100.0 * units.rpm, 27.0 * units.deg)
        .add_sample(3306.7 * units.rpm, -15.0 * units.deg)
        .add_sample(3513.3 * units.rpm, -45.0 * units.deg)

    engine
        .add_crankshaft(c0.c)
        .add_cylinder_bank(bank.bank0)
        .add_cylinder_bank(bank.bank1)
        .add_ignition_module(ignition)
}

label spacing_factor(1.8)
label flange_density(1.0 * exhaust_delay_coeff)
label em_unit(bore * spacing_factor / flange_density)

private node es0{
    output es0: exhaust0; output es1: exhaust0; output es2: exhaust0; output es3: exhaust0;
    output es4: exhaust0; output es5: exhaust0; output es6: exhaust0; output es7: exhaust0;
    output pl0: _pl0; output pl1: _pl1; output pl2: _pl2; output pl3: _pl3;
    output pl4: _pl4; output pl5: _pl5; output pl6: _pl6; output pl7: _pl7;
    output jitter: 0.7;
    output noise: 0.2;
    output hf_gain: 0.007;

    label collector_cross_section_area(36.6)
    label exhaust_pipe_length_0(370.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 100.0) // Litres

    label sh(33.0 * units.cm)

    float _pl0 ((5.0 * em_unit) * units.mm + sh)
    float _pl2 ((4.0 * em_unit) * units.mm + sh)
    float _pl5 ((2.0 * em_unit) * units.mm + sh)
    float _pl7 ((1.0 * em_unit) * units.mm + sh)

    float _pl1 ((3.5 * em_unit) * units.mm)
    float _pl3 ((2.5 * em_unit) * units.mm)
    float _pl4 ((2.0 * em_unit) * units.mm)
    float _pl6 ((1.0 * em_unit) * units.mm)

    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(635.1),
        collector_cross_section_area: 36.6 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 52.9 * units.cm,
        primary_flow_rate: k_carb(158.8),
        velocity_decay: 1.0,
        volume: (exhaust_volume_0 + 3.0) * units.L
    )

    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_56.wav", volume: 0.00025))
}

// Sd-Kfz 221, 222, 223, 260, 261
public node veh {
    alias output __out: vehicle;
    label car_mass(4400.0) // 4500.0 loaded
    vehicle vehicle(
        mass: car_mass * units.kg,
        drag_coefficient: 0.5,
        cross_sectional_area: (2000 * units.mm) * (1850 * units.mm),
        diff_ratio: 4.25,
        tire_radius: (815.0 / 2) * units.mm, // 7.00x18
        rolling_resistance: 0.017 * car_mass * 9.81
        )
}

// Sd-Kfz 221, 222, 223, 260, 261
private node trn {
    alias output __out:
        transmission(
            max_clutch_torque: 550 * units.Nm,
            max_clutch_flex: 30 * units.deg,
            limit_clutch_flex: true,
            clutch_stiffness: 20 * units.Nm / units.deg,

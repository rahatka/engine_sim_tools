import "../../fuel/30.mr"

units units()
constants constants()

label bore(82.55) // 3 1/4
label stroke(114.3) // 4 1/2
label compression_ratio(5.25)
label con_rod(301.625) // 11 7/8
label compression_height(50.8 * units.mm) // 2
label intake_valve_diameter(50.8) // 2
label exhaust_valve_diameter(50.8) // 2
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(3700.0) // RPM

label cyl(12.0)
label cyl_base(cyl / row)
label row(2.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label rot_base(cycle / cyl_base)
label vee(65.0 * units.deg)

// Lincoln 448 V12
label IVL(8.73125) // 1932 11/32; 1933 5/16
label EVL(8.73125) // 1932 11/32; 1933 5/16
label IVO(21.0 * units.deg) //BTDC
label IVC(47.0 * units.deg) //ABDC or 49
label EVO(57.0 * units.deg) //BBDC
label EVC(11.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(963.884) // 34 oz
label piston_mass(600.0) // ?
label crank_mass(50.0) // ? kg
label flywheel_mass(10.0) // kg

label exhaust_delay_coeff(1.5)


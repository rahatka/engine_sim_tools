units units()
constants constants()

label bore(90.4875) // 3 9/16
label stroke(114.3) // 4 1/2
label compression_ratio(5.88)
label con_rod(240.5065) // 9 15/32
label compression_height(119.856 / 2 * units.mm) // 4 23/32  len
label intake_valve_diameter(42.0687) // 1.69=1 11/16 ; 1 1/2 clear ; 3/8 stem
label exhaust_valve_diameter(41.275) // 1.62=1 5/8 ; 1 3/8 clear; 3/8 stem
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(2800.0) // RPM

label cyl(6.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// White 250/270
label IVL(10.0584) // .396
label EVL(10.0584) // .396
label IVO(10.0 * units.deg) //BTDC
label IVC(35.0 * units.deg) //ABDC
label EVO(45.0 * units.deg) //BBDC
label EVC(5.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(1315.42) // 2.9 lbs
label piston_mass(1365.313) // 3.01 lbs
label crank_mass(44.225) // 97.5
label flywheel_mass(5.0) // kg

label exhaust_delay_coeff(1.5)


units units()
constants constants()

label bore(92.075) // 3 5/8
label stroke(95.25) // 3 3/4
label compression_ratio(6.25)
label con_rod(190.5) // 7 1/2
label compression_height(121.444 / 2 * units.mm) // 4 25/32 len 505
label intake_valve_diameter(44.45) // 1 3/4 ? ; 1 1/2 c ; 3/8 stem 505
label exhaust_valve_diameter(38.8937) // 1 17/32 ? ; 1 13/32 c ; 13/32 stem 505
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(2800.0) // RPM

label cyl(12.0)
label row(2.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label vee(rot180)

label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)

// White 505
label IVL(9.525) // 3/8
label EVL(9.525) // 3/8
label IVO(10.0 * units.deg) //BTDC
label IVC(40.0 * units.deg) //ABDC
label EVO(38.0 * units.deg) //BBDC
label EVC(5.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label piston_mass(1555.822) // 3.43 lb 505
label con_rod_mass(1143.053) // 2.52 lb
label crank_mass(55.565) // 122.5 lb
label flywheel_mass(15.0) // kg

label exhaust_delay_coeff(1.5)


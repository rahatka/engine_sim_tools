public node camshaft_0 {
    output intake_cam_0: _intake_cam_0;
    output exhaust_cam_0: _exhaust_cam_0;

    output intake_cam_1: _intake_cam_1;
    output exhaust_cam_1: _exhaust_cam_1;

    camshaft _intake_cam_0(base_radius: 13.1 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 13.1 * units.mm, lobe_profile: exhaust_lobe_profile)
    
    camshaft _intake_cam_1(base_radius: 13.1 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_1(base_radius: 13.1 * units.mm, lobe_profile: exhaust_lobe_profile)

    _intake_cam_0
        .add_lobe(ILC + 0 * rot)
        .add_lobe(ILC + 8 * rot)
        .add_lobe(ILC + 4 * rot)
        .add_lobe(ILC + 10 * rot)
        .add_lobe(ILC + 2 * rot)
        .add_lobe(ILC + 6 * rot)
    _exhaust_cam_0
        .add_lobe(ELC + 0 * rot)
        .add_lobe(ELC + 8 * rot)
        .add_lobe(ELC + 4 * rot)
        .add_lobe(ELC + 10 * rot)
        .add_lobe(ELC + 2 * rot)
        .add_lobe(ELC + 6 * rot)
        
    _intake_cam_1
        .add_lobe(ILC + 0 * rot + vee + invert)
        .add_lobe(ILC + 8 * rot + vee + invert)
        .add_lobe(ILC + 4 * rot + vee + invert)
        .add_lobe(ILC + 10 * rot + vee + invert)
        .add_lobe(ILC + 2 * rot + vee + invert)
        .add_lobe(ILC + 6 * rot + vee + invert)
    _exhaust_cam_1
        .add_lobe(ELC + 0 * rot + vee + invert)
        .add_lobe(ELC + 8 * rot + vee + invert)
        .add_lobe(ELC + 4 * rot + vee + invert)
        .add_lobe(ELC + 10 * rot + vee + invert)
        .add_lobe(ELC + 2 * rot + vee + invert)
        .add_lobe(ELC + 6 * rot + vee + invert)
}

public node camshaft_1 {
    output intake_cam_0: _intake_cam_0;
    output exhaust_cam_0: _exhaust_cam_0;

    output intake_cam_1: _intake_cam_1;

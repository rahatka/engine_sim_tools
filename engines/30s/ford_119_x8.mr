public node camshaft_builder_0 {
    output intake_cam_0: _intake_cam_0;
    output exhaust_cam_0: _exhaust_cam_0;

    output intake_cam_1: _intake_cam_1;
    output exhaust_cam_1: _exhaust_cam_1;
    
    output intake_cam_2: _intake_cam_2;
    output exhaust_cam_2: _exhaust_cam_2;

    output intake_cam_3: _intake_cam_3;
    output exhaust_cam_3: _exhaust_cam_3;



    camshaft _intake_cam_0(base_radius: 13.1 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 13.1 * units.mm, lobe_profile: exhaust_lobe_profile)
    
    camshaft _intake_cam_1(base_radius: 13.1 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_1(base_radius: 13.1 * units.mm, lobe_profile: exhaust_lobe_profile)
    
    camshaft _intake_cam_2(base_radius: 13.1 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_2(base_radius: 13.1 * units.mm, lobe_profile: exhaust_lobe_profile)
    
    camshaft _intake_cam_3(base_radius: 13.1 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_3(base_radius: 13.1 * units.mm, lobe_profile: exhaust_lobe_profile)

    _intake_cam_0
        .add_lobe(ILC + 0 * rot) // 1
        .add_lobe(ILC + 6 * rot) // 5
    _exhaust_cam_0
        .add_lobe(ELC + 0 * rot) // 1
        .add_lobe(ELC + 6 * rot) // 5
        
    _intake_cam_1
        .add_lobe(ILC + 5 * rot) // 2
        .add_lobe(ILC + 3 * rot) // 6
    _exhaust_cam_1
        .add_lobe(ELC + 5 * rot) // 2
        .add_lobe(ELC + 3 * rot) // 6
        
    _intake_cam_2
        .add_lobe(ILC + 2 * rot) // 3
        .add_lobe(ILC + 4 * rot) // 7
    _exhaust_cam_2
        .add_lobe(ELC + 2 * rot) // 3
        .add_lobe(ELC + 4 * rot) // 7
        
    _intake_cam_3
        .add_lobe(ILC + 7 * rot) // 4
        .add_lobe(ILC + 1 * rot) // 8
    _exhaust_cam_3
        .add_lobe(ELC + 7 * rot) // 4
        .add_lobe(ELC + 1 * rot) // 8
}


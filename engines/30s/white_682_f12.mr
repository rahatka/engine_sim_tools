units units()
constants constants()

label bore(104.775) // 4 1/8
label stroke(107.95) // 4 1/4
label compression_ratio(5.88)
label con_rod(203.99375) // 8 1/32
label compression_height(111.76 / 2 * units.mm) // 4.4 len
label intake_valve_diameter(46.99) // 1.85 ; 1.62 cl ; .401 stem
label exhaust_valve_diameter(44.45) // 1.75 ; 1.62 cl ; .401 stem
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(3200.0) // RPM

label cyl(12.0)
label row(2.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label vee(rot180)

label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)

// White 505 timing, 682 lift
label IVL(9.6774) // .381
label EVL(9.6774) // .381
label IVO(10.0 * units.deg) //BTDC
label IVC(40.0 * units.deg) //ABDC
label EVO(38.0 * units.deg) //BBDC
label EVC(5.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label piston_mass(1247.38) // 44
label con_rod_mass(1460.0) // 51.5
label crank_mass(32.0) // ? kg
label flywheel_mass(15.0) // kg

label exhaust_delay_coeff(1.5)


units units()
constants constants()

label bore(114.3) // 4 1/2
label stroke(120.65) // 4 3/4
label compression_ratio(4.77)
label con_rod(244.475) // 9 5/8
label compression_height(123.698 / 2 * units.mm) // 4.87 len
label intake_valve_diameter(50.8) // 2 ; 1 3/4 c
label exhaust_valve_diameter(50.8) // 2 ; 1 3/4 c
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(2400.0) // RPM

label cyl(6.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// YXC only lifts
label IVL(9.8552) // .388
label EVL(9.8552) // .388
label IVO(2.0 * units.deg) //BTDC
label IVC(47.0 * units.deg) //ABDC
label EVO(45.0 * units.deg) //BBDC
label EVC(2.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(1814.37) // 64
label piston_mass(2409.71) // 85
label crank_mass(28.0) // ?
label flywheel_mass(10.0) // kg

label exhaust_delay_coeff(1.5)


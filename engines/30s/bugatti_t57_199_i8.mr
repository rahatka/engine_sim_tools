import "engine_sim.mr"
import "../../fuel/30.mr"

units units()
constants constants()

label bore(72.0)
label stroke(100.0)
label compression_ratio(6.2)
label con_rod(180.0)

label compression_height(37.0 * units.mm) // ?
label intake_valve_diameter(41.0)
label exhaust_valve_diameter(41.0)
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(5500.0) // RPM

label cyl(8.0)
label row(1.0)
label cycle(720.0 * units.deg)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label rot(cycle / cyl)

// Bugatti T57
label IVL(9.0) // ?
label EVL(9.0) // ?
label IVO(5.0 * units.deg) //BTDC
label IVC(60.0 * units.deg) //ABDC
label EVO(51.0 * units.deg) //BBDC
label EVC(9.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(600.0) // ?
label piston_mass(700.0) // ?
label crank_mass(55.0) // kg
label flywheel_mass(4.0) // kg

label exhaust_delay_coeff(1.5)


import "engine_sim.mr"
import "../../fuel/30.mr"

units units()
constants constants()

label vee(60.0 * units.deg)
label bore(86.0)
label stroke(100.0)
label compression_ratio(5.6)
label con_rod(225.0)
label compression_height(50.0 * units.mm) // ~
label intake_valve_diameter(40.0) // 34.8 clear
label exhaust_valve_diameter(40.0) // 34.8 clear
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(3000.0) // RPM

label cyl(12.0)
label cyl_base(cyl / row)
label row(2.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label rot_base(cycle / cyl_base)

// Maybach; only lifts are known
label IVL(7.9)
label EVL(7.9)
label IVO(6.0 * units.deg) //BTDC
label IVC(40.0 * units.deg) //ABDC
label EVO(40.0 * units.deg) //BBDC
label EVC(6.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label piston_mass(700.0) // ?
label con_rod_mass(800.0) // ?
label crank_mass(27.0) // kg
label flywheel_mass(12.0) // kg

label exhaust_delay_coeff(1.5)


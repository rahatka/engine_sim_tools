units units()
constants constants()

label bore(127.0) // 5
label stroke(152.4) // 6
label compression_ratio(4.83)
label con_rod(307.975) // 12 1/8
label compression_height(127.762 / 2 * units.mm) // 5.03 len
label intake_valve_diameter(61.9125) // 2 7/16
label exhaust_valve_diameter(55.16565) // 2 11/64
label intake_valves(1.0)
label exhaust_valves(1.0)

label cyl(6.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)

label redline(2250.0) // RPM

// GMC 707 only lifts
label IVL(10.4902) // .413
label EVL(10.4902) // .413
label IVO(5.0 * units.deg) //BTDC
label IVC(40.0 * units.deg) //ABDC
label EVO(50.0 * units.deg) //BBDC
label EVC(10.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label piston_mass(2182.91) // 77
label con_rod_mass(2182.91) // 77
label crank_mass(30.0) // ? kg
label flywheel_mass(9.0) // kg

label exhaust_delay_coeff(1.5)


public node camshaft_builder {
    output intake_cam_0: _intake_cam_0;
    output exhaust_cam_0: _exhaust_cam_0;

    output intake_cam_1: _intake_cam_1;
    output exhaust_cam_1: _exhaust_cam_1;

    camshaft _intake_cam_0(base_radius: 13.0 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 13.0 * units.mm, lobe_profile: exhaust_lobe_profile)
    
    camshaft _intake_cam_1(base_radius: 13.0 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_1(base_radius: 13.0 * units.mm, lobe_profile: exhaust_lobe_profile)

    _intake_cam_0
        .add_lobe(ILC + 0 * rot)
        .add_lobe(ILC + 7 * rot)
        .add_lobe(ILC + 5 * rot)
        .add_lobe(ILC + 2 * rot)
    _exhaust_cam_0
        .add_lobe(ELC + 0 * rot)
        .add_lobe(ELC + 7 * rot)
        .add_lobe(ELC + 5 * rot)
        .add_lobe(ELC + 2 * rot)
        
    _intake_cam_1
        .add_lobe(ILC + 1 * rot)
        .add_lobe(ILC + 4 * rot)
        .add_lobe(ILC + 6 * rot)
        .add_lobe(ILC + 3 * rot)
    _exhaust_cam_1
        .add_lobe(ELC + 1 * rot)
        .add_lobe(ELC + 4 * rot)
        .add_lobe(ELC + 6 * rot)
        .add_lobe(ELC + 3 * rot)
}

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node head {
    // port_flow.py v1.5
    // intake port area: 9.3 cm²; saturated lift: 8.39 mm
    // exhaust port area: 9.3 cm²; saturated lift: 8.39 mm
    // cylinder volume: 490.2 cm³ (29.91 CI); engine volume: 3.922 L (239.32 CI)
    // chamber volume: 95.2 cm³ (5.81 CI)
    // 16 harmonic intake runner length: 25.9 cm; diameter: 3.2 cm
    // primary length: 44.9 cm, area: 10.2 cm², diameter: 3.6 cm
    // collector diameter: 5.6 cm, area: 24.6 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 95.187 * units.cc,
        intake_runner_volume: 204.0 * units.cc,
        intake_runner_cross_section_area: 7.9 * units.cm2,
        exhaust_runner_volume: 68.0 * units.cc,
        exhaust_runner_cross_section_area: 10.2 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "Mercury V8",
        starter_torque: 100 *units.Nm,
        starter_speed: 300 * units.rpm,
        redline: redline * units.rpm,
        fuel: s30(),
        throttle_gamma: 1.5,
        jitter: 0.8,
        noise: 0.3,
        hf_gain: 0.007,
        simulation_frequency: 10000 
    )

    wires wires()

    connecting_rod_parameters cr_params(
        mass: con_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.g,
            length: con_rod * units.mm
        ),
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    label crank_moment(disk_moment_of_inertia(mass: crank_mass * units.kg, radius: (stroke / 2.0) * units.mm))
    label flywheel_moment(disk_moment_of_inertia(mass: flywheel_mass * units.kg, radius: (stroke * 3.5) * units.mm))
    label other_moment(disk_moment_of_inertia(mass: 1 * units.kg, radius: 1.0 * units.cm))

    crankshaft c0(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: flywheel_mass * units.kg,
        mass: crank_mass * units.kg,
        friction_torque: 5.9 * units.Nm,
        moment_of_inertia: crank_moment + flywheel_moment + other_moment,
        position_x: 0.0,
        position_y: 0.0,
        tdc: 0.0
    )

    rod_journal rj0(angle: rot * 0 + rot90 + vee / 2)
    rod_journal rj1(angle: rot * 7 + rot90 + vee / 2)
    rod_journal rj2(angle: rot * 5 + rot90 + vee / 2)
    rod_journal rj3(angle: rot * 2 + rot90 + vee / 2)
    
    c0
        .add_rod_journal(rj0)
        .add_rod_journal(rj1)
        .add_rod_journal(rj2)
        .add_rod_journal(rj3)

    piston_parameters piston_params(
        mass: piston_mass * units.g,
        blowby: k_28inH2O(0.025),
        compression_height: compression_height,
        wrist_pin_position: 0.0,
        displacement: 0.0
    )

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    intake intake(
        plenum_volume: 2.1 * units.L,
        plenum_cross_section_area: 31.5 * units.cm2,
        runner_length: 25.9 * units.cm,
        intake_flow_rate: k_carb(245.1),
        idle_flow_rate: k_carb(0.02),
        idle_throttle_plate_position: 0.993,
        runner_flow_rate: k_carb(61.3),
        velocity_decay: 0.05
    )
 
    cylinder_bank b0(bank_params, angle: vee / 2)
    cylinder_bank b1(bank_params, angle: -vee / 2)

    em0 em()

    b0
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj0,
            intake: intake,
            exhaust_system: em.es0,
            primary_length: em.pl0,
            ignition_wire: wires.wire1
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj1,
            intake: intake,
            exhaust_system: em.es1,
            primary_length: em.pl1,
            ignition_wire: wires.wire2
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj2,
            intake: intake,
            exhaust_system: em.es2,
            primary_length: em.pl2,
            ignition_wire: wires.wire3
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj3,
            intake: intake,
            exhaust_system: em.es3,
            primary_length: em.pl3,
            ignition_wire: wires.wire4
        )
        
    b1
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj0,
            intake: intake,
            exhaust_system: em.es4,
            primary_length: em.pl4,
            ignition_wire: wires.wire5
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj1,
            intake: intake,
            exhaust_system: em.es5,
            primary_length: em.pl5,
            ignition_wire: wires.wire6
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj2,
            intake: intake,
            exhaust_system: em.es6,
            primary_length: em.pl6,
            ignition_wire: wires.wire7
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj3,
            intake: intake,
            exhaust_system: em.es7,
            primary_length: em.pl7,
            ignition_wire: wires.wire8
        )

    engine
        .add_cylinder_bank(b0)
        .add_cylinder_bank(b1)

    engine.add_crankshaft(c0)

    camshaft_builder camshaft()

    b0.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_0,
            exhaust_camshaft: camshaft.exhaust_cam_0,
            flip_display: true
        )
    )
    b1.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_1,
            exhaust_camshaft: camshaft.exhaust_cam_1
        )
    )

    function timing_curve(250.0 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 4.0 * units.deg)
        .add_sample(250.0 * units.rpm, 4.0 * units.deg)
        .add_sample(500.0 * units.rpm, 5.2 * units.deg)
        .add_sample(750.0 * units.rpm, 7.9 * units.deg)
        .add_sample(1000.0 * units.rpm, 10.2 * units.deg)
        .add_sample(1250.0 * units.rpm, 12.2 * units.deg)
        .add_sample(1500.0 * units.rpm, 13.9 * units.deg)
        .add_sample(1750.0 * units.rpm, 15.3 * units.deg)
        .add_sample(2000.0 * units.rpm, 16.6 * units.deg)
        .add_sample(2250.0 * units.rpm, 17.6 * units.deg)
        .add_sample(2500.0 * units.rpm, 18.5 * units.deg)
        .add_sample(2750.0 * units.rpm, 19.3 * units.deg)
        .add_sample(3000.0 * units.rpm, 20.0 * units.deg)
        .add_sample(3250.0 * units.rpm, 20.0 * units.deg)
        .add_sample(3500.0 * units.rpm, 20.0 * units.deg)
        .add_sample(3750.0 * units.rpm, 20.0 * units.deg)
        .add_sample(4000.0 * units.rpm, -15.0 * units.deg)
        .add_sample(4250.0 * units.rpm, -45.0 * units.deg)

    engine.add_ignition_module(
        eng_distributor(
            wires: wires,
            timing_curve: timing_curve
        )
    )
}

// stock manifold
private node em0{
    output es0: exhaust0;
    output es1: exhaust0;
    output es2: exhaust0;
    output es3: exhaust0;
    output es4: exhaust0;
    output es5: exhaust0;
    output es6: exhaust0;
    output es7: exhaust0;
    output pl0: _pl0;
    output pl1: _pl1;
    output pl2: _pl2;
    output pl3: _pl3;
    output pl4: _pl4;
    output pl5: _pl5;
    output pl6: _pl6;
    output pl7: _pl7;

    label spacing_factor(1.2)
    label flange_density(1.0 * exhaust_delay_coeff)
    label sh(50.0 * units.cm)
    label collector_cross_section_area(24.6)
    label exhaust_pipe_length_0(400.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 100.0) // Litres

    float _pl0((1.5 * bore * spacing_factor / flange_density) * units.mm)
    float _pl1((1.5 * bore * spacing_factor / flange_density) * units.mm)
    float _pl2((1.5 * bore * spacing_factor / flange_density) * units.mm)
    float _pl3((2.5 * bore * spacing_factor / flange_density) * units.mm)
    float _pl4((1.6 * bore * spacing_factor / flange_density) * units.mm + sh)
    float _pl5((1.6 * bore * spacing_factor / flange_density) * units.mm + sh)
    float _pl6((1.6 * bore * spacing_factor / flange_density) * units.mm + sh)
    float _pl7((2.7 * bore * spacing_factor / flange_density) * units.mm + sh)

    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(408.5),
        collector_cross_section_area: 24.6 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 44.9 * units.cm,
        primary_flow_rate: k_carb(102.1),
        velocity_decay: 1.0,
        volume: (exhaust_volume_0 + 5.0) * units.L
    )

    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_56.wav", volume: 0.00025))
}

private node em1{
    output es0: exhaust0;
    output es1: exhaust1;
    output es2: exhaust1;
    output es3: exhaust0;
    output es4: exhaust1;
    output es5: exhaust0;
    output es6: exhaust0;
    output es7: exhaust1;
    output pl0: _pl0;
    output pl1: _pl1;
    output pl2: _pl2;
    output pl3: _pl3;
    output pl4: _pl4;
    output pl5: _pl5;
    output pl6: _pl6;
    output pl7: _pl7;

    label es_diff(3.0)
    label spacing_factor(1.2)
    label flange_density(1.5 * exhaust_delay_coeff)
    label collector_cross_section_area(12.3)
    label exhaust_pipe_length_0(250.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 100.0) // Litres
    

    float _pl0((3.0 * bore * spacing_factor / flange_density) * units.mm) // 1
    float _pl1(((2.0 + es_diff) * bore * spacing_factor / flange_density) * units.mm) // 2
    float _pl2(((2.0 + es_diff) * bore * spacing_factor / flange_density) * units.mm) // 2
    float _pl3((1.0 * bore * spacing_factor / flange_density) * units.mm) // 1
    
    float _pl4(((3.0 + es_diff) * bore * spacing_factor / flange_density) * units.mm) // 2
    float _pl5((2.0 * bore * spacing_factor / flange_density) * units.mm) // 1
    float _pl6((2.0 * bore * spacing_factor / flange_density) * units.mm) // 1
    float _pl7(((1.0 + es_diff) * bore * spacing_factor / flange_density) * units.mm) // 2

    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(204.3),
        collector_cross_section_area: 12.3 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 44.9 * units.cm,
        primary_flow_rate: k_carb(102.1),
        velocity_decay: 0.5,
        volume: (exhaust_volume_0 + 5.0) * units.L
    )

    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_14.wav", volume: 0.00025))
    exhaust_system exhaust1(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_14.wav", volume: 0.00025))
 
}

// Cadillac 353
public node veh {
    alias output __out: vehicle;
    vehicle vehicle(
        mass:  1989 * units.kg,
        drag_coefficient: 0.48,
        cross_sectional_area: (1920 * units.mm) * (1200 * units.mm),
        diff_ratio: 4.39,
        tire_radius: (837.0 / 2) * units.mm, // 7.00x19
        rolling_resistance: 20
        )
}

// Cadillac 353
private node trn {
    alias output __out:
        transmission(
            max_clutch_torque: 400 * units.Nm,
            max_clutch_flex: 10 * units.deg,
            limit_clutch_flex: true,
            clutch_stiffness: 20 * units.Nm / units.deg,
            clutch_damping: 2.0,
            simulate_flex: true
        )
        .add_gear(2.50)
        .add_gear(1.51)
        .add_gear(1.0);
}

run(
    engine: eng(),
    transmission: trn(),
    vehicle: veh()
)

import "engine_sim.mr"
import "../../fuel/30.mr"

units units()
constants constants()

label bore(77.7875) // 3 1/16 
label stroke(120.65) // 4 3/4
label compression_ratio(6.2)
label con_rod(241.3) // 9 1/2
label compression_height(53.975 * units.mm) // ?
label intake_valve_diameter(39.6875) // 1 9/16
label exhaust_valve_diameter(35.7188) // 1 13/32
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(3750.0) // RPM

label cyl(6.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// Lycoming WF
label IVL(7.9375) // 5/16
label EVL(7.9375) // 5/16
label IVO(5.0 * units.deg) //BTDC; 1935 7.5
label IVC(40.0 * units.deg) //ABDC; 1935 37.5
label EVO(50.0 * units.deg) //BBDC; 1935 50.0
label EVC(10.0 * units.deg) //ATDC; 1935 5.0

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(1061.406) // 2.34 lbs
label piston_mass(621.42155) // 21.92 oz
label crank_mass(18.0) // ?
label flywheel_mass(4.0) // kg

label exhaust_delay_coeff(1.5)

public node eng_distributor {
    input wires;
    input timing_curve;
    input rev_limit: 4500 * units.rpm;
    input limiter_duration: 0.0001;
    alias output __out:
        ignition_module(timing_curve: timing_curve, rev_limit: rev_limit, limiter_duration: limiter_duration)
            .connect_wire(wires.wire1, rot * 0)
            .connect_wire(wires.wire5, rot * 1)
            .connect_wire(wires.wire3, rot * 2)
            .connect_wire(wires.wire6, rot * 3)
            .connect_wire(wires.wire2, rot * 4)
            .connect_wire(wires.wire4, rot * 5);
}

private node wires {
    output wire1: ignition_wire();
    output wire2: ignition_wire();
    output wire3: ignition_wire();
    output wire4: ignition_wire();
    output wire5: ignition_wire();
    output wire6: ignition_wire();
}


public node veh {
    alias output __out: vehicle;
    vehicle vehicle(
        mass: 1520  * units.kg,
        drag_coefficient: 0.45,
        cross_sectional_area: (1854 * units.mm) * (1753 * units.mm),
        diff_ratio: 3.0,
        tire_radius: (673.0 / 2) * units.mm,
        rolling_resistance: 20
        )
}


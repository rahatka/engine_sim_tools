label compression_height(44.45 * units.mm) // 1.75
label intake_valve_diameter(39.0398) // 1.537
label exhaust_valve_diameter(39.0398) // 1.537
label intake_valves(1.0)
label exhaust_valves(1.0)

label cyl(12.0)
label row(2.0)
label cycle(720.0 * units.deg)
label vee(75.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)

label volume(4.380) // L;
label redline(4250.0) // RPM

// 1936 Zephyr
label IVL(7.4168) // Zephyr .292 lift at cam
label EVL(7.4168) // Zephyr .292 lift at cam
label IVO(19.3 * units.deg) //BTDC; 1938-39 10.42
label IVC(54.3 * units.deg) //ABDC; 1938-39 35.58
label EVO(57.3 * units.deg) //BBDC; 1938-39 50.92
label EVC(16.3 * units.deg) //ATDC; 1938-39 8.08

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(638.0)
label piston_mass(500.0) // 326 without rings or pin
label crank_mass(30.0) // ? kg
label flywheel_mass(10.0) // kg

label exhaust_delay_coeff(1.5)


import "../../fuel/30.mr"

units units()
constants constants()

label bore(98.425) // 3 7/8
label stroke(107.95) // 4 1/4
label compression_ratio(4.6)
label con_rod(190.5) // 7 1/2
label compression_height(48.387 * units.mm) // 1.905
label intake_valve_diameter(37.6936) // 1.484 ; 1.375 clear ; 5/16 stem
label exhaust_valve_diameter(37.6936) // 1.484 ; 1.375 clear ; 5/16 stem
label intake_valves(1.0)
label exhaust_valves(1.0)
label offset(3.175)

label cyl(4.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

label redline(3250.0) // RPM


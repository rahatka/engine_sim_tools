public node camshaft_builder {
    output intake_cam_0: _intake_cam_0;
    output exhaust_cam_0: _exhaust_cam_0;

    camshaft _intake_cam_0(base_radius: 13.5 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 13.5 * units.mm, lobe_profile: exhaust_lobe_profile)

    _intake_cam_0
        .add_lobe(ILC + 0 * rot)
        .add_lobe(ILC + 1 * rot)
        .add_lobe(ILC + 3 * rot)
        .add_lobe(ILC + 2 * rot)
    _exhaust_cam_0
        .add_lobe(ELC + 0 * rot)
        .add_lobe(ELC + 1 * rot)
        .add_lobe(ELC + 3 * rot)
        .add_lobe(ELC + 2 * rot)
}

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node head {
    // port_flow.py v1.7
    // intake port area: 9.1 cm²; saturated lift: 8.28 mm
    // exhaust port area: 9.1 cm²; saturated lift: 8.28 mm
    // cylinder volume: 821.5 cm³ (50.13 CI); engine volume: 3.286 L (200.52 CI)
    // chamber volume: 228.2 cm³ (13.92 CI); conrod/stroke 1.765; N.A.C.C. 24.02
    // 32 harmonic intake runner length: 15.1 cm; diameter: 3.6 cm
    // primary length: 44.0 cm, area: 10.0 cm², diameter: 3.6 cm
    // collector diameter: 3.9 cm, area: 12.0 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 228.185 * units.cc,
        intake_runner_volume: 154.7 * units.cc,
        intake_runner_cross_section_area: 10.3 * units.cm2,
        exhaust_runner_volume: 51.6 * units.cc,
        exhaust_runner_cross_section_area: 10.0 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "GAZ M-1",
        starter_torque: 70 * units.Nm,
        starter_speed: 300 * units.rpm,
        redline: redline * units.rpm,
        fuel: s30(),
        throttle_gamma: 1.5,
        jitter: 0.7,
        noise: 0.1,
        hf_gain: 0.005,
        simulation_frequency: 12000,
        dyno_min_speed: 500 * units.rpm
    )

    wires wires()

    label crank_moment(disk_moment_of_inertia(mass: crank_mass * units.kg, radius: (stroke / 2.0) * units.mm))
    label flywheel_moment(disk_moment_of_inertia(mass: flywheel_mass * units.kg, radius: (stroke * 2.0) * units.mm))
    label other_moment(disk_moment_of_inertia(mass: 1 * units.kg, radius: 1.0 * units.cm))

    crankshaft c0(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: 17.8602 * units.kg,
        mass: 12.0 * units.kg,
        friction_torque: 4.9 * units.Nm,
        moment_of_inertia: crank_moment + flywheel_moment + other_moment,
        position_x: -offset * units.mm,
        position_y: 0.0,
        tdc: 0.0
    )

    label tdc_comp(-0.744 * units.deg) // 90 - math.acos(x_shift / (stroke / 2 + con_rod))

    rod_journal rj0(angle: rot * 0 + rot90 + tdc_comp)
    rod_journal rj1(angle: rot * 1 + rot90 + tdc_comp)
    rod_journal rj2(angle: rot * 3 + rot90 + tdc_comp)
    rod_journal rj3(angle: rot * 2 + rot90 + tdc_comp)
    
    c0
        .add_rod_journal(rj0)
        .add_rod_journal(rj1)
        .add_rod_journal(rj2)
        .add_rod_journal(rj3)

    piston_parameters piston_params(
        mass: 578.0 * units.g,
        blowby: k_28inH2O(0.031),
        compression_height: compression_height,
        wrist_pin_position: 0.0,
        displacement: 0.0
    )

    connecting_rod_parameters cr_params(
        mass: con_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.g,
            length: con_rod * units.mm
        ),
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    label collector_cross_section_area(12.0)

    label exhaust_pipe_length_0(250.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 100.0) // Litres
    
    label spacing_factor(1.16)
    label flange_density(1.0 * exhaust_delay_coeff)

    intake intake(
        plenum_volume: 1.2 * units.L,
        plenum_cross_section_area: 20.5 * units.cm2,
        runner_length: 15.1 * units.cm,
        intake_flow_rate: k_carb(155.7),
        idle_flow_rate: k_carb(0.01),
        idle_throttle_plate_position: 0.994,
        runner_flow_rate: k_carb(77.9),
        velocity_decay: 0.05
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(243.3),
        collector_cross_section_area: 12.0 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 44.0 * units.cm,
        primary_flow_rate: k_carb(121.7),
        velocity_decay: 1.0,
        volume: exhaust_volume_0 * units.L
    )

    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_56.wav", volume: 0.00025))
 
    cylinder_bank b0(bank_params, angle: 0)

    // ГАЗ М
    label pl0((4.90 * bore * spacing_factor / flange_density) * units.mm)
    label pl1((3.66 * bore * spacing_factor / flange_density) * units.mm)
    label pl2((2.99 * bore * spacing_factor / flange_density) * units.mm)
    label pl3((1.20 * bore * spacing_factor / flange_density) * units.mm)

    b0
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj0,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl0,
            ignition_wire: wires.wire1
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj1,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl1,
            ignition_wire: wires.wire2
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj2,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl2,
            ignition_wire: wires.wire3
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj3,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl3,
            ignition_wire: wires.wire4
        )

    engine
        .add_cylinder_bank(b0)

    engine.add_crankshaft(c0)

    camshaft_builder camshaft()

    b0.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_0,
            exhaust_camshaft: camshaft.exhaust_cam_0
        )
    )

    function timing_curve(216.7 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 0.0 * units.deg)
        .add_sample(216.7 * units.rpm, 0.0 * units.deg)
        .add_sample(433.3 * units.rpm, 1.5 * units.deg)
        .add_sample(650.0 * units.rpm, 4.1 * units.deg)
        .add_sample(866.7 * units.rpm, 6.3 * units.deg)
        .add_sample(1083.3 * units.rpm, 8.0 * units.deg)
        .add_sample(1300.0 * units.rpm, 9.4 * units.deg)
        .add_sample(1516.7 * units.rpm, 10.5 * units.deg)
        .add_sample(1733.3 * units.rpm, 11.4 * units.deg)
        .add_sample(1950.0 * units.rpm, 12.1 * units.deg)
        .add_sample(2166.7 * units.rpm, 12.7 * units.deg)
        .add_sample(2383.3 * units.rpm, 13.1 * units.deg)
        .add_sample(2600.0 * units.rpm, 13.5 * units.deg)
        .add_sample(2816.7 * units.rpm, 13.8 * units.deg)
        .add_sample(3033.3 * units.rpm, 14.0 * units.deg)
        .add_sample(3250.0 * units.rpm, 14.0 * units.deg)
        .add_sample(3466.7 * units.rpm, -15.0 * units.deg)
        .add_sample(3683.3 * units.rpm, -45.0 * units.deg)

    engine.add_ignition_module(
        eng_distributor(
            wires: wires,
            timing_curve: timing_curve
        )
    )
}


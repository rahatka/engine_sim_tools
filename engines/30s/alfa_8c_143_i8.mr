import "../../fuel/30.mr"

units units()
constants constants()

label bore(65.0)
label stroke(88.0)
label compression_ratio(5.75)
label con_rod(189.0)
label compression_height(85.0 / 2 * units.mm) // 85 PH
label intake_valve_diameter(34.0)
label exhaust_valve_diameter(34.0) // ?
label intake_valves(1.0)
label exhaust_valves(1.0)

label cyl(8.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)

label redline(5600.0) // RPM

// 1931 Alfa 8C 2300
label IVL(7.0)
label EVL(7.0) // ?
label IVO(12.0 * units.deg) //BTDC
label IVC(55.0 * units.deg) //ABDC
label EVO(55.0 * units.deg) //BBDC
label EVC(12.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label piston_mass(300.0) // ?
label con_rod_mass(400.0) // ?
label crank_mass(15.0) // ? kg
label flywheel_mass(3.0) // kg

label exhaust_delay_coeff(1.5)


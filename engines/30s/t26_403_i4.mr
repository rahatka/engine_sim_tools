import "engine_sim.mr"
import "../../fuel/10.mr"

units units()
constants constants()

label bore(120.0) // 4 3/4 ?
label stroke(146.0) // 146.5 doc 5 3/4 ?
label compression_ratio(4.8)
label con_rod(279.4) // ~ 11
label compression_height(50.8 * units.mm) // ~ 2
label intake_valve_diameter(58.0)
label exhaust_valve_diameter(54.6)
label intake_valves(1.0)
label exhaust_valves(1.0)
label offset(9.525) // ~ 3/8

label redline(2300.0) // RPM

label cyl(4.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// T-26 valve timings
label IVL(13.0) // 13.6 max
label EVL(13.0) // 13.6 max
label IVO(17.3 * units.deg) //BTDC
label IVC(75.0 * units.deg) //ABDC
label EVO(67.0 * units.deg) //BBDC
label EVC(28.3 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label piston_mass(2000.0) // ? g
label con_rod_mass(3500.0) // ? g
label crank_mass(50.0) // ? kg
label flywheel_mass(10.0) // kg

label exhaust_delay_coeff(1.5)

public node eng_distributor {
    input wires;
    input timing_curve;
    input rev_limit: 3000 * units.rpm;
    input limiter_duration: 0.0001;
    alias output __out:
        ignition_module(timing_curve: timing_curve, rev_limit: rev_limit, limiter_duration: limiter_duration)
            .connect_wire(wires.wire1, rot * 0)
            .connect_wire(wires.wire2, rot * 1)
            .connect_wire(wires.wire4, rot * 2)
            .connect_wire(wires.wire3, rot * 3);
}

private node wires {
    output wire1: ignition_wire();
    output wire2: ignition_wire();
    output wire3: ignition_wire();
    output wire4: ignition_wire();
}


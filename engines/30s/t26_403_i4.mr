public node camshaft_builder {
    output intake_cam_0: _intake_cam_0;
    output exhaust_cam_0: _exhaust_cam_0;

    camshaft _intake_cam_0(base_radius: 23.8 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 23.8 * units.mm, lobe_profile: exhaust_lobe_profile)

    _intake_cam_0
        .add_lobe(ILC + 0 * rot)
        .add_lobe(ILC + 1 * rot)
        .add_lobe(ILC + 3 * rot)
        .add_lobe(ILC + 2 * rot)
    _exhaust_cam_0
        .add_lobe(ELC + 0 * rot)
        .add_lobe(ELC + 1 * rot)
        .add_lobe(ELC + 3 * rot)
        .add_lobe(ELC + 2 * rot)
}

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node head {
    // port_flow.py v1.7
    // intake port area: 20.5 cm²; saturated lift: 12.44 mm
    // exhaust port area: 18.2 cm²; saturated lift: 11.71 mm
    // cylinder volume: 1652.3 cm³ (100.83 CI); engine volume: 6.609 L (403.31 CI)
    // chamber volume: 434.8 cm³ (26.53 CI); conrod/stroke 1.914; N.A.C.C. 35.71
    // 32 harmonic intake runner length: 20.0 cm; diameter: 4.7 cm
    // primary length: 57.8 cm, area: 20.0 cm², diameter: 5.0 cm
    // collector diameter: 5.5 cm, area: 24.0 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 434.803 * units.cc,
        intake_runner_volume: 348.6 * units.cc,
        intake_runner_cross_section_area: 17.4 * units.cm2,
        exhaust_runner_volume: 116.2 * units.cc,
        exhaust_runner_cross_section_area: 20.0 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "T-26",
        starter_torque: 100 * units.Nm,
        starter_speed: 300 * units.rpm,
        redline: redline * units.rpm,
        fuel: s10(),
        throttle_gamma: 1.5,
        jitter: 0.7,
        noise: 0.2,
        hf_gain: 0.005,
        simulation_frequency: 12000,
        dyno_min_speed: 500 * units.rpm
    )

    wires wires()

    connecting_rod_parameters cr_params(
        mass: con_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.g,
            length: con_rod * units.mm
        ),
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    label crank_moment(disk_moment_of_inertia(mass: crank_mass * units.kg, radius: (stroke / 2.0) * units.mm))
    label flywheel_moment(disk_moment_of_inertia(mass: flywheel_mass * units.kg, radius: (stroke * 2.7) * units.mm))
    label other_moment(disk_moment_of_inertia(mass: 1 * units.kg, radius: 1.0 * units.cm))

    crankshaft c0(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: flywheel_mass * units.kg,
        mass: crank_mass * units.kg,
        friction_torque: 9.9 * units.Nm,
        moment_of_inertia: crank_moment + flywheel_moment + other_moment,
        position_x: 0,
        position_y: -offset * units.mm,
        tdc: 0
    )

    label tdc_comp(-1.55 * units.deg) // 90 - math.acos(x_shift / (stroke / 2 + con_rod))

    rod_journal rj0(angle: rot * 0 + rot180 + tdc_comp)
    rod_journal rj1(angle: rot * 1 + rot180 + tdc_comp)
    rod_journal rj2(angle: rot * 3 + rot180 + tdc_comp)
    rod_journal rj3(angle: rot * 2 + rot180 + tdc_comp)
    
    c0
        .add_rod_journal(rj0)
        .add_rod_journal(rj1)
        .add_rod_journal(rj2)
        .add_rod_journal(rj3)

    piston_parameters piston_params(
        mass: piston_mass * units.g,
        blowby: k_28inH2O(0.038),
        compression_height: compression_height,
        wrist_pin_position: 0.0,
        displacement: 0.0
    )

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    label collector_cross_section_area(24.0)

    label exhaust_pipe_length_0(100.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 100.0) // Litres
    
    label spacing_factor(1.3)
    label flange_density(1.0 * exhaust_delay_coeff)
    

    intake intake(
        plenum_volume: 1.9 * units.L,
        plenum_cross_section_area: 34.8 * units.cm2,
        runner_length: 20.0 * units.cm,
        intake_flow_rate: k_carb(228.0),
        idle_flow_rate: k_carb(0.02),
        idle_throttle_plate_position: 0.996,
        runner_flow_rate: k_carb(114.0),
        velocity_decay: 0.05
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(361.0),
        collector_cross_section_area: 24.0 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 57.8 * units.cm,
        primary_flow_rate: k_carb(180.5),
        velocity_decay: 1.0,
        volume: (exhaust_volume_0 + 10.0) * units.L
    )

    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_55.wav", volume: 0.00025))
 
    cylinder_bank b0(bank_params, angle: rot90)

    label pl0((4.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl1((3.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl2((2.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl3((1.0 * bore * spacing_factor / flange_density) * units.mm)

    b0
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj0,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl0,
            ignition_wire: wires.wire1
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj1,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl1,
            ignition_wire: wires.wire2
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj2,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl2,
            ignition_wire: wires.wire3
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj3,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl3,
            ignition_wire: wires.wire4
        )

    engine
        .add_cylinder_bank(b0)

    engine.add_crankshaft(c0)

    camshaft_builder camshaft()

    b0.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_0,
            exhaust_camshaft: camshaft.exhaust_cam_0,
            flip_display: true
        )
    )

    function timing_curve(153.3 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 3.0 * units.deg)
        .add_sample(153.3 * units.rpm, 3.0 * units.deg)
        .add_sample(306.7 * units.rpm, 5.3 * units.deg)
        .add_sample(460.0 * units.rpm, 8.1 * units.deg)
        .add_sample(613.3 * units.rpm, 10.6 * units.deg)
        .add_sample(766.7 * units.rpm, 12.7 * units.deg)
        .add_sample(920.0 * units.rpm, 14.6 * units.deg)
        .add_sample(1073.3 * units.rpm, 16.1 * units.deg)
        .add_sample(1226.7 * units.rpm, 17.5 * units.deg)
        .add_sample(1380.0 * units.rpm, 18.7 * units.deg)
        .add_sample(1533.3 * units.rpm, 19.7 * units.deg)
        .add_sample(1686.7 * units.rpm, 20.6 * units.deg)
        .add_sample(1840.0 * units.rpm, 21.3 * units.deg)
        .add_sample(1993.3 * units.rpm, 22.0 * units.deg)
        .add_sample(2146.7 * units.rpm, 22.5 * units.deg)
        .add_sample(2300.0 * units.rpm, 23.0 * units.deg)
        .add_sample(2453.3 * units.rpm, -12.5 * units.deg)
        .add_sample(2606.7 * units.rpm, -25.0 * units.deg)

    engine.add_ignition_module(
        eng_distributor(
            wires: wires,
            timing_curve: timing_curve
        )
    )
}

// T-26
public node veh {
    alias output __out: vehicle;

    label car_mass(9500) // 9300 empty

    vehicle vehicle(
        mass: car_mass * units.kg,
        drag_coefficient: 0.6,
        cross_sectional_area: (2440 * units.mm) * (1900 * units.mm),
        diff_ratio: 5.46 * 1.68,
        tire_radius: (669.4 / 2) * units.mm,
        rolling_resistance: 0.05 * car_mass * 9.81
        )
}


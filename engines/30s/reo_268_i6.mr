public node camshaft_builder {
    output intake_cam_0: _intake_cam_0;
    output exhaust_cam_0: _exhaust_cam_0;

    camshaft _intake_cam_0(base_radius: 14.5 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 14.5 * units.mm, lobe_profile: exhaust_lobe_profile)

    _intake_cam_0
        .add_lobe(ILC + 0 * rot)
        .add_lobe(ILC + 4 * rot)
        .add_lobe(ILC + 2 * rot)
        .add_lobe(ILC + 5 * rot)
        .add_lobe(ILC + 1 * rot)
        .add_lobe(ILC + 3 * rot)
    _exhaust_cam_0
        .add_lobe(ELC + 0 * rot)
        .add_lobe(ELC + 4 * rot)
        .add_lobe(ELC + 2 * rot)
        .add_lobe(ELC + 5 * rot)
        .add_lobe(ELC + 1 * rot)
        .add_lobe(ELC + 3 * rot)
}

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node head {
    // port_flow.py v1.7
    // intake port area: 13.0 cm²; saturated lift: 9.89 mm
    // exhaust port area: 13.0 cm²; saturated lift: 9.89 mm
    // cylinder volume: 733.0 cm³ (44.73 CI); engine volume: 4.398 L (268.39 CI)
    // chamber volume: 170.5 cm³ (10.40 CI); conrod/stroke 2.100; N.A.C.C. 27.34
    // 16 harmonic intake runner length: 28.8 cm; diameter: 3.7 cm
    // primary length: 40.3 cm, area: 14.3 cm², diameter: 4.3 cm
    // collector diameter: 5.7 cm, area: 25.7 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 170.467 * units.cc,
        intake_runner_volume: 301.3 * units.cc,
        intake_runner_cross_section_area: 10.5 * units.cm2,
        exhaust_runner_volume: 120.5 * units.cc,
        exhaust_runner_cross_section_area: 14.3 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}


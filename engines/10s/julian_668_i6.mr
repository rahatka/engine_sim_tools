import "engine_sim.mr"
import "../../fuel/10.mr"

units units()
constants constants()

label bore(114.3) // 4 1/2
label stroke(177.8) // 7
label compression_ratio(4.0) // ?
label con_rod(355.6) // 14
label compression_height(165.1 / 2 * units.mm) // 6 1/2 l
label intake_valve_diameter(77.7875) // 3 1/16 ; 1/2 stem
label exhaust_valve_diameter(77.7875) // 3 1/16 ; 1/2 stem
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(2200.0) // RPM

label cyl(6.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// Julian only lifts
label IVL(9.525) // 3/8
label EVL(9.525) // 3/8
label IVO(-15.0 * units.deg) //BTDC
label IVC(38.0 * units.deg) //ABDC
label EVO(45.0 * units.deg) //BBDC
label EVC(10.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(1360.78) // 3
label piston_mass(2154.56) // 4 3/4
label crank_mass(25.0) // ?
label flywheel_mass(5.0) // kg

label exhaust_delay_coeff(1.5)


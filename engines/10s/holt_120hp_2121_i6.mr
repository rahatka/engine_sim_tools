units units()
constants constants()

label bore(190.5) // 7 1/2
label stroke(203.2) // 8
label compression_ratio(4.0) // ?
label con_rod(431.8) // 17
label compression_height(215.9 / 2 * units.mm) // 8 1/2 long
label intake_valve_diameter(76.2) // 3
label exhaust_valve_diameter(76.2) // 3
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(800.0) // RPM

label cyl(6.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// ?
label IVL(11.1125) // 7/16
label EVL(12.7) // 1/2
label IVO(0.0 * units.deg) //BTDC
label IVC(55.0 * units.deg) //ABDC
label EVO(55.0 * units.deg) //BBDC
label EVC(0.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(4800.0) // ?
label piston_mass(3000.0) // ?
label crank_mass(90.0) // ?
label flywheel_mass(30.0) // kg

label exhaust_delay_coeff(1.5)


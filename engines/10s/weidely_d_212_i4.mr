units units()
constants constants()

label bore(88.9) // 3 1/2
label stroke(139.7) // 5 1/2
label compression_ratio(4.0) // ~
label con_rod(279.4) // 11
label compression_height(120.65 / 2 * units.mm) // 4 3/4 L
label intake_valve_diameter(47.625) // 1 7/8 ; 1 13/16 clear
label exhaust_valve_diameter(47.625) // 1 7/8 ; 1 13/16 clear
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(2600.0) // RPM

label cyl(4.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// Weidely D
label IVL(7.9375) // 5/16
label EVL(7.9375) // 5/16
label IVO(-5.0 * units.deg) //BTDC
label IVC(50.0 * units.deg) //ABDC
label EVO(40.0 * units.deg) //BBDC
label EVC(0.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(2069.51519) // 4 lb 9 oz
label piston_mass(1077.2819) // 2 lb 6 oz
label crank_mass(15.0) // ?
label flywheel_mass(5.0) // kg

label exhaust_delay_coeff(1.5)


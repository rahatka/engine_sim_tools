import "engine_sim.mr"
import "../../fuel/10.mr"

units units()
constants constants()

label bore(95.25) // 3 3/4
label stroke(139.7) // 5 1/2
label compression_ratio(4.25) // ~
label con_rod(311.15) // 12 1/4
label compression_height(127.0 / 2 * units.mm) // 5 l
label intake_valve_diameter(42.8625) // 1 11/16 c
label exhaust_valve_diameter(42.8625) // 1 11/16 c
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(2300.0) // RPM

label cyl(6.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// Buda
label IVL(9.525) // 3/8
label EVL(9.525) // 3/8
label IVO(-5.0 * units.deg) //BTDC
label IVC(45.0 * units.deg) //ABDC
label EVO(55.0 * units.deg) //BBDC
label EVC(5.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(1637.18) // 66 3/4
label piston_mass(1304.08) // 55 1/4
label crank_mass(30.0) // ?
label flywheel_mass(8.0) // kg

label exhaust_delay_coeff(1.5)

public node variant {
    output s1: 0;
    output s5: 1;
    output s3: 2;
    output s6: 3;
    output s2: 4;
    output s4: 5;
}


private node add_lobes {
    input base;
    input shift: 0.0;
    input this;
    alias output __out: this;

    this.add_lobe(base + 0 * rot * row + shift)
    this.add_lobe(base + 3 * rot * row + shift)
    this.add_lobe(base + 1 * rot * row + shift)
    this.add_lobe(base + 2 * rot * row + shift)
}

public node bank_builder {
    input angle;
    input phasing;
    input crank;
    input ignition;
    input intake;
    input exhaust;

    input flip: false;

    output bank: _b0;

    label spacing_factor(1.2)
    label flange_density(1.0 * exhaust_delay_coeff)

    label pl0 ((4.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl1 ((3.2 * bore * spacing_factor / flange_density) * units.mm)
    label pl2 ((1.8 * bore * spacing_factor / flange_density) * units.mm)
    label pl3 ((1.0 * bore * spacing_factor / flange_density) * units.mm)

    ignition_wire _wire1()
    ignition_wire _wire2()
    ignition_wire _wire3()
    ignition_wire _wire4()

    piston_parameters piston_params(
        mass: piston_mass * units.g,
        blowby: k_28inH2O(0.024),
        compression_height: compression_height,
        wrist_pin_position: 0.0,
        displacement: 0.0
    )

    connecting_rod_parameters cr_params(
        mass: con_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.g,
            length: con_rod * units.mm
        ),
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    camshaft _intake_cam_0(base_radius: 11.1 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 11.1 * units.mm, lobe_profile: exhaust_lobe_profile)

    _intake_cam_0.add_lobes(ILC, phasing)
    _exhaust_cam_0.add_lobes(ELC, phasing)

    ignition.connect_wire(_wire1, rot_base * 0 + phasing)
    ignition.connect_wire(_wire3, rot_base * 1 + phasing)
    ignition.connect_wire(_wire4, rot_base * 2 + phasing)
    ignition.connect_wire(_wire2, rot_base * 3 + phasing)

    cylinder_bank _b0(bank_params, angle: angle)

    _b0
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crank.rj0,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl0,
            ignition_wire: _wire1
        )
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crank.rj1,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl1,
            ignition_wire: _wire2
        )
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crank.rj2,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl2,
            ignition_wire: _wire3
        )
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crank.rj3,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl3,
            ignition_wire: _wire4
        )
    .set_cylinder_head(
        head(
            intake_camshaft: _intake_cam_0,
            exhaust_camshaft: _exhaust_cam_0,
            flip_display: flip
        )
    )
}

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node head {
    // port_flow.py v1.7
    // intake port area: 6.6 cm²; saturated lift: 7.05 mm
    // exhaust port area: 6.6 cm²; saturated lift: 7.05 mm
    // cylinder volume: 463.3 cm³ (28.27 CI); engine volume: 3.707 L (226.19 CI)
    // chamber volume: 143.4 cm³ (8.75 CI); conrod/stroke 2.000; N.A.C.C. 28.80
    // 32 harmonic intake runner length: 17.4 cm; diameter: 2.8 cm
    // primary length: 46.3 cm, area: 7.3 cm², diameter: 3.0 cm
    // collector diameter: 4.7 cm, area: 17.4 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 143.447 * units.cc,
        intake_runner_volume: 105.1 * units.cc,
        intake_runner_cross_section_area: 6.0 * units.cm2,
        exhaust_runner_volume: 37.5 * units.cc,
        exhaust_runner_cross_section_area: 7.3 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

public node crank {
    input phasing;

    output c: _c;
    output rj0: _rj0;
    output rj1: _rj1;
    output rj2: _rj2;
    output rj3: _rj3;

    label crank_moment(disk_moment_of_inertia(mass: crank_mass * units.kg, radius: (stroke / 2.0) * units.mm))
    label flywheel_moment(disk_moment_of_inertia(mass: flywheel_mass * units.kg, radius: (stroke * 3.5) * units.mm))
    label other_moment(disk_moment_of_inertia(mass: 1 * units.kg, radius: 1.0 * units.cm))

    crankshaft _c(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: flywheel_mass * units.kg,
        mass: crank_mass * units.kg,
        friction_torque: 5.6 * units.Nm,
        moment_of_inertia: crank_moment + flywheel_moment + other_moment,
        tdc: 0.0
    )

    rod_journal _rj0(angle: rot_base * 0 + phasing)
    rod_journal _rj1(angle: rot_base * 3 + phasing)
    rod_journal _rj2(angle: rot_base * 1 + phasing)
    rod_journal _rj3(angle: rot_base * 2 + phasing)

    _c
        .add_rod_journal(_rj0)
        .add_rod_journal(_rj1)
        .add_rod_journal(_rj2)
        .add_rod_journal(_rj3)
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "Davis Eight",
        starter_torque: 200 * units.Nm,
        starter_speed: 200 * units.rpm,
        redline: redline * units.rpm,
        fuel: s10(),
        throttle_gamma: 1.5,
        jitter: 0.7,
        noise: 0.3,
        hf_gain: 0.007,
        simulation_frequency: 12000,
        dyno_min_speed: 500 * units.rpm
    )

    ignition_module ignition(timing_curve: timing_curve, rev_limit: redline * 1.05 * units.rpm, limiter_duration: 0.07)

    crank c0(vee / 2 + rot90)

    bank_builder bank0(
        angle: vee / 2,
        phasing: 0,
        crank: c0,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust0,
        flip: true
    )

    bank_builder bank1(
        angle: -vee / 2,
        phasing: vee,
        crank: c0,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust1
    )

    label collector_cross_section_area(8.7)

    label exhaust_pipe_length_0(305.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 100.0) // Litres
    
    label exhaust_pipe_length_1(300.0) // cm
    label exhaust_volume_1(collector_cross_section_area * exhaust_pipe_length_1 / 100.0) // Litres
    
    intake intake(
        plenum_volume: 1.6 * units.L,
        plenum_cross_section_area: 24.1 * units.cm2,
        runner_length: 17.4 * units.cm,
        intake_flow_rate: k_carb(185.3),
        idle_flow_rate: k_carb(0.01),
        idle_throttle_plate_position: 0.992,
        runner_flow_rate: k_carb(46.3),
        velocity_decay: 0.05
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(154.4),
        collector_cross_section_area: 8.7 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 46.3 * units.cm,
        primary_flow_rate: k_carb(77.2),
        velocity_decay: 1.0,
        volume: exhaust_volume_0 * units.L
    )
    
    exhaust_system_parameters es_params1(
        outlet_flow_rate: k_carb(154.4),
        collector_cross_section_area: 8.7 * units.cm2,
        length: exhaust_pipe_length_1 * units.cm,
        primary_tube_length: 46.3 * units.cm,
        primary_flow_rate: k_carb(77.2),
        velocity_decay: 1.0,
        volume: exhaust_volume_1 * units.L
    )

    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_51.wav", volume: 0.00033))
    exhaust_system exhaust1(es_params1, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_52.wav", volume: 0.00033))
 
    function timing_curve(200.0 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 5.0 * units.deg)
        .add_sample(200.0 * units.rpm, 5.0 * units.deg)
        .add_sample(400.0 * units.rpm, 5.6 * units.deg)
        .add_sample(600.0 * units.rpm, 6.2 * units.deg)
        .add_sample(800.0 * units.rpm, 6.9 * units.deg)
        .add_sample(1000.0 * units.rpm, 7.6 * units.deg)
        .add_sample(1200.0 * units.rpm, 8.5 * units.deg)
        .add_sample(1400.0 * units.rpm, 9.4 * units.deg)
        .add_sample(1600.0 * units.rpm, 10.5 * units.deg)
        .add_sample(1800.0 * units.rpm, 11.7 * units.deg)
        .add_sample(2000.0 * units.rpm, 13.0 * units.deg)
        .add_sample(2200.0 * units.rpm, 14.4 * units.deg)
        .add_sample(2400.0 * units.rpm, 16.0 * units.deg)
        .add_sample(2600.0 * units.rpm, 17.8 * units.deg)
        .add_sample(2800.0 * units.rpm, 19.8 * units.deg)
        .add_sample(3000.0 * units.rpm, 22.0 * units.deg)
        .add_sample(3200.0 * units.rpm, -12.5 * units.deg)
        .add_sample(3400.0 * units.rpm, -25.0 * units.deg)

    engine
        .add_crankshaft(c0.c)
        .add_cylinder_bank(bank0.bank)
        .add_cylinder_bank(bank1.bank)
        .add_ignition_module(ignition)
}

// Cadillac V12 1932
public node veh {
    alias output __out: vehicle;
    vehicle vehicle(
        mass: 2812.0  * units.kg,
        drag_coefficient: 0.48,
        cross_sectional_area: (1890 * units.mm) * (1700 * units.mm),
        diff_ratio: 4.60, // 4.60 high, 4.80 low
        tire_radius: (837.0 / 2) * units.mm, // 7.00x19
        rolling_resistance: 20
        )
}

// Cadillac V12 1932
private node trn {
    alias output __out:
        transmission(
            max_clutch_torque: 500 * units.Nm,
            max_clutch_flex: 10 * units.deg,
            limit_clutch_flex: true,
            clutch_stiffness: 20 * units.Nm / units.deg,
            clutch_damping: 2.0,
            simulate_flex: true
        )
        .add_gear(2.40)
        .add_gear(1.47)
        .add_gear(1.0);
}

run(
    engine: eng(),
    transmission: trn(),
    vehicle: veh()
)

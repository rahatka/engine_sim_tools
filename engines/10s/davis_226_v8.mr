import "engine_sim.mr"
import "../../fuel/10.mr"

units units()
constants constants()

label bore(76.2) // 3
label stroke(101.6) // 4
label compression_ratio(4.23) // 31% piston displ ; 1.31 / 0.31
label con_rod(203.2) // 8
label compression_height(95.25 / 2 * units.mm) // 3 3/4 len
label intake_valve_diameter(33.3375) // 1 5/16
label exhaust_valve_diameter(33.3375) // 1 5/16
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(3000.0) // RPM

label cyl(8.0)
label row(2.0)
label cycle(720.0 * units.deg)
label vee(90.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)

// 1915 Davis Eight
label IVL(5.55625) // 7/32
label EVL(5.55625) // 7/32
label IVO(-10.0 * units.deg) //BTDC
label IVC(30.0 * units.deg) //ABDC
label EVO(45.0 * units.deg) //BBDC
label EVC(5.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(750.0) // ? g
label piston_mass(600.0) // ? g
label crank_mass(15.0) // ? kg
label flywheel_mass(7.0) // kg

label exhaust_delay_coeff(1.5)


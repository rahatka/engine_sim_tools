units units()
constants constants()

label bore(112.7125) // 4 7/16
label stroke(139.7) // 5 1/2
label compression_ratio(3.846) // 26% of total
label con_rod(295.275) // 11 5/8
label compression_height(165.1 / 2 * units.mm) // ?
label intake_valve_diameter(57.15) //  2 1/4 ?
label exhaust_valve_diameter(57.15) // 2 1/4 ?
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(2200.0) // RPM

label cyl(6.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// ?
label IVL(11.1125) // 7/16
label EVL(11.1125) // 7/16
label IVO(-15.0 * units.deg) //BTDC
label IVC(38.0 * units.deg) //ABDC
label EVO(45.0 * units.deg) //BBDC
label EVC(10.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(1913.593) // 67.5
label piston_mass(2154.56) // ?
label crank_mass(25.0) // ?
label flywheel_mass(5.0) // kg

label exhaust_delay_coeff(1.5)


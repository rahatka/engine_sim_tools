import "engine_sim.mr"
import "../../fuel/10.mr"

units units()
constants constants()

label bore(73.025) // 2 7/8
label stroke(127.0) // 5
label compression_ratio(4.615) // 8.98 CI chamber
label con_rod(254.0) // 10
label compression_height(88.9 / 2 * units.mm) // 3 1/2 len
label intake_valve_diameter(33.3375) // 1 1/2 ; 1 5/16
label exhaust_valve_diameter(33.3375) // 1 1/2 ; 1 5/16
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(2700.0) // RPM

label cyl(12.0)
label row(2.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label vee(60.0 * units.deg)

label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)

// ?
label IVL(6.35) // 1/4
label EVL(6.35) // 1/4
label IVO(-6.0 * units.deg) //BTDC
label IVC(40.0 * units.deg) //ABDC
label EVO(40.5 * units.deg) //BBDC
label EVC(6.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(1176.51) // 41 1/2 oz
label piston_mass(623.69) // 22 oz
label crank_mass(30.0) // ? kg
label flywheel_mass(6.0) // kg

label exhaust_delay_coeff(1.5)


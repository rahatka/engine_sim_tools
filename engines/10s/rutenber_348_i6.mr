units units()
constants constants()

label bore(95.25) // 3 3/4
label stroke(133.35) // 5 1/4
label compression_ratio(4.0) // ?
label con_rod(304.8) // 12
label compression_height(bore / 2 * units.mm) // ?
label intake_valve_diameter(46.8312) // 1 27/32
label exhaust_valve_diameter(46.8312) // 1 27/32
label intake_valves(1.0)
label exhaust_valves(1.0)
label offset(12.7) // 1/2

label redline(2300.0) // RPM

label cyl(6.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// 1913 Rutenber 28 only timing
label IVL(8.73125) // 11/32
label EVL(8.73125) // 11/32
label IVO(-18.0 * units.deg) //BTDC
label IVC(46.0 * units.deg) //ABDC
label EVO(47.0 * units.deg) //BBDC
label EVC(15.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(893.01) // 31.5
label piston_mass(779.61189) // 27.5
label crank_mass(17.0) // ?
label flywheel_mass(4.0) // kg

label exhaust_delay_coeff(1.5)


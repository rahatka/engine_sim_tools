import "engine_sim.mr"
import "../../fuel/10.mr"

units units()
constants constants()

label bore(76.2) // 3
label stroke(107.95) // 4 1/4
label compression_ratio(4.803) // 7.9 CI Falls N
label con_rod(215.9) // 8 1/2
label compression_height(82.55 / 2 * units.mm) // 3 1/4 L
label intake_valve_diameter(34.925) // 1 3/8 or 1 1/4 ; 5/16 stem
label exhaust_valve_diameter(34.925) // 1 3/8 or  1 1/4 ; 5/16 stem
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(2200.0) // RPM

label cyl(6.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// 1917 Elgin F
label IVL(7.14375) // 9/32
label EVL(7.14375) // 9/32
label IVO(-13.0 * units.deg) //BTDC
label IVC(42.0 * units.deg) //ABDC
label EVO(45.0 * units.deg) //BBDC
label EVC(10.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(793.787) // 28
label piston_mass(793.787) // 28
label crank_mass(20.3) // ?
label flywheel_mass(5.0) // kg

label exhaust_delay_coeff(1.5)

public node variant_0 {
    output s1: 0;
    output s4: 1;
    output s2: 2;
    output s6: 3;
    output s3: 4;
    output s5: 5;
}


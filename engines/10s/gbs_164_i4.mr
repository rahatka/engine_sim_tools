label compression_height(96.8375 / 2 * units.mm) // ?
label intake_valve_diameter(46.0375) // 1 13/16 ; 1 5/8 clear ; 3/8 stem
label exhaust_valve_diameter(46.0375) // 1 13/16 ; 1 5/8 clear ; 3/8 stem
label intake_valves(1.0)
label exhaust_valves(1.0)
label offset(15.875) // 5/8

label cyl(4.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

label redline(2200.0) // RPM

// GBS 1914
label IVL(5.55625) // 7/32
label EVL(5.55625) // 7/32
label IVO(-10.0 * units.deg) //BTDC
label IVC(40.0 * units.deg) //ABDC
label EVO(50.0 * units.deg) //BBDC
label EVC(6.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(766.5711) // 1.69 lb
label piston_mass(1274.595) // ?

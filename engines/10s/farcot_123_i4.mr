import "../../fuel/10.mr"

units units()
constants constants()

label bore(80.0) // 3.150
label stroke(100.0) // 3.937
label compression_ratio(4.0) // ?
label con_rod(200.0)
label compression_height(bore / 2.0 * units.mm) // ?
label intake_valve_diameter(35.0) // ?
label exhaust_valve_diameter(35.0) // ?
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(1800.0) // RPM

label cyl(4.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// Farcot only timing
label IVL(7.0)
label EVL(7.0)
label IVO(-6.0 * units.deg) //BTDC
label IVC(10.0 * units.deg) //ABDC
label EVO(36.0 * units.deg) //BBDC
label EVC(2.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(1200.0) // ?
label piston_mass(600.0) // ?
label crank_mass(15.0) // ?
label flywheel_mass(8.0) // kg

label exhaust_delay_coeff(1.5)


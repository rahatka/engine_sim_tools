import "engine_sim.mr"
import "../../fuel/10.mr"

units units()
constants constants()

label bore(63.5) // 2 1/2
label stroke(127.0) // 5
label compression_ratio(4.5) // ?
label con_rod(279.4) // 11
label compression_height(76.2 / 2 * units.mm) // 3 piston length
label intake_valve_diameter(38.1) // 1 1/2
label exhaust_valve_diameter(38.1) // 1 1/2
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(3000.0) // RPM

label cyl(12.0)
label row(2.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label vee(60.0 * units.deg)

label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)

// Supreme only lifts, Davis 16H timing
label IVL(7.14375) // 9/32
label EVL(7.14375) // 9/32
label IVO(00.0 * units.deg) //BTDC
label IVC(33.0 * units.deg) //ABDC
label EVO(67.0 * units.deg) //BBDC
label EVC(00.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(793.7866) // 1.75
label piston_mass(530.7031) // 1.17
label crank_mass(30.0) // ? kg
label flywheel_mass(6.0) // kg

label exhaust_delay_coeff(1.5)


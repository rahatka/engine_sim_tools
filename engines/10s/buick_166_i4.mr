import "engine_sim.mr"
import "../../fuel/10.mr"

units units()
constants constants()

label bore(95.25) // 3 3/4
label stroke(95.25) // 3 3/4
label compression_ratio(4.0) // ?
label con_rod(177.8) // 7
label compression_height(bore / 2 * units.mm) // ?
label intake_valve_diameter(31.75) // 1 1/4
label exhaust_valve_diameter(31.75) // 1 1/4
label intake_valves(1.0)
label exhaust_valves(1.0)

label cyl(4.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

label redline(2500.0) // RPM

// Buick C24
label IVL(6.35) // 1/4
label EVL(6.35) // 1/4
label IVO(-16.18 * units.deg) //BTDC
label IVC(35.68 * units.deg) //ABDC
label EVO(56.31 * units.deg) //BBDC
label EVC(13.18 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(800.0) // ?
label piston_mass(600.0) // ?
label crank_mass(12.0) // ? kg
label flywheel_mass(4.0) // kg

label exhaust_delay_coeff(1.5)


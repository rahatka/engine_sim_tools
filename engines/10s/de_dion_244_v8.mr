import "../../fuel/10.mr"

units units()
constants constants()

label bore(70.0)
label stroke(130.0)
label compression_ratio(4.0) // ?
label con_rod(265.0)
label compression_height(42.0 * units.mm) // 84 L
label intake_valve_diameter(33.5) // 28.5 clear
label exhaust_valve_diameter(33.5) // 28.5 clear
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(1700.0) // RPM

label cyl(8.0)
label row(2.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label vee(90.0 * units.deg)

label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)

// only lifts are known
label IVL(6.0)
label EVL(6.0)
label IVO(-10.0 * units.deg) //BTDC
label IVC(35.0 * units.deg) //ABDC
label EVO(40.0 * units.deg) //BBDC
label EVC(5.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(700.0) // ?
label piston_mass(500.0) // ?
label crank_mass(25.0) // ? kg
label flywheel_mass(6.0) // kg

label exhaust_delay_coeff(1.5)


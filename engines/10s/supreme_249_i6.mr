import "engine_sim.mr"
import "../../fuel/10.mr"

units units()
constants constants()

label bore(82.55) // 3 1/4
label stroke(127.0) // 5
label compression_ratio(4.25) // ?
label con_rod(254.0) // 10
label compression_height(101.6 / 2 * units.mm) // 4 L
label intake_valve_diameter(44.45) // 1 3/4 ; 1 5/8 clear ; 3/8 stem
label exhaust_valve_diameter(44.45) // 1 3/4 ; 1 5/8 clear ; 3/8 stem
label intake_valves(1.0)
label exhaust_valves(1.0)
label offset(15.875) // 5/8

label cyl(6.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

label redline(2750.0) // RPM

// Supreme only lifts, Davis 16H timing
label IVL(7.14375) // 9/32
label EVL(7.14375) // 9/32
label IVO(00.0 * units.deg) //BTDC
label IVC(33.0 * units.deg) //ABDC
label EVO(67.0 * units.deg) //BBDC
label EVC(00.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(848.2177) // 1.87 lb
label piston_mass(907.185) // 2.0 lb
label crank_mass(13.0) // ? kg
label flywheel_mass(5.0) // kg

label exhaust_delay_coeff(1.5)

// Davis practice
public node variant_0 {
    output s1: 0;
    output s5: 1;
    output s3: 2;
    output s6: 3;
    output s2: 4;
    output s4: 5;
}


import "../../fuel/10.mr"

units units()
constants constants()

label bore(120.65) // 4 3/4
label stroke(146.05) // 5 3/4
label compression_ratio(4.0)
label con_rod(319.087) // 12 9/16
label compression_height(bore / 2 * units.mm) // ?
label intake_valve_diameter(47.625) // 1 7/8
label exhaust_valve_diameter(47.625) // 1 7/8
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(2000.0) // RPM

label cyl(4.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// Cunn
label IVL(7.9375) // 5/16
label EVL(9.525) // 3/8
label IVO(-15.0 * units.deg) //BTDC
label IVC(15.0 * units.deg) //ABDC
label EVO(40.0 * units.deg) //BBDC
label EVC(5.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(900.0) // ?
label piston_mass(1400.0) // ?
label crank_mass(18.0) // ?
label flywheel_mass(7.0) // kg

label exhaust_delay_coeff(1.5)


import "engine_sim.mr"
import "../../fuel/10.mr"

units units()
constants constants()

label bore(98.425) // 3 7/8
label stroke(127.0) // 5
label compression_ratio(4.4)
label con_rod(273.05) // 10 3/4
label compression_height(110.998 / 2 * units.mm) // 4.37 len
label intake_valve_diameter(46.0375) // 1 13/16
label exhaust_valve_diameter(42.8625) // 1 11/16
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(2800.0) // RPM

label cyl(6.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// 1927 Erskine Six
label IVL(7.9375) // 5/16
label EVL(7.9375) // 5/16
label IVO(0.0 * units.deg) //BTDC
label IVC(35.0 * units.deg) //ABDC
label EVO(42.0 * units.deg) //BBDC
label EVC(5.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(1247.38) // 44
label piston_mass(1374.952) // 48.5
label crank_mass(30.0) // ?
label flywheel_mass(5.0) // kg

label exhaust_delay_coeff(1.5)

public node variant {
    output s1: 0;
    output s5: 1;
    output s3: 2;
    output s6: 3;
    output s2: 4;
    output s4: 5;
}


import "engine_sim.mr"
import "../../fuel/10.mr"

units units()
constants constants()

label bore(82.55) // 3 1/4
label stroke(127.0) // 5
label compression_ratio(4.5) // ?
label con_rod(292.1) // 11 1/2 8-22
label compression_height(bore / 2 * units.mm) // ?
label intake_valve_diameter(39.6875) // 1 9/16 ; .37375 stem
label exhaust_valve_diameter(39.6875) // 1 9/16 ; .37375 stem
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(2300.0) // RPM

label cyl(8.0)
label row(2.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label vee(90.0 * units.deg)

label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)

// Apperson 8
label IVL(9.525) // 11/32
label EVL(9.525) // 11/32
label IVO(-15.0 * units.deg) //BTDC
label IVC(45.0 * units.deg) //ABDC
label EVO(55.0 * units.deg) //BBDC
label EVC(10.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(1247.38) // 44 oz
label piston_mass(943.4721) // ?
label crank_mass(20.0) // ? kg
label flywheel_mass(6.0) // kg

label exhaust_delay_coeff(1.5)


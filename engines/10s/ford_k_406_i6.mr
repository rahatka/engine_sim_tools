units units()
constants constants()

label bore(114.3) // 4 1/2
label stroke(107.95) // 4 1/4
label compression_ratio(3.5) // ?
label con_rod(215.9) // ~ 8 1/2
label compression_height(bore / 2 * units.mm) // ~
label intake_valve_diameter(44.45) // ~ 1 3/4
label exhaust_valve_diameter(44.45) // ~ 1 3/4
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(2000.0) // RPM

label cyl(6.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// ?
label IVL(11.1125) // 7/16 ?
label EVL(11.1125) // 7/16 ?
label IVO(-20.0 * units.deg) //BTDC
label IVC(50.0 * units.deg) //ABDC
label EVO(50.0 * units.deg) //BBDC
label EVC(0.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(1000.0) // ?
label piston_mass(900.0) // ?
label crank_mass(20.0) // ?
label flywheel_mass(29.4835) // 65 lbs

label exhaust_delay_coeff(1.5)


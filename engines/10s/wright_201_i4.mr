import "engine_sim.mr"
import "../../fuel/10.mr"

units units()
constants constants()

label bore(101.6) // 4
label stroke(101.6) // 4
label compression_ratio(3.5) // 4.4 or 3.5
label con_rod(254.0) // 10
label compression_height(127.0 / 2 * units.mm) // PH 5IN
label intake_valve_diameter(50.8) // 2
label exhaust_valve_diameter(50.8) // 2
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(1200.0) // RPM

label cyl(4.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// ? 180 IOD
label IVL(9.525) // 3/8 ?
label EVL(9.525) // 3/8 ?
label IVO(-25.0 * units.deg) //BTDC
label IVC(25.0 * units.deg) //ABDC
label EVO(35.0 * units.deg) //BBDC
label EVC(-15.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(900.0) // ?
label piston_mass(900.0) // ?
label crank_mass(17.0) // ?
label flywheel_mass(7.0) // kg

label exhaust_delay_coeff(1.5)


units units()
constants constants()

label bore(69.85) // 2 3/4
label stroke(120.65) // 4 3/4
label compression_ratio(4.0)
label con_rod(269.875) // 10 5/8
label compression_height(84.9312 / 2 * units.mm) // 3 11/32 piston length
label intake_valve_diameter(34.925) // 1 5/16 Automotive Industries  1916-07-27: Vol 35 Iss 4 ; 1 3/8 NATIONAL Highway “12” Booklet
label exhaust_valve_diameter(34.925) // 1 5/16 Automotive Industries  1916-07-27: Vol 35 Iss 4 ; 1 3/8 NATIONAL Highway “12” Booklet
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(3000.0) // RPM

label cyl(12.0)
label row(2.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label vee(60.0 * units.deg)

label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)

// National 1917 ; AH -5 49 55 5
label IVL(9.525) // 3/8
label EVL(9.525) // 3/8
label IVO(-6.0 * units.deg) //BTDC
label IVC(42.0 * units.deg) //ABDC
label EVO(43.5 * units.deg) //BBDC
label EVC(2.4 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(800.0) // ? g
label piston_mass(600.0) // 8 oz
label crank_mass(30.0) // ? kg
label flywheel_mass(6.0) // kg

label exhaust_delay_coeff(1.5)


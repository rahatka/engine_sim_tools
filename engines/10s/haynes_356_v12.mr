units units()
constants constants()

label bore(69.85) // 2 3/4
label stroke(127.0) // 5
label compression_ratio(4.0) // ?
label con_rod(260.858) // 10.27 haynes 48
label compression_height(bore / 2 * units.mm) // ?
label intake_valve_diameter(34.925) // 1.375 ; .3125 stem
label exhaust_valve_diameter(34.925) // 1.375 ; .3125 stem
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(3000.0) // RPM

label cyl(12.0)
label row(2.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label vee(60.0 * units.deg)

label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)

// haynes 43-44
label IVL(7.9375) // ? 5/16
label EVL(7.9375) // ? 5/16
label IVO(-5.0 * units.deg) //BTDC
label IVC(35.0 * units.deg) //ABDC
label EVO(47.0 * units.deg) //BBDC
label EVC(2.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(907.185) // 32 oz
label piston_mass(600.0) // ?
label crank_mass(30.0) // ? kg
label flywheel_mass(6.0) // kg

label exhaust_delay_coeff(1.5)


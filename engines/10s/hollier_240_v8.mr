import "engine_sim.mr"
import "../../fuel/10.mr"

units units()
constants constants()

label bore(76.2) // 3
label stroke(107.95) // 4 1/4
label compression_ratio(4.0) // ?
label con_rod(254.0) // 10
label L1(214.312) // ~ 8 7/16
label R1(39.6875) // ~ 1 9/16
label psi(90.0 * units.deg) // ~
label compression_height(bore / 2 * units.mm) // ?
label intake_valve_diameter(34.925) // 1 3/8
label exhaust_valve_diameter(34.925) // 1 3/8
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(2200.0) // RPM

label cyl(8.0)
label row(2.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label vee(90.0 * units.deg)

label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)

// only lifts are known
label IVL(9.525) // 3/8
label EVL(9.525) // 3/8
label IVO(-10.0 * units.deg) //BTDC
label IVC(35.0 * units.deg) //ABDC
label EVO(45.0 * units.deg) //BBDC
label EVC(5.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(700.0) // ?
label piston_mass(500.0) // ?
label crank_mass(25.0) // ? kg
label flywheel_mass(6.0) // kg

label exhaust_delay_coeff(1.5)


import "engine_sim.mr"
import "../../fuel/10.mr"

units units()
constants constants()

label bore(104.775) // 4 1/8
label stroke(133.35) // 5 1/4
label compression_ratio(4.0) // ?
label con_rod(266.7) // 10 1/2
label compression_height(127.0 / 2 * units.mm) // 5 l
label intake_valve_diameter(57.15) // ~ 2 1/4 ; 2 c
label exhaust_valve_diameter(57.15) // ~ 2 1/4 ; 2 c
label intake_valves(1.0)
label exhaust_valves(1.0)

label cyl(4.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

label redline(1900.0) // RPM

// abbott
label IVL(8.73125) // 11/32 ?
label EVL(8.73125) // 11/32 ?
label IVO(-11.5 * units.deg) //BTDC
label IVC(44.2 * units.deg) //ABDC
label EVO(45.8 * units.deg) //BBDC
label EVC(11.5 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(1600.0) // ?
label piston_mass(1200.0) // ?
label crank_mass(18.0) // ? kg
label flywheel_mass(4.0) // kg

label exhaust_delay_coeff(1.5)


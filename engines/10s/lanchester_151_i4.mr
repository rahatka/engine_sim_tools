import "../../fuel/10.mr"

units units()
constants constants()

label bore(101.6) // 4
label stroke(76.2) // 3
label compression_ratio(4.0) // ?
label con_rod(177.8) // ~ 7
label compression_height(65.278 * units.mm) // ~ 2.57
label intake_valve_diameter(38.1) // 1 1/2
label exhaust_valve_diameter(38.1) // 1 1/2
label intake_valves(1.0)
label exhaust_valves(1.0)

label cyl(4.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

label redline(2200.0) // RPM

// Lanchester
label IVL(9.525) // 3/8
label EVL(9.525) // 3/8
label IVO(7.0 * units.deg) //BTDC
label IVC(15.0 * units.deg) //ABDC
label EVO(38.0 * units.deg) //BBDC
label EVC(7.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(900.0) // ?
label piston_mass(800.0) // ?
label crank_mass(15.0) // ? kg
label flywheel_mass(4.0) // kg

label exhaust_delay_coeff(1.5)


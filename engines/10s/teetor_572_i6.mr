import "engine_sim.mr"
import "../../fuel/10.mr"

units units()
constants constants()

label bore(114.3) // 4 1/2
label stroke(152.4) // 6
label compression_ratio(4.5)
label con_rod(304.8) // 12
label compression_height(158.75 / 2 * units.mm) // 6 1/4 length
label intake_valve_diameter(38.1) // 2 1/16 stock, 1 1/2 ; sim_automotive-industries_1920-09-09_43_11
label exhaust_valve_diameter(38.1) // 2 1/16 stock, 1 1/2 ; sim_automotive-industries_1920-09-09_43_11
label intake_valves(2.0)
label exhaust_valves(2.0)

label redline(2500.0) // RPM

label cyl(6.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// McFarlan valve timings
label IVL(8.636) // .34 ark:/13960/t6zw1f700
label EVL(8.636) // .34 ark:/13960/t6zw1f700
label IVO(3.5 * units.deg) //BTDC
label IVC(32.5 * units.deg) //ABDC
label EVO(40.5 * units.deg) //BBDC
label EVC(11.5 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label piston_mass(963.884) // 34 oz
label con_rod_mass(2664.86) // 94 oz
label crank_mass(30.0) //
label flywheel_mass(8.0) // kg

label exhaust_delay_coeff(1.5)

public node eng_distributor {
    input wires;
    input timing_curve;
    input rev_limit: 4500 * units.rpm;
    input limiter_duration: 0.0001;
    alias output __out:
        ignition_module(timing_curve: timing_curve, rev_limit: rev_limit, limiter_duration: limiter_duration)
            .connect_wire(wires.wire1, rot * 0)
            .connect_wire(wires.wire5, rot * 1)
            .connect_wire(wires.wire3, rot * 2)
            .connect_wire(wires.wire6, rot * 3)
            .connect_wire(wires.wire2, rot * 4)
            .connect_wire(wires.wire4, rot * 5);
}

private node wires {
    output wire1: ignition_wire();
    output wire2: ignition_wire();
    output wire3: ignition_wire();
    output wire4: ignition_wire();
    output wire5: ignition_wire();
    output wire6: ignition_wire();
}


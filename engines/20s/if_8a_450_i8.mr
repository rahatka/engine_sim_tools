private node add_lobes {
    input base;
    input shift: 0.0;
    input var;
    input this;
    alias output __out: this;

    this.add_lobe(base + var.s1 * rot + shift)
    this.add_lobe(base + var.s2 * rot + shift)
    this.add_lobe(base + var.s3 * rot + shift)
    this.add_lobe(base + var.s4 * rot + shift)
    this.add_lobe(base + var.s1 * rot + shift + sb_rot)
    this.add_lobe(base + var.s2 * rot + shift + sb_rot)
    this.add_lobe(base + var.s3 * rot + shift + sb_rot)
    this.add_lobe(base + var.s4 * rot + shift + sb_rot)
}

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node head {
    // port_flow.py v1.7
    // intake port area: 9.1 cm²; saturated lift: 8.28 mm
    // exhaust port area: 9.1 cm²; saturated lift: 8.28 mm
    // cylinder volume: 921.5 cm³ (56.23 CI); engine volume: 7.372 L (449.85 CI)
    // chamber volume: 230.4 cm³ (14.06 CI); conrod/stroke 2.123; N.A.C.C. 44.76
    // 32 harmonic intake runner length: 19.4 cm; diameter: 3.8 cm
    // primary length: 54.3 cm, area: 9.6 cm², diameter: 3.5 cm
    // collector diameter: 5.4 cm, area: 23.0 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 230.367 * units.cc,
        intake_runner_volume: 223.1 * units.cc,
        intake_runner_cross_section_area: 11.5 * units.cm2,
        exhaust_runner_volume: 74.4 * units.cc,
        exhaust_runner_cross_section_area: 9.6 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

public node bank_builder {
    input var;
    input incline;
    input phasing;
    input crankshaft;
    input ignition;
    input intake;
    input exhaust;
    input flip: false;

    output b0: _b0;

    ignition_wire _wire1()
    ignition_wire _wire2()
    ignition_wire _wire3()
    ignition_wire _wire4()
    ignition_wire _wire5()
    ignition_wire _wire6()
    ignition_wire _wire7()
    ignition_wire _wire8()

    piston_parameters piston_params(
        mass: piston_mass * units.g,
        blowby: k_28inH2O(0.030),
        compression_height: compression_height
    )

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    connecting_rod_parameters cr_params(
        mass: con_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.g,
            length: con_rod * units.mm
        ),
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    camshaft _intake_cam_0(base_radius: 18.0 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 18.0 * units.mm, lobe_profile: exhaust_lobe_profile)

    _intake_cam_0.add_lobes(ILC, phasing, var)
    _exhaust_cam_0.add_lobes(ELC, phasing, var)

    ignition.connect_wire(_wire1, rot * var.s1 + phasing)
    ignition.connect_wire(_wire2, rot * var.s2 + phasing)
    ignition.connect_wire(_wire3, rot * var.s3 + phasing)
    ignition.connect_wire(_wire4, rot * var.s4 + phasing)
    ignition.connect_wire(_wire5, rot * var.s1 + phasing + sb_rot)
    ignition.connect_wire(_wire6, rot * var.s2 + phasing + sb_rot)
    ignition.connect_wire(_wire7, rot * var.s3 + phasing + sb_rot)
    ignition.connect_wire(_wire8, rot * var.s4 + phasing + sb_rot)

    cylinder_bank _b0(bank_params, angle: incline)

    label spacing_factor(1.15)
    label flange_density(1.0 * exhaust_delay_coeff)
    label pl0 ((5.3 * bore * spacing_factor / flange_density) * units.mm)
    label pl1 ((4.1 * bore * spacing_factor / flange_density) * units.mm)
    label pl2 ((3.5 * bore * spacing_factor / flange_density) * units.mm)
    label pl3 ((3.5 * bore * spacing_factor / flange_density) * units.mm)
    label pl4 ((1.5 * bore * spacing_factor / flange_density) * units.mm)
    label pl5 ((1.5 * bore * spacing_factor / flange_density) * units.mm)
    label pl6 ((2.1 * bore * spacing_factor / flange_density) * units.mm)
    label pl7 ((3.3 * bore * spacing_factor / flange_density) * units.mm)

    _b0
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj0,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl0,
            ignition_wire: _wire1
        )
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj1,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl1,
            ignition_wire: _wire2
        )
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj2,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl2,
            ignition_wire: _wire3
        )
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj3,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl3,
            ignition_wire: _wire4
        )
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj4,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl4,
            ignition_wire: _wire5
        )
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj5,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl5,
            ignition_wire: _wire6
        )
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj6,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl6,
            ignition_wire: _wire7
        )
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj7,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pl7,
            ignition_wire: _wire8
        )

    _b0.set_cylinder_head(
        head(
            intake_camshaft: _intake_cam_0,
            exhaust_camshaft: _exhaust_cam_0,
            flip_display: flip
        )
    )
}

public node crank {
    input var;
    input phasing;

    output c: _c;
    output rj0: _rj0;
    output rj1: _rj1;
    output rj2: _rj2;
    output rj3: _rj3;
    output rj4: _rj4;
    output rj5: _rj5;
    output rj6: _rj6;
    output rj7: _rj7;

    label crank_moment(disk_moment_of_inertia(mass: crank_mass * units.kg, radius: (stroke / 2.0) * units.mm))
    label flywheel_moment(disk_moment_of_inertia(mass: flywheel_mass * units.kg, radius: (stroke * 3.5) * units.mm))
    label other_moment(disk_moment_of_inertia(mass: 1 * units.kg, radius: 1.0 * units.cm))

    crankshaft _c(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: flywheel_mass * units.kg,
        mass: crank_mass * units.kg,
        friction_torque: 11.1 * units.Nm,
        moment_of_inertia: crank_moment + flywheel_moment + other_moment,
        tdc: 0.0
    )

    rod_journal _rj0(angle: rot * var.s1 + phasing)
    rod_journal _rj1(angle: rot * var.s2 + phasing)
    rod_journal _rj2(angle: rot * var.s3 + phasing)
    rod_journal _rj3(angle: rot * var.s4 + phasing)
    rod_journal _rj4(angle: rot * var.s1 + phasing + sb_rot)
    rod_journal _rj5(angle: rot * var.s2 + phasing + sb_rot)
    rod_journal _rj6(angle: rot * var.s3 + phasing + sb_rot)
    rod_journal _rj7(angle: rot * var.s4 + phasing + sb_rot)

    _c
        .add_rod_journal(_rj0)
        .add_rod_journal(_rj1)
        .add_rod_journal(_rj2)
        .add_rod_journal(_rj3)
        .add_rod_journal(_rj4)
        .add_rod_journal(_rj5)
        .add_rod_journal(_rj6)
        .add_rod_journal(_rj7)
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "Isotta Fraschini Tipo 8A",
        starter_torque: 100 * units.Nm,
        starter_speed: 150 * units.rpm,
        redline: redline * units.rpm,
        fuel: s10(),
        throttle_gamma: 1.6,
        jitter: 0.8,
        noise: 0.05,
        hf_gain: 0.005,
        simulation_frequency: 8000,
        dyno_min_speed: 500 * units.rpm
    )

    ////////////////////////////////////////
    ///////// change variants here /////////
    ////////////////////////////////////////
    variant_0 variant()

    bank_builder bank0(
        var: variant,
        incline: 0,
        phasing: 0,
        crankshaft: c0,
        ignition: ignition,
        intake: intake,
        exhaust: exhaust0,
        flip: true
    )
    crank c0(variant, rot90)

    ignition_module ignition(timing_curve: timing_curve, rev_limit: 7250 * units.rpm, limiter_duration: 0.03)

    label collector_cross_section_area(23.0)

    label exhaust_pipe_length_0(300.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 100.0) // Litres

    intake intake(
        plenum_volume: 2.7 * units.L,
        plenum_cross_section_area: 46.1 * units.cm2,
        runner_length: 19.4 * units.cm,
        intake_flow_rate: k_carb(307.2),
        idle_flow_rate: k_carb(0.01),
        idle_throttle_plate_position: 0.994,
        runner_flow_rate: k_carb(76.8),
        velocity_decay: 0.05
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(511.9),
        collector_cross_section_area: 23.0 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 54.3 * units.cm,
        primary_flow_rate: k_carb(128.0),
        velocity_decay: 1.0,
        volume: exhaust_volume_0 * units.L
    )

    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_60.wav", volume: 0.00025))

    engine
        .add_cylinder_bank(bank0.b0)
        .add_crankshaft(c0.c)
        .add_ignition_module(ignition)

    function timing_curve(166.7 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 5.0 * units.deg)
        .add_sample(166.7 * units.rpm, 5.0 * units.deg)
        .add_sample(333.3 * units.rpm, 7.5 * units.deg)
        .add_sample(500.0 * units.rpm, 10.3 * units.deg)
        .add_sample(666.7 * units.rpm, 12.8 * units.deg)
        .add_sample(833.3 * units.rpm, 15.0 * units.deg)
        .add_sample(1000.0 * units.rpm, 17.0 * units.deg)
        .add_sample(1166.7 * units.rpm, 18.7 * units.deg)
        .add_sample(1333.3 * units.rpm, 20.2 * units.deg)
        .add_sample(1500.0 * units.rpm, 21.6 * units.deg)
        .add_sample(1666.7 * units.rpm, 22.8 * units.deg)
        .add_sample(1833.3 * units.rpm, 23.8 * units.deg)
        .add_sample(2000.0 * units.rpm, 24.8 * units.deg)
        .add_sample(2166.7 * units.rpm, 25.6 * units.deg)
        .add_sample(2333.3 * units.rpm, 26.3 * units.deg)
        .add_sample(2500.0 * units.rpm, 27.0 * units.deg)
        .add_sample(2666.7 * units.rpm, -12.5 * units.deg)
        .add_sample(2833.3 * units.rpm, -25.0 * units.deg)
}


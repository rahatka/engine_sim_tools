import "engine_sim.mr"
import "../../fuel/10.mr"

units units()
constants constants()

label bore(85.725) // 3 3/8
label stroke(114.3) // 4 1/2
label compression_ratio(5.3)
label con_rod(228.6) // 9
label compression_height(58.7375 * units.mm) // 2 5/16
label intake_valve_diameter(38.1) // 1 1/2 ; 1 5/8 13K
label exhaust_valve_diameter(34.925) // 1 3/8 ; 1 9/16 13K
label intake_valves(1.0)
label exhaust_valves(1.0)

label cyl(8.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

label redline(3400.0) // RPM

// 1929 dupont
label IVL(9.398) // .370
label EVL(9.398) // .370
label IVO(-5.0 * units.deg) //BTDC
label IVC(50.0 * units.deg) //ABDC
label EVO(40.0 * units.deg) //BBDC
label EVC(5.0 * units.deg) //ATDC

// 1931 13K
// label IVL(7.9375) // 5/16
// label EVL(7.9375) // 5/16
// label IVO(-2.0 * units.deg) //BTDC
// label IVC(47.0 * units.deg) //ABDC
// label EVO(43.0 * units.deg) //BBDC
// label EVC(2.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(1204.85) // 42 1/2
label piston_mass(609.515) // 21 1/2
label crank_mass(30.0) // ?
label flywheel_mass(7.0) // kg

label exhaust_delay_coeff(1.5)

public node variant {
    output s1: 0;
    output s6: 1;
    output s2: 2;
    output s5: 3;
    output s8: 4;
    output s3: 5;
    output s7: 6;
    output s4: 7;
}


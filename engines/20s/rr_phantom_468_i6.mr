import "engine_sim.mr"
import "../../fuel/10.mr"

units units()
constants constants()

label bore(107.95) // 4 1/4
label stroke(139.7) // 5 1/2
label compression_ratio(4.5)
label con_rod(279.4) // 11 ~
label compression_height(bore / 2 * units.mm) // ?
label intake_valve_diameter(48.26) // 1.9 ; .433 stem
label exhaust_valve_diameter(48.26) // 1.9 ; .433 stem
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(2500.0) // RPM

label cyl(6.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// 1931 RR
label IVL(10.795) // .425
label EVL(10.795) // .425
label IVO(-14.0 * units.deg) //BTDC
label IVC(30.0 * units.deg) //ABDC
label EVO(34.0 * units.deg) //BBDC
label EVC(2.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(1300.0) // ?
label piston_mass(700.0) // ?
label crank_mass(30.0) // ?
label flywheel_mass(5.0) // kg

label exhaust_delay_coeff(1.5)

public node variant {
    output s1: 0;
    output s4: 1;
    output s2: 2;
    output s6: 3;
    output s3: 4;
    output s5: 5;
}


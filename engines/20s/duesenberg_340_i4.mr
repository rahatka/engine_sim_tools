import "engine_sim.mr"
import "../../fuel/10.mr"

units units()
constants constants()

label bore(107.95) // 4 1/4 ; other sources say 4 1/2
label stroke(152.4) // 6
label compression_ratio(4.0) // ?
label con_rod(304.8) // 12
label compression_height(120.65 / 2 * units.mm) // 4 3/4 l
label intake_valve_diameter(55.5625) // 2 3/16 ; 2 c
label exhaust_valve_diameter(55.5625) // 2 3/16 ; 2 c
label intake_valves(1.0)
label exhaust_valves(1.0)

label cyl(4.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

label redline(2800.0) // RPM

// Rochester-Duesenberg G-1 only lifts known
label IVL(12.3031) // 31/64
label EVL(12.3031) // 31/64
label IVO(00.0 * units.deg) //BTDC
label IVC(45.0 * units.deg) //ABDC
label EVO(45.0 * units.deg) //BBDC
label EVC(15.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(1757.67) // 62
label piston_mass(479.1069) // 16.9
label crank_mass(18.0) // ? kg
label flywheel_mass(4.0) // kg

label exhaust_delay_coeff(1.5)

public node variant_0 {
    output s1: 0;
    output s3: 1;
    output s4: 2;
    output s2: 3;
}


import "../../fuel/10.mr"

units units()
constants constants()

label bore(73.025) // 2 7/8
label stroke(114.3) // 4 1/2
label compression_ratio(4.5) // ?
label con_rod(254.0) // 10
label compression_height(79.375 / 2 * units.mm) // ?
label intake_valve_diameter(28.575) // 1 1/8 Northway 209
label exhaust_valve_diameter(28.575) // 1 1/8 Northway 209
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(2500.0) // ? RPM

label cyl(8.0)
label row(2.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label vee(90.0 * units.deg)

label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)

// Northway 209
label IVL(8.73125) // 11/32
label EVL(8.73125) // 11/32
label IVO(-15.0 * units.deg) //BTDC
label IVC(38.0 * units.deg) //ABDC
label EVO(45.0 * units.deg) //BBDC
label EVC(10.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(798.039) // 28 3/20 Northway 209
label piston_mass(1790.27) // 63 3/20 Northway 209
label crank_mass(20.0) // ? kg
label flywheel_mass(6.0) // kg

label exhaust_delay_coeff(1.5)


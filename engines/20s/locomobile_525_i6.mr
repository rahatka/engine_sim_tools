label compression_height(79.248 * units.mm) // 3.12
label intake_valve_diameter(58.7375) // 2 5/16
label exhaust_valve_diameter(58.7375) // 2 5/16
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(2300.0) // RPM

label cyl(6.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// 1925 Locomobile 48
label IVL(10.35812) // .4078
label EVL(10.35812) // .4078
label IVO(20.0 * units.deg) //BTDC
label IVC(70.0 * units.deg) //ABDC
label EVO(57.0 * units.deg) //BBDC
label EVC(21.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(1190.68) // 42 oz
label piston_mass(1871.0685) // 66 oz
label crank_mass(40.0) // ?
label flywheel_mass(8.0) // kg

label exhaust_delay_coeff(1.5)


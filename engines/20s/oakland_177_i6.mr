import "engine_sim.mr"
import "../../fuel/10.mr"

units units()
constants constants()

label bore(71.4375) // 2 13/16
label stroke(120.65) // 4 3/4
label compression_ratio(4.5) // ?
label con_rod(241.3) // 9 1/2
label compression_height(45.212 * units.mm) // ?
label intake_valve_diameter(31.75) // 1 1/4 ? ; 1 1/8 c
label exhaust_valve_diameter(31.75) // 1 1/4 ? ; 1 1/8 c
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(2700.0) // RPM

label cyl(6.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// 1920 Oakland 34
label IVL(7.9375) // 5/16
label EVL(7.9375) // 5/16
label IVO(-17.5 * units.deg) //BTDC
label IVC(36.0 * units.deg) //ABDC
label EVO(42.5 * units.deg) //BBDC
label EVC(7.5 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(793.78665) // 11 oz
label piston_mass(566.99046) // 20 oz
label crank_mass(25.0) // ?
label flywheel_mass(5.0) // kg

label exhaust_delay_coeff(1.5)

public node variant {
    output s1: 0;
    output s5: 1;
    output s3: 2;
    output s6: 3;
    output s2: 4;
    output s4: 5;
}


import "engine_sim.mr"
import "../../fuel/10.mr"

units units()
constants constants()

label bore(76.2) // 3
label stroke(107.95) // 4 1/4
label compression_ratio(4.66)
label con_rod(228.6) // 9
label compression_height(57.15 * units.mm) // 2 1/4
label intake_valve_diameter(36.5125) // 1 7/16
label exhaust_valve_diameter(33.3375) // 1 5/16
label intake_valves(1.0)
label exhaust_valves(1.0)

label cyl(8.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

label redline(3100.0) // RPM

// 1926 12Z
label IVL(7.9375) // 5/16
label EVL(7.9375) // 5/16
label IVO(-4.0 * units.deg) //BTDC
label IVC(46.0 * units.deg) //ABDC
label EVO(41.0 * units.deg) //BBDC
label EVC(1.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(1134.0) // 40 oz
label piston_mass(879.0) // 31 oz
label crank_mass(30.0) // ?
label flywheel_mass(7.0) // kg

label exhaust_delay_coeff(1.5)

public node variant {
    output s1: 0;
    output s6: 1;
    output s2: 2;
    output s5: 3;
    output s8: 4;
    output s3: 5;
    output s7: 6;
    output s4: 7;
}


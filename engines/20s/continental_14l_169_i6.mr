import "engine_sim.mr"
import "../../fuel/10.mr"

units units()
constants constants()

label bore(69.85) // 2 3/4
label stroke(120.65) // 4 3/4
label compression_ratio(4.79)
label con_rod(228.6) // 9
label compression_height(49.2125 * units.mm) // 1 15/16
label intake_valve_diameter(34.925) // 1 3/8 ; 1 7/16
label exhaust_valve_diameter(31.75) // 1 1/4 ; 1 5/16
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(2600.0) // RPM

label cyl(6.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// 1928 Erskine Six 51
label IVL(7.9375) // 5/16
label EVL(7.9375) // 5/16
label IVO(-4.0 * units.deg) //BTDC
label IVC(46.0 * units.deg) //ABDC
label EVO(41.0 * units.deg) //BBDC
label EVC(1.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(737.088) // 26
label piston_mass(457.136) // 16 1/8
label crank_mass(20.0) // ?
label flywheel_mass(5.0) // kg

label exhaust_delay_coeff(1.5)


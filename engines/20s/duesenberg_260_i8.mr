public node eng {
    alias output __out: engine;

    engine engine(
        name: "Duesenberg A",
        starter_torque: 100 * units.Nm,
        starter_speed: 300 * units.rpm,
        redline: redline * units.rpm,
        fuel: s10(),
        throttle_gamma: 1.5,
        jitter: es.jitter,
        noise: es.noise,
        hf_gain: es.hf_gain,
        simulation_frequency: 12000,
        dyno_min_speed: 500 * units.rpm
    )

    variant_7 variant()

    crank c0(rot90, variant)

    bank_builder bank0(
        var: variant,
        crankshaft: c0,
        incline: 0,
        phasing: 0,
        ignition: ignition,
        intake: intake,
        exhaust: es,
        flip: true
    )

    ignition_module ignition(timing_curve: timing_curve, rev_limit: redline * 1.5 * units.rpm, limiter_duration: 0.03)

    intake intake(
        plenum_volume: 2.3 * units.L,
        plenum_cross_section_area: 37.0 * units.cm2,
        runner_length: 25.8 * units.cm,
        intake_flow_rate: k_carb(269.5),
        idle_flow_rate: k_carb(0.01),
        idle_throttle_plate_position: 0.994,
        runner_flow_rate: k_carb(67.4),
        velocity_decay: 0.05
    )

    es1 es()

    function timing_curve(253.3 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 5.0 * units.deg)
        .add_sample(253.3 * units.rpm, 5.0 * units.deg)
        .add_sample(506.7 * units.rpm, 5.0 * units.deg)
        .add_sample(760.0 * units.rpm, 5.2 * units.deg)
        .add_sample(1013.3 * units.rpm, 9.0 * units.deg)
        .add_sample(1266.7 * units.rpm, 12.8 * units.deg)
        .add_sample(1520.0 * units.rpm, 16.7 * units.deg)
        .add_sample(1773.3 * units.rpm, 20.5 * units.deg)
        .add_sample(2026.7 * units.rpm, 24.3 * units.deg)
        .add_sample(2280.0 * units.rpm, 28.2 * units.deg)
        .add_sample(2533.3 * units.rpm, 30.0 * units.deg)
        .add_sample(2786.7 * units.rpm, 30.0 * units.deg)
        .add_sample(3040.0 * units.rpm, 30.0 * units.deg)
        .add_sample(3293.3 * units.rpm, 30.0 * units.deg)
        .add_sample(3546.7 * units.rpm, 30.0 * units.deg)
        .add_sample(3800.0 * units.rpm, 30.0 * units.deg)
        .add_sample(4053.3 * units.rpm, -12.5 * units.deg)
        .add_sample(4306.7 * units.rpm, -25.0 * units.deg)

    engine
        .add_crankshaft(c0.c)
        .add_cylinder_bank(bank0.bank)
        .add_ignition_module(ignition)
}

label spacing_factor(1.2)
label flange_density(1.0 * exhaust_delay_coeff)
label em_unit(bore * spacing_factor / flange_density)

// std1
private node es0{
    output es0: exhaust0; output es1: exhaust0; output es2: exhaust0; output es3: exhaust0;
    output es4: exhaust0; output es5: exhaust0; output es6: exhaust0; output es7: exhaust0;
    output pl0: _pl0; output pl1: _pl1; output pl2: _pl2; output pl3: _pl3;
    output pl4: _pl4; output pl5: _pl5; output pl6: _pl6; output pl7: _pl7;
    output jitter: 0.7;
    output noise: 0.2;
    output hf_gain: 0.004;

    label collector_cross_section_area(22.6)
    label exhaust_pipe_length_0(354.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 100.0) // Litres

    float _pl0 ((8.5 * em_unit) * units.mm)
    float _pl1 ((7.5 * em_unit) * units.mm)
    float _pl2 ((6.5 * em_unit) * units.mm)
    float _pl3 ((5.5 * em_unit) * units.mm)
    float _pl4 ((4.0 * em_unit) * units.mm)
    float _pl5 ((3.0 * em_unit) * units.mm)
    float _pl6 ((2.0 * em_unit) * units.mm)
    float _pl7 ((1.0 * em_unit) * units.mm)

    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(449.2),
        collector_cross_section_area: 22.6 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 34.6 * units.cm,
        primary_flow_rate: k_carb(112.3),
        velocity_decay: 1.0,
        volume: (exhaust_volume_0 + 5.0) * units.L
    )

    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_58.wav", volume: 0.00025))
}


import "engine_sim.mr"
import "../../fuel/10.mr"

units units()
constants constants()

label bore(73.025) // 2 7/8
label stroke(114.3) // 4 1/2
label compression_ratio(5.43)
label con_rod(234.95) // 9 1/4
label compression_height(53.1812 * units.mm) // 2 3/32
label intake_valve_diameter(35.7188) // 1 13/32 c
label exhaust_valve_diameter(34.1313) // 1 11/32 c
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(3300.0) // RPM

label cyl(6.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// 1928 gp 610
label IVL(8.6614) // .341
label EVL(8.6614) // .341
label IVO(0.0 * units.deg) //BTDC
label IVC(40.0 * units.deg) //ABDC
label EVO(40.0 * units.deg) //BBDC
label EVC(10.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(680.389) // 24 oz
label piston_mass(354.369) // 12 1/2
label crank_mass(30.0) // ?
label flywheel_mass(5.0) // kg

label exhaust_delay_coeff(1.5)

public node variant {
    output s1: 0;
    output s5: 1;
    output s3: 2;
    output s6: 3;
    output s2: 4;
    output s4: 5;
}


import "engine_sim.mr"
import "../../fuel/10.mr"

units units()
constants constants()

label bore(85.725) // 3 3/8
label stroke(114.3) // 4 1/2
label compression_ratio(4.2)
label con_rod(266.7) // 10 1/2
label compression_height(103.886 / 2 * units.mm) // 4.09 len
label intake_valve_diameter(38.1) // 1 1/2 ; jic 1 5/8
label exhaust_valve_diameter(38.1) // 1 1/2 ; jic 1 5/8
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(2500.0) // RPM

label cyl(6.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// 1925 Case X
label IVL(7.9375) // 5/16 ; jic .3125
label EVL(7.9375) // 5/16 ; jic .3125
label IVO(-12.0 * units.deg) //BTDC
label IVC(40.0 * units.deg) //ABDC
label EVO(40.0 * units.deg) //BBDC
label EVC(8.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(1474.18) // 52
label piston_mass(992.23331) // 35
label crank_mass(27.0) // ?
label flywheel_mass(5.0) // kg

label exhaust_delay_coeff(1.5)

public node variant {
    output s1: 0;
    output s5: 1;
    output s3: 2;
    output s6: 3;
    output s2: 4;
    output s4: 5;
}


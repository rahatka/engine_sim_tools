import "engine_sim.mr"
import "../../fuel/10.mr"

units units()
constants constants()

label bore(85.725) // 3 3/8
label stroke(120.65) // 4 3/4
label compression_ratio(4.0) // ?
label con_rod(304.8) // 12
label compression_height(bore / 2 * units.mm) // ?
label intake_valve_diameter(47.625) // 1 7/8 c
label exhaust_valve_diameter(41.275) // 1 5/8 c
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(2200.0) // RPM

label cyl(4.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// 1921 buick four/six
label IVL(8.33437) // 21/64
label EVL(7.9375) // 5/16
label IVO(-22.17 * units.deg) //BTDC
label IVC(45.17 * units.deg) //ABDC
label EVO(53.0 * units.deg) //BBDC
label EVC(9.17 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(1048.93) // 37
label piston_mass(850.486) // ?
label crank_mass(15.3) // ?
label flywheel_mass(4.0) // kg

label exhaust_delay_coeff(1.5)


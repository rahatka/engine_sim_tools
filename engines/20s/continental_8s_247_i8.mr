import "engine_sim.mr"
import "../../fuel/10.mr"

units units()
constants constants()

label bore(73.025) // 2 7/8
label stroke(120.65) // 4 3/4
label compression_ratio(4.65) // 5.16 17S
label con_rod(247.65) // 9 3/4
label compression_height(50.8 * units.mm) // ? 2 5/16 17S
label intake_valve_diameter(38.1) // 1 1/2 ; 1 3/8 17S
label exhaust_valve_diameter(34.925) // 1 3/8 ; 1 1/4 17S
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(3250.0) // RPM

label cyl(8.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// 1925 Jordan J
label IVL(7.9375) // 5/16
label EVL(7.9375) // 5/16
label IVO(-8.0 * units.deg) //BTDC
label IVC(40.0 * units.deg) //ABDC
label EVO(40.0 * units.deg) //BBDC
label EVC(8.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(850.0) // ?
label piston_mass(500.0) // ?
label crank_mass(20.0) // ?
label flywheel_mass(5.0) // kg

label exhaust_delay_coeff(1.5)

public node variant {
    output s1: 0;
    output s6: 1;
    output s2: 2;
    output s5: 3;
    output s8: 4;
    output s3: 5;
    output s7: 6;
    output s4: 7;
}


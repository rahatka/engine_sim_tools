units units()
constants constants()

label bore(95.25) // 3 3/4
label stroke(127.0) // 5
label compression_ratio(5.0)
label con_rod(254.0) // 10
label compression_height(69.85 * units.mm) // 2 3/4
label intake_valve_diameter(43.6563) // 1 23/32
label exhaust_valve_diameter(43.6563) // 1 23/32
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(2800.0) // RPM

label cyl(6.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// 1927 big six
label IVL(7.9375) // 5/16
label EVL(7.9375) // 5/16
label IVO(0.0 * units.deg) //BTDC
label IVC(34.0 * units.deg) //ABDC
label EVO(49.0 * units.deg) //BBDC
label EVC(5.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(1133.98) // 40
label piston_mass(1133.98) // 40
label crank_mass(28.0) // ?
label flywheel_mass(5.0) // kg

label exhaust_delay_coeff(1.5)


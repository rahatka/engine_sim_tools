import "engine_sim.mr"
import "../../fuel/10.mr"

units units()
constants constants()

label bore(84.1375) // 3 5/16
label stroke(114.3) // 4 1/2
label compression_ratio(4.5)
label con_rod(215.9) // 8 1/2
label compression_height(50.292 * units.mm) // 1.98
label intake_valve_diameter(44.45) // 1 3/4
label exhaust_valve_diameter(44.45) // 1 3/4
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(3300.0) // RPM

label cyl(6.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// 1925 Ans. M
label IVL(11.1125) // 0.44 / 7/16
label EVL(11.1125) // 0.44 / 7/16
label IVO(-10.0 * units.deg) //BTDC
label IVC(56.0 * units.deg) //ABDC
label EVO(50.0 * units.deg) //BBDC
label EVC(10.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(900.0) // ?
label piston_mass(612.3496995) // 21.6 oz
label crank_mass(30.0) // ?
label flywheel_mass(5.0) // kg

label exhaust_delay_coeff(1.5)

public node variant {
    output s1: 0;
    output s5: 1;
    output s3: 2;
    output s6: 3;
    output s2: 4;
    output s4: 5;
}


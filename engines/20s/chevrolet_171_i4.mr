import "engine_sim.mr"
import "../../fuel/10.mr"

units units()
constants constants()

label bore(93.6625) // 3 11/16
label stroke(101.6) // 4
label compression_ratio(4.3)
label con_rod(187.325) // 7 3/8
label compression_height(47.498 * units.mm) // 1 7/8
label intake_valve_diameter(33.3375) // 1 5/16
label exhaust_valve_diameter(33.3375) // 1 5/16
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(2200.0) // RPM

label cyl(4.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// 1925 Chevy
label IVL(7.14375) // 9/32 ?
label EVL(7.14375) // 9/32 ?
label IVO(-16.0 * units.deg) //BTDC
label IVC(52.0 * units.deg) //ABDC
label EVO(40.0 * units.deg) //BBDC
label EVC(16.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(822.136) // 29 oz
label piston_mass(1162.33) // 41 oz
label crank_mass(15.3) // ?
label flywheel_mass(4.0) // kg

label exhaust_delay_coeff(1.5)


import "engine_sim.mr"
import "../../fuel/10.mr"

units units()
constants constants()

label bore(88.9) // 3 1/2
label stroke(127.0) // 5
label compression_ratio(6.0)
label con_rod(276.225) // 10 7/8
label compression_height(60.0 * units.mm) // ?
label intake_valve_diameter(46.0375) // 1 13/16
label exhaust_valve_diameter(42.8625) // 1 11/16
label intake_valves(1.0)
label exhaust_valves(1.0)

label cyl(12.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label phase(rot * -1)

label redline(3300.0) // RPM

// Packard 384 
label IVL(9.0932) // .358
label EVL(9.0932) // .358
label IVO(20.0 * units.deg) //BTDC; 33-34 30
label IVC(65.0 * units.deg) //ABDC; 33-34 65
label EVO(65.0 * units.deg) //BBDC; 33-34 65
label EVC(20.0 * units.deg) //ATDC; 33-34 30

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(1337.74) // g 2 lbs 15 3/16 oz
label piston_mass(620.8546) // g 21.9 oz
label crank_mass(75.0) //
label flywheel_mass(7.0) // kg

label exhaust_delay_coeff(1.5)

// standard

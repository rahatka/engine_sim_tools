private node add_lobes {
    input base;
    input var;
    input this;
    alias output __out: this;

    this.add_lobe(base + var.s1 * rot_base)
    this.add_lobe(base + var.s2 * rot_base)
    this.add_lobe(base + var.s3 * rot_base)
    this.add_lobe(base + var.s4 * rot_base)
    this.add_lobe(base + var.s5 * rot_base)
    this.add_lobe(base + var.s6 * rot_base)
    this.add_lobe(base + var.s1 * rot_base + phase)
    this.add_lobe(base + var.s2 * rot_base + phase)
    this.add_lobe(base + var.s3 * rot_base + phase)
    this.add_lobe(base + var.s4 * rot_base + phase)
    this.add_lobe(base + var.s5 * rot_base + phase)
    this.add_lobe(base + var.s6 * rot_base + phase)
}

public node bank_builder {
    input var;
    input crankshaft;
    input incline;
    input ignition;
    input intake;
    input exhaust;
    input flip: false;

    output bank: _b0;

    ignition_wire _wire1()
    ignition_wire _wire2()
    ignition_wire _wire3()
    ignition_wire _wire4()
    ignition_wire _wire5()
    ignition_wire _wire6()
    ignition_wire _wire7()
    ignition_wire _wire8()
    ignition_wire _wire9()
    ignition_wire _wire10()
    ignition_wire _wire11()
    ignition_wire _wire12()

    piston_parameters piston_params(
        mass: piston_mass * units.g,
        blowby: k_28inH2O(0.028),
        compression_height: compression_height,
        wrist_pin_position: 0.0,
        displacement: 0.0
    )

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    connecting_rod_parameters cr_params(
        mass: con_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.g,
            length: con_rod * units.mm
        ),
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    camshaft _intake_cam_0(base_radius: 17.4 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 17.4 * units.mm, lobe_profile: exhaust_lobe_profile)

    _intake_cam_0.add_lobes(ILC, var)
    _exhaust_cam_0.add_lobes(ELC, var)

    ignition.connect_wire(_wire1, rot_base * var.s1)
    ignition.connect_wire(_wire2, rot_base * var.s2)
    ignition.connect_wire(_wire3, rot_base * var.s3)
    ignition.connect_wire(_wire4, rot_base * var.s4)
    ignition.connect_wire(_wire5, rot_base * var.s5)
    ignition.connect_wire(_wire6, rot_base * var.s6)
    ignition.connect_wire(_wire7, rot_base * var.s1 + phase)
    ignition.connect_wire(_wire8, rot_base * var.s2 + phase)
    ignition.connect_wire(_wire9, rot_base * var.s3 + phase)
    ignition.connect_wire(_wire10, rot_base * var.s4 + phase)
    ignition.connect_wire(_wire11, rot_base * var.s5 + phase)
    ignition.connect_wire(_wire12, rot_base * var.s6 + phase)

    cylinder_bank _b0(bank_params, angle: incline)

    label sh (15.0 * units.cm)

    _b0
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj0,
            intake: intake,
            exhaust_system: exhaust.es0,
            primary_length: exhaust.pl0 + sh,
            ignition_wire: _wire1
        )
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj1,
            intake: intake,
            exhaust_system: exhaust.es1,
            primary_length: exhaust.pl1 + sh,
            ignition_wire: _wire2
        )
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj2,
            intake: intake,
            exhaust_system: exhaust.es2,
            primary_length: exhaust.pl2 + sh,
            ignition_wire: _wire3
        )
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj3,
            intake: intake,
            exhaust_system: exhaust.es3,
            primary_length: exhaust.pl3 + sh,
            ignition_wire: _wire4
        )
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj4,
            intake: intake,
            exhaust_system: exhaust.es4,
            primary_length: exhaust.pl4 + sh,
            ignition_wire: _wire5
        )
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj5,
            intake: intake,
            exhaust_system: exhaust.es5,
            primary_length: exhaust.pl5 + sh,
            ignition_wire: _wire6
        )
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj6,
            intake: intake,
            exhaust_system: exhaust.es0,
            primary_length: exhaust.pl5,
            ignition_wire: _wire7
        )
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj7,
            intake: intake,
            exhaust_system: exhaust.es1,
            primary_length: exhaust.pl4,
            ignition_wire: _wire8
        )
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj8,
            intake: intake,
            exhaust_system: exhaust.es2,
            primary_length: exhaust.pl3,
            ignition_wire: _wire9
        )
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj9,
            intake: intake,
            exhaust_system: exhaust.es3,
            primary_length: exhaust.pl2,
            ignition_wire: _wire10
        )
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj10,
            intake: intake,
            exhaust_system: exhaust.es4,
            primary_length: exhaust.pl1,
            ignition_wire: _wire11
        )
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj11,
            intake: intake,
            exhaust_system: exhaust.es5,
            primary_length: exhaust.pl0,
            ignition_wire: _wire12
        )

    _b0.set_cylinder_head(
        head(
            intake_camshaft: _intake_cam_0,
            exhaust_camshaft: _exhaust_cam_0,
            flip_display: flip
        )
    )
}

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node head {
    // port_flow.py v1.7
    // intake port area: 12.7 cm²; saturated lift: 9.78 mm
    // exhaust port area: 11.0 cm²; saturated lift: 9.11 mm
    // cylinder volume: 788.3 cm³ (48.11 CI); engine volume: 9.460 L (577.27 CI)
    // chamber volume: 157.7 cm³ (9.62 CI); conrod/stroke 2.175; N.A.C.C. 58.80
    // 16 harmonic intake runner length: 28.5 cm; diameter: 3.8 cm
    // primary length: 40.3 cm, area: 11.5 cm², diameter: 3.8 cm
    // collector diameter: 7.3 cm, area: 41.5 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 157.662 * units.cc,
        intake_runner_volume: 325.5 * units.cc,
        intake_runner_cross_section_area: 11.4 * units.cm2,
        exhaust_runner_volume: 108.5 * units.cc,
        exhaust_runner_cross_section_area: 11.5 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

public node crank {
    input phasing;
    input var;

    output c: _c;
    output rj0: _rj0;
    output rj1: _rj1;
    output rj2: _rj2;
    output rj3: _rj3;
    output rj4: _rj4;
    output rj5: _rj5;
    output rj6: _rj6;
    output rj7: _rj7;
    output rj8: _rj8;
    output rj9: _rj9;
    output rj10: _rj10;
    output rj11: _rj11;

    label crank_moment(disk_moment_of_inertia(mass: crank_mass * units.kg, radius: (stroke / 2.0) * units.mm))
    label flywheel_moment(disk_moment_of_inertia(mass: flywheel_mass * units.kg, radius: (stroke * 3.5) * units.mm))
    label other_moment(disk_moment_of_inertia(mass: 1 * units.kg, radius: 1.0 * units.cm))

    crankshaft _c(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: flywheel_mass * units.kg,
        mass: crank_mass * units.kg,
        friction_torque: 14.2 * units.Nm,
        moment_of_inertia: crank_moment + flywheel_moment + other_moment,
        tdc: 0.0
    )

    rod_journal _rj0(angle: rot_base * var.s1 + rot90)
    rod_journal _rj1(angle: rot_base * var.s2 + rot90)
    rod_journal _rj2(angle: rot_base * var.s3 + rot90)
    rod_journal _rj3(angle: rot_base * var.s4 + rot90)
    rod_journal _rj4(angle: rot_base * var.s5 + rot90)
    rod_journal _rj5(angle: rot_base * var.s6 + rot90)
    rod_journal _rj6(angle: rot_base * var.s1 + rot90 + phase)
    rod_journal _rj7(angle: rot_base * var.s2 + rot90 + phase)
    rod_journal _rj8(angle: rot_base * var.s3 + rot90 + phase)
    rod_journal _rj9(angle: rot_base * var.s4 + rot90 + phase)
    rod_journal _rj10(angle: rot_base * var.s5 + rot90 + phase)
    rod_journal _rj11(angle: rot_base * var.s6 + rot90 + phase)

    _c
        .add_rod_journal(_rj0)
        .add_rod_journal(_rj1)
        .add_rod_journal(_rj2)
        .add_rod_journal(_rj3)
        .add_rod_journal(_rj4)
        .add_rod_journal(_rj5)
        .add_rod_journal(_rj6)
        .add_rod_journal(_rj7)
        .add_rod_journal(_rj8)
        .add_rod_journal(_rj9)
        .add_rod_journal(_rj10)
        .add_rod_journal(_rj11)
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "Packard Straight 12",
        starter_torque: 100 * units.Nm,
        starter_speed: 300 * units.rpm,
        redline: redline * units.rpm,
        fuel: s10(),
        throttle_gamma: 1.5,
        jitter: es.jitter,
        noise: es.noise,
        hf_gain: es.hf_gain,
        simulation_frequency: 9000,
        dyno_min_speed: 500 * units.rpm
    )

    variant_0 variant()

    crank c0(rot90, variant)

    bank_builder bank0(
        var: variant,
        crankshaft: c0,
        incline: 0,
        ignition: ignition,
        intake: intake,
        exhaust: es,
        flip: true
    )

    ignition_module ignition(timing_curve: timing_curve, rev_limit: redline * 1.5 * units.rpm, limiter_duration: 0.03)

    intake intake(
        plenum_volume: 4.3 * units.L,
        plenum_cross_section_area: 68.5 * units.cm2,
        runner_length: 28.5 * units.cm,
        intake_flow_rate: k_carb(520.3),
        idle_flow_rate: k_carb(0.01),
        idle_throttle_plate_position: 0.9955,
        runner_flow_rate: k_carb(86.7),
        velocity_decay: 0.05
    )

    es0 es()

    function timing_curve(220.0 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 7.0 * units.deg)
        .add_sample(220.0 * units.rpm, 7.0 * units.deg)
        .add_sample(440.0 * units.rpm, 7.0 * units.deg)
        .add_sample(660.0 * units.rpm, 7.4 * units.deg)
        .add_sample(880.0 * units.rpm, 8.9 * units.deg)
        .add_sample(1100.0 * units.rpm, 10.4 * units.deg)
        .add_sample(1320.0 * units.rpm, 11.9 * units.deg)
        .add_sample(1540.0 * units.rpm, 13.4 * units.deg)
        .add_sample(1760.0 * units.rpm, 14.9 * units.deg)
        .add_sample(1980.0 * units.rpm, 16.4 * units.deg)
        .add_sample(2200.0 * units.rpm, 17.9 * units.deg)
        .add_sample(2420.0 * units.rpm, 19.4 * units.deg)
        .add_sample(2640.0 * units.rpm, 20.9 * units.deg)
        .add_sample(2860.0 * units.rpm, 22.0 * units.deg)
        .add_sample(3080.0 * units.rpm, 22.0 * units.deg)
        .add_sample(3300.0 * units.rpm, 22.0 * units.deg)
        .add_sample(3520.0 * units.rpm, -12.5 * units.deg)
        .add_sample(3740.0 * units.rpm, -25.0 * units.deg)

    engine
        .add_crankshaft(c0.c)
        .add_cylinder_bank(bank0.bank)
        .add_ignition_module(ignition)
}

label spacing_factor(1.2)
label flange_density(1.0 * exhaust_delay_coeff)
label em_unit(bore * spacing_factor / flange_density)

private node es0{
    output es0: exhaust0; output es1: exhaust0; output es2: exhaust0; output es3: exhaust0;
    output es4: exhaust0; output es5: exhaust0;
    output pl0: _pl0; output pl1: _pl1; output pl2: _pl2; output pl3: _pl3;
    output pl4: _pl4; output pl5: _pl5;
    output jitter: 0.7;
    output noise: 0.2;
    output hf_gain: 0.007;

    label collector_cross_section_area(41.5)
    label exhaust_pipe_length_0(450.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 100.0) // Litres

    float _pl0 ((5.7 * em_unit) * units.mm)
    float _pl1 ((4.7 * em_unit) * units.mm)
    float _pl2 ((3.7 * em_unit) * units.mm)
    float _pl3 ((3.0 * em_unit) * units.mm)
    float _pl4 ((2.0 * em_unit) * units.mm)
    float _pl5 ((1.0 * em_unit) * units.mm)

    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(867.1),
        collector_cross_section_area: 41.5 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 40.3 * units.cm,
        primary_flow_rate: k_carb(144.5),
        velocity_decay: 1.0,
        volume: (exhaust_volume_0 + 10.0) * units.L
    )

    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_58.wav", volume: 0.00033))
}


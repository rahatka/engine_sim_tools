import "engine_sim.mr"
import "../../fuel/10.mr"

units units()
constants constants()

label bore(76.2) // 3
label stroke(120.65) // 4 3/4
label compression_ratio(5.0)
label con_rod(241.3) // 9 1/2
label compression_height(58.7375 * units.mm) // 5.0
label intake_valve_diameter(37.3062) // 1 15/32 ; 1 17/32
label exhaust_valve_diameter(37.3062) // 1 15/32 ; 1 13/32
label intake_valves(1.0)
label exhaust_valves(1.0)

label cyl(8.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

label redline(3200.0) // RPM

// hupp 1928
label IVL(8.73125) // 11/32 ; .285
label EVL(8.73125) // 11/32 ; .285
label IVO(-4.0 * units.deg) //BTDC
label IVC(51.0 * units.deg) //ABDC
label EVO(47.0 * units.deg) //BBDC
label EVC(0.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(900.0) // ?
label piston_mass(467.767) // 16 1/2
label crank_mass(30.0) // ?
label flywheel_mass(7.0) // kg

label exhaust_delay_coeff(1.5)

public node variant {
    output s1: 0;
    output s4: 1;
    output s7: 2;
    output s3: 3;
    output s8: 4;
    output s5: 5;
    output s2: 6;
    output s6: 7;
}


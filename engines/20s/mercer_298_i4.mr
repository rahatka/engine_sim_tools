import "../../fuel/10.mr"

units units()
constants constants()

label bore(95.25) // 3 3/4
label stroke(171.45) // 6 3/4
label compression_ratio(4.0) // ?
label con_rod(365.125) // 14 3/8
label compression_height(98.425 / 2 * units.mm) // ?
label intake_valve_diameter(60.325) // 2 3/8 ; .37 stem
label exhaust_valve_diameter(60.325) // 2 3/8 ; .37 stem
label intake_valves(1.0)
label exhaust_valves(1.0)

label cyl(4.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

label redline(2200.0) // RPM

// 1920 Mercer
label IVL(9.525) // 3/8
label EVL(9.525) // 3/8
label IVO(-5.0 * units.deg) //BTDC
label IVC(55.0 * units.deg) //ABDC
label EVO(70.0 * units.deg) //BBDC
label EVC(15.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(1500.0) // ?
label piston_mass(800.0) // ?
label crank_mass(18.0) // ? kg
label flywheel_mass(4.0) // kg

label exhaust_delay_coeff(1.5)


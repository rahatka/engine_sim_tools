units units()
constants constants()

label bore(111.125) // 4 3/8
label stroke(120.65) // 4 3/4
label compression_ratio(4.24) // 4.16 ; 4.24 Я-7
label con_rod(241.3) // 9 1/2
label compression_height(122.174 / 2 * units.mm) // 4.81 len
label intake_valve_diameter(50.8) // 1.75 ; 2
label exhaust_valve_diameter(50.8) // 1.75 ; 2
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(2800.0) // RPM

label cyl(6.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// ?
label IVL(10.3187) // 3/8 ; 13/32
label EVL(10.3187) // 3/8 ; 13/32
label IVO(0.0 * units.deg) //BTDC
label IVC(45.0 * units.deg) //ABDC
label EVO(50.0 * units.deg) //BBDC
label EVC(10.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(1545.049) // 54.5
label piston_mass(2211.26) // 78
label crank_mass(28.0) // ?
label flywheel_mass(5.0) // kg

label exhaust_delay_coeff(1.5)


public node camshaft_builder {
    output intake_cam_0: _intake_cam_0;
    output exhaust_cam_0: _exhaust_cam_0;
    output intake_cam_1: _intake_cam_1;
    output exhaust_cam_1: _exhaust_cam_1;

    camshaft _intake_cam_0(base_radius: 17.1 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 17.1 * units.mm, lobe_profile: exhaust_lobe_profile)
    camshaft _intake_cam_1(base_radius: 17.1 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_1(base_radius: 17.1 * units.mm, lobe_profile: exhaust_lobe_profile)

    _intake_cam_0
        .add_lobe(ILC + 0 * rot)
        .add_lobe(ILC + 2 * rot)
        .add_lobe(ILC + 6 * rot)
        .add_lobe(ILC + 4 * rot)
    _exhaust_cam_0
        .add_lobe(ELC + 0 * rot)
        .add_lobe(ELC + 2 * rot)
        .add_lobe(ELC + 6 * rot)
        .add_lobe(ELC + 4 * rot)
        
    _intake_cam_1
        .add_lobe(ILC + 0 * rot + rot360 + rot90)
        .add_lobe(ILC + 2 * rot + rot360 + rot90)
        .add_lobe(ILC + 6 * rot + rot360 + rot90)
        .add_lobe(ILC + 4 * rot + rot360 + rot90)
    _exhaust_cam_1
        .add_lobe(ELC + 0 * rot + rot360 + rot90)
        .add_lobe(ELC + 2 * rot + rot360 + rot90)
        .add_lobe(ELC + 6 * rot + rot360 + rot90)
        .add_lobe(ELC + 4 * rot + rot360 + rot90)
}

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node head {
    // port_flow.py v1.7
    // intake port area: 8.8 cm²; saturated lift: 8.18 mm
    // exhaust port area: 8.8 cm²; saturated lift: 8.18 mm
    // cylinder volume: 494.8 cm³ (30.20 CI); engine volume: 3.959 L (241.57 CI)
    // chamber volume: 116.4 cm³ (7.11 CI); conrod/stroke 2.400; N.A.C.C. 31.25
    // 16 harmonic intake runner length: 23.9 cm; diameter: 3.3 cm
    // primary length: 32.3 cm, area: 9.3 cm², diameter: 3.4 cm
    // collector diameter: 5.3 cm, area: 22.3 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 116.431 * units.cc,
        intake_runner_volume: 205.5 * units.cc,
        intake_runner_cross_section_area: 8.6 * units.cm2,
        exhaust_runner_volume: 68.5 * units.cc,
        exhaust_runner_cross_section_area: 9.3 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "Lancia Dilambda V8",
        starter_torque: 75 * units.Nm,
        starter_speed: 400 * units.rpm,
        redline: redline * units.rpm,
        fuel: s10(),
        throttle_gamma: 1.5,
        jitter: 0.7,
        noise: 0.1,
        hf_gain: 0.008,
        simulation_frequency: 10000 
    )

    wires wires()

    label crank_moment(disk_moment_of_inertia(mass: crank_mass * units.kg, radius: (stroke / 2.0) * units.mm))
    label flywheel_moment(disk_moment_of_inertia(mass: flywheel_mass * units.kg, radius: (stroke * 3.5) * units.mm))
    label other_moment(disk_moment_of_inertia(mass: 1 * units.kg, radius: 1.0 * units.cm))

    crankshaft c0(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: flywheel_mass * units.kg,
        mass: crank_mass * units.kg,
        friction_torque: 5.9 * units.Nm,
        moment_of_inertia: crank_moment + flywheel_moment + other_moment,
        tdc: 0.0
    )

    rod_journal rj0(angle: rot * 0 + rot90 + vee / 2)
    rod_journal rj1(angle: rot * 2 + rot90 + vee / 2)
    rod_journal rj2(angle: rot * 6 + rot90 + vee / 2)
    rod_journal rj3(angle: rot * 4 + rot90 + vee / 2)
    rod_journal rj4(angle: rot * 0 + rot360 + rot180 - vee / 2)
    rod_journal rj5(angle: rot * 2 + rot360 + rot180 - vee / 2)
    rod_journal rj6(angle: rot * 6 + rot360 + rot180 - vee / 2)
    rod_journal rj7(angle: rot * 4 + rot360 + rot180 - vee / 2)
    
    c0
        .add_rod_journal(rj4)
        .add_rod_journal(rj0)
        .add_rod_journal(rj5)
        .add_rod_journal(rj1)
        .add_rod_journal(rj6)
        .add_rod_journal(rj2)
        .add_rod_journal(rj7)
        .add_rod_journal(rj3)

    piston_parameters piston_params(
        mass: piston_mass * units.g,
        blowby: k_28inH2O(0.025),
        compression_height: compression_height,
        wrist_pin_position: 0.0,
        displacement: 0.0
    )

    connecting_rod_parameters cr_params(
        mass: con_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.g,
            length: con_rod * units.mm
        ),
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    label collector_cross_section_area(11.1)

    label exhaust_pipe_length_0(315.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 100.0) // Litres
    
    label exhaust_pipe_length_1(300.0) // cm
    label exhaust_volume_1(collector_cross_section_area * exhaust_pipe_length_1 / 100.0) // Litres
    
    label spacing_factor(1.8)
    label flange_density(1.0 * exhaust_delay_coeff)
    

    intake intake(
        plenum_volume: 2.4 * units.L,
        plenum_cross_section_area: 34.5 * units.cm2,
        runner_length: 23.9 * units.cm,
        intake_flow_rate: k_carb(287.4),
        idle_flow_rate: k_carb(0.02),
        idle_throttle_plate_position: 0.9965,
        runner_flow_rate: k_carb(73.7),
        velocity_decay: 0.05
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(242.5),
        collector_cross_section_area: 11.1 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 32.3 * units.cm,
        primary_flow_rate: k_carb(122.8),
        velocity_decay: 1.0,
        volume: (exhaust_volume_0 + 3.0) * units.L
    )
    
    exhaust_system_parameters es_params1(
        outlet_flow_rate: k_carb(242.5),
        collector_cross_section_area: 11.1 * units.cm2,
        length: exhaust_pipe_length_1 * units.cm,
        primary_tube_length: 32.3 * units.cm,
        primary_flow_rate: k_carb(122.8),
        velocity_decay: 1.0,
        volume: (exhaust_volume_1 + 3.0) * units.L
    )

    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_56.wav", volume: 0.00025))
    exhaust_system exhaust1(es_params1, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_57.wav", volume: 0.00025))
 
    cylinder_bank b0(bank_params, angle: vee / 2)
    cylinder_bank b1(bank_params, angle: -vee / 2)

    label pl0((1.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl1((2.3 * bore * spacing_factor / flange_density) * units.mm)
    label pl2((2.7 * bore * spacing_factor / flange_density) * units.mm)
    label pl3((4.0 * bore * spacing_factor / flange_density) * units.mm)

    b0
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj0,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl0,
            ignition_wire: wires.wire1
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj1,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl1,
            ignition_wire: wires.wire2
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj2,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl2,
            ignition_wire: wires.wire3
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj3,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl3,
            ignition_wire: wires.wire4
        )
    b1
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj4,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl0,
            ignition_wire: wires.wire5
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj5,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl1,
            ignition_wire: wires.wire6
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj6,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl2,
            ignition_wire: wires.wire7
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj7,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl3,
            ignition_wire: wires.wire8
        )

    engine
        .add_cylinder_bank(b0)
        .add_cylinder_bank(b1)

    engine.add_crankshaft(c0)

    camshaft_builder camshaft()

    b0.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_0,
            exhaust_camshaft: camshaft.exhaust_cam_0,
            flip_display: true
        )
    )
    
    b1.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_1,
            exhaust_camshaft: camshaft.exhaust_cam_1
        )
    )

    function timing_curve(283.3 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 8.0 * units.deg)
        .add_sample(283.3 * units.rpm, 8.0 * units.deg)
        .add_sample(566.7 * units.rpm, 9.0 * units.deg)
        .add_sample(850.0 * units.rpm, 13.0 * units.deg)
        .add_sample(1133.3 * units.rpm, 16.5 * units.deg)
        .add_sample(1416.7 * units.rpm, 19.6 * units.deg)
        .add_sample(1700.0 * units.rpm, 22.3 * units.deg)
        .add_sample(1983.3 * units.rpm, 24.6 * units.deg)
        .add_sample(2266.7 * units.rpm, 26.7 * units.deg)
        .add_sample(2550.0 * units.rpm, 28.5 * units.deg)
        .add_sample(2833.3 * units.rpm, 30.0 * units.deg)
        .add_sample(3116.7 * units.rpm, 31.4 * units.deg)
        .add_sample(3400.0 * units.rpm, 32.6 * units.deg)
        .add_sample(3683.3 * units.rpm, 33.0 * units.deg)
        .add_sample(3966.7 * units.rpm, 33.0 * units.deg)
        .add_sample(4250.0 * units.rpm, 33.0 * units.deg)
        .add_sample(4533.3 * units.rpm, -12.5 * units.deg)
        .add_sample(4816.7 * units.rpm, -25.0 * units.deg)

    engine.add_ignition_module(
        eng_distributor(
            wires: wires,
            timing_curve: timing_curve
        )
    )
}

// Dilambda
public node veh {
    alias output __out: vehicle;
    vehicle vehicle(
        mass: 2100 * units.kg,
        drag_coefficient: 0.47,
        cross_sectional_area: (1747 * units.mm) * (1970 * units.mm),
        diff_ratio: 4.727,
        tire_radius: (792.0 / 2) * units.mm, // 16x50
        rolling_resistance: 20
        )
}

// Dilambda
private node trn {
    alias output __out:
        transmission(
            max_clutch_torque: 300 * units.Nm,
            max_clutch_flex: 30 * units.deg,
            limit_clutch_flex: true,
            clutch_stiffness: 10 * units.Nm / units.deg,
            clutch_damping: 1.0,
            simulate_flex: true
        )
        .add_gear(2.80)
        .add_gear(2.20)
        .add_gear(1.488)
        .add_gear(1.0);
}

run(
    engine: eng(),
    transmission: trn(),
    vehicle: veh()
)

import "engine_sim.mr"
import "../../fuel/10.mr"

units units()
constants constants()

label bore(82.55) // 3 1/4 ; 1922 3 3/8
label stroke(127.0) // 5
label compression_ratio(4.0) // ?
label con_rod(292.1) // 11 1/2
label compression_height(88.9 / 2 * units.mm) // 3 1/2 l
label intake_valve_diameter(44.45) // 1920 1 11/32 c ; 3/8 stem
label exhaust_valve_diameter(44.45) // 1920 1 11/32 c ; 3/8 stem
label intake_valves(1.0)
label exhaust_valves(1.0)

label cyl(4.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

label redline(2700.0) // RPM

// Nash 4 only lifts
label IVL(9.12812) // 23/64
label EVL(9.12812) // 23/64
label IVO(-5.0 * units.deg) //BTDC
label IVC(45.0 * units.deg) //ABDC
label EVO(45.0 * units.deg) //BBDC
label EVC(5.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(992.233) // 35
label piston_mass(725.7478) // ?
label crank_mass(18.0) // ? kg
label flywheel_mass(4.0) // kg

label exhaust_delay_coeff(1.5)

public node variant_0 {
    output s1: 0;
    output s3: 1;
    output s4: 2;
    output s2: 3;
}


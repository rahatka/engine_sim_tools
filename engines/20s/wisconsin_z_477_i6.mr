import "engine_sim.mr"
import "../../fuel/10.mr"

units units()
constants constants()

label bore(114.3) // 4 1/2
label stroke(127.0) // 5
label compression_ratio(4.5)
label con_rod(266.7) // 10 1/2
label compression_height(120.65 / 2 * units.mm) // 4 3/4 len
label intake_valve_diameter(52.3875) // 2 1/16
label exhaust_valve_diameter(52.3875) // 2 1/16
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(2600.0) // RPM

label cyl(6.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// Wisconsin Z
label IVL(9.525) // 3/8
label EVL(9.525) // 3/8
label IVO(-5.0 * units.deg) //BTDC
label IVC(60.0 * units.deg) //ABDC
label EVO(45.0 * units.deg) //BBDC
label EVC(5.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(1842.72) // 65 oz
label piston_mass(800.0) // ?
label crank_mass(28.0) // ?
label flywheel_mass(5.0) // kg

label exhaust_delay_coeff(1.5)


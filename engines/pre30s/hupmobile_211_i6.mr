import "engine_sim.mr"
import "../../fuel/10.mr"

units units()
constants constants()

label bore(82.55) // 3 1/4
label stroke(107.95) // 4 1/4
label compression_ratio(4.8)
label con_rod(222.25) // 8 3/4
label compression_height(43.18 * units.mm) // ?
label intake_valve_diameter(38.1) // 1 1/2 ; 1 17/32
label exhaust_valve_diameter(38.1) // 1 1/2 ; 1 17/32
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(3300.0) // RPM

label cyl(6.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// 1928 hupp 6
label IVL(7.9375) // 5/16
label EVL(7.9375) // 5/16
label IVO(-4.0 * units.deg) //BTDC
label IVC(51.0 * units.deg) //ABDC
label EVO(47.0 * units.deg) //BBDC
label EVC(0.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(800.0) // ?
label piston_mass(538.641) // ?
label crank_mass(30.0) // ?
label flywheel_mass(5.0) // kg

label exhaust_delay_coeff(1.5)

public node variant {
    output s1: 0;
    output s5: 1;
    output s3: 2;
    output s6: 3;
    output s2: 4;
    output s4: 5;
}


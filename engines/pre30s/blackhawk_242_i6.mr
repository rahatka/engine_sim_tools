import "../../fuel/10.mr"

units units()
constants constants()

label bore(85.725) // 3 3/8
label stroke(114.3) // 4 1/2
label compression_ratio(5.25)
label con_rod(234.95) // 9 1/4
label compression_height(61.5156 * units.mm) // 2 27/64
label intake_valve_diameter(42.0687) // 1 21/32
label exhaust_valve_diameter(42.0687) // 1 21/32
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(3400.0) // RPM

label cyl(6.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// 1929 Blackhawk
label IVL(8.73125) // 11/32
label EVL(8.73125) // 11/32
label IVO(-7.0 * units.deg) //BTDC
label IVC(47.0 * units.deg) //ABDC
label EVO(49.0 * units.deg) //BBDC
label EVC(7.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(1020.58) // 36
label piston_mass(765.437) // 27
label crank_mass(25.0) // ?
label flywheel_mass(4.0) // kg

label exhaust_delay_coeff(1.5)


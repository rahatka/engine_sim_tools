import "engine_sim.mr"
import "../../fuel/10.mr"

units units()
constants constants()

label bore(82.55) // 3 1/4
label stroke(101.6) // 4
label compression_ratio(4.1)
label con_rod(203.2) // 8
label compression_height(46.736 * units.mm) // 1.84
label intake_valve_diameter(38.1) // W-6 1 1/2
label exhaust_valve_diameter(33.3375) // W-6 1 5/16
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(3000.0) // RPM

label cyl(8.0)
label row(2.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label vee(60.0 * units.deg)

label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)

// Wills Ste. Claire 68
label IVL(7.9375) // 5/16 ?
label EVL(7.9375) // 5/16 ?
label IVO(-10.0 * units.deg) //BTDC
label IVC(35.0 * units.deg) //ABDC
label EVO(40.0 * units.deg) //BBDC
label EVC(5.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(666.2137934) // 23.5 oz
label piston_mass(793.78665) // 28 oz
label crank_mass(25.0) // ? kg
label flywheel_mass(6.0) // kg

label exhaust_delay_coeff(1.5)


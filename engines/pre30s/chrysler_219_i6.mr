import "engine_sim.mr"
import "../../fuel/10.mr"

units units()
constants constants()

label bore(79.375) // 3 1/8
label stroke(120.65) // 4 3/4
label compression_ratio(4.8)
label con_rod(254.0) // 10
label compression_height(50.8 * units.mm) // 2
label intake_valve_diameter(36.5125) // 1 7/16 ; 1.594 1930
label exhaust_valve_diameter(36.5125) // 1 7/16 ; 1.531 1930
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(3200.0) // RPM

label cyl(6.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// 1927 Chr
label IVL(7.9375) // 5/16
label EVL(7.9375) // 5/16
label IVO(-6.0 * units.deg) //BTDC
label IVC(46.0 * units.deg) //ABDC
label EVO(42.0 * units.deg) //BBDC
label EVC(8.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(1105.63) // 39
label piston_mass(411.068) // 14 1/2
label crank_mass(25.0) // ?
label flywheel_mass(5.0) // kg

label exhaust_delay_coeff(1.5)


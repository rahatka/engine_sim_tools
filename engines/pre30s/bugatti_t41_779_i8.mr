import "engine_sim.mr"
import "../../fuel/30.mr"

units units()
constants constants()

label bore(125.0)
label stroke(130.0)
label compression_ratio(5.0) // 6.4 train
label con_rod(265.0) // ~

label compression_height(bore / 2 * units.mm) // ~
label intake_valve_diameter(35.0) // 1.35 ? ; 1.378 other source
label exhaust_valve_diameter(52.0) // 1.98 ? ; 2.047 other source
label intake_valves(2.0)
label exhaust_valves(1.0)

label redline(2250.0) // RPM

label cyl(8.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// Bugatti U16
label IVL(12.0) // 12mm ; .653 other source
label EVL(12.0) // 12mm ; .7 other source
label IVO(0.0 * units.deg) //BTDC
label IVC(45.0 * units.deg) //ABDC
label EVO(45.0 * units.deg) //BBDC
label EVC(15.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label piston_mass(1000.0) // g ?
label con_rod_mass(1587.0) // 3 lb 8 oz U16
label crank_mass(43.68) // 96 lb 5 oz U16
label flywheel_mass(15.0) // kg

label exhaust_delay_coeff(1.5)

// standard
public node variant_0 {
    output s1: 0;
    output s6: 1;
    output s2: 2;
    output s5: 3;
    output s8: 4;
    output s3: 5;
    output s7: 6;
    output s4: 7;
    output seq: "1-6-2-5-8-3-7-4";
}


import "engine_sim.mr"
import "../../fuel/10.mr"

units units()
constants constants()

label bore(76.2) // 3
label stroke(127.0) // 5
label compression_ratio(4.2) // ?
label con_rod(279.4) // 11
label compression_height(88.9 / 2 * units.mm) // 3 1/2 L
label intake_valve_diameter(38.1) // 1 3/8 clear ; .435 stem
label exhaust_valve_diameter(38.1) // 1 3/8 clear ; .435 stem
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(2300.0) // RPM

label cyl(8.0)
label row(2.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label vee(90.0 * units.deg)

label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)

// H-S V
label IVL(8.73125) // 11/32
label EVL(8.73125) // 11/32
label IVO(-8.0 * units.deg) //BTDC
label IVC(45.0 * units.deg) //ABDC
label EVO(45.0 * units.deg) //BBDC
label EVC(8.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(1100.0) // ?
label piston_mass(600.0) // ?
label crank_mass(20.0) // ? kg
label flywheel_mass(6.0) // kg

label exhaust_delay_coeff(1.5)

public node variant_0 {
    output s1: 0;
    output s3: 1;
    output s4: 2;
    output s2: 3;
}


import "engine_sim.mr"
import "../../fuel/10.mr"

units units()
constants constants()

label bore(67.4687) // 2 21/32
label stroke(114.3) // 4 1/2
label compression_ratio(5.9) // The compression volume is designed to be 17% of the total volume
label con_rod(228.6) // 9 ?
label compression_height(35.0 * units.mm) // ?
label intake_valve_diameter(36.5125) // 1 7/16 Automotive Industries  1916-05-04: Vol 34 Iss 18
label exhaust_valve_diameter(36.5125) // 1 7/16 Automotive Industries  1916-05-04: Vol 34 Iss 18
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(3500.0) // RPM

label cyl(12.0)
label row(2.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label vee(60.0 * units.deg)

label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)

// Packard 299
label IVL(8.636) // 0.34
label EVL(8.636) // 0.34
label IVO(-5.0 * units.deg) //BTDC
label IVC(40.0 * units.deg) //ABDC
label EVO(47.0 * units.deg) //BBDC
label EVC(5.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(900.0) // ? g
label piston_mass(500.0) // ?
label crank_mass(25.0) // ? kg
label flywheel_mass(8.0) // kg

label exhaust_delay_coeff(1.5)


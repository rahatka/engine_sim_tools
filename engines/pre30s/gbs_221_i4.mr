import "engine_sim.mr"
import "../../fuel/10.mr"

units units()
constants constants()

label bore(95.25) // 3 3/4
label stroke(127.0) // 5
label compression_ratio(5.348) // 12.7 CI chamber
label con_rod(260.35) // 10 1/4
label compression_height(95.25 / 2 * units.mm) // 3 3/4 L
label intake_valve_diameter(42.8625) // 1 11/16 c
label exhaust_valve_diameter(42.8625) // 1 11/16 c
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(2300.0) // RPM

label cyl(4.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// GBS 1914
label IVL(9.12812) // 23/64
label EVL(9.12812) // 23/64
label IVO(-10.0 * units.deg) //BTDC
label IVC(40.0 * units.deg) //ABDC
label EVO(50.0 * units.deg) //BBDC
label EVC(6.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(1387.993) // 3.06
label piston_mass(1020.583) // 2.25
label crank_mass(15.0) // ?
label flywheel_mass(5.0) // kg

label exhaust_delay_coeff(1.5)

public node variant_0 {
    output s1: 0;
    output s2: 1;
    output s4: 2;
    output s3: 3;
}


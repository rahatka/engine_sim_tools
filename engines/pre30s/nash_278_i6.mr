units units()
constants constants()

label bore(87.3125) // 3 7/16
label stroke(127.0) // 5
label compression_ratio(4.6)
label con_rod(254.0) // 10
label compression_height(41.275 * units.mm) // 1 5/8
label intake_valve_diameter(45.3898) // 1.787
label exhaust_valve_diameter(45.3898) // 1.787
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(2700.0) // RPM

label cyl(6.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// 1926 nash
label IVL(8.8392) // .348
label EVL(8.8392) // .348
label IVO(-15.0 * units.deg) //BTDC
label IVC(45.0 * units.deg) //ABDC
label EVO(45.0 * units.deg) //BBDC
label EVC(10.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(1318.25) // 46 1/2
label piston_mass(1119.81) // 39 1/2
label crank_mass(25.0) // ?
label flywheel_mass(4.0) // kg

label exhaust_delay_coeff(1.5)


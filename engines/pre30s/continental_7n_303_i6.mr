import "engine_sim.mr"
import "../../fuel/10.mr"

units units()
constants constants()

label bore(88.9) // 3 1/2
label stroke(133.35) // 5 1/4
label compression_ratio(4.0) // ?
label con_rod(260.35) // 10 1/4
label compression_height(95.25 / 2 * units.mm) // 3 3/4 L
label intake_valve_diameter(42.8625) // 1 11/16 ; 3/8 stem
label exhaust_valve_diameter(42.8625) // 1 11/16 ; 3/8 stem
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(2200.0) // RPM

label cyl(6.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// 1917 cont 7n
label IVL(8.61017) // 20/59
label EVL(8.61017) // 20/59
label IVO(-10.0 * units.deg) //BTDC
label IVC(28.0 * units.deg) //ABDC
label EVO(40.0 * units.deg) //BBDC
label EVC(14.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(1417.48) // 50
label piston_mass(850.486) // ?
label crank_mass(20.0) // ?
label flywheel_mass(5.0) // kg

label exhaust_delay_coeff(1.5)


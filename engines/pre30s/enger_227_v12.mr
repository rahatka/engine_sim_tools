units units()
constants constants()

label bore(66.675) // 2 5/8
label stroke(88.9) // 3 1/2
label compression_ratio(4.0) // ?
label con_rod(190.5) // 7 1/2 Automotive Industries  1915-09-30: Vol 33 Iss 14
label compression_height(73.025 / 2 * units.mm) // 2 7/8 length
label intake_valve_diameter(33.3375) // ~ 1 5/16 ?
label exhaust_valve_diameter(33.3375) // ~ 1 5/16 ?
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(3250.0) // RPM

label cyl(12.0)
label row(2.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label vee(60.0 * units.deg)

label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)

// ?
label IVL(7.9375) // 5/16 ?
label EVL(7.9375) // 5/16 ?
label IVO(0.0 * units.deg) //BTDC
label IVC(40.0 * units.deg) //ABDC
label EVO(40.0 * units.deg) //BBDC
label EVC(10.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(734.2526) // 25.9 oz Automotive Industries  1915-09-30: Vol 33 Iss 14
label piston_mass(600.0) // ?
label crank_mass(30.0) // ? kg
label flywheel_mass(18.1437) // 40 lb 14 in dia

label exhaust_delay_coeff(1.5)


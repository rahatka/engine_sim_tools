import "engine_sim.mr"
import "../../fuel/10.mr"

units units()
constants constants()

label bore(69.85) // 2 3/4
label stroke(120.65) // 4 3/4
label compression_ratio(4.94)
label con_rod(241.3) // 9 1/2
label compression_height(49.2125 * units.mm) // 1 15/16
label intake_valve_diameter(35.7188) // 1 13/32 ; Auburn 1 3/4 ; 1 3/8 Gardner
label exhaust_valve_diameter(35.7188) // 1 13/32 ; Auburn 1 1/4 
label intake_valves(1.0)
label exhaust_valves(1.0)

label cyl(8.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

label redline(3300.0) // RPM

// 1927 Elcar
label IVL(8.73125) // 11/32
label EVL(8.73125) // 11/32
label IVO(0.0 * units.deg) //BTDC
label IVC(45.0 * units.deg) //ABDC
label EVO(50.0 * units.deg) //BBDC
label EVC(10.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(1337.74) // 37 oz
label piston_mass(566.99046) // 20 oz
label crank_mass(45.0) // ?
label flywheel_mass(5.0) // kg

label exhaust_delay_coeff(1.5)

// standard
public node variant_0 {
    output s1: 0;
    output s6: 1;
    output s2: 2;
    output s5: 3;
    output s8: 4;
    output s3: 5;
    output s7: 6;
    output s4: 7;
    output seq: "1-6-2-5-8-3-7-4";
}


import "engine_sim.mr"
import "../../fuel/10.mr"

units units()
constants constants()

label bore(107.95) // 4 1/4
label stroke(133.35) // 5 1/4
label compression_ratio(3.75) // ?
label con_rod(279.4) // 11
label compression_height(127.0 / 2 * units.mm) // 5 l
label intake_valve_diameter(44.8469) // 1 49/64 ; 3/8 stem
label exhaust_valve_diameter(44.8469) // 1 49/64 ; 3/8 stem
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(1800.0) // RPM

label cyl(6.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// 1914 northway
label IVL(8.73125) // 11/32
label EVL(8.73125) // 11/32
label IVO(-15.0 * units.deg) //BTDC
label IVC(38.0 * units.deg) //ABDC
label EVO(45.0 * units.deg) //BBDC
label EVC(10.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(1200.0) // ?
label piston_mass(1000.0) // ?
label crank_mass(25.0) // ?
label flywheel_mass(5.0) // kg

label exhaust_delay_coeff(1.5)


import "engine_sim.mr"
import "../../fuel/10.mr"

units units()
constants constants()

label bore(90.4875) // 3 9/16
label stroke(120.65) // 4 3/4
label compression_ratio(5.15)
label con_rod(250.825) // ~ 9 7/8
label compression_height(64.6 * units.mm) // ~ 64.6
label intake_valve_diameter(25.4) // 1
label exhaust_valve_diameter(30.9562) // 1 7/32
label intake_valves(1.0)
label exhaust_valves(1.0)

label cyl(4.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

label redline(1700.0) // RPM

// Wolseley
label IVL(5.55625) // 7/32 ?
label EVL(6.35) // 1/4 ?
label IVO(-11.0 * units.deg) //BTDC
label IVC(19.0 * units.deg) //ABDC
label EVO(38.0 * units.deg) //BBDC
label EVC(7.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(1200.0) // ?
label piston_mass(1000.0) // ?
label crank_mass(18.0) // ? kg
label flywheel_mass(4.0) // kg

label exhaust_delay_coeff(1.5)

public node variant_0 {
    output s1: 0;
    output s3: 1;
    output s4: 2;
    output s2: 3;
}


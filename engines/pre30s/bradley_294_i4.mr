import "engine_sim.mr"
import "../../fuel/10.mr"

units units()
constants constants()

label bore(104.775) // 4 1/8
label stroke(139.7) // 5 1/2
label compression_ratio(4.828) // 19.2 CI chamber
label con_rod(304.8) // 12
label compression_height(127.0 / 2 * units.mm) // 5 1/2 L
label intake_valve_diameter(42.39) // 1914 1 1/4 x 1 3/4 = 2 3/16 area = 42.39 dia eq ; 1919 2 x 1 3/4 = 3 1/2 area = 53.62 dia eq
label exhaust_valve_diameter(42.39) // 1 1/4 x 1 3/4 1914 ; 2 x 1 3/4 1919
label intake_valves(1.0)
label exhaust_valves(1.0)

label cyl(4.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

label redline(2000.0) // RPM

// 1914 Bradley
label IVL(10.6) // rotary
label EVL(10.6) // rotary
label IVO(-13.0 * units.deg) //BTDC
label IVC(5.0 * units.deg) //ABDC
label EVO(20.0 * units.deg) //BBDC
label EVC(10.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(1899.42) // 6.2
label piston_mass(1899.42) // 4.38
label crank_mass(18.0) // ? kg
label flywheel_mass(4.0) // kg

label exhaust_delay_coeff(1.5)


units units()
constants constants()

label bore(73.025) // 2 7/8
label stroke(107.95) // 4 1/4
label compression_ratio(4.7)
label con_rod(215.9) // 8 1/2
label compression_height(bore / 1.66 * units.mm) // ?
label intake_valve_diameter(31.75) // 1 1/4
label exhaust_valve_diameter(31.75) // 1 1/4
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(2800.0) // RPM

label cyl(6.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// 1925 Cleveland
label IVL(7.9375) // 5/16
label EVL(7.9375) // 5/16
label IVO(2.0 * units.deg) //BTDC
label IVC(52.0 * units.deg) //ABDC
label EVO(53.0 * units.deg) //BBDC
label EVC(16.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(900.0) // ?
label piston_mass(715.0) // ?
label crank_mass(30.3) // ?
label flywheel_mass(5.0) // kg

label exhaust_delay_coeff(1.5)


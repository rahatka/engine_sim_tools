import "engine_sim.mr"
import "../../fuel/10.mr"

units units()
constants constants()

label bore(82.55) // 3 1/4
label stroke(133.35) // 5 1/4
label compression_ratio(4.0) // ?
label con_rod(323.85) // 12 3/4
label compression_height(bore / 2 * units.mm) // ?
label intake_valve_diameter(44.45) // 1 3/4
label exhaust_valve_diameter(44.45) // 1 3/4
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(3000.0) // ? RPM

label cyl(8.0)
label row(2.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label vee(90.0 * units.deg)

label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)

// lafayette
label IVL(9.525) // 3/8
label EVL(9.525) // 3/8
label IVO(0.0 * units.deg) //BTDC
label IVC(46.0 * units.deg) //ABDC
label EVO(46.0 * units.deg) //BBDC
label EVC(0.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(988.69) // 2 lb 2 7/8 oz forked ; 2 lb 2 3/8 oz single
label piston_mass(1204.85) // 2 1/2 lb + 2 1/2 oz
label crank_mass(13.83) // 30 1/2 lb
label flywheel_mass(6.0) // kg

label exhaust_delay_coeff(1.5)

public node variant_0 {
    output s1: 0;
    output s3: 1;
    output s4: 2;
    output s2: 3;
}


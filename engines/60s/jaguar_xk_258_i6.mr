import "engine_sim.mr"
import "../../fuel/50.mr"

units units()
constants constants()

label bore(92.075) // 3 5/8
label stroke(106.0)
label compression_ratio(7.8)
label con_rod(196.85) // 7 3/4
label compression_height(41.5 * units.mm)
label intake_valve_diameter(44.45) // 1 3/4 ; .31 stem
label exhaust_valve_diameter(41.275) // 1 5/8 ; .31 stem
label intake_valves(1.0)
label exhaust_valves(1.0)

label cyl(6.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)

label redline(5600.0) // RPM

// JAG1 OE Spec
label IVL(9.7)
label EVL(9.7)
label IVO(17.0 * units.deg) //BTDC
label IVC(59.0 * units.deg) //ABDC
label EVO(59.0 * units.deg) //BBDC
label EVC(17.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label piston_mass(500.0) // ?
label con_rod_mass(744.0) // ~
label crank_mass(19.0) // ? kg
label flywheel_mass(3.0) // kg

label exhaust_delay_coeff(1.5)


public node camshaft_builder {
    output intake_cam_0: _intake_cam_0;
    output exhaust_cam_0: _exhaust_cam_0;

    output intake_cam_1: _intake_cam_1;
    output exhaust_cam_1: _exhaust_cam_1;

    camshaft _intake_cam_0(base_radius: 21.6 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 21.6 * units.mm, lobe_profile: exhaust_lobe_profile)
    
    camshaft _intake_cam_1(base_radius: 21.6 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_1(base_radius: 21.6 * units.mm, lobe_profile: exhaust_lobe_profile)

    _intake_cam_0
        .add_lobe(ILC + 0 * rot)
        .add_lobe(ILC + 3 * rot)
        .add_lobe(ILC + 5 * rot)
        .add_lobe(ILC + 2 * rot)
    _exhaust_cam_0
        .add_lobe(ELC + 0 * rot)
        .add_lobe(ELC + 3 * rot)
        .add_lobe(ELC + 5 * rot)
        .add_lobe(ELC + 2 * rot)
        
    _intake_cam_1
        .add_lobe(ILC + 1 * rot)
        .add_lobe(ILC + 4 * rot)
        .add_lobe(ILC + 6 * rot)
        .add_lobe(ILC + 7 * rot)
    _exhaust_cam_1
        .add_lobe(ELC + 1 * rot)
        .add_lobe(ELC + 4 * rot)
        .add_lobe(ELC + 6 * rot)
        .add_lobe(ELC + 7 * rot)
}

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node head {
    // port_flow.py v1.5
    // intake port area: 25.3 cm²; saturated lift: 9.78 mm
    // exhaust port area: 16.8 cm²; saturated lift: 7.97 mm
    // cylinder volume: 591.0 cm³ (36.1 CI); engine volume: 4.728 L (288.5 CI)
    // chamber volume: 65.7 cm³ (4.0 CI)
    // 8 harmonic intake runner length: 21.7 cm; diameter: 5.0 cm
    // primary length: 19.5 cm, area: 17.6 cm², diameter: 4.7 cm
    // collector diameter: 9.5 cm, area: 70.5 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 65.667 * units.cc,
        intake_runner_volume: 433.9 * units.cc,
        intake_runner_cross_section_area: 20.0 * units.cm2,
        exhaust_runner_volume: 144.6 * units.cc,
        exhaust_runner_cross_section_area: 17.6 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "Ford 289 V8",
        starter_torque: 100 * units.Nm,
        starter_speed: 500 * units.rpm,
        redline: redline * units.rpm,
        fuel: r50(),
        throttle_gamma: 1.5,
        jitter: 0.8,
        noise: 0.1,
        hf_gain: 0.008,
        simulation_frequency: 12000
    )

    wires wires()

    label crank_moment(disk_moment_of_inertia(mass: crank_mass * units.kg, radius: (stroke / 2.0) * units.mm))
    label flywheel_moment(disk_moment_of_inertia(mass: flywheel_mass * units.kg, radius: (stroke * 3.5) * units.mm))
    label other_moment(disk_moment_of_inertia(mass: 1 * units.kg, radius: 1.0 * units.cm))

    crankshaft c0(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: flywheel_mass * units.kg,
        mass: crank_mass * units.kg,
        friction_torque: 7.1 * units.Nm,
        moment_of_inertia: crank_moment + flywheel_moment + other_moment,
        tdc: 0.0
    )

    rod_journal rj0(angle: rot * 0 + 90.0 * units.deg + vee / 2)
    rod_journal rj1(angle: rot * 3 + 90.0 * units.deg + vee / 2)
    rod_journal rj2(angle: rot * 5 + 90.0 * units.deg + vee / 2)
    rod_journal rj3(angle: rot * 2 + 90.0 * units.deg + vee / 2)
    
    c0
        .add_rod_journal(rj0)
        .add_rod_journal(rj1)
        .add_rod_journal(rj2)
        .add_rod_journal(rj3)

    piston_parameters piston_params(
        mass: piston_mass * units.g,
        blowby: k_28inH2O(0.032),
        compression_height: compression_height,
        wrist_pin_position: 0.0,
        displacement: 0.0
    )

    connecting_rod_parameters cr_params(
        mass: con_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.g,
            length: con_rod * units.mm
        ),
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )
    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    label collector_cross_section_area(35.3)

    label exhaust_pipe_length_0(150.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 100.0) // Litres
    
    label exhaust_pipe_length_1(150.0) // cm
    label exhaust_volume_1(collector_cross_section_area * exhaust_pipe_length_1 / 100.0) // Litres

    intake intake(
        plenum_volume: 5.6 * units.L,
        plenum_cross_section_area: 79.9 * units.cm2,
        runner_length: 21.7 * units.cm,
        intake_flow_rate: k_carb(664.9),
        idle_flow_rate: k_carb(0.02),
        idle_throttle_plate_position: 0.997,
        runner_flow_rate: k_carb(187.0),
        velocity_decay: 0.05
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(588.7),
        collector_cross_section_area: 35.3 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 19.5 * units.cm,
        primary_flow_rate: k_carb(312.7),
        velocity_decay: 0.88,
        volume: exhaust_volume_0 * units.L
    )
    
    exhaust_system_parameters es_params1(
        outlet_flow_rate: k_carb(588.7),
        collector_cross_section_area: 35.3 * units.cm2,
        length: exhaust_pipe_length_1 * units.cm,
        primary_tube_length: 19.5 * units.cm,
        primary_flow_rate: k_carb(312.7),
        velocity_decay: 0.88,
        volume: exhaust_volume_1 * units.L
    )

    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_52.wav", volume: 0.00025))
    exhaust_system exhaust1(es_params1, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_52.wav", volume: 0.00025))
 
    cylinder_bank b0(bank_params, angle: vee / 2)
    cylinder_bank b1(bank_params, angle: -vee / 2)
    
    // GT40
    label es_diff(1.5)
    label spacing_factor(1.1)
    label flange_density(1.1 * exhaust_delay_coeff)

    label pl0 ((3.5 * bore * spacing_factor / flange_density) * units.mm) // 1
    label pl1 (((2.5 + es_diff) * bore * spacing_factor / flange_density) * units.mm) // 2
    label pl2 (((2.0 + es_diff) * bore * spacing_factor / flange_density) * units.mm) // 2
    label pl3 ((1.0 * bore * spacing_factor / flange_density) * units.mm) // 1
    
    label pl4 (((3.5 + es_diff) * bore * spacing_factor / flange_density) * units.mm) // 2
    label pl5 ((2.5 * bore * spacing_factor / flange_density) * units.mm) // 1
    label pl6 ((2.0 * bore * spacing_factor / flange_density) * units.mm) // 1
    label pl7 (((1.0 + es_diff) * bore * spacing_factor / flange_density) * units.mm) // 2

    b0
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj0,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl0,
            ignition_wire: wires.wire1
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj1,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl1,
            ignition_wire: wires.wire2
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj2,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl2,
            ignition_wire: wires.wire3
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj3,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl3,
            ignition_wire: wires.wire4
        )
        
    b1
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj0,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl4,
            ignition_wire: wires.wire5
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj1,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl5,
            ignition_wire: wires.wire6
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj2,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl6,
            ignition_wire: wires.wire7
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj3,
            intake: intake,
            exhaust_system: exhaust1,
            primary_length: pl7,
            ignition_wire: wires.wire8
        )

    engine
        .add_cylinder_bank(b0)
        .add_cylinder_bank(b1)

    engine.add_crankshaft(c0)

    camshaft_builder camshaft()

    b0.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_0,
            exhaust_camshaft: camshaft.exhaust_cam_0,
            flip_display: true
        )
    )
    b1.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_1,
            exhaust_camshaft: camshaft.exhaust_cam_1
        )
    )

    function timing_curve(500.0 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 3.0 * units.deg)
        .add_sample(500.0 * units.rpm, 3.0 * units.deg)
        .add_sample(1000.0 * units.rpm, 9.5 * units.deg)
        .add_sample(1500.0 * units.rpm, 15.8 * units.deg)
        .add_sample(2000.0 * units.rpm, 20.6 * units.deg)
        .add_sample(2500.0 * units.rpm, 24.3 * units.deg)
        .add_sample(3000.0 * units.rpm, 27.1 * units.deg)
        .add_sample(3500.0 * units.rpm, 29.2 * units.deg)
        .add_sample(4000.0 * units.rpm, 30.8 * units.deg)
        .add_sample(4500.0 * units.rpm, 32.1 * units.deg)
        .add_sample(5000.0 * units.rpm, 33.0 * units.deg)
        .add_sample(5500.0 * units.rpm, 33.0 * units.deg)
        .add_sample(6000.0 * units.rpm, 33.0 * units.deg)
        .add_sample(6500.0 * units.rpm, 33.0 * units.deg)
        .add_sample(7000.0 * units.rpm, 33.0 * units.deg)
        .add_sample(7500.0 * units.rpm, 33.0 * units.deg)
        .add_sample(8000.0 * units.rpm, -15.0 * units.deg)
        .add_sample(8500.0 * units.rpm, -45.0 * units.deg)

    engine.add_ignition_module(
        eng_distributor(
            wires: wires,
            timing_curve: timing_curve
        )
    )
}

// Mk I
public node veh {
    alias output __out: vehicle;
    vehicle vehicle(
        mass: (953 + 100) * units.kg,
        drag_coefficient: 0.39,
        cross_sectional_area: (1778 * units.mm) * (1029 * units.mm),
        diff_ratio: 3.55,
        tire_radius: (881.38 / 2) * units.mm, // 185/70 R14
        rolling_resistance: 20
        )
}

// Mk I
private node trn {
    alias output __out:
        transmission(
            max_clutch_torque: 500 * units.Nm,
            max_clutch_flex: 30 * units.deg,
            limit_clutch_flex: true,
            clutch_stiffness: 10 * units.Nm / units.deg,
            clutch_damping: 1.0,
            simulate_flex: true
        )
        .add_gear(2.50)
        .add_gear(1.70)
        .add_gear(1.29)
        .add_gear(1.00);
}

run(
    engine: eng(),
    transmission: trn(),
    vehicle: veh()
)

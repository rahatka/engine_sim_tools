public node camshaft_builder {
    output intake_cam_0: _intake_cam_0;
    output exhaust_cam_0: _exhaust_cam_0;

    camshaft _intake_cam_0(base_radius: 16.6 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 16.6 * units.mm, lobe_profile: exhaust_lobe_profile)

    _intake_cam_0
        .add_lobe(ILC + 0 * rot)
        .add_lobe(ILC + 1 * rot)
        .add_lobe(ILC + 3 * rot)
        .add_lobe(ILC + 2 * rot)
    _exhaust_cam_0
        .add_lobe(ELC + 0 * rot)
        .add_lobe(ELC + 1 * rot)
        .add_lobe(ELC + 3 * rot)
        .add_lobe(ELC + 2 * rot)
}

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node head {
    // port_flow.py v1.7
    // intake port area: 12.8 cm²; saturated lift: 9.80 mm
    // exhaust port area: 7.5 cm²; saturated lift: 7.51 mm
    // cylinder volume: 611.6 cm³ (37.32 CI); engine volume: 2.446 L (149.28 CI)
    // chamber volume: 84.9 cm³ (5.18 CI); conrod/stroke 1.826; N.A.C.C. 20.99
    // 16 harmonic intake runner length: 19.6 cm; diameter: 4.0 cm
    // primary length: 27.4 cm, area: 8.2 cm², diameter: 3.2 cm
    // collector diameter: 4.1 cm, area: 13.2 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 84.942 * units.cc,
        intake_runner_volume: 240.5 * units.cc,
        intake_runner_cross_section_area: 12.3 * units.cm2,
        exhaust_runner_volume: 80.2 * units.cc,
        exhaust_runner_cross_section_area: 8.2 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "ZMZ-24",
        starter_torque: 100 * units.Nm,
        starter_speed: 300 * units.rpm,
        redline: redline * units.rpm,
        fuel: s50(),
        throttle_gamma: 1.5,
        jitter: 0.7,
        noise: 0.2,
        hf_gain: 0.002,
        simulation_frequency: 12000 
    )

    wires wires()

    connecting_rod_parameters cr_params(
        mass: con_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.g,
            length: con_rod * units.mm
        ),
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    label crank_moment(disk_moment_of_inertia(mass: crank_mass * units.kg, radius: (stroke / 2.0) * units.mm))
    label flywheel_moment(disk_moment_of_inertia(mass: flywheel_mass * units.kg, radius: (stroke * 2.7) * units.mm))
    label other_moment(disk_moment_of_inertia(mass: 1 * units.kg, radius: 1.0 * units.cm))

    crankshaft c0(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: flywheel_mass * units.kg,
        mass: crank_mass * units.kg,
        friction_torque: 3.7 * units.Nm,
        moment_of_inertia: crank_moment + flywheel_moment + other_moment,
        tdc: 0.0
    )

    rod_journal rj0(angle: rot * 0 + rot90)
    rod_journal rj1(angle: rot * 1 + rot90)
    rod_journal rj2(angle: rot * 3 + rot90)
    rod_journal rj3(angle: rot * 2 + rot90)
    
    c0
        .add_rod_journal(rj0)
        .add_rod_journal(rj1)
        .add_rod_journal(rj2)
        .add_rod_journal(rj3)

    piston_parameters piston_params(
        mass: piston_mass * units.g,
        blowby: k_28inH2O(0.029),
        compression_height: compression_height,
        wrist_pin_position: 0.0,
        displacement: 0.0
    )

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    label collector_cross_section_area(13.2)

    label exhaust_pipe_length_0(200.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 100.0) // Litres
    
    label spacing_factor(1.2)
    label flange_density(1.0 * exhaust_delay_coeff)
    

    intake intake(
        plenum_volume: 1.3 * units.L,
        plenum_cross_section_area: 24.6 * units.cm2,
        runner_length: 19.6 * units.cm,
        intake_flow_rate: k_carb(163.1),
        idle_flow_rate: k_carb(0.02),
        idle_throttle_plate_position: 0.995,
        runner_flow_rate: k_carb(81.5),
        velocity_decay: 0.05
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(244.6),
        collector_cross_section_area: 13.2 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 27.4 * units.cm,
        primary_flow_rate: k_carb(122.3),
        velocity_decay: 1.0,
        volume: exhaust_volume_0 * units.L
    )

    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_40.wav", volume: 0.00025))
 
    cylinder_bank b0(bank_params, angle: 0)

    // ЗМЗ-24
    label pl0((3.2 * bore * spacing_factor / flange_density) * units.mm)
    label pl1((1.2 * bore * spacing_factor / flange_density) * units.mm)
    label pl2((1.1 * bore * spacing_factor / flange_density) * units.mm)
    label pl3((2.2 * bore * spacing_factor / flange_density) * units.mm)

    b0
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj0,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl0,
            ignition_wire: wires.wire1
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj1,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl1,
            ignition_wire: wires.wire2
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj2,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl2,
            ignition_wire: wires.wire3
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj3,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl3,
            ignition_wire: wires.wire4
        )

    engine
        .add_cylinder_bank(b0)

    engine.add_crankshaft(c0)

    camshaft_builder camshaft()

    b0.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_0,
            exhaust_camshaft: camshaft.exhaust_cam_0,
            flip_display: true
        )
    )

    function timing_curve(333.3 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 2.0 * units.deg)
        .add_sample(333.3 * units.rpm, 2.0 * units.deg)
        .add_sample(666.7 * units.rpm, 5.2 * units.deg)
        .add_sample(1000.0 * units.rpm, 10.4 * units.deg)
        .add_sample(1333.3 * units.rpm, 14.1 * units.deg)
        .add_sample(1666.7 * units.rpm, 16.8 * units.deg)
        .add_sample(2000.0 * units.rpm, 18.8 * units.deg)
        .add_sample(2333.3 * units.rpm, 20.2 * units.deg)
        .add_sample(2666.7 * units.rpm, 21.2 * units.deg)
        .add_sample(3000.0 * units.rpm, 22.0 * units.deg)
        .add_sample(3333.3 * units.rpm, 22.0 * units.deg)
        .add_sample(3666.7 * units.rpm, 22.0 * units.deg)
        .add_sample(4000.0 * units.rpm, 22.0 * units.deg)
        .add_sample(4333.3 * units.rpm, 22.0 * units.deg)
        .add_sample(4666.7 * units.rpm, 22.0 * units.deg)
        .add_sample(5000.0 * units.rpm, 22.0 * units.deg)
        .add_sample(5333.3 * units.rpm, -12.5 * units.deg)
        .add_sample(5666.7 * units.rpm, -25.0 * units.deg)

    engine.add_ignition_module(
        eng_distributor(
            wires: wires,
            timing_curve: timing_curve
        )
    )
}

// ГАЗ-24
public node veh {
    alias output __out: vehicle;

    label car_mass(1600) // 1420 empty; 1820 full

    vehicle vehicle(
        mass: car_mass * units.kg,
        drag_coefficient: 0.45,
        cross_sectional_area: (1800 * units.mm) * (1450 * units.mm),
        diff_ratio: 4.1,
        tire_radius: (668.0 / 2) * units.mm, // 7,35-14
        rolling_resistance: 0.012 * car_mass * 9.81
        )
}

// ГАЗ-24
private node trn {
    alias output __out:
        transmission(
            max_clutch_torque: 175 * units.Nm,
            max_clutch_flex: 30 * units.deg,
            limit_clutch_flex: true,
            clutch_stiffness: 10 * units.Nm / units.deg,
            clutch_damping: 1.0,
            simulate_flex: true
        )
        .add_gear(3.50)
        .add_gear(2.26)
        .add_gear(1.45)
        .add_gear(1.00);
}

run(
    engine: eng(),
    transmission: trn(),
    vehicle: veh()
)

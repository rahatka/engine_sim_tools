import "engine_sim.mr"
import "../../fuel/50.mr"

units units()
constants constants()

label bore(94.0)
label stroke(71.0)
label compression_ratio(8.0)
label con_rod(131.0) // 127 1.7-1.8
label compression_height(35.5 * units.mm)
label intake_valve_diameter(42.0) // 39.3 1.7 ; 41 1.8
label exhaust_valve_diameter(36.0) // 32.9 1.7 ; 34.0 1.8
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(5500.0)

label cyl(4.0)
label row(2.0)
label cycle(720.0 * units.deg)
label vee(180.0 * units.deg)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label rot(cycle / cyl)

// Type 4
label IVL(8.36)
label EVL(7.92)
label IVO(12.0 * units.deg) //BTDC
label IVC(42.0 * units.deg) //ABDC
label EVO(43.0 * units.deg) //BBDC
label EVC(4.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(750.0)
label piston_mass(500.0)
label crank_mass(8.0) // kg
label flywheel_mass(3.0) // kg

label exhaust_delay_coeff(1.5)

public node eng_distributor {
    input wires;
    input timing_curve;
    input rev_limit: 6000.0 * units.rpm;
    input limiter_duration: 0.1;
    alias output __out:
        ignition_module(timing_curve: timing_curve, rev_limit: rev_limit, limiter_duration: limiter_duration)
            .connect_wire(wires.wire1, rot * 0)
            .connect_wire(wires.wire2, rot * 1)
            .connect_wire(wires.wire3, rot * 2)
            .connect_wire(wires.wire4, rot * 3);
}

private node wires {
    output wire1: ignition_wire();
    output wire2: ignition_wire();
    output wire3: ignition_wire();
    output wire4: ignition_wire();
}


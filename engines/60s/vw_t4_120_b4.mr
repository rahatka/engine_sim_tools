public node camshaft_builder {
    output intake_cam_0: _intake_cam_0;
    output exhaust_cam_0: _exhaust_cam_0;

    output intake_cam_1: _intake_cam_1;
    output exhaust_cam_1: _exhaust_cam_1;

    camshaft _intake_cam_0(base_radius: 14.2 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 14.2 * units.mm, lobe_profile: exhaust_lobe_profile)
    
    camshaft _intake_cam_1(base_radius: 14.2 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_1(base_radius: 14.2 * units.mm, lobe_profile: exhaust_lobe_profile)

    _intake_cam_0
        .add_lobe(ILC + 0 * rot)
        .add_lobe(ILC + 1 * rot)
    _exhaust_cam_0
        .add_lobe(ELC + 0 * rot)
        .add_lobe(ELC + 1 * rot)
        
    _intake_cam_1
        .add_lobe(ILC + 2 * rot)
        .add_lobe(ILC + 3 * rot)
    _exhaust_cam_1
        .add_lobe(ELC + 2 * rot)
        .add_lobe(ELC + 3 * rot)
}

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node head {
    // port_flow.py v1.7
    // intake port area: 11.0 cm²; saturated lift: 9.15 mm
    // exhaust port area: 7.8 cm²; saturated lift: 7.58 mm
    // cylinder volume: 492.7 cm³ (30.07 CI); engine volume: 1.971 L (120.27 CI)
    // chamber volume: 70.4 cm³ (4.30 CI); conrod/stroke 1.845; N.A.C.C. 21.91
    // 16 harmonic intake runner length: 18.3 cm; diameter: 3.7 cm
    // primary length: 25.4 cm, area: 8.6 cm², diameter: 3.3 cm
    // collector diameter: 4.2 cm, area: 13.7 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 70.389 * units.cc,
        intake_runner_volume: 201.2 * units.cc,
        intake_runner_cross_section_area: 11.0 * units.cm2,
        exhaust_runner_volume: 67.1 * units.cc,
        exhaust_runner_cross_section_area: 8.6 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "Porsche 914 2.0",
        starter_torque: 50 * units.Nm,
        starter_speed: 500 * units.rpm,
        redline: redline * units.rpm,
        fuel: s50(),
        throttle_gamma: 1.5,
        jitter: 0.8,
        noise: 0.1,
        hf_gain: 0.005,
        simulation_frequency: 10000
    )

    wires wires()

    connecting_rod_parameters cr_params(
        mass: con_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.g,
            length: con_rod * units.mm
        ),
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    label crank_moment(disk_moment_of_inertia(mass: crank_mass * units.kg, radius: (stroke / 2.0) * units.mm))
    label flywheel_moment(disk_moment_of_inertia(mass: flywheel_mass * units.kg, radius: (stroke * 3.5) * units.mm))
    label other_moment(disk_moment_of_inertia(mass: 1 * units.kg, radius: 1.0 * units.cm))

    crankshaft c0(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: flywheel_mass * units.kg,
        mass: crank_mass * units.kg,
        friction_torque: 3.0 * units.Nm,
        moment_of_inertia: crank_moment + flywheel_moment + other_moment,
        tdc: 0.0
    )

    rod_journal rj0(angle: rot * 0 + rot90 + vee / 2)
    rod_journal rj1(angle: rot * 1 + rot90 + vee / 2)
    rod_journal rj2(angle: rot * 2 + rot90 - vee / 2)
    rod_journal rj3(angle: rot * 3 + rot90 - vee / 2)
    
    c0
        .add_rod_journal(rj0)
        .add_rod_journal(rj1)
        .add_rod_journal(rj2)
        .add_rod_journal(rj3)

    piston_parameters piston_params(
        mass: piston_mass * units.g,
        blowby: k_28inH2O(0.030),
        compression_height: compression_height,
        wrist_pin_position: 0.0,
        displacement: 0.0
    )

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    label collector_cross_section_area(13.7)

    label exhaust_pipe_length_0(120.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 100.0) // Litres
    
    intake intake(
        plenum_volume: 1.6 * units.L,
        plenum_cross_section_area: 22.0 * units.cm2,
        intake_flow_rate: k_carb(189.7),
        idle_flow_rate: k_carb(0.01),
        idle_throttle_plate_position: 0.996,
        runner_flow_rate: k_carb(99.6),
        runner_length: 18.3 * units.cm,
        velocity_decay: 0.05
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(324.1),
        collector_cross_section_area: 13.7 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 25.4 * units.cm,
        primary_flow_rate: k_carb(166.1),
        velocity_decay: 1.0,
        volume: (exhaust_volume_0 + 3.0) * units.L
    )

    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_56.wav", volume: 0.00025))
 
    cylinder_bank b0(bank_params, angle: vee / 2)
    cylinder_bank b1(bank_params, angle: -vee / 2)

    label spacing_factor(1.2)
    label flange_density(1.0 * exhaust_delay_coeff)
    label sh((70.0 / exhaust_delay_coeff) * units.cm)
    label pl0 ((2.7 * bore * spacing_factor / flange_density) * units.mm)
    label pl1 ((1.0 * bore * spacing_factor / flange_density) * units.mm)

    b0
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj0,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl0,
            ignition_wire: wires.wire1
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj2,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl1,
            ignition_wire: wires.wire2
        )
        
    b1
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj1,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl0 + sh,
            ignition_wire: wires.wire3
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj3,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl1 + sh,
            ignition_wire: wires.wire4
        )

    engine
        .add_cylinder_bank(b0)
        .add_cylinder_bank(b1)

    engine.add_crankshaft(c0)

    camshaft_builder camshaft()

    b0.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_0,
            exhaust_camshaft: camshaft.exhaust_cam_0,
            flip_display: true
        )
    )
    b1.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_1,
            exhaust_camshaft: camshaft.exhaust_cam_1
        )
    )

    function timing_curve(366.7 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 2.0 * units.deg)
        .add_sample(366.7 * units.rpm, 2.0 * units.deg)
        .add_sample(733.3 * units.rpm, 2.0 * units.deg)
        .add_sample(1100.0 * units.rpm, 4.4 * units.deg)
        .add_sample(1466.7 * units.rpm, 8.2 * units.deg)
        .add_sample(1833.3 * units.rpm, 11.3 * units.deg)
        .add_sample(2200.0 * units.rpm, 13.7 * units.deg)
        .add_sample(2566.7 * units.rpm, 15.7 * units.deg)
        .add_sample(2933.3 * units.rpm, 17.3 * units.deg)
        .add_sample(3300.0 * units.rpm, 18.6 * units.deg)
        .add_sample(3666.7 * units.rpm, 19.6 * units.deg)
        .add_sample(4033.3 * units.rpm, 20.5 * units.deg)
        .add_sample(4400.0 * units.rpm, 21.2 * units.deg)
        .add_sample(4766.7 * units.rpm, 21.7 * units.deg)
        .add_sample(5133.3 * units.rpm, 22.0 * units.deg)
        .add_sample(5500.0 * units.rpm, 22.0 * units.deg)
        .add_sample(5866.7 * units.rpm, -12.5 * units.deg)
        .add_sample(6233.3 * units.rpm, -25.0 * units.deg)

    engine.add_ignition_module(
        eng_distributor(
            wires: wires,
            timing_curve: timing_curve
        )
    )
}

// 914
public node veh {
    alias output __out: vehicle;
    vehicle vehicle(
        mass: 1000 * units.kg, // 900 empty
        drag_coefficient: 0.363,
        cross_sectional_area: (1650  * units.mm) * (1230 * units.mm),
        diff_ratio: 4.428,
        tire_radius: (599.0 / 2) * units.mm, // 205-50x15
        rolling_resistance: 20
        )
}

// 901 type
private node trn {
    alias output __out:
        transmission(
            max_clutch_torque: 250 * units.Nm,
            max_clutch_flex: 10 * units.deg,
            limit_clutch_flex: true,
            clutch_stiffness: 20 * units.Nm / units.deg,
            clutch_damping: 2.0,
            simulate_flex: true
        )
        .add_gear(3.09)
        .add_gear(1.89)
        .add_gear(1.26)
        .add_gear(0.93)
        .add_gear(0.71);
}

run(
    engine: eng(),
    transmission: trn(),
    vehicle: veh()
)

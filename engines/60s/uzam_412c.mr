import "engine_sim.mr"
import "_fuel_60.mr"

units units()
constants constants()

// Engine Sim V0.1.14A
// УЗАМ-412 Crossplane 75HP @5800
// uzam_412c
// Created by oror 2023
// 1-3-2-4

label bore(82.0)
label stroke(70.0)
label compression_ratio(8.8)
label con_rod(140.0)
label IVL(8.9)
label EVL(8.9)
label compression_height(40.0 * units.mm)
label intake_valve_diameter(38.0)
label exhaust_valve_diameter(34.0)
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(6250.0) // RPM

label cyl(4.0)
label row(1.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label slant(10.0 * units.deg)

// UZAM-412 valve timings
label IVO(30.0 * units.deg) //BTDC
label IVC(70.0 * units.deg) //ABDC
label EVO(70.0 * units.deg) //BBDC
label EVC(30.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(770.0) // g
label crank_mass(17.2) // kg
label flywheel_mass(5.0) // kg

label exhaust_delay_coeff(1.5)

public node eng_distributor {
    input wires;
    input timing_curve;
    input rev_limit: 7500 * units.rpm;
    input limiter_duration: 0.0001;
    alias output __out:
        ignition_module(timing_curve: timing_curve, rev_limit: rev_limit, limiter_duration: limiter_duration)
            .connect_wire(wires.wire1, 0.0 * rot)
            .connect_wire(wires.wire3, 1.5 * rot)
            .connect_wire(wires.wire2, 2.5 * rot)
            .connect_wire(wires.wire4, 3.0 * rot);
}

private node wires {
    output wire1: ignition_wire();
    output wire2: ignition_wire();
    output wire3: ignition_wire();
    output wire4: ignition_wire();
}


public node camshaft_builder {
    output intake_cam_0: _intake_cam_0;
    output exhaust_cam_0: _exhaust_cam_0;

    camshaft _intake_cam_0(base_radius: 15.6 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 15.6 * units.mm, lobe_profile: exhaust_lobe_profile)

    _intake_cam_0
        .add_lobe(ILC + 0.0 * rot)
        .add_lobe(ILC + 2.5 * rot)
        .add_lobe(ILC + 1.5 * rot)
        .add_lobe(ILC + 3.0 * rot)
    _exhaust_cam_0
        .add_lobe(ELC + 0.0 * rot)
        .add_lobe(ELC + 2.5 * rot)
        .add_lobe(ELC + 1.5 * rot)
        .add_lobe(ELC + 3.0 * rot)
}

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node head {
    // port_flow.py v1.5
    // intake port area: 8.9 cm²; saturated lift: 8.22 mm
    // exhaust port area: 7.2 cm²; saturated lift: 7.36 mm
    // cylinder volume: 369.7 cm³ (22.6 CI); engine volume: 1.479 L (90.2 CI)
    // chamber volume: 47.4 cm³ (2.9 CI)
    // 8 harmonic intake runner length: 28.6 cm; diameter: 3.5 cm
    // primary length: 25.1 cm, area: 7.9 cm², diameter: 3.2 cm
    // collector diameter: 3.5 cm, area: 9.4 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 47.394 * units.cc,
        intake_runner_volume: 273.5 * units.cc,
        intake_runner_cross_section_area: 9.6 * units.cm2,
        exhaust_runner_volume: 91.2 * units.cc,
        exhaust_runner_cross_section_area: 7.9 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "UZAM-412 CrossP (f)",
        starter_torque: 50 * units.Nm,
        starter_speed: 500 * units.rpm,
        redline: redline * units.rpm,
        fuel: s60(),
        throttle_gamma: 1.5,
        jitter: 0.7,
        noise: 0.1,
        hf_gain: 0.007,
        simulation_frequency: 12000
    )

    wires wires()

    connecting_rod_parameters cr_params(
        mass: con_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.g,
            length: con_rod * units.mm
        ),
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    label crank_moment(disk_moment_of_inertia(mass: crank_mass * units.kg, radius: (stroke / 2.0) * units.mm))
    label flywheel_moment(disk_moment_of_inertia(mass: flywheel_mass * units.kg, radius: (stroke * 2.5) * units.mm))
    label other_moment(disk_moment_of_inertia(mass: 1 * units.kg, radius: 1.0 * units.cm))

    crankshaft c0(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: flywheel_mass * units.kg,
        mass: crank_mass * units.kg,
        friction_torque: 2.2 * units.Nm,
        moment_of_inertia: crank_moment + flywheel_moment + other_moment,
        tdc: 0.0
    )

    rod_journal rj0(angle: rot * 0.0 + rot90 + slant)
    rod_journal rj1(angle: rot * 2.5 + rot90 + slant)
    rod_journal rj2(angle: rot * 1.5 + rot90 + slant)
    rod_journal rj3(angle: rot * 3.0 + rot90 + slant)
    
    c0
        .add_rod_journal(rj0)
        .add_rod_journal(rj1)
        .add_rod_journal(rj2)
        .add_rod_journal(rj3)

    piston_parameters piston_params(
        mass: 422.0 * units.g,
        blowby: k_28inH2O(0.026),
        compression_height: compression_height,
        wrist_pin_position: 0.0,
        displacement: 0.0
    )

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    label collector_cross_section_area(9.4)

    label exhaust_pipe_length_0(300.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 100.0) // Litres

    intake intake(
        plenum_volume: 1.4 * units.L,
        plenum_cross_section_area: 19.1 * units.cm2,
        runner_length: 28.6 * units.cm,
        intake_flow_rate: k_carb(161.7),
        idle_flow_rate: k_carb(0.01),
        idle_throttle_plate_position: 0.9975,
        runner_flow_rate: k_carb(84.9),
        velocity_decay: 0.05
    )

    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(276.3),
        collector_cross_section_area: 9.4 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 25.1 * units.cm,
        primary_flow_rate: k_carb(141.6),
        velocity_decay: 1.0,
        volume: (exhaust_volume_0 + 5.0) * units.L
    )
    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_55.wav", volume: 0.00025))
 
    cylinder_bank b0(bank_params, angle: slant)

    label spacing_factor(1.1)
    label flange_density(1.1 * exhaust_delay_coeff)

    label pl0((2.50 * bore * spacing_factor / flange_density) * units.mm)
    label pl1((1.75 * bore * spacing_factor / flange_density) * units.mm)
    label pl2((1.00 * bore * spacing_factor / flange_density) * units.mm)
    label pl3((1.75 * bore * spacing_factor / flange_density) * units.mm)

    b0
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj0,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl0,
            ignition_wire: wires.wire1
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj1,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl1,
            ignition_wire: wires.wire2
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj2,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl2,
            ignition_wire: wires.wire3
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj3,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl3,
            ignition_wire: wires.wire4
        )

    engine
        .add_cylinder_bank(b0)

    engine.add_crankshaft(c0)

    camshaft_builder camshaft()

    b0.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_0,
            exhaust_camshaft: camshaft.exhaust_cam_0,
            flip_display: true
        )
    )

    // ignition_timing.py v0.3 from = 5.0, to = 33.0, i = 500, r = 6250
    function timing_curve(625.0 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 0.0 * units.deg)
        .add_sample(625.0 * units.rpm, 5.0 * units.deg)
        .add_sample(1250.0 * units.rpm, 11.2 * units.deg)
        .add_sample(1875.0 * units.rpm, 16.3 * units.deg)
        .add_sample(2500.0 * units.rpm, 20.4 * units.deg)
        .add_sample(3125.0 * units.rpm, 23.7 * units.deg)
        .add_sample(3750.0 * units.rpm, 26.4 * units.deg)
        .add_sample(4375.0 * units.rpm, 28.6 * units.deg)
        .add_sample(5000.0 * units.rpm, 30.4 * units.deg)
        .add_sample(5625.0 * units.rpm, 31.8 * units.deg)
        .add_sample(6250.0 * units.rpm, 33.0 * units.deg)
        .add_sample(6875.0 * units.rpm, -15.0 * units.deg)
        .add_sample(7500.0 * units.rpm, -60.0 * units.deg)

    engine.add_ignition_module(
        eng_distributor(
            wires: wires,
            timing_curve: timing_curve
        )
    )
}

// M-412
public node veh {
    alias output __out: vehicle;
    vehicle vehicle(
        mass: 1045 * units.kg,
        drag_coefficient: 0.41,
        cross_sectional_area: (1550 * units.mm) * (1480 * units.mm),
        diff_ratio: 4.22,
        tire_radius: (610.0 / 2) * units.mm, // P175/80R13
        rolling_resistance: 20
        )
}

// M-412
private node trn {
    alias output __out:
        transmission(
            max_clutch_torque: 150 * units.Nm,
            max_clutch_flex: 30 * units.deg,
            limit_clutch_flex: true,
            clutch_stiffness: 10 * units.Nm / units.deg,
            clutch_damping: 1.0,
            simulate_flex: true
        )
        .add_gear(3.49)
        .add_gear(2.04)
        .add_gear(1.33)
        .add_gear(1.0);
}

run(
    engine: eng(),
    transmission: trn(),
    vehicle: veh()
)
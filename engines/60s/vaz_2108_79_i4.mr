public node camshaft_builder {
    output intake_cam_0: _intake_cam_0;
    output exhaust_cam_0: _exhaust_cam_0;

    camshaft _intake_cam_0(base_radius: 13.5 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 13.5 * units.mm, lobe_profile: exhaust_lobe_profile)

    _intake_cam_0
        .add_lobe(ILC + 0 * rot)
        .add_lobe(ILC + 3 * rot)
        .add_lobe(ILC + 1 * rot)
        .add_lobe(ILC + 2 * rot)
    _exhaust_cam_0
        .add_lobe(ELC + 0 * rot)
        .add_lobe(ELC + 3 * rot)
        .add_lobe(ELC + 1 * rot)
        .add_lobe(ELC + 2 * rot)
}

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node head {
    // port_flow.py v1.7
    // intake port area: 8.4 cm²; saturated lift: 7.93 mm
    // exhaust port area: 6.1 cm²; saturated lift: 6.75 mm
    // cylinder volume: 322.1 cm³ (19.66 CI); engine volume: 1.288 L (78.62 CI)
    // chamber volume: 36.2 cm³ (2.21 CI); conrod/stroke 1.704; N.A.C.C. 14.32
    // 8 harmonic intake runner length: 29.3 cm; diameter: 3.2 cm
    // primary length: 22.7 cm, area: 6.7 cm², diameter: 2.9 cm
    // collector diameter: 3.2 cm, area: 8.0 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 36.190 * units.cc,
        intake_runner_volume: 231.7 * units.cc,
        intake_runner_cross_section_area: 7.9 * units.cm2,
        exhaust_runner_volume: 77.2 * units.cc,
        exhaust_runner_cross_section_area: 6.7 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "VAZ-2108",
        starter_torque: 50 * units.Nm,
        starter_speed: 500 * units.rpm,
        redline: redline * units.rpm,
        fuel: s50(),
        throttle_gamma: 1.5,
        jitter: 1.0,
        noise: 0.3,
        hf_gain: 0.005,
        simulation_frequency: 12000 
    )

    wires wires()

    label crank_moment(disk_moment_of_inertia(mass: crank_mass * units.kg, radius: (stroke / 2.0) * units.mm))
    label flywheel_moment(disk_moment_of_inertia(mass: flywheel_mass * units.kg, radius: (stroke * 2.0) * units.mm))
    label other_moment(disk_moment_of_inertia(mass: 1 * units.kg, radius: 1.0 * units.cm))

    connecting_rod_parameters cr_params(
        mass: con_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.g,
            length: con_rod * units.mm
        ),
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    crankshaft c0(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: flywheel_mass * units.kg,
        mass: crank_mass * units.kg,
        friction_torque: 1.9 * units.Nm,
        moment_of_inertia: crank_moment + flywheel_moment + other_moment,
        position_x: 0.0,
        position_y: 0.0,
        tdc: 0.0
    )

    rod_journal rj0(angle: rot * 0 + rot90)
    rod_journal rj1(angle: rot * 3 + rot90)
    rod_journal rj2(angle: rot * 1 + rot90)
    rod_journal rj3(angle: rot * 2 + rot90)
    
    c0
        .add_rod_journal(rj0)
        .add_rod_journal(rj1)
        .add_rod_journal(rj2)
        .add_rod_journal(rj3)

    piston_parameters piston_params(
        mass: piston_mass * units.g,
        blowby: k_28inH2O(0.024),
        compression_height: compression_height,
        wrist_pin_position: 0.0,
        displacement: 0.0
    )

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    label collector_cross_section_area(8.0)

    label exhaust_pipe_length_0(250.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 100.0) // Litres
    
    label spacing_factor(1.1)
    label flange_density(1.0)

    intake intake(
        plenum_volume: 1.1 * units.L,
        plenum_cross_section_area: 15.8 * units.cm2,
        runner_length: 29.3 * units.cm,
        intake_flow_rate: k_carb(123.5),
        idle_flow_rate: k_carb(0.02),
        idle_throttle_plate_position: 0.9955,
        runner_flow_rate: k_carb(61.7),
        velocity_decay: 0.05
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(205.8),
        collector_cross_section_area: 8.0 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 22.7 * units.cm,
        primary_flow_rate: k_carb(102.9),
        velocity_decay: 1.0,
        volume: exhaust_volume_0 * units.L
    )

    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_55.wav", volume: 0.00025))
 
    cylinder_bank b0(bank_params, angle: 0)

    label pl0((3.2 * bore * spacing_factor / flange_density) * units.mm)
    label pl1((1.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl2((1.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl3((2.8 * bore * spacing_factor / flange_density) * units.mm)

    b0
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj0,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl0,
            ignition_wire: wires.wire1
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj1,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl1,
            ignition_wire: wires.wire2
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj2,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl2,
            ignition_wire: wires.wire3
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj3,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl3,
            ignition_wire: wires.wire4
        )

    engine
        .add_cylinder_bank(b0)

    engine.add_crankshaft(c0)

    camshaft_builder camshaft()

    b0.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_0,
            exhaust_camshaft: camshaft.exhaust_cam_0
        )
    )

    function timing_curve(383.3 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 4.0 * units.deg)
        .add_sample(383.3 * units.rpm, 4.0 * units.deg)
        .add_sample(766.7 * units.rpm, 7.1 * units.deg)
        .add_sample(1150.0 * units.rpm, 10.8 * units.deg)
        .add_sample(1533.3 * units.rpm, 13.9 * units.deg)
        .add_sample(1916.7 * units.rpm, 16.4 * units.deg)
        .add_sample(2300.0 * units.rpm, 18.4 * units.deg)
        .add_sample(2683.3 * units.rpm, 20.2 * units.deg)
        .add_sample(3066.7 * units.rpm, 21.5 * units.deg)
        .add_sample(3450.0 * units.rpm, 22.7 * units.deg)
        .add_sample(3833.3 * units.rpm, 23.6 * units.deg)
        .add_sample(4216.7 * units.rpm, 24.0 * units.deg)
        .add_sample(4600.0 * units.rpm, 24.0 * units.deg)
        .add_sample(4983.3 * units.rpm, 24.0 * units.deg)
        .add_sample(5366.7 * units.rpm, 24.0 * units.deg)
        .add_sample(5750.0 * units.rpm, 24.0 * units.deg)
        .add_sample(6133.3 * units.rpm, -15.0 * units.deg)
        .add_sample(6516.7 * units.rpm, -45.0 * units.deg)

    engine.add_ignition_module(
        eng_distributor(
            wires: wires,
            timing_curve: timing_curve
        )
    )
}

// ВАЗ-2101
public node veh {
    alias output __out: vehicle;

    label car_mass(1150) // 1030 empty; 1455 full

    vehicle vehicle(
        mass: car_mass * units.kg,
        drag_coefficient: 0.52,
        cross_sectional_area: (1611 * units.mm) * (1440 * units.mm),
        diff_ratio: 4.3,
        tire_radius: (600.0 / 2) * units.mm, // 6,15-13
        rolling_resistance: 0.012 * car_mass * 9.81
        )
}

// ВАЗ-2101
private node trn {
    alias output __out:
        transmission(
            max_clutch_torque: 150 * units.Nm,
            max_clutch_flex: 20 * units.deg,
            limit_clutch_flex: true,
            clutch_stiffness: 20 * units.Nm / units.deg,
            clutch_damping: 2.0,
            simulate_flex: true
        )
        .add_gear(3.753)
        .add_gear(2.303)
        .add_gear(1.493)
        .add_gear(1.000);
}

run(
    engine: eng(),
    transmission: trn(),
    vehicle: veh()
)
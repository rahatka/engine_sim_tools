private node add_lobes {
    input base;
    input shift: 0.0;
    input var;
    input this;
    alias output __out: this;

    this.add_lobe(base + var.s1 * rot_base + shift)
    this.add_lobe(base + var.s2 * rot_base + shift)
    this.add_lobe(base + var.s3 * rot_base + shift)
    this.add_lobe(base + var.s4 * rot_base + shift)
    this.add_lobe(base + var.s5 * rot_base + shift)
    this.add_lobe(base + var.s6 * rot_base + shift)
}

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node head {
    // port_flow.py v1.7
    // intake port area: 18.8 cm²; saturated lift: 11.93 mm
    // exhaust port area: 13.5 cm²; saturated lift: 10.11 mm
    // cylinder volume: 958.1 cm³ (58.47 CI); engine volume: 11.497 L (701.59 CI)
    // chamber volume: 147.4 cm³ (8.99 CI); conrod/stroke 2.007; N.A.C.C. 99.81
    // 32 harmonic intake runner length: 19.1 cm; diameter: 3.8 cm
    // primary length: 53.4 cm, area: 14.9 cm², diameter: 4.4 cm
    // collector diameter: 8.3 cm, area: 53.5 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 147.398 * units.cc,
        intake_runner_volume: 220.3 * units.cc,
        intake_runner_cross_section_area: 11.5 * units.cm2,
        exhaust_runner_volume: 66.7 * units.cc,
        exhaust_runner_cross_section_area: 14.9 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

public node bank_builder {
    input var;
    input crankshaft;
    input incline;
    input phasing;
    input ignition;
    input intake;
    input exhaust;
    input pls;
    input sh;
    input flip: false;

    output bank: _b0;

    ignition_wire _wire1()
    ignition_wire _wire2()
    ignition_wire _wire3()
    ignition_wire _wire4()
    ignition_wire _wire5()
    ignition_wire _wire6()

    piston_parameters piston_params(
        mass: piston_mass * units.g,
        blowby: k_28inH2O(0.036),
        compression_height: compression_height,
        wrist_pin_position: 0.0,
        displacement: 0.0
    )

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    connecting_rod_parameters cr_params(
        mass: con_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.g,
            length: con_rod * units.mm
        ),
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    camshaft _intake_cam_0(base_radius: 23.3 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 23.3 * units.mm, lobe_profile: exhaust_lobe_profile)

    _intake_cam_0.add_lobes(ILC, phasing, var)
    _exhaust_cam_0.add_lobes(ELC, phasing, var)

    ignition.connect_wire(_wire1, rot_base * var.s1 + phasing)
    ignition.connect_wire(_wire2, rot_base * var.s2 + phasing)
    ignition.connect_wire(_wire3, rot_base * var.s3 + phasing)
    ignition.connect_wire(_wire4, rot_base * var.s4 + phasing)
    ignition.connect_wire(_wire5, rot_base * var.s5 + phasing)
    ignition.connect_wire(_wire6, rot_base * var.s6 + phasing)

    cylinder_bank _b0(bank_params, angle: incline)

    label spacing_factor(1.1)
    label flange_density(1.0 * exhaust_delay_coeff)
    label pl0 ((5.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl1 ((4.2 * bore * spacing_factor / flange_density) * units.mm)
    label pl2 ((5.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl3 ((2.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl4 ((1.2 * bore * spacing_factor / flange_density) * units.mm)
    label pl5 ((2.0 * bore * spacing_factor / flange_density) * units.mm)

    _b0
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj0,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pls.pl0 + sh,
            ignition_wire: _wire1
        )
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj1,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pls.pl1 + sh,
            ignition_wire: _wire2
        )
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj2,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pls.pl2 + sh,
            ignition_wire: _wire3
        )
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj3,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pls.pl3 + sh,
            ignition_wire: _wire4
        )
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj4,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pls.pl4 + sh,
            ignition_wire: _wire5
        )
    .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: crankshaft.rj5,
            intake: intake,
            exhaust_system: exhaust,
            primary_length: pls.pl5 + sh,
            ignition_wire: _wire6
        )

    _b0.set_cylinder_head(
        head(
            intake_camshaft: _intake_cam_0,
            exhaust_camshaft: _exhaust_cam_0,
            flip_display: flip
        )
    )
}

public node crank {
    input phasing;
    input var;

    output c: _c;
    output rj0: _rj0;
    output rj1: _rj1;
    output rj2: _rj2;
    output rj3: _rj3;
    output rj4: _rj4;
    output rj5: _rj5;

    label crank_moment(disk_moment_of_inertia(mass: crank_mass * units.kg, radius: (stroke / 2.0) * units.mm))
    label flywheel_moment(disk_moment_of_inertia(mass: flywheel_mass * units.kg, radius: (stroke * 3.5) * units.mm))
    label other_moment(disk_moment_of_inertia(mass: 1 * units.kg, radius: 1.0 * units.cm))

    crankshaft _c(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: flywheel_mass * units.kg,
        mass: crank_mass * units.kg,
        friction_torque: 17.2 * units.Nm,
        moment_of_inertia: crank_moment + flywheel_moment + other_moment,
        tdc: 0.0
    )

    rod_journal _rj0(angle: rot_base * var.s1 + rot90 + vee / 2)
    rod_journal _rj1(angle: rot_base * var.s2 + rot90 + vee / 2)
    rod_journal _rj2(angle: rot_base * var.s3 + rot90 + vee / 2)
    rod_journal _rj3(angle: rot_base * var.s4 + rot90 + vee / 2)
    rod_journal _rj4(angle: rot_base * var.s5 + rot90 + vee / 2)
    rod_journal _rj5(angle: rot_base * var.s6 + rot90 + vee / 2)

    _c
        .add_rod_journal(_rj0)
        .add_rod_journal(_rj1)
        .add_rod_journal(_rj2)
        .add_rod_journal(_rj3)
        .add_rod_journal(_rj4)
        .add_rod_journal(_rj5)
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: variant.seq,
        starter_torque: 250 * units.Nm,
        starter_speed: 500 * units.rpm,
        redline: redline * units.rpm,
        fuel: s50(),
        throttle_gamma: 1.5,
        jitter: 0.8,
        noise: 0.02,
        hf_gain: 0.008,
        simulation_frequency: 8000,
        dyno_min_speed: 500 * units.rpm
    )

    ////////////////////////////////////////
    ///////// change variants here /////////
    ////////////////////////////////////////
    variant_0 variant()

    crank c0(vee / 2 + rot90, variant)

    es1 es()

    bank_builder bank0(
        var: variant,
        crankshaft: c0,
        incline: vee / 2,
        phasing: 0,
        ignition: ignition,
        intake: intake,
        exhaust: es.es0,
        pls: es,
        sh: es.sh0,
        flip: true
    )

    bank_builder bank1(
        var: variant,
        crankshaft: c0,
        incline: -vee / 2,
        phasing: vee + rot360,
        ignition: ignition,
        intake: intake,
        exhaust: es.es1,
        pls: es,
        sh: es.sh1
    )

    ignition_module ignition(timing_curve: timing_curve, rev_limit: redline * 1.05 * units.rpm, limiter_duration: 0.05)
    
    label spacing_factor(1.1)

    intake intake(
        plenum_volume: 4.3 * units.L,
        plenum_cross_section_area: 69.2 * units.cm2,
        runner_length: 19.1 * units.cm,
        intake_flow_rate: k_carb(553.3),
        idle_flow_rate: k_carb(0.001),
        idle_throttle_plate_position: 0.996,
        runner_flow_rate: k_carb(96.8),
        velocity_decay: 0.05
    )
    
    function timing_curve(183.3 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 2.0 * units.deg)
        .add_sample(183.3 * units.rpm, 2.0 * units.deg)
        .add_sample(366.7 * units.rpm, 2.0 * units.deg)
        .add_sample(550.0 * units.rpm, 4.8 * units.deg)
        .add_sample(733.3 * units.rpm, 13.2 * units.deg)
        .add_sample(916.7 * units.rpm, 19.2 * units.deg)
        .add_sample(1100.0 * units.rpm, 23.4 * units.deg)
        .add_sample(1283.3 * units.rpm, 26.5 * units.deg)
        .add_sample(1466.7 * units.rpm, 28.6 * units.deg)
        .add_sample(1650.0 * units.rpm, 30.2 * units.deg)
        .add_sample(1833.3 * units.rpm, 31.3 * units.deg)
        .add_sample(2016.7 * units.rpm, 32.0 * units.deg)
        .add_sample(2200.0 * units.rpm, 32.0 * units.deg)
        .add_sample(2383.3 * units.rpm, 32.0 * units.deg)
        .add_sample(2566.7 * units.rpm, 32.0 * units.deg)
        .add_sample(2750.0 * units.rpm, 32.0 * units.deg)
        .add_sample(2933.3 * units.rpm, -12.5 * units.deg)
        .add_sample(3116.7 * units.rpm, -25.0 * units.deg)

    engine
        .add_crankshaft(c0.c)
        .add_cylinder_bank(bank0.bank)
        .add_cylinder_bank(bank1.bank)
        .add_ignition_module(ignition)
}

// two pipes
private node es0{
    output es0: exhaust0;
    output es1: exhaust1;
    output pl0: _pl0;
    output pl1: _pl1;
    output pl2: _pl2;
    output pl3: _pl3;
    output pl4: _pl4;
    output pl5: _pl5;
    output sh0: _sh0;
    output sh1: _sh1;

    label flange_density(1.0 * exhaust_delay_coeff)
    label collector_cross_section_area(26.8)
    label exhaust_pipe_length_0(425.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 100.0) // Litres
    label exhaust_pipe_length_1(400.0) // cm
    label exhaust_volume_1(collector_cross_section_area * exhaust_pipe_length_1 / 100.0) // Litres
    label spacing_factor(1.3)

    float _pl0 ((5.0 * bore * spacing_factor / flange_density) * units.mm)
    float _pl1 ((4.2 * bore * spacing_factor / flange_density) * units.mm)
    float _pl2 ((5.0 * bore * spacing_factor / flange_density) * units.mm)
    float _pl3 ((2.0 * bore * spacing_factor / flange_density) * units.mm)
    float _pl4 ((1.2 * bore * spacing_factor / flange_density) * units.mm)
    float _pl5 ((2.0 * bore * spacing_factor / flange_density) * units.mm)
    float _sh0 (0 * units.mm)
    float _sh1 (0 * units.mm)

    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(472.6),
        collector_cross_section_area: 26.8 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 53.4 * units.cm,
        primary_flow_rate: k_carb(161.5),
        velocity_decay: 1.0,
        volume: (exhaust_volume_0 + 5.0) * units.L
    )
    
    exhaust_system_parameters es_params1(
        outlet_flow_rate: k_carb(472.6),
        collector_cross_section_area: 26.8 * units.cm2,
        length: exhaust_pipe_length_1 * units.cm,
        primary_tube_length: 53.4 * units.cm,
        primary_flow_rate: k_carb(161.5),
        velocity_decay: 1.0,
        volume: (exhaust_volume_1 + 5.0) * units.L
    )

    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_40.wav", volume: 0.0012))
    exhaust_system exhaust1(es_params1, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_40.wav", volume: 0.0010))
}

// one pipe
private node es1{
    output es0: exhaust0;
    output es1: exhaust0;
    output pl0: _pl0;
    output pl1: _pl1;
    output pl2: _pl2;
    output pl3: _pl3;
    output pl4: _pl4;
    output pl5: _pl5;
    output sh0: _sh0;
    output sh1: _sh1;

    label collector_cross_section_area(53.5)
    label flange_density(1.0 * exhaust_delay_coeff)
    label exhaust_pipe_length_0(425.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 100.0) // Litres
    label spacing_factor(1.3)

    float _pl0 ((5.0 * bore * spacing_factor / flange_density) * units.mm)
    float _pl1 ((4.2 * bore * spacing_factor / flange_density) * units.mm)
    float _pl2 ((5.0 * bore * spacing_factor / flange_density) * units.mm)
    float _pl3 ((2.0 * bore * spacing_factor / flange_density) * units.mm)
    float _pl4 ((1.2 * bore * spacing_factor / flange_density) * units.mm)
    float _pl5 ((2.0 * bore * spacing_factor / flange_density) * units.mm)
    float _sh0 (0 * units.mm)
    float _sh1 ((610.0 / exhaust_delay_coeff) * units.mm)

    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(945.2),
        collector_cross_section_area: 53.5 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 53.4 * units.cm,
        primary_flow_rate: k_carb(161.5),
        velocity_decay: 1.0,
        volume: (exhaust_volume_0 + 10.0) * units.L
    )

    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_55.wav", volume: 0.00025))
}

// МАЗ-504

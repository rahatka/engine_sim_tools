import "engine_sim.mr"
import "../../fuel/50.mr"

units units()
constants constants()

label bore(115.824)
label stroke(90.932)
label compression_ratio(7.5)
label con_rod(182.5) // 7.185 ?
label compression_height(50.0 * units.mm) // ~
label intake_valve_diameter(54.7687) // 2 5/32
label exhaust_valve_diameter(46.4344) // 1 53/64
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(2750.0) // RPM

label cyl(12.0)
label cyl_base(cyl / row)
label row(2.0)
label cycle(720.0 * units.deg)
label vee(60.0 * units.deg)
label rot(cycle / cyl)
label rot_base(cycle / cyl_base)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)

// GMC 702
label IVL(12.7) // ? .50
label EVL(12.7) // ? .50
label IVO(11.0 * units.deg) //BTDC
label IVC(41.0 * units.deg) //ABDC
label EVO(50.0 * units.deg) //BBDC
label EVC(10.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(1255.0) // ? g
label piston_mass(700.0) // ?
label crank_mass(81.6466) // kg
label flywheel_mass(15.0) // kg

label exhaust_delay_coeff(1.5)


public node veh {
    alias output __out: vehicle;
    vehicle vehicle(
        mass: 650  * units.kg,
        drag_coefficient: 0.38,
        cross_sectional_area: (1395 * units.mm) * (1450 * units.mm),
        diff_ratio: 4.79,
        tire_radius: (519.2 / 2) * units.mm,
        rolling_resistance: 20
        )
}

// ZAZ-965
private node trn {
    alias output __out:
        transmission(
            max_clutch_torque: 100 * units.Nm,
            max_clutch_flex: 20 * units.deg,
            limit_clutch_flex: true,
            clutch_stiffness: 10 * units.Nm / units.deg,
            clutch_damping: 1.0,
            simulate_flex: true
        )
        .add_gear(3.83)
        .add_gear(2.29)
        .add_gear(1.39)
        .add_gear(0.897);
}

run(
    engine: eng(),
    transmission: trn(),
    vehicle: veh()
)
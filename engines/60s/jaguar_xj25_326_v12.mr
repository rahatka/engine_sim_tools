import "../../fuel/50.mr"

units units()
constants constants()

label bore(90.0)
label stroke(70.0)
label compression_ratio(9.0)
label con_rod(151.3)
label compression_height(39.624 * units.mm)
label intake_valve_diameter(41.3)
label exhaust_valve_diameter(34.6)
label intake_valves(1.0)
label exhaust_valves(1.0)

label redline(7000.0) // RPM

label cyl(12.0)
label cyl_base(cyl / row)
label row(2.0)
label cycle(720.0 * units.deg)
label rot(cycle / cyl)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label rot_base(cycle / cyl_base)
label vee(60.0 * units.deg)

// https://forums.jag-lovers.com/t/xjs-v12-camshaft-c41249/367629/6
label IVL(9.525) // .375
label EVL(9.525) // .375
label IVO(0.0 * units.deg) //BTDC
label IVC(40.0 * units.deg) //ABDC
label EVO(41.0 * units.deg) //BBDC
label EVC(1.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(500.0) // ? g
label piston_mass(400.0) // ?
label crank_mass(25.0) // ?
label flywheel_mass(7.0) // kg

label exhaust_delay_coeff(1.5)


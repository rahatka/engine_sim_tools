import "_fuel_60.mr"

units units()
constants constants()

label bore(130.175) // 5.125
label stroke(98.044) // 3.86
label compression_ratio(7.5)
label con_rod(190.5) // 7.5 ?
label compression_height(54.5 * units.mm) // ~
label intake_valve_diameter(60.325) // 2 3/8
label exhaust_valve_diameter(47.625) // 1 7/8
label intake_valves(1.0)
label exhaust_valves(1.0)

label cyl(6.0)
label row(2.0)
label cycle(720.0 * units.deg)
label rot90(90.0 * units.deg)
label rot180(180.0 * units.deg)
label rot360(360.0 * units.deg)
label rot(cycle / cyl)
label cyl_base(cyl / row)
label rot_base(cycle / cyl_base)
label vee(60.0 * units.deg)

label comp(vee - rot)

label redline(3600.0) // RPM

// GMC 637 V8
label IVL(12.7) // .5 ?
label EVL(12.7) // .5 ?
label IVO(27.0 * units.deg) //BTDC
label IVC(61.0 * units.deg) //ABDC
label EVO(82.0 * units.deg) //BBDC
label EVC(36.0 * units.deg) //ATDC

label intake_duration(IVO + IVC + rot180)
label exhaust_duration(EVO + EVC + rot180)

label ILC((rot180 - IVO + IVC) / 2.0 + rot360)
label ELC((rot180 - EVO + EVC) / 2.0 + rot180)

label con_rod_mass(1255.0) // ? g
label piston_mass(700.0) // ?
label crank_mass(40.0) // ? kg
label flywheel_mass(6.0) // kg

label exhaust_delay_coeff(1.5)


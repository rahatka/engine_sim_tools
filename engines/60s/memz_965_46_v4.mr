public node camshaft_builder {
    output intake_cam_0: _intake_cam_0;
    output exhaust_cam_0: _exhaust_cam_0;

    output intake_cam_1: _intake_cam_1;
    output exhaust_cam_1: _exhaust_cam_1;

    camshaft _intake_cam_0(base_radius: 12.2 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 12.2 * units.mm, lobe_profile: exhaust_lobe_profile)
    
    camshaft _intake_cam_1(base_radius: 12.2 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_1(base_radius: 12.2 * units.mm, lobe_profile: exhaust_lobe_profile)

    _intake_cam_0
        .add_lobe(ILC + 0 * rot)
        .add_lobe(ILC + 3 * rot)
    _exhaust_cam_0
        .add_lobe(ELC + 0 * rot)
        .add_lobe(ELC + 3 * rot)
        
    _intake_cam_1
        .add_lobe(ILC + 1 * rot)
        .add_lobe(ILC + 2 * rot)
    _exhaust_cam_1
        .add_lobe(ELC + 1 * rot)
        .add_lobe(ELC + 2 * rot)
}

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node head {
    // port_flow.py v1.7
    // intake port area: 4.3 cm²; saturated lift: 5.72 mm
    // exhaust port area: 3.9 cm²; saturated lift: 5.40 mm
    // cylinder volume: 186.5 cm³ (11.38 CI); engine volume: 0.746 L (45.51 CI)
    // chamber volume: 33.9 cm³ (2.07 CI); conrod/stroke 2.110; N.A.C.C. 10.80
    // 32 harmonic intake runner length: 11.4 cm; diameter: 2.1 cm
    // primary length: 31.9 cm, area: 4.1 cm², diameter: 2.3 cm
    // collector diameter: 3.2 cm, area: 8.1 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 33.901 * units.cc,
        intake_runner_volume: 37.8 * units.cc,
        intake_runner_cross_section_area: 3.3 * units.cm2,
        exhaust_runner_volume: 14.0 * units.cc,
        exhaust_runner_cross_section_area: 4.1 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "MeMZ-965",
        starter_torque: 25 * units.Nm,
        starter_speed: 500 * units.rpm,
        redline: redline * units.rpm,
        fuel: s50(),
        throttle_gamma: 1.5,
        jitter: 0.8,
        noise: 0.2,
        hf_gain: 0.005,
        simulation_frequency: 12000
    )

    wires wires()

    connecting_rod_parameters cr_params(
        mass: con_rod_mass * units.g,
        moment_of_inertia: rod_moment_of_inertia(
            mass: con_rod_mass * units.g,
            length: con_rod * units.mm
        ),
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    label crank_moment(disk_moment_of_inertia(mass: crank_mass * units.kg, radius: (stroke / 2.0) * units.mm))
    label flywheel_moment(disk_moment_of_inertia(mass: flywheel_mass * units.kg, radius: (stroke * 3.5) * units.mm))
    label other_moment(disk_moment_of_inertia(mass: 1 * units.kg, radius: 1.0 * units.cm))

    crankshaft c0(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: flywheel_mass * units.kg,
        mass: crank_mass * units.kg,
        friction_torque: 1.1 * units.Nm,
        moment_of_inertia: crank_moment + flywheel_moment + other_moment,
        position_x: 0.0,
        position_y: 0.0,
        tdc: 0.0
    )

    rod_journal rj0(angle: rot * 0 + rot90 + vee / 2)
    rod_journal rj1(angle: rot * 3 + rot90 - vee / 2)
    rod_journal rj2(angle: rot * 1 + rot90 + vee / 2)
    rod_journal rj3(angle: rot * 2 + rot90 - vee / 2)
    
    c0
        .add_rod_journal(rj0)
        .add_rod_journal(rj1)
        .add_rod_journal(rj2)
        .add_rod_journal(rj3)

    piston_parameters piston_params(
        mass: 250.0 * units.g,
        blowby: k_28inH2O(0.021),
        compression_height: compression_height,
        wrist_pin_position: 0.0,
        displacement: 0.0
    )

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    label collector_cross_section_area(8.1)

    label exhaust_pipe_length_0(100.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 1000.0) // Litres
    
    label spacing_factor(1.5)
    label flange_density(1.0)
    

    intake intake(
        plenum_volume: 0.4 * units.L,
        plenum_cross_section_area: 6.7 * units.cm2,
        runner_length: 11.4 * units.cm,
        intake_flow_rate: k_carb(55.9),
        idle_flow_rate: k_carb(0.01),
        idle_throttle_plate_position: 0.994,
        runner_flow_rate: k_carb(28.0),
        velocity_decay: 0.05
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(93.2),
        collector_cross_section_area: 8.1 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 31.9 * units.cm,
        primary_flow_rate: k_carb(46.6),
        velocity_decay: 1.0,
        volume: (exhaust_volume_0 + 2.0) * units.L
    )

    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_51.wav", volume: 0.00025))
 
    cylinder_bank b0(bank_params, angle: vee / 2)
    cylinder_bank b1(bank_params, angle: -vee / 2)
    
    label sh(40.0 / flange_density * units.cm)
    label pl0 ((1.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl1 ((4.0 * bore * spacing_factor / flange_density) * units.mm)

    b0
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj0,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl0,
            ignition_wire: wires.wire1
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj2,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl1,
            ignition_wire: wires.wire2
        )
        
    b1
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj1,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl0 + sh,
            ignition_wire: wires.wire3
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj3,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl1 + sh,
            ignition_wire: wires.wire4
        )

    engine
        .add_cylinder_bank(b0)
        .add_cylinder_bank(b1)

    engine.add_crankshaft(c0)

    camshaft_builder camshaft()

    b0.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_0,
            exhaust_camshaft: camshaft.exhaust_cam_0,
            flip_display: true
        )
    )
    b1.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_1,
            exhaust_camshaft: camshaft.exhaust_cam_1
        )
    )

    function timing_curve(300.0 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 2.0 * units.deg)
        .add_sample(300.0 * units.rpm, 2.0 * units.deg)
        .add_sample(600.0 * units.rpm, 2.0 * units.deg)
        .add_sample(900.0 * units.rpm, 4.3 * units.deg)
        .add_sample(1200.0 * units.rpm, 8.2 * units.deg)
        .add_sample(1500.0 * units.rpm, 11.4 * units.deg)
        .add_sample(1800.0 * units.rpm, 14.1 * units.deg)
        .add_sample(2100.0 * units.rpm, 16.2 * units.deg)
        .add_sample(2400.0 * units.rpm, 18.0 * units.deg)
        .add_sample(2700.0 * units.rpm, 19.5 * units.deg)
        .add_sample(3000.0 * units.rpm, 20.6 * units.deg)
        .add_sample(3300.0 * units.rpm, 21.6 * units.deg)
        .add_sample(3600.0 * units.rpm, 22.4 * units.deg)
        .add_sample(3900.0 * units.rpm, 23.1 * units.deg)
        .add_sample(4200.0 * units.rpm, 23.6 * units.deg)
        .add_sample(4500.0 * units.rpm, 24.1 * units.deg)
        .add_sample(4800.0 * units.rpm, -12.5 * units.deg)
        .add_sample(5100.0 * units.rpm, -25.0 * units.deg)

    engine.add_ignition_module(
        eng_distributor(
            wires: wires,
            timing_curve: timing_curve
        )
    )
}

// ZAZ-965
public node veh {
    alias output __out: vehicle;
    vehicle vehicle(
        mass: 650  * units.kg,
        drag_coefficient: 0.38,
        cross_sectional_area: (1395 * units.mm) * (1450 * units.mm),
        diff_ratio: 4.79,
        tire_radius: (519.2 / 2) * units.mm,
        rolling_resistance: 20
        )
}

// ZAZ-965
private node trn {
    alias output __out:
        transmission(
            max_clutch_torque: 100 * units.Nm,
            max_clutch_flex: 20 * units.deg,
            limit_clutch_flex: true,
            clutch_stiffness: 10 * units.Nm / units.deg,
            clutch_damping: 1.0,
            simulate_flex: true
        )
        .add_gear(3.83)
        .add_gear(2.29)
        .add_gear(1.39)
        .add_gear(0.897);
}

run(
    engine: eng(),
    transmission: trn(),
    vehicle: veh()
)
public node camshaft_builder {
    output intake_cam_0: _intake_cam_0;
    output exhaust_cam_0: _exhaust_cam_0;

    output intake_cam_1: _intake_cam_1;
    output exhaust_cam_1: _exhaust_cam_1;

    camshaft _intake_cam_0(base_radius: 12.3 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_0(base_radius: 12.3 * units.mm, lobe_profile: exhaust_lobe_profile)
    
    camshaft _intake_cam_1(base_radius: 12.3 * units.mm, lobe_profile: intake_lobe_profile)
    camshaft _exhaust_cam_1(base_radius: 12.3 * units.mm, lobe_profile: exhaust_lobe_profile)

    _intake_cam_0
        .add_lobe(ILC + 0 * rot)
        .add_lobe(ILC + 1 * rot)
    _exhaust_cam_0
        .add_lobe(ELC + 0 * rot)
        .add_lobe(ELC + 1 * rot)
        
    _intake_cam_1
        .add_lobe(ILC + 2 * rot)
        .add_lobe(ILC + 3 * rot)
    _exhaust_cam_1
        .add_lobe(ELC + 2 * rot)
        .add_lobe(ELC + 3 * rot)
}

private node add_flow_sample {
    input lift;
    input flow;
    input this;
    alias output __out: this;

    this.add_sample(lift * units.mm, k_28inH2O(flow))
}

public node head {
    // port_flow.py v1.7
    // intake port area: 7.8 cm²; saturated lift: 7.67 mm
    // exhaust port area: 6.3 cm²; saturated lift: 6.89 mm
    // cylinder volume: 321.3 cm³ (19.61 CI); engine volume: 1.285 L (78.43 CI)
    // chamber volume: 51.0 cm³ (3.11 CI); conrod/stroke 1.988; N.A.C.C. 14.70
    // 16 harmonic intake runner length: 16.7 cm; diameter: 3.2 cm
    // primary length: 22.4 cm, area: 6.6 cm², diameter: 2.9 cm
    // collector diameter: 3.2 cm, area: 7.9 cm²

    input intake_camshaft;
    input exhaust_camshaft;
    input flip_display: false;
    
    alias output __out: head;
    generic_cylinder_head head(
        chamber_volume: 51.001 * units.cc,
        intake_runner_volume: 133.7 * units.cc,
        intake_runner_cross_section_area: 8.0 * units.cm2,
        exhaust_runner_volume: 44.6 * units.cc,
        exhaust_runner_cross_section_area: 6.6 * units.cm2,

        intake_port_flow: intake_flow,
        exhaust_port_flow: exhaust_flow,
        valvetrain: standard_valvetrain(
            intake_camshaft: intake_camshaft,
            exhaust_camshaft: exhaust_camshaft
        ),
        flip_display: flip_display
    )
}

public node eng {
    alias output __out: engine;

    engine engine(
        name: "VW Beetle 1300",
        starter_torque: 50 * units.Nm,
        starter_speed: 500 * units.rpm,
        redline: redline * units.rpm,
        fuel: s50(),
        throttle_gamma: 1.5,
        jitter: 0.9,
        noise: 0.1,
        simulation_frequency: 10000
    )

    wires wires()

    crankshaft c0(
        throw: (stroke / 2.0) * units.mm,
        flywheel_mass: 2.0 * units.kg,
        mass: 6.0 * units.kg,
        friction_torque: 1.9 * units.Nm,
        moment_of_inertia: 0.11,
        tdc: 0.0
    )

    rod_journal rj0(angle: rot * 0 + rot90 + vee / 2)
    rod_journal rj1(angle: rot * 1 + rot90 + vee / 2)
    rod_journal rj2(angle: rot * 2 + rot90 - vee / 2)
    rod_journal rj3(angle: rot * 3 + rot90 - vee / 2)
    
    c0
        .add_rod_journal(rj0)
        .add_rod_journal(rj1)
        .add_rod_journal(rj2)
        .add_rod_journal(rj3)

    piston_parameters piston_params(
        mass: 305.0 * units.g,
        blowby: k_28inH2O(0.024),
        compression_height: compression_height
    )

    connecting_rod_parameters cr_params(
        mass: 438.0 * units.g,
        moment_of_inertia: 0.001,
        center_of_mass: 0.0,
        length: con_rod * units.mm
    )

    cylinder_bank_parameters bank_params(
        bore: bore * units.mm,
        deck_height: (con_rod + stroke / 2.0) * units.mm + compression_height
    )

    label collector_cross_section_area(7.9)

    label exhaust_pipe_length_0(120.0) // cm
    label exhaust_volume_0(collector_cross_section_area * exhaust_pipe_length_0 / 100.0) // Litres
    
    label spacing_factor(1.2)
    label flange_density(1.0)

    intake intake(
        plenum_volume: 1.1 * units.L,
        plenum_cross_section_area: 16.1 * units.cm2,
        intake_flow_rate: k_carb(101.7),
        idle_flow_rate: k_carb(0.001),
        idle_throttle_plate_position: 0.996,
        runner_flow_rate: k_carb(50.9),
        runner_length: 16.7 * units.cm,
        velocity_decay: 0.05
    )
    
    exhaust_system_parameters es_params0(
        outlet_flow_rate: k_carb(169.6),
        collector_cross_section_area: 7.9 * units.cm2,
        length: exhaust_pipe_length_0 * units.cm,
        primary_tube_length: 22.4 * units.cm,
        primary_flow_rate: k_carb(84.8),
        velocity_decay: 1.0,
        volume: (exhaust_volume_0 + 3.0) * units.L
    )

    exhaust_system exhaust0(es_params0, impulse_response: impulse_response(filename: "../../impulse_responses/smooth_55.wav", volume: 0.00025))
 
    cylinder_bank b0(bank_params, angle: vee / 2)
    cylinder_bank b1(bank_params, angle: -vee / 2)
    
    label sh(4.0 * units.cm)
    label pl0 ((1.0 * bore * spacing_factor / flange_density) * units.mm)
    label pl1 ((5.0 * bore * spacing_factor / flange_density) * units.mm)

    b0
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj0,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl0,
            ignition_wire: wires.wire1
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj2,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl1,
            ignition_wire: wires.wire2
        )
        
    b1
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj1,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl0 + sh,
            ignition_wire: wires.wire3
        )
        .add_cylinder(
            piston: piston(piston_params),
            connecting_rod: connecting_rod(cr_params),
            rod_journal: rj3,
            intake: intake,
            exhaust_system: exhaust0,
            primary_length: pl1 + sh,
            ignition_wire: wires.wire4
        )

    engine
        .add_cylinder_bank(b0)
        .add_cylinder_bank(b1)

    engine.add_crankshaft(c0)

    camshaft_builder camshaft()

    b0.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_0,
            exhaust_camshaft: camshaft.exhaust_cam_0,
            flip_display: true
        )
    )
    b1.set_cylinder_head (
        head(
            intake_camshaft: camshaft.intake_cam_1,
            exhaust_camshaft: camshaft.exhaust_cam_1
        )
    )

    function timing_curve(316.7 * units.rpm)
    timing_curve
        .add_sample(0.0 * units.rpm, 5.0 * units.deg)
        .add_sample(316.7 * units.rpm, 5.0 * units.deg)
        .add_sample(633.3 * units.rpm, 6.5 * units.deg)
        .add_sample(950.0 * units.rpm, 9.8 * units.deg)
        .add_sample(1266.7 * units.rpm, 12.7 * units.deg)
        .add_sample(1583.3 * units.rpm, 15.3 * units.deg)
        .add_sample(1900.0 * units.rpm, 17.5 * units.deg)
        .add_sample(2216.7 * units.rpm, 19.5 * units.deg)
        .add_sample(2533.3 * units.rpm, 21.2 * units.deg)
        .add_sample(2850.0 * units.rpm, 22.7 * units.deg)
        .add_sample(3166.7 * units.rpm, 24.0 * units.deg)
        .add_sample(3483.3 * units.rpm, 25.2 * units.deg)
        .add_sample(3800.0 * units.rpm, 26.2 * units.deg)
        .add_sample(4116.7 * units.rpm, 27.1 * units.deg)
        .add_sample(4433.3 * units.rpm, 27.9 * units.deg)
        .add_sample(4750.0 * units.rpm, 28.6 * units.deg)
        .add_sample(5066.7 * units.rpm, -12.5 * units.deg)
        .add_sample(5383.3 * units.rpm, -25.0 * units.deg)

    engine.add_ignition_module(
        eng_distributor(
            wires: wires,
            timing_curve: timing_curve
        )
    )
}

